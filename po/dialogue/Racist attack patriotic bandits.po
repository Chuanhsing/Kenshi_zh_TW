#. Id: 42645-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Skeleton
#: 42645-Dialogue.mod:0
msgid "/OI/, /RACISTNAME/! !"
msgstr ""

#. Id: 42646-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42645-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 42646-Dialogue.mod:0
msgid "No /RACISTNAME/s allowed... This is OUR land..."
msgstr ""

#. Id: 42647-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42646-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 42647-Dialogue.mod:0
msgid "Go back to the junkyard! We don't want you here!"
msgstr "回到垃圾場！我們不要你在這裡！"

#. Id: 42647-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42646-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 42647-Dialogue.mod:1
msgid "Go back to the junk heap! You ain't welcome here!"
msgstr "回到垃圾堆！你在這裡不受歡迎！"

#. Id: 42648-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42647-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 42648-Dialogue.mod:0
msgid "/GODDAMN/ freak!"
msgstr ""

#. Id: 54645-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42645-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54645-Dialogue.mod:0
msgid "Where's your soul?"
msgstr "你的靈魂在哪裡？"

#. Id: 54646-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (54645-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54646-Dialogue.mod:0
msgid "Oh yeah, that's right, YOU DON'T HAVE ONE, /QUICKLAUGH/!"
msgstr ""

#. Id: 54648-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (54647-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#. Id: 54572-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (54571-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54648-Dialogue.mod:0 54572-Dialogue.mod:0
msgid "/LAUGH/! Good one, boss..."
msgstr ""

#. Id: 54654-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (54648-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54654-Dialogue.mod:0
msgid "Ahh that was a low blow, eh? Hurts, eh?... Well, don't cry, /RACISTNAME/..."
msgstr ""

#. Id: 54655-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (54654-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54655-Dialogue.mod:0
msgid "Wait, I almost forgot... YOU CAN'T CRY, /LAUGH/!"
msgstr ""

#. Id: 54656-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (54655-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54656-Dialogue.mod:0
msgid "/LAUGH/! Stop... stop I can't take no more, boss... *wheeze*"
msgstr ""

#. Id: 54649-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (54656-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54649-Dialogue.mod:0
msgid "Anyway, freak woes aside, do one, /RACISTNAME/! You ain't welcome here!"
msgstr ""

#. Id: 54650-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42645-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54650-Dialogue.mod:0
msgid "When did ya last get laid?"
msgstr "你什麼時候最後得到的？"

#. Id: 54651-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (54650-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54651-Dialogue.mod:0
msgid "Oh yeah, that's right, YOU GOT NO JUNK! Junk, get it? /LAUGH/!"
msgstr ""

#. Id: 54536-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 54536-Dialogue.mod:0
msgid "Is that... Is that a /RACISTNAME/ in OUR outlands?"
msgstr ""

#. Id: 54536-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 54536-Dialogue.mod:1
msgid "Well would'ya look at that. We got us a /RACISTNAME/ takin' a walk in OUR desert, eh?"
msgstr ""

#. Id: 54536-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 54536-Dialogue.mod:2
msgid "Yet ANOTHER /RACISTNAME/ in OUR United Cities, eh?"
msgstr ""

#. Id: 54536-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 54536-Dialogue.mod:3
msgid "Ugh! Another /RACISTNAME/ in our land... One o' these days they're gonna take over all together..."
msgstr ""

#. Id: 54536-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 54536-Dialogue.mod:4
msgid "Tch. Another /RACISTNAME/ to clog our great cities..."
msgstr ""

#. Id: 54537-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54537-Dialogue.mod:0
msgid "Go back to the mud hives where you belong, /RACISTNAME/!"
msgstr ""

#. Id: 54537-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54537-Dialogue.mod:1
msgid "Why don't you go back to the dirt hive where you crawled out from, /RACISTNAME/!"
msgstr ""

#. Id: 54537-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54537-Dialogue.mod:2
msgid "We're clean folk in the United Cities! Go back to your own filthy hive village and muddy that up instead, eh, /RACISTNAME/!"
msgstr ""

#. Id: 54541-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:0
msgid "I'm sick o' these /DIRTY/ tribals bringin' down our proud cities!"
msgstr ""

#. Id: 54541-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:1
msgid "I'm getting tired o' seeing these twig men takin' over our proud Empire cities!"
msgstr "我看到這些樹枝男人在我們驕傲的帝國城市上tak！！！！！！！！！"

#. Id: 54541-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:2
msgid "I'm sick o' these bug men /RACIST_CLAIM1/ our /RACIST_CLAIM2/! This is OUR land!"
msgstr ""

#. Id: 54541-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:3
msgid "I'm fed up o' these /DIRTY/ tribals bringin' down our noble cities!"
msgstr ""

#. Id: 54541-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:4
msgid "I'm getting tired o' seeing these insect people takin' over our proud Empire cities!"
msgstr "我看到這些昆蟲們在我們自豪的帝國城市上tak！getting！！！！！"

#. Id: 54541-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:5
msgid "I'm getting tired o' seeing these bug freaks takin' over our noble Empire cities!"
msgstr "我已經厭倦了，看到這些蟲子們在我們崇高的帝國城市上tak！！"

#. Id: 54541-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (54544-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#: 54541-Dialogue.mod:6
msgid "I'm sick o' these insect people /RACIST_CLAIM1/ our /RACIST_CLAIM2/! This is OUR nation!"
msgstr ""

#. Id: 54545-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (54541-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54545-Dialogue.mod:0
msgid "Too right, /NAME/! Then we'll just have to take this /RACISTNAME/ infestation into our own hands, eh?"
msgstr ""

#. Id: 54545-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (54541-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54545-Dialogue.mod:1
msgid "Rightly said, /NAME/! I say we take this VERMIN problem into our own hands!"
msgstr ""

#. Id: 54545-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (54541-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54545-Dialogue.mod:2
msgid "Well said, /NAME/! Now, how about we take pest control of these freaks into our own hands!?"
msgstr ""

#. Id: 54581-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (54547-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#: 54581-Dialogue.mod:0
msgid "/QUICKLAUGH/, then allow me to start!"
msgstr ""

#. Id: 54581-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (54547-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#: 54581-Dialogue.mod:1
msgid "I'm already on it boss, /LAUGH/!"
msgstr ""

#. Id: 54548-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (54581-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. Id: 65019-october 17.mod
#. -- Index: 7.0
#. -- Parent line: 6 (54581-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IN_A_NON_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54548-Dialogue.mod:0 65019-october?17.mod:0
msgid "Oh ain't nothing better than bashin' in /RACISTNAME/ skulls!"
msgstr ""

#. Id: 54548-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (54581-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. Id: 65019-october 17.mod
#. -- Index: 7.1
#. -- Parent line: 6 (54581-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IN_A_NON_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54548-Dialogue.mod:1 65019-october?17.mod:1
msgid "Ain't nothing better than /STABBIN/ freaks like you, /RACISTNAME/!"
msgstr ""

#. Id: 54551-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (54545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54551-Dialogue.mod:0
msgid "You can't run from us, /RACISTNAME/! Get over 'ere!"
msgstr ""

#. Id: 54551-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (54545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54551-Dialogue.mod:1
msgid "Yeah, just you try runnin', /RACISTNAME/! We'll get you and teach you to stay away from the UC!"
msgstr ""

#. Id: 54551-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (54545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54551-Dialogue.mod:2
msgid "Run all you want, /RACISTNAME/! But we'll show you not to intrude on UC territory!"
msgstr ""

#. Id: 54551-Dialogue.mod
#. -- Index: 5.3
#. -- Parent line: 4 (54545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54551-Dialogue.mod:3
msgid "Tryin' to run, eh? You won't get away from us, /RACISTNAME/! We've still got a lesson to teach you..."
msgstr ""

#. Id: 54538-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54538-Dialogue.mod:0
msgid "I don't like bug men... Know what I do when I don't like something?"
msgstr "我不喜歡蟲子...當我不喜歡什麼時，知道我做了什麼嗎？"

#. Id: 54538-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54538-Dialogue.mod:1
msgid "I don't like peg legs... Know what I do when I don't like something?"
msgstr "我不喜歡釘腿......當我不喜歡的時候知道我做什麼嗎？"

#. Id: 54538-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 54538-Dialogue.mod:2
msgid "I dislike twig men... You know what I do if there's something I don't like?"
msgstr "我不喜歡這些男人......如果有什麼我不喜歡的話，你知道我該怎麼做嗎？"

#. Id: 54540-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (54538-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54540-Dialogue.mod:0
msgid "I make it disappear! Poof! But how are we gonna make YOU disappear, eh?"
msgstr "我讓它消失了！噗！但是我們怎麼會讓你消失呢，嗯？"

#. Id: 54540-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (54538-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54540-Dialogue.mod:1
msgid "I make it disappear!... Question is, how are we gonna make YOU disappear, eh, /RACISTNAME/?"
msgstr ""

#. Id: 54555-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (54554-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54555-Dialogue.mod:0
msgid "Ohh I can think of a couple ways. Ain't gonna feel too nice though, that's for sure, /QUICKLAUGH/..."
msgstr ""

#. Id: 54555-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (54554-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54555-Dialogue.mod:1
msgid "I can think of plenty o' ways, don't you worry. Ain't gonna be pretty though, /QUICKLAUGH/..."
msgstr ""

#. Id: 54557-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (54556-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54557-Dialogue.mod:0
msgid "Not like it's got any feelin's anyway, it's just a brainless /RACISTNAME/ drone..."
msgstr ""

#. Id: 54557-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (54556-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54557-Dialogue.mod:1
msgid "Everyone knows bug men don't have feelings anyway. They're just brainless drones..."
msgstr "無論如何，每個人都知道男人沒有感情。他們只是無腦無人機......"

#. Id: 54558-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (54557-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54558-Dialogue.mod:0
msgid "'Course. An' I want it out of my desert! Make it disappear, boys..."
msgstr "'課程。 '我想要它離開我的沙漠！讓它消失，男孩......"

#. Id: 54559-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (54558-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#: 54559-Dialogue.mod:0
msgid "Bleed!... Or don't bleed.. Whatever the fuck it is you bug freaks do!"
msgstr "流血！...或者不要流血..無論你他媽的是你的怪胎！"

#. Id: 65023-october 17.mod
#. -- Index: 8.0
#. -- Parent line: 7 (54558-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_A_PLAYER_TOWN=1, Tag: -1
#: 65023-october?17.mod:0
msgid "You're gonna bleed!... Or not bleed.."
msgstr "你會流血！......或者不流血......"

#. Id: 65024-october 17.mod
#. -- Index: 9.0
#. -- Parent line: 8 (65023-october 17.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Racist attack patriotic bandits
#: 65024-october?17.mod:0
msgid "Whatever the fuck it is you bug freaks do!"
msgstr "無論他媽的是你的怪物怪物！"

#. Id: 42629-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek or Scorchlander
#: 42629-Dialogue.mod:0
msgid "/OI/, /RACISTNAME/! "
msgstr ""

#. Id: 42635-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42629-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 42635-Dialogue.mod:0
msgid "Where's your queen?"
msgstr "你女王在哪裡？"

#. Id: 42635-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42629-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone
#: 42635-Dialogue.mod:1
msgid "Where's your precious queen now, eh?"
msgstr "現在你的寶貝女王在哪裡，是嗎？"

#. Id: 42636-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42635-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#: 42636-Dialogue.mod:0
msgid "Oh yeah, that's right. I forgot, she's IMAGINARY! Ha!"
msgstr "哦是的，沒錯。我忘了，她是想像的！哈！"

#. Id: 42637-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (54572-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 42637-Dialogue.mod:0
msgid "Now, freak queens aside, do one, /RACISTNAME/! You ain't welcome here!"
msgstr ""

#. Id: 54576-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (42637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54576-Dialogue.mod:0
msgid "Bleed the /RACISTNAME/!"
msgstr ""

#. Id: 54576-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (42637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54576-Dialogue.mod:1
msgid "Get 'im! Take the /RACISTNAME/ down!"
msgstr ""

#. Id: 54576-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (42637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54576-Dialogue.mod:2
msgid "Show 'im the exit... But first, show 'im pain!"
msgstr "顯示'即時退出......但首先，顯示'我很痛苦！"

#. Id: 54542-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54536-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Racist attack patriotic bandits
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 54542-Dialogue.mod:0
msgid "Comin' here /RACIST_CLAIM1/ our /RACIST_CLAIM2/ no doubt, eh..."
msgstr ""

