#. Id: 60036-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60036-Dialogue.mod:0
msgid "It's Tinfist!"
msgstr "這是Tinfist！"

#. Id: 60036-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60036-Dialogue.mod:2
msgid "An agent of darkness!"
msgstr "黑暗的代理人！"

#. Id: 60036-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60036-Dialogue.mod:3
msgid "The skeleton king, Tinfist!"
msgstr "骷髏王，Tinfist！"

#. Id: 60036-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60036-Dialogue.mod:4
msgid "Tinfist!"
msgstr "Tinfist！"

#. Id: 60036-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60036-Dialogue.mod:5
msgid "Okran protect us, it's a skeleton!"
msgstr "奧克蘭保護我們，這是一個骷髏！"

#. Id: 60037-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (60036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60037-Dialogue.mod:0
msgid "It's hideous..."
msgstr "這很可怕......"

#. Id: 60037-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (60036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60037-Dialogue.mod:3
msgid "Okran protect us..."
msgstr "奧克蘭保護我們......"

#. Id: 60037-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (60036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60037-Dialogue.mod:5
msgid "Don't look it in the eye..."
msgstr "別看它的眼睛......"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:0
msgid "It can't be...?"
msgstr "它不可能......？"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:1
msgid "Is she... passed on?"
msgstr "她被傳遞了嗎？"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:2
msgid "Moll?"
msgstr "莫爾？"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:3
msgid "Let me see, let me see!"
msgstr "讓我看看，讓我看看！"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:4
msgid "The rebellion is finally over!"
msgstr "叛亂終於結束了！"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:5
msgid "Amazing! It's a miracle of Okran!"
msgstr "驚人！這是奧克蘭的奇蹟！"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:6
msgid "Okran has ended the rebellion!"
msgstr "奧克蘭結束了叛亂！"

#. Id: 60040-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60040-Dialogue.mod:7
msgid "Lord of Light!"
msgstr "光明之王！"

#. Id: 60043-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (60040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60043-Dialogue.mod:0
msgid "Thank you, Lord of Light, for bringing an end to the rebellion..."
msgstr "感謝光明之王，結束叛亂......"

#. Id: 60043-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (60040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60043-Dialogue.mod:1
msgid "Blessings to you, holy brother... You're a hero..."
msgstr "祝福你，聖潔的兄弟......你是英雄......"

#. Id: 60043-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (60040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60043-Dialogue.mod:2
msgid "Love and devotion, holy brother..."
msgstr "愛和奉獻，聖潔的兄弟......"

#. Id: 60043-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (60040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60043-Dialogue.mod:3
msgid "Hooray! The rebellion is over!"
msgstr "萬歲！叛亂結束了！"

#. Id: 60043-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (60040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60043-Dialogue.mod:4
msgid "Okran has calmed woman's wrath..."
msgstr "奧克蘭已經平息了女人的憤怒......"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:0
msgid "Whaaat?!"
msgstr "Whaaat？"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:1
msgid "Women shouldn't have muscles!"
msgstr "女人不應該有肌肉！"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:3
msgid "The golem of the south?"
msgstr "南方的傀儡？"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:4
msgid "The golem!"
msgstr "傀儡！"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:5
msgid "The devil queen?"
msgstr "魔鬼女王？"

#. Id: 60041-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60041-Dialogue.mod:6
msgid "Let me see, I want to see!"
msgstr "讓我看看，我想看！"

#. Id: 60042-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (60041-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60042-Dialogue.mod:0
msgid "Okran, we thank you for ridding us of the devil queen... Amen."
msgstr "Okran，我們感謝你們讓我們擺脫了魔鬼女王......阿門。"

#. Id: 60042-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (60041-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60042-Dialogue.mod:1
msgid "A miracle from the Lord of Light!"
msgstr "來自光明之王的奇蹟！"

#. Id: 60042-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (60041-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60042-Dialogue.mod:2
msgid "Thank Okran!"
msgstr "謝謝Okran！"

#. Id: 60042-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (60041-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60042-Dialogue.mod:3
msgid "Truly a miracle from Okran..."
msgstr "真的是奧克蘭的奇蹟......"

#. Id: 60044-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60044-Dialogue.mod:0
msgid "Tengu?"
msgstr "天狗？"

#. Id: 60044-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60044-Dialogue.mod:1
msgid "The eastern emperor?"
msgstr "東方皇帝？"

#. Id: 60044-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60044-Dialogue.mod:2
msgid "Is it really him?"
msgstr "真的是他嗎？"

#. Id: 60045-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (60044-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60045-Dialogue.mod:0
msgid "Why is he here?"
msgstr "他為什麼來這兒？"

#. Id: 60045-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (60044-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60045-Dialogue.mod:1
msgid "Okran protect us, the desert nobles will be out for blood..."
msgstr "奧克蘭保護我們，沙漠貴族將出血......"

#. Id: 60045-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (60044-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy civilian sees carried target crowd trigger
#: 60045-Dialogue.mod:3
msgid "Is he... dead?"
msgstr "他......死了嗎？"
