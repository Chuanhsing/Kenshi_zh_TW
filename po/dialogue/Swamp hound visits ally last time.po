msgid "*Howl*"
msgstr "*嗥*"

#. Id: 98722-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98721-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#. Id: 98728-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (98727-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Swamp hound visits ally last time
#: 98722-Dialogue.mod:0 98728-Dialogue.mod:0
msgid "*Howwwwl*"
msgstr "* Howwwwl *"

#. Id: 98729-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (98728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98729-Dialogue.mod:0
msgid "HOUNDS!"
msgstr "HOUNDS！"

#. Id: 98730-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (98729-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98730-Dialogue.mod:0
msgid "Ain't you hearin' your brothers howlin'?"
msgstr "你不是在聽'你的兄弟們怎麼樣'？"

#. Id: 98731-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (98730-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98731-Dialogue.mod:0
msgid "Come talk to us already!"
msgstr "快來跟我們說吧！"

#. Id: 98733-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (98732-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98733-Dialogue.mod:0
msgid "Guess you're busy, eh?"
msgstr "猜猜你很忙，是嗎？"

#. Id: 98734-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (98733-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98734-Dialogue.mod:0
msgid "Talk to us next time."
msgstr "下次再和我們聯繫。"

#. Id: 98736-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#. Id: 98748-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98736-Dialogue.mod:0 98748-Dialogue.mod:0
msgid "Hoy, hounds!"
msgstr "好，獵犬！"

#. Id: 98739-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98738-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Swamp hound visits ally last time
#: 98739-Dialogue.mod:0
msgid "Tell 'em I'm bustin' for a smoke."
msgstr "告訴他們我是巴斯汀因吸煙。"

#. Id: 98740-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (98739-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98740-Dialogue.mod:0
msgid "GOT ANY SMOKES AROUND HERE?"
msgstr "在這附近有什麼吸煙？"

#. Id: 98743-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (98742-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Swamp hound visits ally last time
#: 98743-Dialogue.mod:0
msgid "An' fish, ask 'em if they got any fish."
msgstr "一條'魚，問他們是否有魚。"

#. Id: 98744-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (98743-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98744-Dialogue.mod:0
msgid "AND FISH?"
msgstr "和魚？"

#. Id: 98745-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (98744-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98745-Dialogue.mod:0
msgid "My hounds are hungry!"
msgstr "我的獵犬很餓！"

#. Id: 98746-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (98745-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98746-Dialogue.mod:0
msgid "Hoy, you gonna come talk to us?"
msgstr "好的，你會來跟我們說話嗎？"

#. Id: 98750-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (98749-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98750-Dialogue.mod:0
msgid "Grim needs her cut!"
msgstr "嚴峻需要她切！"

#. Id: 98752-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (98751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98752-Dialogue.mod:0
msgid "Come on already!"
msgstr "來吧！"

#. Id: 98754-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (98753-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98754-Dialogue.mod:0
msgid "Gonna keep me waitin' in this bog?"
msgstr "要讓我在這個沼澤中等待嗎？"

#. Id: 98755-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (98754-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp hound visits ally last time
#: 98755-Dialogue.mod:0
msgid "Fine."
msgstr "精細。"
