#: 18629-Dialogue.mod:0
msgid "Ugh, animals..."
msgstr "呃，動物......"

#. Id: 18629-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18629-Dialogue.mod:1
msgid "Unruly /PEASANT/s..."
msgstr ""

#. Id: 18629-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18629-Dialogue.mod:2
msgid "Such a ruckus!"
msgstr "這樣的騷動！"

#. Id: 18629-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18629-Dialogue.mod:3
msgid "What a shambles..."
msgstr "多麼糟糕......"

#. Id: 18631-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 18631-Dialogue.mod:0
msgid "This is madness!"
msgstr "這太瘋狂了！"

#. Id: 18631-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 18631-Dialogue.mod:2
msgid "Stop this madness!"
msgstr "停止這種瘋狂！"

#. Id: 18632-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 18632-Dialogue.mod:1
msgid "Somebody do something, NOW!"
msgstr "現在有人做某事！"

#. Id: 18632-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 18632-Dialogue.mod:3
msgid "Guards! Protect me!"
msgstr "逆天！保護我！"

#. Id: 18637-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#: 18637-Dialogue.mod:0
msgid "Somebody stop /HIM/! Now!"
msgstr ""

#. Id: 18637-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble witnesses crime
#: 18637-Dialogue.mod:3
msgid "Guards! Put an end to this ruckus!"
msgstr "逆天！結束這種騷動！"
