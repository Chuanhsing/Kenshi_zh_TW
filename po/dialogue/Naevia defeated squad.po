#. Id: 51128-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:0
msgid "Naevia, I'll have done you proud..."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:1
msgid "Still standing! Naevia would be impressed..."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:2
msgid "Still in one piece! Naevia would be proud..."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:3
msgid "Nailed it."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:4
msgid "The hurt only makes me stronger."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:5
msgid "Thank Okran it's over..."
msgstr ""

#. Id: 51128-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51128-Dialogue.mod:6
msgid "They told me I couldn't fight. I proved them wrong."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:0
msgid "They preach love, but they act only out of hate. I pity them."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:1
msgid "Deserting the Holy Nation, it was the best decision I ever made..."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:2
msgid "Cowards hiding behind sacred words, that's all they are. A disgrace to Okran."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:3
msgid "A disgrace to god. "
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:4
msgid "... Good riddance."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:5
msgid "You're an insult to Okran."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:6
msgid "Spouting hate and nonsense... You're an insult to Okran."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:7
msgid "May Okran have mercy on your hateful souls."
msgstr ""

#. Id: 51142-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Naevia defeated squad
#: 51142-Dialogue.mod:8
msgid "Okran, show mercy to these lost souls."
msgstr ""

