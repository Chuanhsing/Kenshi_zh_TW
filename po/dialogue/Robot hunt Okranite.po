#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46360-Dialogue.mod:0 1533120-__world?reactions?W.mod:0
msgid "Skeletons!!!"
msgstr "骷髏！"

#. Id: 46360-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46360-Dialogue.mod:1
msgid "Okran save us... ROBOTS!"
msgstr "Okran救了我們......機器人！"

#. Id: 46360-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#. Id: 46364-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46360-Dialogue.mod:2 46364-Dialogue.mod:4
msgid "ROBOTS!"
msgstr "機器人！"

#. Id: 46363-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:0
msgid "/IWE/ need help!"
msgstr ""

#. Id: 46363-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:1
msgid "Regroup!"
msgstr "重組！"

#. Id: 46363-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:2
msgid "Narko has cursed us!"
msgstr "Narko詛咒了我們！"

#. Id: 46363-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:3
msgid "/IWE/ need more men!"
msgstr ""

#. Id: 46363-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:4
msgid "There's too many of them, /IWE/ can't win!"
msgstr ""

#. Id: 46363-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:7
msgid "WE'RE ALL GOING TO DIE!"
msgstr "我們都要死了！"

#. Id: 46363-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:8
msgid "SHRIEK!"
msgstr "尖叫！"

#. Id: 46363-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (46360-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#: 46363-Dialogue.mod:9
msgid "*Uncontrollable sobbing*"
msgstr "*無法控制的抽泣*"

#. Id: 46364-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46364-Dialogue.mod:0
msgid "An army... an army of SKELETONS!"
msgstr "一支軍隊......一支由SKELETONS組成的軍隊！"

#. Id: 46364-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46364-Dialogue.mod:1
msgid "The Skeletons are invading!"
msgstr "骷髏正在入侵！"

#. Id: 46364-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46364-Dialogue.mod:2
msgid "Okran be with us... The skeletons have come for us!"
msgstr "Okran和我們在一起......骷髏來了我們！"

#. Id: 46364-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46364-Dialogue.mod:3
msgid "The robots have come for us!"
msgstr "機器人來找我們了！"

#. Id: 46364-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>10, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46364-Dialogue.mod:6
msgid "Robots... so many robots!"
msgstr "機器人......這麼多機器人！"
