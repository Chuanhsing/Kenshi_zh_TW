#. Id: 46344-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:0
msgid "Skeleton! Kill it!"
msgstr "骨架！殺死它！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:1
msgid "Robot! Kill it!"
msgstr "機器人！殺死它！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:2
msgid "Abomination! Kill it!"
msgstr "厭惡！殺死它！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:3
msgid "Kill! Kill it, kill all the robots!"
msgstr "殺！殺死它，殺死所有的機器人！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:4
msgid "Destroy the robots! For Okran! "
msgstr "摧毀機器人！對於奧克蘭！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:5
msgid "The light will prevail! Kill the abomination!"
msgstr "光將佔上風！殺死可憎的！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:6
msgid "/BATTLECRY/! Robot! Kill it!"
msgstr ""

#. Id: 46344-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:8
msgid "Okran guide my sword... Die, abomination!"
msgstr "奧克蘭指導我的劍......死，憎惡！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:10
msgid "Die, robot!"
msgstr "死，機器人！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:11
msgid "/GREASY/ robot!"
msgstr ""

#. Id: 46344-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:12
msgid "ROBOT!"
msgstr "機器人！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:13
msgid "SKELETON!"
msgstr "骨架！"

#. Id: 46344-Dialogue.mod
#. -- Index: 0.14
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46344-Dialogue.mod:14
msgid "Evil! Die, Skeleton devil!"
msgstr "邪惡！死，骷髏魔鬼！"

#. Id: 46346-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46346-Dialogue.mod:0
msgid "Abominations aren't welcome on Okran's land!"
msgstr "奧克蘭的土地上不歡迎憎惡！"

#. Id: 46346-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46346-Dialogue.mod:1
msgid "The Okranites will rid the lands of all Skeletons!"
msgstr "Okranites將擺脫所有骷髏王國的土地！"

#. Id: 46346-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46346-Dialogue.mod:2
msgid "The Okranites will purify the lands of all robot abominations!"
msgstr "Okranites將淨化所有機器人可憎的土地！"

#. Id: 46346-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46346-Dialogue.mod:3
msgid "Okran's Will will be done! Die, Skeleton!"
msgstr "奧克蘭的意志將完成！死，骷髏！"

#. Id: 46346-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46346-Dialogue.mod:5
msgid "Skeletons! Attack!"
msgstr "骷髏！攻擊！"

#. Id: 46360-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
