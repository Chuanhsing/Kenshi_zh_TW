msgid "Um..."
msgstr "嗯..."

#. Id: 42261-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42261-Dialogue.mod:1
msgid "Excuse me...?"
msgstr "勞駕...？"

#. Id: 42261-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42261-Dialogue.mod:2
msgid "Um... hello..."
msgstr "嗯...你好......"

#. Id: 42261-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (16088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42261-Dialogue.mod:4
msgid "Er... you're still here..."
msgstr "呃......你還在這裡......"

#. Id: 42261-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (16088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42261-Dialogue.mod:5
msgid "Oh.. uh.. you're not leaving..."
msgstr "哦..呃..你不會離開......"

#. Id: 42263-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42261-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42263-Dialogue.mod:0
msgid "This is awkward..."
msgstr "這很尷尬......"

#. Id: 42263-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42261-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42263-Dialogue.mod:1
msgid "Bit awkward..."
msgstr "有點尷尬......"

#. Id: 42263-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (42261-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42263-Dialogue.mod:2
msgid "Can you leave now?"
msgstr "你現在可以離開嗎？"

#. Id: 42263-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (42261-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42263-Dialogue.mod:3
msgid "What are  you doing?"
msgstr "你在做什麼？"

#. Id: 42279-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42263-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 42279-Dialogue.mod:0
msgid "/GUARDS/! A little help?"
msgstr ""

