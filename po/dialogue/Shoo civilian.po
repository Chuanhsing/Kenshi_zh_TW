#. -- Index: 3.1
#. -- Parent line: 2 (42263-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 42279-Dialogue.mod:1
msgid "/GUARDS/! /GUARDS/! "
msgstr ""

#. Id: 42279-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (42263-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 42279-Dialogue.mod:2
msgid "What do you want from me?! /GUARDS/! Get /HIM/ out! "
msgstr ""

#. Id: 42279-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (42263-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 42279-Dialogue.mod:3
msgid "What do I do? /GUARDS/, I need help!"
msgstr ""

#. Id: 49898-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 49898-Dialogue.mod:0
msgid "Help me! Save me Okran!"
msgstr "幫我！救我Okran！"

#. Id: 49898-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (49897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 49898-Dialogue.mod:2
msgid "Skeletons are here! God save us all!"
msgstr "骷髏在這裡！上帝救了我們所有人！"

#. Id: 49898-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (49897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 49898-Dialogue.mod:3
msgid "Holy Lord Phoenix save us all!"
msgstr "神聖鳳凰拯救我們所有人！"

