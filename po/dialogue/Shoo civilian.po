#. Id: 13927-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:2
msgid "You're can't come in here"
msgstr "你不能來這裡"

#. Id: 13927-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:3
msgid "Why are you in here?"
msgstr "你為什麼在這裡？"

#. Id: 13927-Escapes.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:4
msgid "You're not allowed in here"
msgstr "你不能進入這裡"

#. Id: 13927-Escapes.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. Id: 3125-gamedata.base
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shoo civ
#: 13927-Escapes.mod:5 3125-gamedata.base:0
msgid "Could you get out of my house?"
msgstr "你能離開我的房子嗎？"

#. Id: 13927-Escapes.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:7
msgid "You know you don't live here, right?"
msgstr "你知道你不住在這裡，對嗎？"

#. Id: 13927-Escapes.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:8
msgid "Not seeing guests right now."
msgstr "現在沒有見到客人。"

#. Id: 13927-Escapes.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:9
msgid "Are you lost?"
msgstr "你迷路了嗎？"

#. Id: 13927-Escapes.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:10
msgid "Don't think you should be here."
msgstr "不要以為你應該在這裡。"

#. Id: 13927-Escapes.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 13927-Escapes.mod:11
msgid "Could you show yourself out?"
msgstr "你能展示自己嗎？"

#. Id: 13927-Escapes.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. Id: 45737-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45735-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 42979-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. Id: 42979-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 13927-Escapes.mod:12 45737-Dialogue.mod:0 42979-Dialogue.mod:0 42979-Dialogue.mod:7
msgid "Please leave."
msgstr "請離開。"

#. Id: 13928-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13928-Escapes.mod:1
msgid "Please get out"
msgstr "請出去"

#. Id: 13928-Escapes.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13928-Escapes.mod:2
msgid "Leave, please"
msgstr "請離開"

#. Id: 13928-Escapes.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13928-Escapes.mod:3
msgid "Please go"
msgstr "請離開"

#. Id: 50069-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13928-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 50069-Dialogue.mod:0
msgid "You're not in Holy Nation land now, 'brother'... You can't do as you please here..."
msgstr "你現在不在神聖的國度，'兄弟'......你不能在這裡做你想做的事......"

#. Id: 50069-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13928-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 50069-Dialogue.mod:1
msgid "There are no paladins to protect you here, brother. Trespass and you'll be punished."
msgstr "兄弟，這裡沒有保護你的聖騎士。非法侵入，你將受到懲罰。"

#. Id: 50069-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13928-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 50069-Dialogue.mod:2
msgid "Brother, I'm warning you, you need to leave now."
msgstr "兄弟，我在警告你，你現在需要離開。"

#. Id: 50071-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (50069-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 50071-Dialogue.mod:0
msgid "Get out. Before the ninjas teach you a lesson in manners..."
msgstr "出去。在忍者教你禮儀之前......"

#. Id: 50072-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (50071-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#: 50072-Dialogue.mod:0
msgid "Intruder! In here, help!"
msgstr "入侵者！在這裡，幫助！"

#. Id: 13939-Escapes.mod
#. -- Index: 1.4
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 13929-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13939-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 13945-Escapes.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13943-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13939-Escapes.mod:4 13929-Escapes.mod:2 13945-Escapes.mod:3
msgid "Shoo!"
msgstr "噓！"

#. Id: 42251-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (13929-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42251-Dialogue.mod:1
msgid "I need help! /GUARDS/! Intruder!"
msgstr ""

#. Id: 42251-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (13929-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42251-Dialogue.mod:2
msgid "/GUARDS/! Intruder! Help!"
msgstr ""

#. Id: 42251-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (13929-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42251-Dialogue.mod:3
msgid "Help me! Intruder! Somebody help me!"
msgstr "幫我！入侵者！來人幫幫我！"

#. Id: 45436-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45436-Dialogue.mod:1
msgid "Stay out of here in future please."
msgstr "請在將來離開這裡。"

#. Id: 45436-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13927-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45436-Dialogue.mod:2
msgid "You know where the door is..."
msgstr "你知道門在哪裡......"

#. Id: 13943-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:3
msgid "Don't know you or why you're here."
msgstr "不認識你或為什麼你在這裡。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:4
msgid "You don't belong here."
msgstr "你不屬於這裡。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:5
msgid "This is my home. Now get out."
msgstr "這是我的家。現在出去吧。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:6
msgid "You're trespassing."
msgstr "你擅自闖入。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:7
msgid "If you're here to kill or rob me, hurry up or get out."
msgstr "如果你在這裡殺人或搶劫我，快點或出去。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:8
msgid "I'll let you go. Long as don't take anything."
msgstr "我會讓你走的。只要不採取任何措施。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:9
msgid "You're not welcome in this house. Out."
msgstr "在這所房子裡你不受歡迎。出。"

#. Id: 13943-Escapes.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 13943-Escapes.mod:10
msgid "You have the wrong house."
msgstr "你錯了房子。"

#. Id: 13946-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:0
msgid "Hey! I said get out!"
msgstr "嘿!我說出去了！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:1
msgid "Hey! I said leave!"
msgstr "嘿!我說離開！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 46936-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (46934-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 13946-Escapes.mod:2 46936-Dialogue.mod:0
msgid "What's wrong with you? Get out!"
msgstr "你這人怎麼回事？出去！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.3
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:3
msgid "Why are you still here?! Shoo!"
msgstr "你怎麼還在這？！噓！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.4
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:4
msgid "Why are you still here!? Go away!"
msgstr "你怎麼還在這！？走開！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.5
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:5
msgid "Why are you still here!? Get out!"
msgstr "你怎麼還在這！？出去！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.6
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:6
msgid "I said leave already!"
msgstr "我已經離開了！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.7
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:7
msgid "Just get out already!"
msgstr "剛出門！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.8
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:8
msgid "Hello? I said get out!"
msgstr "你好？我說出去了！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.9
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:9
msgid "Why are you still here?"
msgstr "你為什麼還在這兒？"

#. Id: 13946-Escapes.mod
#. -- Index: 2.10
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:10
msgid "Hey! Will you hurry up and leave?!"
msgstr "嘿!你快點離開嗎？！"

#. Id: 13946-Escapes.mod
#. -- Index: 2.11
#. -- Parent line: 1 (13945-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 13946-Escapes.mod:11
msgid "What's wrong with you? Leave already!"
msgstr "你這人怎麼回事？離開了！"

#. Id: 42257-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (13946-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IM_UNARMED=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 2751-gamedata.base
#. -- Index: 10.0
#. -- Parent line: 9 (2749-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: crumblejon
#: 42257-Dialogue.mod:2 2751-gamedata.base:0
msgid "LEAVE ME ALONE!"
msgstr "請別打擾我！"

#. Id: 42257-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (13946-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IM_UNARMED=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42257-Dialogue.mod:3
msgid "Get out of my house!"
msgstr "滾出我的房子！"

#. Id: 42257-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (13946-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IM_UNARMED=0, Tag: -1
