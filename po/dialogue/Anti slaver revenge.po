#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Anti slaver revenge
#: 64391-Dialogue.mod:5
msgid "It's time to eat fist, asshole/S/!"
msgstr ""

#. Id: 64391-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Anti slaver revenge
#: 64391-Dialogue.mod:6
msgid "Choke on fist, asshole/S/!"
msgstr ""

#. Id: 64391-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Anti slaver revenge
#: 64391-Dialogue.mod:7
msgid "Eat my fist, slaver/S/!"
msgstr ""

#. Id: 64391-Dialogue.mod
#. -- Index: 0.8
