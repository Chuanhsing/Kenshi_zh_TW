#. Id: 63886-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63886-Dialogue.mod:0
msgid "Well! Ain't you guys brave for settin' up in the swamps, eh?"
msgstr "好！難道你們不敢勇敢地站在沼澤地，呃？"

#. Id: 63912-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63886-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63912-Dialogue.mod:0
msgid "But, ya know..."
msgstr "但是，你知道......"

#. Id: 63913-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63912-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63913-Dialogue.mod:0
msgid "Bein' brave gets you dead!"
msgstr "貝因勇敢讓你死！"

#. Id: 63887-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63887-Dialogue.mod:0
msgid "Know where you built your walls, /INSULT/s?"
msgstr ""

#. Id: 63890-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63887-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63890-Dialogue.mod:1
msgid "You built 'em on swamper land."
msgstr "你在沼澤地上建造了它們。"

#. Id: 63891-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63891-Dialogue.mod:0
msgid "Got the stones to set up on swamper land?"
msgstr "有石頭建在沼澤地上嗎？"

#. Id: 63892-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63891-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63892-Dialogue.mod:0
msgid "Well, have ya?"
msgstr "好吧，你有嗎？"

#. Id: 63893-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63893-Dialogue.mod:0
msgid "Let's put you to the test, shall we?"
msgstr "我們讓你參加考試，好嗎？"

#. Id: 63896-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63894-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63896-Dialogue.mod:0
msgid "You're just one /GUY/?"
msgstr ""

#. Id: 63897-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63896-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63897-Dialogue.mod:0
msgid "/HEIS/ just one /GUY/!"
msgstr ""

#. Id: 63899-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (63898-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Red sabre announce
#: 63899-Dialogue.mod:0
msgid "/QUICKLAUGH/! One /GUY/?!"
msgstr ""

#. Id: 63901-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (63900-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Red sabre announce
#: 63901-Dialogue.mod:0
msgid "You can't survive in the swamps on your own, /GUY/!"
msgstr ""

#. Id: 63902-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (63901-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Red sabre announce
#: 63902-Dialogue.mod:0
msgid "What a /IDIOT/..."
msgstr ""

#. Id: 63905-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#. Id: 63906-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63905-Dialogue.mod:0 63906-Dialogue.mod:0
msgid "The swamps is OURS!"
msgstr "沼澤是我們的！"

#. Id: 63905-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#. Id: 63906-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63905-Dialogue.mod:1 63906-Dialogue.mod:1
msgid "This is OUR swamp!"
msgstr "這是我們的沼澤！"

#. Id: 63907-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63905-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63907-Dialogue.mod:0
msgid "An' ain't no /DIRTY/ nobles movin' into our space, got it?"
msgstr ""

#. Id: 63907-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63905-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63907-Dialogue.mod:1
msgid "Ain't no sake drinkin' fat Cats settin' up in MY swamp!"
msgstr "在我的沼澤地裡，'胖貓'在我的沼澤地裡不是沒有清酒！"

#. Id: 63907-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (63905-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63907-Dialogue.mod:2
msgid "I ain't havin' no noble fat /BASTARD/ movin' in on MY territory!"
msgstr ""

#. Id: 63907-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (63905-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63907-Dialogue.mod:3
msgid "Fuck you, nobles!"
msgstr "操你，貴族！"

#. Id: 63908-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63907-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63908-Dialogue.mod:0
msgid "I'm gonna enjoy breakin' some desert crawlin' skulls!"
msgstr "我會喜歡打破'沙漠爬行'的頭骨！"

#. Id: 63908-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (63907-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Red sabre announce
#: 63908-Dialogue.mod:1
msgid "Break their shit!"
msgstr "打破他們的狗屎！"
