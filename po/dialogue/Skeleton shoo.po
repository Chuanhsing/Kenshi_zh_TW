#. Id: 45735-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: My Race(s): Skeleton
#: 45735-Dialogue.mod:0
msgid "You're not supposed to be here..."
msgstr "你不應該在這裡......"

#. Id: 45735-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: My Race(s): Skeleton
#: 45735-Dialogue.mod:2
msgid "You're not allowed in here..."
msgstr "你不被允許進入這裡......"

#. Id: 45735-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: My Race(s): Skeleton
#: 45735-Dialogue.mod:3
msgid "You're not meant to be here..."
msgstr "你不應該在這裡......"

#. Id: 45735-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: My Race(s): Skeleton
#: 45735-Dialogue.mod:4
msgid "This is private, you can't be here..."
msgstr "這是私人的，你不能在這裡......"

#. Id: 45737-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45735-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 46428-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (46427-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shoo shop
#. -- Condition: Who: TARGET_IF_PLAYER, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 45737-Dialogue.mod:1 46428-Dialogue.mod:2
msgid "Please go."
msgstr "請離開。"

#. Id: 45737-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45735-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45737-Dialogue.mod:3
msgid "You need to go."
msgstr "你需要去。"

#. Id: 45737-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (45735-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45737-Dialogue.mod:4
msgid "You can't stay here."
msgstr "你不能留在這裡。"

#. Id: 45740-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45740-Dialogue.mod:0
msgid "I don't want violence. Leave."
msgstr "我不想要暴力。離開。"

#. Id: 45740-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45740-Dialogue.mod:1
msgid "I dislike violence, but I'll use it if I have to, /OUTLANDER/."
msgstr ""

#. Id: 45740-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45740-Dialogue.mod:2
msgid "I despise violence, /OUTLANDER/, but if you push me, I'll use it."
msgstr ""

#. Id: 45740-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45740-Dialogue.mod:3
msgid "Here is my first and final warning to you /OUTLANDER/: Leave, before I remove you."
msgstr ""

#. Id: 45742-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (45740-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45742-Dialogue.mod:0
msgid "3"
msgstr "3"

#. Id: 45744-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (45742-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45744-Dialogue.mod:0
msgid "2"
msgstr "2"

#. Id: 45746-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (45744-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45746-Dialogue.mod:0
msgid "1"
msgstr "1"

#. Id: 45750-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (45746-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45750-Dialogue.mod:1
msgid "I warned you."
msgstr "我警告過你。"

#. Id: 55233-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (45751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 55233-Dialogue.mod:0
msgid "Predictable /RACENAME/. You should learn to listen to sense, for your own good..."
msgstr ""

#. Id: 55233-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (45751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 55233-Dialogue.mod:1
msgid "/RACENAME/s, I've yet to find a single one that will listen to reason..."
msgstr ""

#. Id: 55233-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (45751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 55233-Dialogue.mod:2
msgid "Overstepping boundaries, it's what /RACENAME/s do best..."
msgstr ""

#. Id: 55233-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (45751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 55233-Dialogue.mod:3
msgid "I'll never understand why /RACENAME/s cannot listen to simple reason..."
msgstr ""

#. Id: 55233-Dialogue.mod
#. -- Index: 4.4
#. -- Parent line: 3 (45751-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 55233-Dialogue.mod:4
msgid "Why won't you listen, /RACENAME/?"
msgstr ""

#. Id: 55236-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 55236-Dialogue.mod:0
msgid "You don't look broken... Why do you not listen?"
msgstr "你看起來不破...你為什麼不聽？"

#. Id: 55236-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 55236-Dialogue.mod:1
msgid "Are you damaged?... Why will you not leave when I tell you to?"
msgstr "你有損嗎？...當我告訴你的時候你為什麼不離開？"

#. Id: 55236-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 55236-Dialogue.mod:2
msgid "Are your receptors broken? Why are you just standing there?"
msgstr "你的受體壞了嗎？你為什麼只站在那裡？"

#. Id: 55236-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (45737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 55236-Dialogue.mod:3
msgid "Are you faulty? Why are you still here?"
msgstr "你有缺陷嗎？你為什麼還在這兒？"
