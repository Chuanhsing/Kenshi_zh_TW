#. Id: 44777-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:0
msgid "This town’ll eat you alive."
msgstr "這個小鎮會讓你活著。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:1
msgid "Got a problem? Tell it to someone who cares."
msgstr "有問題嗎？告訴關心的人。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:3
msgid "You're in my space."
msgstr "你在我的空間裡。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:4
msgid "What are you /LOOKING/at?"
msgstr ""

#. Id: 44777-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:5
msgid "Sit down, drink some rum and shut up."
msgstr "坐下來喝一些朗姆酒然後閉嘴。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:6
msgid "You're invading my space. Back up. "
msgstr "你正在入侵我的空間。後退。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:7
msgid "I ain't in the mood for talkin'."
msgstr "我不喜歡說話。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:8
msgid "Go talk to someone who gives a shit."
msgstr "和那些給狗屎的人談談。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:9
msgid "Leave me alone."
msgstr "請別打擾我。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:11
msgid "Scram, worm."
msgstr "Scram，蠕蟲。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:12
msgid "You here to gossip, or you here to drink?"
msgstr "你在這里八卦，還是你在這裡喝酒？"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:13
msgid "Don't distract me."
msgstr "不要分散我的注意力。"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.16
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:16
msgid "*snort*"
msgstr "*哼*"

#. Id: 44777-Dialogue.mod
#. -- Index: 0.17
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug1
#: 44777-Dialogue.mod:17
msgid "I ain't the talkin' type."
msgstr "我不是說話的類型。"
