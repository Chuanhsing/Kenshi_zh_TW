#. Id: 63934-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63934-Dialogue.mod:0
msgid "/FLATSKIN//S/!"
msgstr ""

#. Id: 63935-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63934-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63935-Dialogue.mod:0
msgid "Licked your wounds yet?"
msgstr ""

#. Id: 63936-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63935-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63936-Dialogue.mod:0
msgid "You fought so badly in our last battle..."
msgstr ""

#. Id: 63937-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63936-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63937-Dialogue.mod:0
msgid "It's been a long time since we beat someone so terribly..."
msgstr ""

#. Id: 63937-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (63936-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63937-Dialogue.mod:1
msgid "... If you can even call that a battle..."
msgstr ""

#. Id: 63938-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63937-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63938-Dialogue.mod:0
msgid "Victory doesn't taste so sweet when it falls in to your hands so easily, you know..."
msgstr ""

#. Id: 63941-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (63940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63941-Dialogue.mod:0
msgid "How do such weaklings survive to adulthood in these lands?"
msgstr ""

#. Id: 63942-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (63941-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: tribute announce 3
#: 63942-Dialogue.mod:0
msgid "They amuse me!"
msgstr ""

#. Id: 63943-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (63942-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63943-Dialogue.mod:0
msgid "I, too!"
msgstr ""

#. Id: 63944-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (63943-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63944-Dialogue.mod:0
msgid "Do you leave us here to laugh at you, /FLATSKIN//S/?"
msgstr ""

#. Id: 63945-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (63944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63945-Dialogue.mod:0
msgid "So cowardly you cannot even defend your own name?"
msgstr ""

#. Id: 63946-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (63945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63946-Dialogue.mod:0
msgid "And /IWE/ expected a bigger welcome, as your defeaters!"
msgstr ""

#. Id: 63948-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (63946-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: tribute announce 3
#: 63948-Dialogue.mod:0
msgid "Tribute!"
msgstr ""

#. Id: 63950-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (63949-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: tribute announce 3
#: 63950-Dialogue.mod:0
msgid "Bring us tribute!"
msgstr ""

#. Id: 63951-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (63950-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63951-Dialogue.mod:0
msgid "Enough!"
msgstr ""

#. Id: 63952-Dialogue.mod
#. -- Index: 17.0
#. -- Parent line: 16 (63951-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63952-Dialogue.mod:0
msgid "If it's another battle you want..."
msgstr ""

#. Id: 63953-Dialogue.mod
#. -- Index: 18.0
#. -- Parent line: 17 (63952-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: tribute announce 3
#: 63953-Dialogue.mod:0
msgid "Another battle, you will have."
msgstr ""

