#. Id: 40653-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:0
msgid "Wretched woman! Leave!"
msgstr "猥瑣的女人！離開！"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:1
msgid "Leave, woman! Or I'll call the paladins."
msgstr "離開，女人！或者我會打電話給聖騎士。"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:2
msgid "You're not allowed in here, temptress"
msgstr "你不被允許進入這裡，誘惑"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:3
msgid "Get out of here, temptress"
msgstr "離開這裡，誘惑"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:4
msgid "What are you doing in here? Wretched woman!"
msgstr "你在這做什麼？猥瑣的女人！"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. Id: 40652-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (47446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40653-Dialogue.mod:7 40652-Dialogue.mod:1
msgid "Shoo! Shoo!"
msgstr "噓！噓！"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. Id: 40652-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (47446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40653-Dialogue.mod:9 40652-Dialogue.mod:6
msgid "You're not allowed in here!"
msgstr "你不被允許進入這裡！"

#. Id: 40653-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 40653-Dialogue.mod:10
msgid "Wretched woman! Get out!"
msgstr "猥瑣的女人！出去！"

#. Id: 47446-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#: 47446-Dialogue.mod:0
msgid "This is private property."
msgstr "這是私人財產。"

#. Id: 47446-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#: 47446-Dialogue.mod:1
msgid "You're intruding..."
msgstr "你在干擾......"

#. Id: 47446-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#: 47446-Dialogue.mod:2
msgid "You're not supposed to be in here."
msgstr "你不應該在這裡。"

#. Id: 47446-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#: 47446-Dialogue.mod:3
msgid "Please leave, you shouldn't be here."
msgstr "請離開，你不應該在這裡。"

#. Id: 47446-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. Id: 13927-Escapes.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. Id: 3127-gamedata.base
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shoo civ
#: 47446-Dialogue.mod:4 13927-Escapes.mod:1 3127-gamedata.base:0
msgid "What are you doing in here?"
msgstr "你在這做什麼？"

#. Id: 40652-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (47446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40652-Dialogue.mod:3
msgid "Get out, intruder! Paladins!"
msgstr "出去，入侵！聖騎士！"

#. Id: 40652-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (47446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40652-Dialogue.mod:4
msgid "Leave, or I'll call the paladins"
msgstr "離開，或者我會打電話給聖騎士"

#. Id: 40652-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (47446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40652-Dialogue.mod:5
msgid "Leave at once!"
msgstr "馬上離開吧！"

#. Id: 42327-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (40652-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42327-Dialogue.mod:0
msgid "I'm warning you!"
msgstr "我警告你！"

#. Id: 42327-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (40652-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42327-Dialogue.mod:1
msgid "What's wrong with you? I said leave!"
msgstr "你這人怎麼回事？我說離開！"

#. Id: 42327-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40652-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42327-Dialogue.mod:2
msgid "You! Get out!"
msgstr "您！出去！"

#. Id: 42327-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40652-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42327-Dialogue.mod:3
msgid "I'm talking to you!"
msgstr "我在跟你說話！"

#. Id: 42328-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (42327-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42328-Dialogue.mod:2
msgid "Paladins! Paladins, over here!"
msgstr "聖騎士！聖騎士，在這裡！"

#. Id: 42328-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (42327-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42328-Dialogue.mod:3
msgid "Paladins! I need help!"
msgstr "聖騎士！我需要幫助！"

#. Id: 42328-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (42327-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42328-Dialogue.mod:4
msgid "Paladins! Arrest /HIM/!"
msgstr ""

#. Id: 42328-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (42327-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42328-Dialogue.mod:5
msgid "Paladins, /SEIZE/ /HIM/!"
msgstr ""

#. Id: 42328-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (42327-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42328-Dialogue.mod:6
msgid "Paladins, don't let /HIM/ escape!"
msgstr ""
