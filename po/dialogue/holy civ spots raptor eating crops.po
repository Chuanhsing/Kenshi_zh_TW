#: 44008-Dialogue.mod:0
msgid "Devil creatures! They'll bring famine to us all!"
msgstr "惡魔生物！他們會給我們所有人帶來飢荒！"

#. Id: 44008-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:1
msgid "Cursed /ANIMALTYPE/! Shoo!"
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:2
msgid "Spawns of Narko! The pests will bring famine to us all!"
msgstr "Narko的產卵！害蟲將給我們所有人帶來飢荒！"

#. Id: 44008-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:3
msgid "/GREASY/ creatures! Go away! "
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:4
msgid "/GREASY/ animals! Shoo, go away!"
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:5
msgid "/GREASY/ beasts! Shoo, leave my crops alone!"
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:6
msgid "Leave those crops alone! /GREASY/ animals!"
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:7
msgid "Leave my crops alone! /GREASY/ pest!"
msgstr ""

#. Id: 44008-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 44008-Dialogue.mod:8
msgid "Curses! Cursed creatures! "
msgstr "詛咒！被詛咒的生物！"

#. Id: 44035-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:0
msgid "/ANIMALTYPE/s! Urgh!"
msgstr ""

#. Id: 44035-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:1
msgid "No! /ANIMALTYPE/s!"
msgstr ""

#. Id: 44035-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:2
msgid "Agh! Foul! Foul creatures!"
msgstr "哎！犯規！犯規生物！"

#. Id: 44035-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:3
msgid "Eurgh, /DIRTY/! /DIRTY/ beasts!"
msgstr ""

#. Id: 44035-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:4
msgid "/YUCK/, /ANIMALTYPE/s!"
msgstr ""

#. Id: 44035-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 44035-Dialogue.mod:5
msgid "/YUCK/, /ANIMALTYPE/s! Go away!"
msgstr ""

#. Id: 44036-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (44035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44036-Dialogue.mod:0
msgid "Make them stop! They're eating the crops!"
msgstr "讓他們停下來！他們正在吃莊稼！"

#. Id: 44036-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (44035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44036-Dialogue.mod:1
msgid "They're eating /MYOUR/ crops! Make them stop!"
msgstr ""

#. Id: 44036-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44036-Dialogue.mod:2
msgid "/MYOUR/ crops! Somebody do something!"
msgstr ""

#. Id: 44036-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (44035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44036-Dialogue.mod:3
msgid "/AAH/, my crops! My farm will be destroyed!"
msgstr ""

#. Id: 44036-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (44035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44036-Dialogue.mod:4
msgid "Somebody kill it! Do something, it's eating the crops!"
msgstr "有人殺了它！做點什麼，它正在吃莊稼！"

#. Id: 44038-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44038-Dialogue.mod:1
msgid "No! /MYOUR/ crops!"
msgstr ""

#. Id: 44038-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44038-Dialogue.mod:2
msgid "He's eating my crops!"
msgstr "他在吃我的莊稼！"

#. Id: 44038-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44038-Dialogue.mod:3
msgid "Oh no, no, no! My crops!"
msgstr "哦不，不，不！我的莊稼！"

#. Id: 44038-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44038-Dialogue.mod:4
msgid "No! No, leave my crops alone!"
msgstr "沒有！不，讓我的莊稼獨自一人！"

#. Id: 44039-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (44038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44039-Dialogue.mod:0
msgid "Curse you, gluttonous beast!"
msgstr "詛咒你，貪吃的野獸！"

#. Id: 44039-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (44038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44039-Dialogue.mod:1
msgid "Damn you, pest!"
msgstr "該死的，害蟲！"

#. Id: 44039-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44039-Dialogue.mod:2
msgid "Curse you, greedy beast!"
msgstr "詛咒你，貪婪的野獸！"

#. Id: 44039-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (44038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44039-Dialogue.mod:3
msgid "Damned pests!"
msgstr "該死的害蟲！"

#. Id: 44039-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (44038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44039-Dialogue.mod:4
msgid "Ruinous pests!"
msgstr "毀滅性的害蟲！"

#. Id: 44045-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:0
msgid "Gluttonous devil beasts!"
msgstr "貪吃惡魔的野獸！"

#. Id: 44045-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:1
msgid "Gluttonous demon creatures!"
msgstr "貪吃的惡魔生物！"

#. Id: 44045-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:2
msgid "Dark creatures! They'll bring famine to us all!"
msgstr "黑暗生物！他們會給我們所有人帶來飢荒！"

#. Id: 44045-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:3
msgid "/DIRTY/ beasts! Their gluttony will doom us all!"
msgstr ""

#. Id: 44045-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:5
msgid "No, /ANIMALTYPE/s! "
msgstr ""

#. Id: 44045-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:6
msgid "/ANIMALTYPE/s! We're infested!"
msgstr ""

#. Id: 44045-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44045-Dialogue.mod:7
msgid "/ANIMALTYPE/s! Narko has truly cursed us all!"
msgstr ""

#. Id: 44046-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:1
msgid "Protect the farm!"
msgstr "保護農場！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:2
msgid "My crops! My crops are being destroyed!"
msgstr "我的莊稼！我的莊稼被毀了！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:3
msgid "My crops! Lord Okran have mercy on my crops!"
msgstr "我的莊稼！奧克蘭勳爵憐憫我的莊稼！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:4
msgid "Lord Okran, aid me against these spawns of evil!"
msgstr "奧克蘭勳爵，幫助我對抗這些邪惡的催生！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:5
msgid "Lord Okran, grant my crops protection!"
msgstr "奧克蘭勳爵，保護我的莊稼！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:6
msgid "Lord Okran, protect my crops!"
msgstr "奧克蘭勳爵，保護我的莊稼！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:7
msgid "I pray for my crops! My livelihood!"
msgstr "我為我的莊稼祈禱！我的生計！"

#. Id: 44046-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (44045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy civ spots raptor eating crops
#: 44046-Dialogue.mod:8
msgid "A walking nightmare! Somebody save the crops!"
msgstr "一場噩夢！有人拯救莊稼！"
