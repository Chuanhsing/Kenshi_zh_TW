#. Id: 45980-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45980-Dialogue.mod:0
msgid "This... is MY kingdom... And I will fight for it until the end..."
msgstr ""

#. Id: 45981-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45980-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Immortal king
#: 45981-Dialogue.mod:0
msgid "Okay... so where are all the people?"
msgstr ""

#. Id: 45982-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45981-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45982-Dialogue.mod:0
msgid "They were killed by outside invaders, every last one of them. By outsiders like you."
msgstr ""

#. Id: 45984-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (45982-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45984-Dialogue.mod:0
msgid "For them, I will continue to protect their home, their graves..."
msgstr ""

#. Id: 45983-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45980-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Immortal king
#: 45983-Dialogue.mod:0
msgid "Fight for it? What are you talking about?"
msgstr ""

#. Id: 45986-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45983-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45986-Dialogue.mod:0
msgid "Leave this land!"
msgstr ""

#. Id: 45987-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (45986-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45987-Dialogue.mod:0
msgid "You'll leave, even if I have to kill you first!"
msgstr ""

#. Id: 45988-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45980-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Immortal king
#: 45988-Dialogue.mod:0
msgid "Then go ahead. Fight me for it!"
msgstr ""

#. Id: 45990-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45988-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45990-Dialogue.mod:0
msgid "You will not take this land!"
msgstr ""

#. Id: 45991-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (45990-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45991-Dialogue.mod:0
msgid "I'll make sure of it myself!"
msgstr ""

#. Id: 45989-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45980-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Immortal king
#: 45989-Dialogue.mod:0
msgid "I'll just be leaving then..."
msgstr ""

#. Id: 45992-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (45989-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45992-Dialogue.mod:0
msgid "I've lived too many years on this world to know not to trust an outsider..."
msgstr ""

#. Id: 45993-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (45992-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45993-Dialogue.mod:0
msgid "Only through ruthlessness can my kingdom truly be safe!"
msgstr ""

#. Id: 45994-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (45993-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Immortal king
#: 45994-Dialogue.mod:0
msgid "I'll make sure you CAN'T return here!"
msgstr ""

