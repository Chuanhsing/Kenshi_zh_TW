#. Id: 1532549-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532549-Dialogue.mod:0
msgid "Fishman Island... There's supposed to be a famous sword here..."
msgstr ""

#. Id: 1532552-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1532550-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#. Id: 1532589-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1532551-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532552-Dialogue.mod:0 1532589-Dialogue.mod:0
msgid "Wouldn't mind getting my hands on that small fortune..."
msgstr ""

#. Id: 1532553-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532552-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#. Id: 1532590-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532589-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532553-Dialogue.mod:0 1532590-Dialogue.mod:0
msgid "Pretty sure the Tech Hunters cleared the island out long ago though, last I heard."
msgstr ""

#. Id: 1532595-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532555-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532595-Dialogue.mod:0
msgid "The demon sword Narko forged from the abyss?"
msgstr ""

#. Id: 1532596-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (1532595-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532596-Dialogue.mod:0
msgid "I've heard stories but... I don't know much of the south."
msgstr ""

#. Id: 1532597-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (1532596-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532597-Dialogue.mod:0
msgid "I don't know about demons... but a giant fish man wields it."
msgstr ""

#. Id: 1532598-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (1532611-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532598-Dialogue.mod:0
msgid "... It just gets weirder and weirder on the outside."
msgstr ""

#. Id: 1532599-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (1532598-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532599-Dialogue.mod:0
msgid "Perhaps I should have stayed in the Holy Plains."
msgstr ""

#. Id: 1532600-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (1532599-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532600-Dialogue.mod:0
msgid "Tch. Okranite logic."
msgstr ""

#. Id: 1532601-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (1532600-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532601-Dialogue.mod:0
msgid "You can close your eyes all you want, but your fears won't go away."
msgstr ""

#. Id: 1532602-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (1532601-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532602-Dialogue.mod:0
msgid "... Not to mention you'll be defenseless against the fish men."
msgstr ""

#. Id: 1532613-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532556-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532613-Dialogue.mod:0
msgid "The demon sword? I used to hear priests preach of Narko's sword..."
msgstr ""

#. Id: 1532616-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (1532613-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532616-Dialogue.mod:0
msgid "I don't know about demon swords..."
msgstr ""

#. Id: 1532617-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (1532616-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532617-Dialogue.mod:0
msgid "But a giant fish man wields it."
msgstr ""

#. Id: 1532619-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (1532618-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532619-Dialogue.mod:0
msgid "... It just gets weirder and weirder outside the Holy Plains."
msgstr ""

#. Id: 1532554-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532560-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532554-Dialogue.mod:0
msgid "Right. Because your wealth of tech hunting knowledge has been so accurate so far."
msgstr ""

#. Id: 1532557-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (1532554-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532557-Dialogue.mod:0
msgid "Hey! Not my fault, you're a jinx..."
msgstr ""

#. Id: 1532558-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (1532557-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532558-Dialogue.mod:0
msgid "If I had my vote you'd be out of the squad already."
msgstr ""

#. Id: 1532559-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (1532558-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532559-Dialogue.mod:0
msgid "Me, a jinx? I'm the reason you're not rotting in a cannibal den so far!"
msgstr ""

#. Id: 1532561-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (1532559-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532561-Dialogue.mod:0
msgid "Shhh..."
msgstr ""

#. Id: 1532562-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (1532561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532562-Dialogue.mod:0
msgid "Less talking or the gurglers will hear us."
msgstr ""

#. Id: 1532563-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (1532562-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#. Id: 1532606-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532604-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532563-Dialogue.mod:0 1532606-Dialogue.mod:0
msgid "... Gurglers?"
msgstr ""

#. Id: 1532568-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (1532563-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#. Id: 1532607-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (1532606-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532568-Dialogue.mod:0 1532607-Dialogue.mod:0
msgid "Fish men..."
msgstr ""

#. Id: 1532567-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (1532568-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532567-Dialogue.mod:0
msgid "For one, they don't have ears so I'll talk all I want."
msgstr ""

#. Id: 1532570-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (1532569-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532570-Dialogue.mod:0
msgid "AS WILL I."
msgstr ""

#. Id: 1532571-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (1532570-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532571-Dialogue.mod:0
msgid "COWARDS WHISPER."
msgstr ""

#. Id: 1532572-Dialogue.mod
#. -- Index: 17.0
#. -- Parent line: 16 (1532571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532572-Dialogue.mod:0
msgid "Mercy, Okran... Shhh."
msgstr ""

#. Id: 1532573-Dialogue.mod
#. -- Index: 18.0
#. -- Parent line: 17 (1532572-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532573-Dialogue.mod:0
msgid "ARE YOU A COWARD, KIN?"
msgstr ""

#. Id: 1532574-Dialogue.mod
#. -- Index: 19.0
#. -- Parent line: 18 (1532573-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532574-Dialogue.mod:0
msgid "Oh god, why won't you stop shouting..."
msgstr ""

#. Id: 1532575-Dialogue.mod
#. -- Index: 20.0
#. -- Parent line: 19 (1532574-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532575-Dialogue.mod:0
msgid "Hmph. These so-called fish men, I'll show them who is coward..."
msgstr ""

#. Id: 1532583-Dialogue.mod
#. -- Index: 21.0
#. -- Parent line: 20 (1532575-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532583-Dialogue.mod:0
msgid "... But..."
msgstr ""

#. Id: 1532584-Dialogue.mod
#. -- Index: 22.0
#. -- Parent line: 21 (1532583-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532584-Dialogue.mod:0
msgid "But nobody said you were a coward in the first place..."
msgstr ""

#. Id: 1532577-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (1532576-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532577-Dialogue.mod:0
msgid "Ugh, not like I haven't seen a million gurglers before..."
msgstr ""

#. Id: 1532579-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (1532577-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532579-Dialogue.mod:0
msgid "Still, I've had better conversations with them than I have with most humans, I'll give them that."
msgstr ""

#. Id: 1532580-Dialogue.mod
#. -- Index: 17.0
#. -- Parent line: 16 (1532579-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532580-Dialogue.mod:0
msgid "I thought they could only make gurgling sounds, totally incapable of intelligible speech?"
msgstr ""

#. Id: 1532581-Dialogue.mod
#. -- Index: 18.0
#. -- Parent line: 17 (1532580-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532581-Dialogue.mod:0
msgid "Correct."
msgstr ""

#. Id: 1532582-Dialogue.mod
#. -- Index: 19.0
#. -- Parent line: 18 (1532581-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532582-Dialogue.mod:0
msgid "... Hum."
msgstr ""

#. Id: 1532565-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (1532564-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532565-Dialogue.mod:0
msgid "Nonsense, let me handle those fish men..."
msgstr ""

#. Id: 1532566-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (1532565-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532566-Dialogue.mod:0
msgid "I'm fluent in crustacean, you know."
msgstr ""

#. Id: 1532612-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (1532603-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532612-Dialogue.mod:0
msgid "This I have to see."
msgstr ""

#. Id: 1532586-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (1532553-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#: 1532586-Dialogue.mod:0
msgid "Anyway, we should be quiet or the gurglers might hear us."
msgstr ""

#. Id: 1532588-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (1532587-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Fish island biome enty
#: 1532588-Dialogue.mod:0
msgid "Ugh, nothing new. Not like I haven't seen a million gurglers before..."
msgstr ""

#. Id: 1532592-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532591-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532592-Dialogue.mod:0
msgid "Last you heard 10 years ago before getting yourself trapped in Mongrel. Accurate."
msgstr ""

#. Id: 1532593-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (1532592-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532593-Dialogue.mod:0
msgid "... It was a well needed vacation for a tired Tech Hunter."
msgstr ""

#. Id: 1532594-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (1532593-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Fish island biome enty
#: 1532594-Dialogue.mod:0
msgid "Sure... Mongrel is a lovely place to settle down."
msgstr ""

#. Id: 1532605-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1532549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#. Id: 1532614-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1532610-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532605-Dialogue.mod:0 1532614-Dialogue.mod:0
msgid "As well as gurglers..."
msgstr ""

#. Id: 1532608-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532607-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Fish island biome enty
#. Id: 1532615-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532614-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Fish island biome enty
#: 1532608-Dialogue.mod:0 1532615-Dialogue.mod:0
msgid "We should be quiet or they'll hear us."
msgstr ""

