#. Id: 15027-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:0
msgid "Don't even try it, /FLATSKIN/"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:1
msgid "Cut it out. /FLATSKIN/"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:2
msgid "Keep doing that and I'll snap your arms off"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:3
msgid "You. No."
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:4
msgid "Don't make me break your arms, /FLATSKIN/"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:5
msgid "Don't give me a reason to break you, /FLATSKIN/"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:6
msgid "Don't even think about it"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:7
msgid "Keep picking, /FLATSKIN/, see what happens"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:8
msgid "Keep picking, /FLATSKIN/. I dare you."
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:9
msgid "Don't push it, /FLATSKIN/"
msgstr ""

#. Id: 15027-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees prisoner lockpick
#: 15027-Dialogue.mod:10
msgid "Open it up, /FLATSKIN/. I'm waiting for you right here."
msgstr ""

