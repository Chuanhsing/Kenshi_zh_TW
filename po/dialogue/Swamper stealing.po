#. Id: 44700-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (12445-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44700-Dialogue.mod:0
msgid "Ah, don't make me have to chase you..."
msgstr "啊，不要讓我追你..."

#. Id: 44700-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (12445-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44700-Dialogue.mod:2
msgid "Keep runnin' if you know what's good for you!"
msgstr "如果你知道什麼對你有好處，那就繼續跑吧！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. Id: 16136-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing imprisoned
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 13871-Escapes.mod:0 16136-Dialogue.mod:3
msgid "Steal from me and I'll stab your eyes out!"
msgstr "偷走我，我會刺傷你的眼睛！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:1
msgid "Thief! I'll cut your face!"
msgstr "賊！我會削減你的臉！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:2
msgid "Robber! I'll pull your arms off! Pull 'em off!"
msgstr "強盜！我會拉開你的手臂！把它們拉下來！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:3
msgid "Thief! I'll kill you!"
msgstr "賊！我會殺了你！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:4
msgid "Steal from me and I'll castrate you!"
msgstr "偷走我，我會閹割你！"

#. Id: 13871-Escapes.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:5
msgid "You thieving /BASTARD/! I'll tear your eyelids off!"
msgstr ""

#. Id: 13871-Escapes.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 13871-Escapes.mod:6
msgid "You thieving /BASTARD/! I'll chop you up!"
msgstr ""

#. Id: 44698-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13871-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44698-Dialogue.mod:0
msgid "Come back here! Just let me cut you a bit!"
msgstr "回到這來！讓我切你一點！"

#. Id: 44698-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13871-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44698-Dialogue.mod:1
msgid "Argh, I hate runnin'! Get back here!"
msgstr "唉，我討厭奔跑'！回到這裡！"

#. Id: 44698-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (13871-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44698-Dialogue.mod:4
msgid "Quit runnin'! Let me cut you!"
msgstr "退出跑步'！讓我切你！"

#. Id: 44698-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (13871-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44698-Dialogue.mod:5
msgid "I'll cut you!"
msgstr "我會砍你的！"

#. Id: 44698-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (13871-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. Id: 48785-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 44698-Dialogue.mod:6 48785-Dialogue.mod:3
msgid "I'll /CUTTHOSELIPS/!"
msgstr ""

#. Id: 44699-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (44698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44699-Dialogue.mod:0
msgid "Just get it over with and let me cut you!"
msgstr "只是把它搞定，讓我切你！"

#. Id: 44699-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (44698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44699-Dialogue.mod:1
msgid "Take your punishment!"
msgstr "接受你的懲罰！"

#. Id: 44699-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (44698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44699-Dialogue.mod:2
msgid "The quicker we get this over with, the better!"
msgstr "我們越快越好，越好！"

#. Id: 44699-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (44698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44699-Dialogue.mod:3
msgid "I'll make you suffer!"
msgstr "我會讓你受苦！"

#. Id: 44699-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (44698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44699-Dialogue.mod:4
msgid "I hate runnin'! You're pissin' me off, /INSULT/!"
msgstr ""

#. Id: 48771-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48771-Dialogue.mod:0
msgid "Stealin' from the Blackshifters?!"
msgstr "來自Blackshifters的斯大林？！"

#. Id: 48771-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48771-Dialogue.mod:1
msgid "You steal from the Blackshifters, you die."
msgstr "你從Blackshifters偷走，你死了。"

#. Id: 48771-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48771-Dialogue.mod:2
msgid "Big mistake stealin' from us, /BOY/!"
msgstr ""

#. Id: 48771-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48771-Dialogue.mod:3
msgid "Nobody steals from the Blackshifters and walks away."
msgstr "沒有人從Blackshifters偷走並走開。"

#. Id: 48771-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48771-Dialogue.mod:4
msgid "Tryin' to cheat us?!"
msgstr "試著欺騙我們？！"

#. Id: 48772-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48771-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48772-Dialogue.mod:0
msgid "Break /HIS/ legs."
msgstr ""

#. Id: 48772-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48771-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48772-Dialogue.mod:1
msgid "Break /HIM/."
msgstr ""

#. Id: 48774-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48771-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 48774-Dialogue.mod:0
msgid "No one outruns /MYFACTION/!"
msgstr ""

#. Id: 48774-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48771-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 48774-Dialogue.mod:1
msgid "You'll never escape /MYFACTION/!"
msgstr ""

#. Id: 48774-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (48771-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 48774-Dialogue.mod:2
msgid "Ha! Think you can escape /MYFACTION/?.. Think again!"
msgstr ""

#. Id: 48784-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#. Id: 48768-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48784-Dialogue.mod:0 48768-Dialogue.mod:1
msgid "Stealin' from /MYFACTION/?!"
msgstr ""

#. Id: 48784-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48784-Dialogue.mod:1
msgid "Stealin' from /MEUS/, /RAT/?!"
msgstr ""

#. Id: 48784-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48784-Dialogue.mod:2
msgid "Tryin' to steal from /MEUS/, worm?!"
msgstr ""

#. Id: 48784-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48784-Dialogue.mod:3
msgid "You stealin' from me? Huh, /RAT/?!"
msgstr ""

#. Id: 48785-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48785-Dialogue.mod:0
msgid "Gut the thief! More /BODYPARTS/ to add to the collection..."
msgstr ""

#. Id: 48785-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48785-Dialogue.mod:1
msgid "I'll cut you up! You won't escape me!"
msgstr "我會打你的！你不會逃避我！"

#. Id: 48785-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48785-Dialogue.mod:2
msgid "I'll gut you... I'll /CUTTHOSELIPS/!"
msgstr ""

#. Id: 48785-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48785-Dialogue.mod:4
msgid "I'll cut you up into pieces!"
msgstr "我會把你切成碎片！"

#. Id: 48785-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (48784-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48785-Dialogue.mod:5
msgid "There'll be nothin' left of you after I'm done with you!"
msgstr "在我和你結束之後，你們還剩下什麼！"

#. Id: 48790-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48790-Dialogue.mod:0
msgid "Bad move, thief!"
msgstr "壞動，小偷！"

#. Id: 48790-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48790-Dialogue.mod:1
msgid "Shouldn't have done that, thief!"
msgstr "不應該那樣做，小偷！"

#. Id: 48790-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48790-Dialogue.mod:2
msgid "Wrong clan to steal fom, thief!"
msgstr "錯誤的戰隊偷了fom，小偷！"

#. Id: 48790-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing
#: 48790-Dialogue.mod:3
msgid "Bad luck thief, no one steals from the Grayflayers and lives!"
msgstr "運氣不好的小偷，沒有人從Grayflayers偷走並生活！"
