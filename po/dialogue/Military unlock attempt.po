#. Id: 15642-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:0
msgid "Do not fail me!"
msgstr "不要讓我失望！"

#. Id: 15642-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:1
msgid "Open it!"
msgstr "打開它！"

#. Id: 15642-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:2
msgid "Get it open!"
msgstr "把它打開！"

#. Id: 15642-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:3
msgid "Open it already!"
msgstr "打開它吧！"

#. Id: 15642-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:4
msgid "Get me out already!"
msgstr "讓我出去！"

#. Id: 15642-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 15642-Dialogue.mod:7
msgid "Faster, hurry!"
msgstr "更快，快點！"

#. Id: 15643-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 15643-Dialogue.mod:0
msgid "Stop floundering! Get it open!"
msgstr "停止掙扎！把它打開！"

#. Id: 15643-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 15643-Dialogue.mod:1
msgid "Quit fumbling! Open it!"
msgstr "不要笨手笨腳！打開它！"

#. Id: 15643-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 15643-Dialogue.mod:2
msgid "You're too slow! Hurry up!"
msgstr "你太慢了！趕快！"

#. Id: 15643-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military unlock attempt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 15643-Dialogue.mod:3
msgid "Focus! Get it open!"
msgstr "焦點！把它打開！"
