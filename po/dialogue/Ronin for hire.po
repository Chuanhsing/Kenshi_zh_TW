#. Id: 20037-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (20032-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin for hire
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>3000, Tag: -1
#: 20037-Dialogue.mod:0
msgid "I want to help you pay your debt"
msgstr ""

#. Id: 20042-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (20037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin for hire
#: 20042-Dialogue.mod:0
msgid "Really? You have the Cats to help me out?"
msgstr ""

#. Id: 20043-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (20042-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin for hire
#: 20043-Dialogue.mod:0
msgid "Sure do. You're hired"
msgstr ""

#. Id: 20046-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (20042-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin for hire
#: 20046-Dialogue.mod:0
msgid "Wait... no, sorry"
msgstr ""

#. Id: 20041-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (20032-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin for hire
#: 20041-Dialogue.mod:0
msgid "Nothing, sorry to bother you"
msgstr ""

#. Id: 20034-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin for hire
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO>30, Tag: -1
#: 20034-Dialogue.mod:0
msgid "Not seen you in a while. What brings you here?"
msgstr ""

