msgstr "......不！"

#. Id: 17917-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: My Race(s): Hive Prince
#: 17917-Dialogue.mod:3
msgid "Argggh! Noooo!"
msgstr "Argggh！不要！"

#. Id: 17917-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: My Race(s): Hive Prince
#: 17917-Dialogue.mod:4
msgid "Ahhhhh!"
msgstr "AHHHHH！"

#. Id: 17917-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: My Race(s): Hive Prince
#: 17917-Dialogue.mod:6
msgid "Arrrgh!"
msgstr "Arrrgh！"

#. Id: 17917-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: My Race(s): Hive Prince
#: 17917-Dialogue.mod:7
msgid "Ahhh! Ahhhhhh!"
msgstr "唉唉！ AHHHHHH！"

#. Id: 17917-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: My Race(s): Hive Prince
#: 17917-Dialogue.mod:8
msgid "No no no no no no!"
msgstr "不不不不不不！"

#. Id: 17918-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17918-Dialogue.mod:0
msgid "I want to go back to the hive!"
msgstr "我想回到蜂巢！"

#. Id: 17918-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17918-Dialogue.mod:1
msgid "I want to be back at the hive!"
msgstr "我想回到蜂巢！"

#. Id: 17918-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17918-Dialogue.mod:2
msgid "Take me back to the hive!"
msgstr "帶我回到蜂巢！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:0
msgid "I don't want to die!"
msgstr "我不想死！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:1
msgid "Nooooooo!"
msgstr "Nooooooo！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:2
msgid "Why is this happening!? Why?!!"
msgstr "為什麼會這樣？！？為什麼？！！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:3
msgid "Why is this happening?!.. WHY?! "
msgstr "為什麼會這樣？？為什麼？！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:5
msgid "ARGGHHH! Please, NOOOO!"
msgstr "ARGGHHH！不，NOOOO！"

#. Id: 17920-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (17917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17920-Dialogue.mod:6
msgid "Please don't do this to me!!!"
msgstr "請不要這樣對我！"

#. Id: 18038-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 18038-Dialogue.mod:0
msgid "No, not me!"
msgstr "不，不是我！"

#. Id: 18038-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 18038-Dialogue.mod:1
msgid "Aaah, I'm next!"
msgstr "啊，我是下一個！"

#. Id: 18038-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 18038-Dialogue.mod:2
msgid "Ahhh, am I next?!"
msgstr "啊，我下一個嗎？！"

#. Id: 18038-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 18038-Dialogue.mod:3
msgid "Arrgh, why are they looking at me like that!"
msgstr "阿爾，為什麼他們這樣看著我！"

#. Id: 17967-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17967-Dialogue.mod:0
msgid "This... this isn't real..."
msgstr "這......這不是真的......"

#. Id: 17967-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17967-Dialogue.mod:1
msgid "This... this isn't happening..."
msgstr "這......這不會發生......"

#. Id: 17967-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18038-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin marked for death
#: 17967-Dialogue.mod:2
msgid "How is this even happening!?"
msgstr "怎麼會發生這種情況！？"
