#. Id: 95619-__Mourn.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95619-__Mourn.mod:0
msgid "You don't know real pain 'til you've adda whistler chew your legs off..."
msgstr ""

#. Id: 95621-__Mourn.mod
#. -- Index: 2.0
#. -- Parent line: 1 (95620-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95621-__Mourn.mod:0
msgid "... Your legs look fine to me."
msgstr ""

#. Id: 95622-__Mourn.mod
#. -- Index: 3.0
#. -- Parent line: 2 (95621-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95622-__Mourn.mod:0
msgid "Don't argue wimme! "
msgstr ""

#. Id: 95730-__Mourn.mod
#. -- Index: 4.0
#. -- Parent line: 3 (95622-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95730-__Mourn.mod:0
msgid "Barkeep! I anna anadda ramm!"
msgstr ""

#. Id: 95623-__Mourn.mod
#. -- Index: 5.0
#. -- Parent line: 4 (95730-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95623-__Mourn.mod:0
msgid "*smash*"
msgstr ""

#. Id: 95626-__Mourn.mod
#. -- Index: 7.0
#. -- Parent line: 6 (95625-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95626-__Mourn.mod:0
msgid "Pipe down! Can't an old adventurer retire in PEACE?!"
msgstr ""

#. Id: 95627-__Mourn.mod
#. -- Index: 8.0
#. -- Parent line: 7 (95626-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95627-__Mourn.mod:0
msgid "You rookies... as bad as living with the nobles..."
msgstr ""

#. Id: 95628-__Mourn.mod
#. -- Index: 9.0
#. -- Parent line: 8 (95627-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95628-__Mourn.mod:0
msgid "Fucking sand munchers!"
msgstr ""

#. Id: 95629-__Mourn.mod
#. -- Index: 10.0
#. -- Parent line: 9 (95628-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95629-__Mourn.mod:0
msgid "... Something we can both agree on at least."
msgstr ""

#. Id: 95630-__Mourn.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95630-__Mourn.mod:0
msgid "Made it past the mist ghouls yet, newbie?"
msgstr ""

#. Id: 95632-__Mourn.mod
#. -- Index: 2.0
#. -- Parent line: 1 (95631-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95632-__Mourn.mod:0
msgid "Ain't no such thing as mist ghouls..."
msgstr ""

#. Id: 95633-__Mourn.mod
#. -- Index: 3.0
#. -- Parent line: 2 (95632-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95633-__Mourn.mod:0
msgid "I was talking to the newbie!"
msgstr ""

#. Id: 95634-__Mourn.mod
#. -- Index: 4.0
#. -- Parent line: 3 (95633-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95634-__Mourn.mod:0
msgid "I made it past 'em, gave 'em a moonin' 'n all."
msgstr ""

#. Id: 95635-__Mourn.mod
#. -- Index: 5.0
#. -- Parent line: 4 (95634-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95635-__Mourn.mod:0
msgid "Next time I'll make it all the way. Had to carry my mate back though..."
msgstr ""

#. Id: 95636-__Mourn.mod
#. -- Index: 6.0
#. -- Parent line: 5 (95635-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95636-__Mourn.mod:0
msgid "Lost his eyes to a howler. Couldn't leave him like that 'cause, y'know, I'm a hero like that."
msgstr ""

#. Id: 95637-__Mourn.mod
#. -- Index: 7.0
#. -- Parent line: 6 (95636-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95637-__Mourn.mod:0
msgid "Squaddies though, they're dead weight. Next-"
msgstr ""

#. Id: 95638-__Mourn.mod
#. -- Index: 8.0
#. -- Parent line: 7 (95637-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95638-__Mourn.mod:0
msgid "Are you still talking?"
msgstr ""

#. Id: 95639-__Mourn.mod
#. -- Index: 9.0
#. -- Parent line: 8 (95638-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95639-__Mourn.mod:0
msgid "Nope. I got an expedition to prepare for."
msgstr ""

#. Id: 95640-__Mourn.mod
#. -- Index: 10.0
#. -- Parent line: 9 (95639-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95640-__Mourn.mod:0
msgid "Got bigger things to do than lurk in a dark rum sink all day."
msgstr ""

#. Id: 95661-__Mourn.mod
#. -- Index: 12.0
#. -- Parent line: 11 (95660-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95661-__Mourn.mod:0
msgid "Hunters these days... Too much talking, not enough walking."
msgstr ""

#. Id: 95662-__Mourn.mod
#. -- Index: 13.0
#. -- Parent line: 12 (95661-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95662-__Mourn.mod:0
msgid "Mmhmm."
msgstr ""

#. Id: 95695-__Mourn.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95695-__Mourn.mod:0
msgid "Headin' out to the Ashlands?"
msgstr ""

#. Id: 95732-__Mourn.mod
#. -- Index: 2.0
#. -- Parent line: 1 (95731-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95732-__Mourn.mod:0
msgid "Well I didn't exactly come here for the entertainment."
msgstr ""

#. Id: 95733-__Mourn.mod
#. -- Index: 3.0
#. -- Parent line: 2 (95732-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95733-__Mourn.mod:0
msgid "No offense, gramps."
msgstr ""

#. Id: 95734-__Mourn.mod
#. -- Index: 4.0
#. -- Parent line: 3 (95733-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95734-__Mourn.mod:0
msgid "That journey 'll screw up the most seasoned of tech hunters, I warn you..."
msgstr ""

#. Id: 95696-__Mourn.mod
#. -- Index: 5.0
#. -- Parent line: 4 (95734-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95696-__Mourn.mod:0
msgid "Just look at Old Moot. Comes in 'ere, eats his gohan and stares at that there wall in silence..."
msgstr ""

#. Id: 95735-__Mourn.mod
#. -- Index: 6.0
#. -- Parent line: 5 (95696-__Mourn.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Lagoon bar patron 1
#: 95735-__Mourn.mod:0
msgid "Yeah, I seen him..."
msgstr ""

#. Id: 95697-__Mourn.mod
#. -- Index: 7.0
#. -- Parent line: 6 (95735-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95697-__Mourn.mod:0
msgid "Some say he made it past Grey Shelf, past the gorillos and whatever lies beyond them...."
msgstr ""

#. Id: 95698-__Mourn.mod
#. -- Index: 8.0
#. -- Parent line: 7 (95697-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95698-__Mourn.mod:0
msgid "Whatever he saw, he hasn't spoken a word about it since."
msgstr ""

#. Id: 95699-__Mourn.mod
#. -- Index: 9.0
#. -- Parent line: 8 (95698-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95699-__Mourn.mod:0
msgid "Not like all these other adventurers, full 'o hot air..."
msgstr ""

#. Id: 95701-__Mourn.mod
#. -- Index: 11.0
#. -- Parent line: 10 (95700-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95701-__Mourn.mod:0
msgid "I ain't full of hot air! I've seen the grievewraiths in flesh..."
msgstr ""

#. Id: 95702-__Mourn.mod
#. -- Index: 12.0
#. -- Parent line: 11 (95701-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95702-__Mourn.mod:0
msgid "You'll know a true survivor of the south when you see one, /OUTLANDER/, believe you me."
msgstr ""

#. Id: 95703-__Mourn.mod
#. -- Index: 13.0
#. -- Parent line: 12 (95702-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95703-__Mourn.mod:0
msgid "Hoy, you ignoring me?"
msgstr ""

#. Id: 95704-__Mourn.mod
#. -- Index: 14.0
#. -- Parent line: 13 (95703-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95704-__Mourn.mod:0
msgid "'Cause a true survivor ain't all there. And he won't be talking about it neither."
msgstr ""

#. Id: 95705-__Mourn.mod
#. -- Index: 15.0
#. -- Parent line: 14 (95704-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95705-__Mourn.mod:0
msgid "I made it past Grey Shelf 'n' I got a 'lil souvenir to prove it. See this gash on my shoulder..."
msgstr ""

#. Id: 95706-__Mourn.mod
#. -- Index: 16.0
#. -- Parent line: 15 (95705-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95706-__Mourn.mod:0
msgid "Survivors are rare. And don't let anyone make you think otherwise."
msgstr ""

#. Id: 95707-__Mourn.mod
#. -- Index: 17.0
#. -- Parent line: 16 (95706-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#: 95707-__Mourn.mod:0
msgid "Deaf old coot."
msgstr ""

#. Id: 95708-__Mourn.mod
#. -- Index: 18.0
#. -- Parent line: 17 (95707-__Mourn.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 95708-__Mourn.mod:0
msgid "My hearing's fine, rookie."
msgstr ""

#. Id: 95709-__Mourn.mod
#. -- Index: 19.0
#. -- Parent line: 18 (95708-__Mourn.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Lagoon bar patron 1
#. Id: 62095-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (62094-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Wander trader base visit
#: 95709-__Mourn.mod:0 62095-Dialogue.mod:0
msgid "Kch. "
msgstr ""

#. Id: 1534036-__SE world states.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 1534036-__SE?world?states.mod:0
msgid "There's something powerful in the Ashlands..."
msgstr ""

#. Id: 1534037-__SE world states.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1534036-__SE world states.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 1534037-__SE?world?states.mod:0
msgid "You can feel it..."
msgstr ""

#. Id: 1534038-__SE world states.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1534037-__SE world states.mod)
#. -- Speaker: Me (0)
#. -- Group title: Lagoon bar patron 1
#: 1534038-__SE?world?states.mod:0
msgid "And it's the only thing you don't ever want to discover."
msgstr ""

