#. Id: 53525-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:0
msgid "Let the /CRAVEN/ run! "
msgstr ""

#. Id: 53525-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:1
msgid "Hmph. The stamina of a deserter."
msgstr ""

#. Id: 53525-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:2
msgid "Hmph. Only quitters and /CRAVEN/s run."
msgstr ""

#. Id: 53525-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:3
msgid "Hmph. Cowardice. "
msgstr ""

#. Id: 53525-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:4
msgid "The /CRAVEN/ is only good for one thing. Fleeing."
msgstr ""

#. Id: 53525-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53525-Dialogue.mod:5
msgid "Hmph. Shameful."
msgstr ""

#. Id: 53526-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53525-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53526-Dialogue.mod:0
msgid "/HEIS/ not worth /MYOUR/ time..."
msgstr ""

#. Id: 53526-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (53525-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53526-Dialogue.mod:1
msgid "Swatting insects is beneath a real warrior. I've no time for it. "
msgstr ""

#. Id: 53526-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (53525-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53526-Dialogue.mod:2
msgid "I've no interest in chasing insects."
msgstr ""

#. Id: 53526-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (53525-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#: 53526-Dialogue.mod:3
msgid "I'll waste no more time chasing weaklings."
msgstr ""

#. Id: 53527-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 53527-Dialogue.mod:0
msgid "Then retreat, /CRAVEN/! After all, it's what a /FLATSKIN/ does best."
msgstr ""

#. Id: 53527-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 53527-Dialogue.mod:1
msgid "Keep running, /FLATSKIN/... You're not worth my breath."
msgstr ""

#. Id: 53527-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek give up chase
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 53527-Dialogue.mod:2
msgid "A /FLATSKIN/ is only good for running. Leave /HIM/."
msgstr ""

