#. Id: 8423-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:0
msgid "NO! NOO! They're coming for me!!!"
msgstr "沒有！ NOO！他們來找我!!!"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:1
msgid "HELP ME! HELP MEEEEE!"
msgstr "幫我！幫助MEEEEE！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:2
msgid "OH GOD! THEY'RE COMING FOR ME!"
msgstr "天啊！他們來找我！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:3
msgid "HELP! NO! Stay away from me, STAY AWAY!"
msgstr "救命！沒有！遠離我，請遠離我！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:4
msgid "No! This can't be happening to me! NOOOO!"
msgstr "沒有！這不可能發生在我身上！不要！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:5
msgid "Someone! Somebody help me! NO, NO NO NO!"
msgstr "有人！來人幫幫我！不不不不！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:6
msgid "Somebody do something! O..oh no!! HELP!"
msgstr "有人做點什麼！哦......不！救命！"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:7
msgid "No! Not me.... NOT ME!!!!"
msgstr "沒有！不是我......不是我!!!!"

#. Id: 8423-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#: 8423-Dialogue.mod:8
msgid "Not like this! No, don't do this to me, no!"
msgstr "不喜歡這個！不，不要這樣對我，不！"

#. Id: 8424-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:0
msgid "/AAH/! Damn /BASTARD/s! Dammit!!"
msgstr ""

#. Id: 8424-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:1
msgid "Damn you all to hell! /CRAPPY/ cannibal /RAT/ /BASTARD/s!"
msgstr ""

#. Id: 8424-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:2
msgid "/AAH/! I fucking hate cannibals!!!"
msgstr ""

#. Id: 8424-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:3
msgid "Goddamn it!"
msgstr "他媽的！"

#. Id: 8424-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:4
msgid "Dammit, no!!! Go away!"
msgstr "該死，不！走開！"

#. Id: 8424-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:5
msgid "No, /FUCKOFF/! Leave me alone!"
msgstr ""

#. Id: 8424-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8424-Dialogue.mod:6
msgid "They're coming for me... No, /FUCKOFF/!"
msgstr ""

#. Id: 8426-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 8426-Dialogue.mod:0
msgid "What? NO! This can't be happening!"
msgstr "什麼？沒有！這不可能發生！"

#. Id: 8426-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 8426-Dialogue.mod:1
msgid "This can't be! No! Not me, not now!!!"
msgstr "這不可能！沒有！不是我，不是現在！"

#. Id: 8426-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 8426-Dialogue.mod:2
msgid "No! Go away! Shoo! NOOOOO! NO!"
msgstr "沒有！走開！噓！真是沒有！沒有！"

#. Id: 8426-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 8426-Dialogue.mod:3
msgid "Oh /FUCK/! No! NOOOOO!"
msgstr ""

#. Id: 8428-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:0
msgid "Damn dirty fucking cannibals!"
msgstr "該死的他媽的食人族！"

#. Id: 8428-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:1
msgid "Just you try it! JUST TRY ME! /LAUGH/!"
msgstr ""

#. Id: 8428-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:2
msgid "They're coming for me... THEY CAN'T HANDLE ME!!! /CRAPPY/ animals!"
msgstr ""

#. Id: 8428-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:3
msgid "/FUCKOFF/! You can't handle THIS, /FUCKING/ puny cannibal /BASTARD/s! "
msgstr ""

#. Id: 8428-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:4
msgid "Oh yeah? /FUCKING/ try me! YOU CAN'T HANDLE ME!"
msgstr ""

#. Id: 8428-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8428-Dialogue.mod:5
msgid "So it's like that is it? /FUCKING/ /BASTARD/ animals! Come near me and I'll 'av your /BODYPARTS/!"
msgstr ""

#. Id: 12249-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 12249-Dialogue?(10).mod:0
msgid "No, /FUCKOFF/! Eat someone else! I taste like shit!"
msgstr ""

#. Id: 12249-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 12249-Dialogue?(10).mod:1
msgid "/AAH/ /FUCKOFF/! Eat that /IDIOT/ over there, not ME!"
msgstr ""

#. Id: 12249-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 12249-Dialogue?(10).mod:2
msgid "Don't eat me! What about THEM over there? They look way meatier!"
msgstr "別吃我！那邊的那個人怎麼樣？他們看起來更有趣！"

#. Id: 12249-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 12249-Dialogue?(10).mod:3
msgid "/FUCKOFF/! I swear I haven't bathed in a month, you don't wanna eat me!"
msgstr ""

#. Id: 12249-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandits marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 12249-Dialogue?(10).mod:4
msgid "No, /FUCKOFF/! Eat someone else! I'm all bone, I swear!"
msgstr ""
