#. Id: 1532720-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532720-Dialogue.mod:0
msgid "Come to grovel before the storm finally hits? Well, you have two choices: pay your overdue taxes and late fees, or face punishment."
msgstr ""

#. Id: 1532721-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1532720-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt tax evasion campaign
#: 1532721-Dialogue.mod:0
msgid "I'll pay..."
msgstr ""

#. Id: 1532724-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1532721-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532724-Dialogue.mod:0
msgid "Good. But the total owed has now gone up to 6000 Cats - It costs money to send an army out, you know. Someone's gotta pay for it..."
msgstr ""

#. Id: 1532734-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532733-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532734-Dialogue.mod:0
msgid "Pay on time next time, eh."
msgstr ""

#. Id: 1532727-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532724-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt tax evasion campaign
#: 1532727-Dialogue.mod:0
msgid "On second thought... nah."
msgstr ""

#. Id: 1532728-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (1532727-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532728-Dialogue.mod:0
msgid "*sigh*..."
msgstr ""

#. Id: 1532729-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (1532728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532729-Dialogue.mod:0
msgid "When will you people learn to just pay your damn taxes..."
msgstr ""

#. Id: 1532738-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532737-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532738-Dialogue.mod:0
msgid "I'll take that as the second choice!"
msgstr ""

#. Id: 1532723-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1532720-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt tax evasion campaign
#: 1532723-Dialogue.mod:0
msgid "I'm a glutton for punishment."
msgstr ""

#. Id: 1532723-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (1532720-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt tax evasion campaign
#: 1532723-Dialogue.mod:1
msgid "Punishment sounds fun."
msgstr ""

#. Id: 1532723-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (1532720-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt tax evasion campaign
#: 1532723-Dialogue.mod:2
msgid "Punishment please."
msgstr ""

#. Id: 1532741-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1532740-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt tax evasion campaign
#: 1532741-Dialogue.mod:0
msgid "It'll be my pleasure!"
msgstr ""

