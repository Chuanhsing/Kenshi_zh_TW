#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#. Id: 8687-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bo healing
#: 8480-Dialogue.mod:0 8687-Dialogue.mod:0
msgid "Here, I'll fix that"
msgstr "在這裡，我會解決這個問題"

#. Id: 8480-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8480-Dialogue.mod:1
msgid "Okay, keep still for a sec"
msgstr "好吧，保持靜止一秒鐘"

#. Id: 8480-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8480-Dialogue.mod:2
msgid "This might sting a bit..."
msgstr "這可能會有點刺痛......"

#. Id: 8480-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8480-Dialogue.mod:3
msgid "Let me help..."
msgstr "讓我來幫忙..."

#. Id: 8483-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (8480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8483-Dialogue.mod:0
msgid "Now we're even"
msgstr "現在我們都是"

#. Id: 8483-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (8480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8483-Dialogue.mod:1
msgid "Now we can be even"
msgstr "現在我們可以平等"

#. Id: 8483-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (8480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8483-Dialogue.mod:2
msgid "You helped me before, now it's my turn to help you"
msgstr "你之前幫過我了，現在輪到我幫你了"

#. Id: 8483-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (8480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8483-Dialogue.mod:3
msgid "Call us even now"
msgstr "現在打電話給我們"

#. Id: 8483-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (8480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Returning the favour
#: 8483-Dialogue.mod:4
msgid "I owe ya"
msgstr "我欠你的"
