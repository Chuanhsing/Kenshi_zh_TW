#. Id: 13771-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13771-Escapes.mod:0
msgid "You're right, let's /TEAMUP/"
msgstr ""

#. Id: 13771-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13771-Escapes.mod:1
msgid "You're right, let's stick together"
msgstr "你是對的，讓我們團結在一起吧"

#. Id: 13777-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13771-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13777-Escapes.mod:0
msgid "Good /MAN/"
msgstr ""

#. Id: 13777-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13771-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13777-Escapes.mod:1
msgid "Good choice"
msgstr "好的選擇"

#. Id: 13777-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13771-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13777-Escapes.mod:2
msgid "Wise choice"
msgstr "明智的選擇"

#. Id: 13777-Escapes.mod
#. -- Index: 2.5
#. -- Parent line: 1 (13771-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13777-Escapes.mod:5
msgid "Let's mess shit up"
msgstr "讓我們搞砸了"

#. Id: 13772-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13772-Escapes.mod:0
msgid "Not in a million years"
msgstr "不是一百萬年"

#. Id: 13772-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13772-Escapes.mod:1
msgid "Never. I have standards"
msgstr "決不。我有標準"

#. Id: 13772-Escapes.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13772-Escapes.mod:2
msgid "Never in a million years"
msgstr "在一百萬年裡都不會發生"

#. Id: 13774-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13772-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13774-Escapes.mod:0
msgid "Pff... stuck up bigshot"
msgstr "Pff ......堅持不懈"

#. Id: 13774-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13772-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13774-Escapes.mod:1
msgid "Tch... snooty /INSULT/"
msgstr ""

#. Id: 13774-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13772-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13774-Escapes.mod:2
msgid "Pff... snotty /INSULT/"
msgstr ""

#. Id: 13775-Escapes.mod
#. -- Index: 3.0
#. -- Parent line: 2 (13774-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13775-Escapes.mod:0
msgid "Your loss!"
msgstr "你的損失！"

#. Id: 13775-Escapes.mod
#. -- Index: 3.1
#. -- Parent line: 2 (13774-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13775-Escapes.mod:1
msgid "Don't go gettin' caught again!"
msgstr "別再去抓了！"

#. Id: 13775-Escapes.mod
#. -- Index: 3.2
#. -- Parent line: 2 (13774-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13775-Escapes.mod:2
msgid "Don't let 'em catch you again, eh"
msgstr "不要讓他們再次抓住你，呃"

#. Id: 13773-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13773-Escapes.mod:0
msgid "I prefer my own company, thanks"
msgstr "我更喜歡自己的公司，謝謝"

#. Id: 13773-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13761-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13773-Escapes.mod:1
msgid "I prefer to go solo, thanks"
msgstr "我更喜歡獨自一人，謝謝"

#. Id: 13762-Escapes.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 13762-Escapes.mod:0
msgid "I vote we keep movin', they could still catch us"
msgstr "我投票我們繼續行動，他們仍然可以抓住我們"

#. Id: 13762-Escapes.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 13762-Escapes.mod:1
msgid "We should keep movin', right?"
msgstr "我們應該保持活力，對嗎？"

#. Id: 13762-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 13762-Escapes.mod:2
msgid "Let's keep goin', I don't wanna get caught again"
msgstr "讓我們繼續吧，我不想再被抓住了"

#. Id: 13780-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13762-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13780-Escapes.mod:0
msgid "Ok, come with me"
msgstr "好的，跟我來"

#. Id: 13780-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13762-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13780-Escapes.mod:1
msgid "Ok, follow me"
msgstr "好的，跟我來"

#. Id: 13781-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13762-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13781-Escapes.mod:0
msgid "You're fine. Relax"
msgstr "你沒事。放鬆"

#. Id: 13781-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13762-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13781-Escapes.mod:1
msgid "Relax. You'll be fine"
msgstr "放鬆。你會沒事的"

#. Id: 13782-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13762-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13782-Escapes.mod:0
msgid "Whatever you want. I'm splitting"
msgstr "任何你想要的。我分手了"

#. Id: 13763-Escapes.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13763-Escapes.mod:0
msgid "What now?..."
msgstr "現在怎麼辦？..."

#. Id: 13763-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13763-Escapes.mod:2
msgid "What do we do now?"
msgstr "我們現在幹什麼？"

#. Id: 13763-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13763-Escapes.mod:3
msgid "Are we safe here? What happens now? "
msgstr "我們在這里安全嗎？現在發生了什麼？"

#. Id: 13783-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13763-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13783-Escapes.mod:0
msgid "We keep moving. Follow me"
msgstr "我們繼續前進。跟著我"

#. Id: 13792-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13783-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13792-Escapes.mod:0
msgid "Right, let's go"
msgstr "好的，我們走了"

#. Id: 13784-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13763-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13784-Escapes.mod:0
msgid "I'm not your mother. Find your own way"
msgstr "我不是你媽媽找到自己的方式"

#. Id: 13785-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13763-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13785-Escapes.mod:0
msgid "We split"
msgstr "我們分手了"

#. Id: 13785-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13763-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13785-Escapes.mod:1
msgid "We go our separate ways"
msgstr "我們分道揚.."

#. Id: 13793-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (13785-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13793-Escapes.mod:0
msgid "O- okay, thanks for your help"
msgstr "好的，謝謝你的幫助"

#. Id: 13793-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13785-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13793-Escapes.mod:1
msgid "O- okay, see you around"
msgstr "好的，見到你"

#. Id: 13793-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13785-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13793-Escapes.mod:2
msgid "R- right, see you"
msgstr "R-對，見到你"

#. Id: 13765-Escapes.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 13765-Escapes.mod:0
msgid "/WEMADEIT/! /WOOT/, you 'n me all the way, baby! How about we /TEAMUP/?"
msgstr ""

#. Id: 13765-Escapes.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 13765-Escapes.mod:1
msgid "/WEMADEIT/! /WOOT/, yeah baby, you 'n me! How's about we /TEAMUP/?"
msgstr ""

#. Id: 13765-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 13765-Escapes.mod:2
msgid "/WEMADEIT/! /WOOT/, yeah baby, that was tense! You 'n me, we gotta /TEAMUP/"
msgstr ""

#. Id: 13786-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13765-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13786-Escapes.mod:0
msgid "Yeah baby!"
msgstr "是的，寶貝！"

#. Id: 13789-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13786-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13789-Escapes.mod:1
msgid "We'll be unstoppable"
msgstr "我們將是不可阻擋的"

#. Id: 13789-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (13786-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Escaped thug
#: 13789-Escapes.mod:2
msgid "Together we'll rule the /SANDS/"
msgstr ""

#. Id: 13787-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13765-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Escaped thug
#: 13787-Escapes.mod:0
msgid "Sure, let's /TEAMUP/"
msgstr ""
