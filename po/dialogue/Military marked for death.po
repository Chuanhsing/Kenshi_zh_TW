#. Id: 15627-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 15627-Dialogue.mod:0
msgid "Th- they're coming for me!"
msgstr "他們來找我吧！"

#. Id: 15627-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 15627-Dialogue.mod:1
msgid "No! Stay away from me!"
msgstr "沒有！離我遠點！"

#. Id: 15627-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 15627-Dialogue.mod:4
msgid "Th- they're coming!"
msgstr "他們來了！"

#. Id: 15627-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 15627-Dialogue.mod:5
msgid "N- no!"
msgstr "不！"

#. Id: 15628-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15627-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#: 15628-Dialogue.mod:1
msgid "Heeeelp!"
msgstr "Heeeelp！"

#. Id: 15629-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 15629-Dialogue.mod:0
msgid "Let me out of here!"
msgstr "讓我離開這裡！"

#. Id: 15629-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 15629-Dialogue.mod:1
msgid "Open this cage!"
msgstr "打開這個籠子！"

#. Id: 15629-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 15629-Dialogue.mod:2
msgid "Let me out, now!"
msgstr "現在讓我出去吧！"

#. Id: 15630-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15629-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military marked for death
#: 15630-Dialogue.mod:0
msgid "You can't do this to me!"
msgstr "你不能這樣對我！"
