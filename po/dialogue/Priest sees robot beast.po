#. Id: 41488-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:0
msgid "Agents of darkness... They've come!"
msgstr "黑暗的代理人......他們來了！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:1
msgid "Skeletons... the dark ones are here!"
msgstr "骷髏......黑暗的人在這裡！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:2
msgid "Robots... unholy ones are upon us!"
msgstr "機器人......不聖潔的人在我們身上！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:5
msgid "Robots...! Father, be with us!"
msgstr "機器人...！父親，和我們在一起！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:6
msgid "Skeletons!... Okran, be with us!"
msgstr "骷髏！...奧克蘭，和我們在一起！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:7
msgid "Agents of darkness! .. Okran protect us!"
msgstr "黑暗的代理人！ ..奧克蘭保護我們！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:8
msgid "Minions of the darkness!"
msgstr "黑暗的奴才！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:9
msgid "Narko's dark agents! They've finally come for us!"
msgstr "Narko的黑暗代理人！他們終於來找我們了！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:10
msgid "The dark agents, they're here! We're doomed! DOOMED!"
msgstr "黑暗的特工，他們在這裡！我們完蛋了！ DOOMED！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:11
msgid "The Skeletons have come for us! The second extinction is upon us!"
msgstr "骷髏隊來找我們！第二次滅絕在我們身上！"

#. Id: 41488-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41488-Dialogue.mod:12
msgid "Skeletons! This is our end! Our crusade is a failure!"
msgstr "骷髏！這是我們的結局！我們的十字軍是一場失敗！"

#. Id: 41489-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:0
msgid "Paladins, /BURNHIM/!"
msgstr ""

#. Id: 41489-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:1
msgid "Paladins, gather! Destroy /HIM/!"
msgstr ""

#. Id: 41489-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:2
msgid "Paladins! Quickly!"
msgstr "聖騎士！很快！"

#. Id: 41489-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:3
msgid "Paladins! Attack!"
msgstr "聖騎士！攻擊！"

#. Id: 41489-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:5
msgid "/BURNHIM/, paladins! Don't let /HIM/ through!"
msgstr ""

#. Id: 41489-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (41488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 41489-Dialogue.mod:6
msgid "/HE/ must not get through! /BURNHIM/!"
msgstr ""

#. Id: 42897-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: RELATIONS<75, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek
#: 42897-Dialogue.mod:0
msgid "Wretched beast..."
msgstr "可憐的野獸......"

#. Id: 42897-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: RELATIONS<75, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek
#: 42897-Dialogue.mod:2
msgid "/GREASY/ beast..."
msgstr ""

#. Id: 42898-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:1
msgid "An unshackled beast is an uneducated beast"
msgstr "一隻沒有束縛的野獸是一隻沒有受過教育的野獸"

#. Id: 42898-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:2
msgid "Serve the pure blooded, repent for your sins..."
msgstr "服務純血統，為你的罪悔改......"

#. Id: 42898-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:3
msgid "To roam our /SACRED/ soil... sacrilege..."
msgstr ""

#. Id: 42898-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:4
msgid "Foul, wretched, foul beast..."
msgstr "犯規，猥瑣，惡魔......"

#. Id: 42898-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:5
msgid "Watch your servant, /BROTHER/. They're YOUR responsibility."
msgstr ""

#. Id: 42898-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:6
msgid "Clean up your servant, /BROTHER/. "
msgstr ""

#. Id: 42898-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:7
msgid "Repent for your sins, beast. Serve the pure blooded, serve Okran."
msgstr "懺悔你的罪孽，野獸。服務於純血統，為奧克蘭服務。"

#. Id: 42898-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:8
msgid "Repent for your sins, beast. Pray, atone, serve!"
msgstr "懺悔你的罪孽，野獸。祈禱，贖罪，服務！"

#. Id: 42898-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Target Race(s): Greenlander
#: 42898-Dialogue.mod:9
msgid "Atone for your darkened ways, beast, Serve your master."
msgstr "為你的黑暗方式，野獸，為你的主人服務。"

#. Id: 42899-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:0
msgid "Paladins! Who let this heretic beast into the city?!"
msgstr "聖騎士！誰讓這個異教徒的野獸進入城市？！"

#. Id: 42899-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:1
msgid "Why is this heretic without a master?!"
msgstr "為什麼這個異教徒沒有大師？！"

#. Id: 42899-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:2
msgid "Paladins! Get this heretic away from me!"
msgstr "聖騎士！讓這個異教徒遠離我！"

#. Id: 42899-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:3
msgid "I will not have the impure taint this /SACRED/ city!"
msgstr ""

#. Id: 42899-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:4
msgid "Paladins! There is a beast that needs taming!"
msgstr "聖騎士！有一隻需要馴服的野獸！"

#. Id: 42899-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (42897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42899-Dialogue.mod:5
msgid "We have a beast that needs taming! Paladins!"
msgstr "我們有一隻需要馴服的野獸！聖騎士！"

#. Id: 42900-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42899-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42900-Dialogue.mod:0
msgid "Take /HIM/ to rebirth and clean /HIM/ up!"
msgstr ""

#. Id: 42900-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42899-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42900-Dialogue.mod:1
msgid "Take /HIM/ to rebirth!"
msgstr ""

#. Id: 42900-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (42899-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#: 42900-Dialogue.mod:2
msgid "Clean /HIM/ up! Take /HIM/ to rebirth!"
msgstr ""
