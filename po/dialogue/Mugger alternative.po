#. -- Index: 1.3
#. -- Parent line: 0 (18267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#: 18268-Dialogue.mod:3
msgid "Don't be greedy rich /BOY/, share some of your /CATS/"
msgstr ""

#. Id: 18268-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (18267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#: 18268-Dialogue.mod:4
msgid "Share some /CATS/, rich /BOY/, don't hog 'em to yourself"
msgstr ""

#. Id: 18268-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (18267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#: 18268-Dialogue.mod:5
msgid "I want some of those /CATS/!"
msgstr ""

#. Id: 18268-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (18267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#: 18268-Dialogue.mod:6
msgid "Gonna share some /CATS/, rich /BOY/?"
msgstr ""

#. Id: 18269-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 18269-Dialogue.mod:0
msgid "/OI/, limpy!"
msgstr ""

#. Id: 18269-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 18269-Dialogue.mod:1
msgid "Limp.. limp.. limp.. Aw, 'wah my leg'! /LAUGH/  "
msgstr ""

#. Id: 18269-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Mugger alternative
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 18269-Dialogue.mod:2
msgid "Watch that bad leg of yours, someone might accidentally bludgeon it with a /BLUNTOBJECT/"
msgstr ""

