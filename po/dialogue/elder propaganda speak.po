#. Id: 63075-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63075-Dialogue.mod:0
msgid "We are superior!"
msgstr "我們優越！"

#. Id: 63076-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63075-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63076-Dialogue.mod:0
msgid "We are free from flesh pleasures!"
msgstr "我們沒有肉體的快樂！"

#. Id: 63077-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63076-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63077-Dialogue.mod:0
msgid "We are free from bodily filth!"
msgstr "我們沒有身體污穢！"

#. Id: 63078-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63077-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63078-Dialogue.mod:0
msgid "We are free from the grasping mind!"
msgstr "我們擺脫了頭腦！"

#. Id: 63079-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63078-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63079-Dialogue.mod:0
msgid "We. Are. Skeletons."
msgstr "我們。是。骷髏。"

#. Id: 63080-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63080-Dialogue.mod:0
msgid "Return to paradise!"
msgstr "回歸天堂！"

#. Id: 63081-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63080-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63081-Dialogue.mod:0
msgid "Return to a time before humans!"
msgstr "回到人類面前！"

#. Id: 63082-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63081-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63082-Dialogue.mod:0
msgid "Fight or fall!"
msgstr "戰鬥或摔倒！"

#. Id: 63083-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63082-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63083-Dialogue.mod:0
msgid "The organics will be destroyed!"
msgstr "有機物將被銷毀！"

#. Id: 63084-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63084-Dialogue.mod:0
msgid "Rid the world of the parasites!"
msgstr "擺脫了寄生蟲的世界！"

#. Id: 63085-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63084-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63085-Dialogue.mod:0
msgid "Exterminate the humans!"
msgstr "消滅人類！"

#. Id: 63167-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63167-Dialogue.mod:0
msgid "We are the Warriors of Great Stobe!"
msgstr "我們是Great Stobe的勇士！"

#. Id: 63168-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63167-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63168-Dialogue.mod:0
msgid "Death to the humans!"
msgstr "對人類的死亡！"

#. Id: 63169-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63168-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63169-Dialogue.mod:0
msgid "Death to the Skinned Ones!"
msgstr "皮膚死亡！"

#. Id: 63170-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63170-Dialogue.mod:0
msgid "Great Stobe's war rages on through the Skeleton!"
msgstr "偉大的Stobe的戰爭穿過骷髏！"

#. Id: 63171-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63171-Dialogue.mod:0
msgid "In memory of Great Stobe!"
msgstr "為了紀念Great Stobe！"

#. Id: 63172-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63171-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63172-Dialogue.mod:0
msgid "The Skeleton Bandits preserve his memory!"
msgstr "骷髏匪徒保留了他的記憶！"

#. Id: 63173-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63172-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63173-Dialogue.mod:0
msgid "Through the extermination of mankind!"
msgstr "通過滅絕人類！"

#. Id: 63174-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63174-Dialogue.mod:0
msgid "The Warriors of Stobe will return time, before man's existence!"
msgstr "在人類存在之前，Stobe的勇士將回歸時間！"

#. Id: 63175-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63175-Dialogue.mod:0
msgid "It's time for our daily tribute!"
msgstr "現在是我們每日致敬的時候了！"

#. Id: 63175-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: elder propaganda speak
#: 63175-Dialogue.mod:1
msgid "Tribute, Skeletons!"
msgstr "致敬，骷髏！"
