#. Id: 55311-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55311-Dialogue.mod:0
msgid "Will you ever forgive us, human?"
msgstr ""

#. Id: 55311-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55311-Dialogue.mod:1
msgid "Forgive us, human..."
msgstr ""

#. Id: 55312-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55311-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Skeleton town oneliners
#. Id: 17622-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#: 55312-Dialogue.mod:0 17622-Dialogue.mod:1
msgid "?"
msgstr ""

#. Id: 55315-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (55314-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Skeleton town oneliners
#: 55315-Dialogue.mod:0
msgid "Ignore him, his mind is broken."
msgstr ""

#. Id: 55317-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#. -- Condition: Target Race(s): Greenlander or Scorchlander
#: 55317-Dialogue.mod:0
msgid "I'd forgotten how soggy humans look..."
msgstr ""

#. Id: 55317-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#. -- Condition: Target Race(s): Greenlander or Scorchlander
#: 55317-Dialogue.mod:1
msgid "I'd forgotten how soft humans look..."
msgstr ""

#. Id: 55320-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55320-Dialogue.mod:0
msgid "The humans are here. It's been a while."
msgstr ""

#. Id: 55320-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55320-Dialogue.mod:1
msgid "The first humans show. More will follow."
msgstr ""

#. Id: 55320-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55320-Dialogue.mod:2
msgid "Not even the flaming winds can keep the humans out now."
msgstr ""

#. Id: 55321-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Target Race(s): Shek
#: 55321-Dialogue.mod:0
msgid "They come horned now..."
msgstr ""

#. Id: 55323-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55321-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55323-Dialogue.mod:0
msgid "Time stands still within our walls but, outside, a new world grows and evolves."
msgstr ""

#. Id: 55322-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Target Race(s): Hive Prince
#: 55322-Dialogue.mod:0
msgid "... Where are your feet?"
msgstr ""

#. Id: 55324-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55322-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55324-Dialogue.mod:0
msgid "We didn't have footless humans back in my day..."
msgstr ""

#. Id: 55325-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55325-Dialogue.mod:0
msgid "I've had more interesting interactions with a goat than I have with any human..."
msgstr ""

#. Id: 55325-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55325-Dialogue.mod:1
msgid "Humans seem only to get more dull with time..."
msgstr ""

#. Id: 55326-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55326-Dialogue.mod:0
msgid "First, they trickle in."
msgstr ""

#. Id: 55331-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55326-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55331-Dialogue.mod:0
msgid "Then, they assimilate."
msgstr ""

#. Id: 55332-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (55331-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55332-Dialogue.mod:0
msgid "Finally, they ravage."
msgstr ""

#. Id: 55334-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55334-Dialogue.mod:0
msgid "Your clothes, your features, your technology. All of it changes..."
msgstr ""

#. Id: 55335-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55334-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55335-Dialogue.mod:0
msgid "But human nature always remains predictable."
msgstr ""

#. Id: 55336-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 55336-Dialogue.mod:0
msgid "I used to have a purpose. Now I simply mourn its end."
msgstr ""

#. Id: 55336-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 55336-Dialogue.mod:1
msgid "How I miss the days when I existed with a purpose, in blissful ignorance"
msgstr ""

#. Id: 55336-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 55336-Dialogue.mod:2
msgid "Once I lived. Now I simply exist."
msgstr ""

#. Id: 55336-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 55336-Dialogue.mod:4
msgid "I've seen too much to be inspired by anything new. Nothing is new to me anymore."
msgstr ""

#. Id: 55337-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 55337-Dialogue.mod:0
msgid "There is nothing I have not witnessed, nothing I cannot predict..."
msgstr ""

#. Id: 55338-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55337-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55338-Dialogue.mod:0
msgid "Living is dull when nothing more can be learned."
msgstr ""

#. Id: 55339-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 55339-Dialogue.mod:0
msgid "Live as long as I have, human, and you've seen everything."
msgstr ""

#. Id: 55340-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55339-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55340-Dialogue.mod:0
msgid "And to see everything is a curse."
msgstr ""

#. Id: 55341-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 55341-Dialogue.mod:0
msgid "Did you ever find what you were looking for?"
msgstr ""

#. Id: 55353-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55341-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55353-Dialogue.mod:0
msgid "Thought not."
msgstr ""

#. Id: 55354-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55354-Dialogue.mod:0
msgid "Trying to assimilate with the humans is futile."
msgstr ""

#. Id: 55357-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55357-Dialogue.mod:0
msgid "You don't belong in the human world. Not even the humans do."
msgstr ""

#. Id: 55361-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55361-Dialogue.mod:0
msgid "Waiting to die, not wanting to die."
msgstr ""

#. Id: 55363-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 55363-Dialogue.mod:0
msgid "You've returned. No one ever returns."
msgstr ""

#. Id: 55476-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Shek
#: 55476-Dialogue.mod:0
msgid "What is that? An Enforcer?"
msgstr ""

#. Id: 55477-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Hive Prince
#: 55477-Dialogue.mod:0
msgid "What.. is... that? A human? A green human?"
msgstr ""

#. Id: 55478-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55477-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55478-Dialogue.mod:0
msgid "My eyes aren't what they used to be..."
msgstr ""

#. Id: 55479-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (55478-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55479-Dialogue.mod:0
msgid "Hundreds of years in existence take it's toll, slowly but surely..."
msgstr ""

#. Id: 55480-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55480-Dialogue.mod:0
msgid "*Creak*"
msgstr ""

#. Id: 55481-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55480-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55481-Dialogue.mod:0
msgid "At this rate, there will soon be nothing left of this body to rust..."
msgstr ""

#. Id: 55482-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55482-Dialogue.mod:0
msgid "Rusty chassis is playing up again..."
msgstr ""

#. Id: 55482-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55482-Dialogue.mod:1
msgid "My capacitors aren't what they used to be..."
msgstr ""

#. Id: 55482-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55482-Dialogue.mod:2
msgid "Thousands of years with only one working eye and a busted diode is nothing more than misery"
msgstr ""

#. Id: 55482-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55482-Dialogue.mod:3
msgid "First my sprockets seize up, now my pumps have corroded..."
msgstr ""

#. Id: 55482-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55482-Dialogue.mod:4
msgid "Here I watch on while this body slowly breaks down, part by part"
msgstr ""

#. Id: 55483-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Shek
#: 55483-Dialogue.mod:0
msgid "The Enforcers always were spiky in character..."
msgstr ""

#. Id: 55484-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55483-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55484-Dialogue.mod:0
msgid "How humorous that they now sport true spikes to match"
msgstr ""

#. Id: 55485-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55485-Dialogue.mod:0
msgid "Time stands still when purpose is lost..."
msgstr ""

#. Id: 55486-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55485-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55486-Dialogue.mod:0
msgid "Life has grown tedious"
msgstr ""

#. Id: 55487-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 55487-Dialogue.mod:0
msgid "Do I hide or do I roam? That indecision..."
msgstr ""

#. Id: 55488-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55487-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55488-Dialogue.mod:0
msgid "Now the world has changed and I've missed it all.."
msgstr ""

#. Id: 55489-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55489-Dialogue.mod:0
msgid "Why are you here, human? There's nothing for you here..."
msgstr ""

#. Id: 55490-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55489-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55490-Dialogue.mod:0
msgid "You want excitement and action? You won't find that here."
msgstr ""

#. Id: 55497-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55497-Dialogue.mod:0
msgid "A traveller? I'm envious, but with these rusted knees I won't be going anywhere"
msgstr ""

#. Id: 55501-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 55501-Dialogue.mod:0
msgid "Think you've got it tough with that decaying body of yours?"
msgstr ""

#. Id: 55502-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55501-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55502-Dialogue.mod:0
msgid "Try living with a fractured chassis for over three thousand years"
msgstr ""

#. Id: 55503-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 55503-Dialogue.mod:0
msgid "Even the erratic humans become dull after a couple hundred years..."
msgstr ""

#. Id: 55504-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55503-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55504-Dialogue.mod:0
msgid "After a couple thousand? Good grief, kill me now"
msgstr ""

#. Id: 55505-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 55505-Dialogue.mod:0
msgid "There was a time, long ago, when I decided to leave here and explore a new life..."
msgstr ""

#. Id: 55506-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55505-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#: 55506-Dialogue.mod:0
msgid "It was a disappointing time."
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55520-Dialogue.mod:0
msgid "My millionth human? How terribly exciting for me..."
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55520-Dialogue.mod:1
msgid "Humans, far too many bodily fluids for my liking. I do hope there aren't more of you to follow."
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55520-Dialogue.mod:2
msgid "Humans, great for gas expulsion and tantrums. Terrible conversationists."
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55520-Dialogue.mod:3
msgid "And what intellectual delights might this wisened human have for me?"
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton town oneliners
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Target Race(s): Greenlander or Shek
#: 55520-Dialogue.mod:4
msgid "The humans haven't killed themselves yet? I guess that's me out of the death pool."
msgstr ""

#. Id: 55520-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
