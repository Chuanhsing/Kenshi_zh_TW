#. Id: 45917-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:0
msgid "Should never have unlocked me!"
msgstr "永遠不應該解鎖我！"

#. Id: 45917-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:1
msgid "You must have a deathwish setting me free!"
msgstr "你必須有一個死亡願望讓我自由！"

#. Id: 45917-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:2
msgid "You wanna die, /IDIOT/? Stickin' me in locks makes me think you do!"
msgstr ""

#. Id: 45917-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:3
msgid "I've been dreamin' about stickin' my sword in you for a long time!"
msgstr "我一直在夢見'長期堅持你的劍！"

#. Id: 45917-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:4
msgid "Vengeance is mine!"
msgstr "復仇是我的！"

#. Id: 45917-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:5
msgid "Oh, you're gonna regret lockin' me up!"
msgstr "哦，你會後悔鎖定我！"

#. Id: 45917-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45917-Dialogue.mod:6
msgid "I'm gonna make you wish you never messed with me!"
msgstr "我會讓你希望你永遠不要跟我搞砸！"

#. Id: 45918-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#. Id: 45924-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45918-Dialogue.mod:1 45924-Dialogue.mod:0
msgid "Damn you!"
msgstr "該死的你！"

#. Id: 45918-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#. Id: 45924-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45918-Dialogue.mod:2 45924-Dialogue.mod:1
msgid "Goddamn you!"
msgstr "去你的！"

#. Id: 45918-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45918-Dialogue.mod:4
msgid "Rggh!"
msgstr "Rggh！"

#. Id: 45921-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45921-Dialogue.mod:0
msgid "You're lucky I'm unarmed!"
msgstr "你很幸運我沒有武裝！"

#. Id: 45921-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45921-Dialogue.mod:1
msgid "If I had a sword...!"
msgstr "如果我有一把劍......！"

#. Id: 45921-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45921-Dialogue.mod:2
msgid "Watch your back, I'll get my vengeance!"
msgstr "看著你的背影，我會報復！"

#. Id: 45922-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#. Id: 44018-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Civ spots swamp raptor crime
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#. -- Condition: Who: ME, Condition: IM_UNARMED=0, Tag: -1
#. Id: 11459-Dialogue (10).mod
#. -- Index: 2.0
#. -- Parent line: 1 (11458-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Unlock Shek Bad relations
#: 45922-Dialogue.mod:0 44018-Dialogue.mod:2 11459-Dialogue?(10).mod:0
msgid "You're dead!"
msgstr "你死定了！"

#. Id: 45922-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45922-Dialogue.mod:1
msgid "I don't take too kindly to being stuck in locks!"
msgstr "我不太善意被困在鎖裡！"

#. Id: 45922-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45922-Dialogue.mod:2
msgid "Get a good kick out of lockin' me up, huh?!"
msgstr "鎖定我的好夥伴，好吧？！"

#. Id: 45922-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45922-Dialogue.mod:3
msgid "Oh you're gonna pay for lockin' me up like that!"
msgstr "哦，你要支付鎖定我這樣的錢！"

#. Id: 45922-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45922-Dialogue.mod:4
msgid "I'm gonna make you pay for lockin' me up, even if it kills me!"
msgstr "即使它殺了我，我也會讓你付出代價來鎖定我！"

#. Id: 45924-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45924-Dialogue.mod:2
msgid "You twisted son of a bitch!"
msgstr "你扭曲了一個婊子的兒子！"

#. Id: 45924-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45924-Dialogue.mod:3
msgid "You won't get away with doing this to me!"
msgstr "你不會對我這樣做！"

#. Id: 45924-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45924-Dialogue.mod:4
msgid "You lock me up, you die!"
msgstr "你把我鎖起來，你死了！"

#. Id: 45924-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: tough merc freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45924-Dialogue.mod:5
msgid "I won't let you catch me again!"
msgstr "我不會再讓你抓到我了！"
