#. Id: 45811-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: My Race(s): Hive Prince
#: 45811-Dialogue.mod:0
msgid "I won't go back in there! Leave me alone!"
msgstr "我不會回到那裡！請別打擾我！"

#. Id: 45811-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: My Race(s): Hive Prince
#: 45811-Dialogue.mod:1
msgid "Don't put me back in there!"
msgstr "別把我放回去！"

#. Id: 45811-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: My Race(s): Hive Prince
#: 45811-Dialogue.mod:2
msgid "I won't let you put me back in there!"
msgstr "我不會讓你讓我回到那裡！"

#. Id: 45811-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: My Race(s): Hive Prince
#: 45811-Dialogue.mod:3
msgid "No! I'm not going back in there! Leave me alone!"
msgstr "沒有！我不會回到那裡！請別打擾我！"

#. Id: 45813-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 45813-Dialogue.mod:0
msgid "... Am I free to go?"
msgstr "我可以自由地去嗎？"

#. Id: 45813-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 45813-Dialogue.mod:1
msgid "... You're letting me go?"
msgstr "......你讓我走了？"

#. Id: 45813-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 45813-Dialogue.mod:3
msgid "I'm free... I'm free again?"
msgstr "我有空......我又有空嗎？"

#. Id: 45813-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: ronin unlocked by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
