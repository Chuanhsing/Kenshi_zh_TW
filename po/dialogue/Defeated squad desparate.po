msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:1
msgid "Food! Check for food!"
msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:2
msgid "/THISGUY/ better have some food on /EM/..."
msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:3
msgid "Food... /HEHAS/ gotta have something on /EM/..."
msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:4
msgid "Please... please have food..."
msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:5
msgid "Please have food... I won't make it much longer without..."
msgstr ""

#. Id: 48031-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48031-Dialogue.mod:6
msgid "Phew... /IWE/ did it..."
msgstr ""

#. Id: 48036-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48036-Dialogue.mod:0
msgid "I don't care about your life... I just want to survive..."
msgstr ""

#. Id: 48036-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48036-Dialogue.mod:1
msgid "I don't care if you die here... I just want to live..."
msgstr ""

#. Id: 48036-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#: 48036-Dialogue.mod:2
msgid "I've gone past caring about people. Nobody ever cared for me."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:0
msgid "I need to eat, I need your food..."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:1
msgid "I haven't eaten in three days..."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:2
msgid "Everyday is torture when you can't afford to eat..."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:3
msgid "Those that have the luxury of never going hungry. I despise them..."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:4
msgid "All I care about is your food..."
msgstr ""

#. Id: 48037-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (48036-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#: 48037-Dialogue.mod:5
msgid "Only thing I care about is your food..."
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:1
msgid "Meat! Carve 'em up, /IWE/ need to eat!"
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:2
msgid "Meat! Today /IWE/ eat!"
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:3
msgid "So hungry... but today... /IWE/ eat steak..."
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:4
msgid "Meat... so much meat..."
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:5
msgid "Mm... meat..."
msgstr ""

#. Id: 48038-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Defeated squad desparate
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
#: 48038-Dialogue.mod:6
msgid "I... I can taste the meat in my mouth already..."
msgstr ""

