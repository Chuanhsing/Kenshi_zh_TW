#. Id: 56052-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#. Id: 56185-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hiver generic assault
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56052-Dialogue.mod:2 56185-Dialogue.mod:2
msgid "And we tenderizes the no-hives, just the way he likes them...."
msgstr "我們按照他喜歡的方式，對無蕁麻疹進行淘汰......"

#. Id: 56052-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#. Id: 56185-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hiver generic assault
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56052-Dialogue.mod:5 56185-Dialogue.mod:5
msgid "Death to the fleshy ones!"
msgstr "肉體的死亡！"

#. Id: 56052-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#. Id: 56185-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hiver generic assault
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56052-Dialogue.mod:6 56185-Dialogue.mod:6
msgid "Meat, meat, meat!"
msgstr "肉，肉，肉！"

#. Id: 56052-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#. Id: 56185-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hiver generic assault
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56052-Dialogue.mod:8 56185-Dialogue.mod:8
msgid "Whack them on the head, soften up the flesh..."
msgstr "把它們砸在頭上，軟化肉體......"

#. Id: 56053-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (56052-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. Id: 56186-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (56185-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Southern hiver generic assault
#: 56053-Dialogue.mod:0 56186-Dialogue.mod:0
msgid "More meats for King!"
msgstr "國王更多肉類！"

#. Id: 56053-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (56052-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#: 56053-Dialogue.mod:1
msgid "Sacrifice for King!"
msgstr "為國王犧牲！"

#. Id: 56054-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56054-Dialogue.mod:0
msgid "The flesh ones cry lots of red..."
msgstr "肉體的人哭得很紅......"

#. Id: 56054-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Scorchlander or Shek
#: 56054-Dialogue.mod:1
msgid "The fleshy ones bleed more red than we..."
msgstr "肉質的人比我們流血更紅..."

#. Id: 56055-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (56054-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#: 56055-Dialogue.mod:0
msgid "King likes the red..."
msgstr "國王喜歡紅..."

#. Id: 56062-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Southern hive get up
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Hive Soldier Drone or Hive Worker Drone or Scorchlander or Shek
