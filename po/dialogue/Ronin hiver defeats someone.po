#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin get up fight
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:4 17925-Dialogue.mod:13
msgid "I can't die yet"
msgstr "我還不能死"

#. Id: 17954-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: My Race(s): Hive Prince
#. Id: 17925-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin get up fight
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:5 17925-Dialogue.mod:12
msgid "I won't die"
msgstr "我不會死"

#. Id: 17954-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:6
msgid "Stay away from me"
msgstr "離我遠點"

#. Id: 17954-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:7
msgid "I will survive. I don't want to die..."
msgstr "我會活下去。我不想死..."

#. Id: 17954-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: My Race(s): Hive Prince
#. Id: 17925-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin get up fight
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:8 17925-Dialogue.mod:11
msgid "I don't want to die. I want to survive..."
msgstr "我不想死。我想生存......"

#. Id: 17954-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: My Race(s): Hive Prince
#. Id: 17925-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin get up fight
#. -- Condition: My Race(s): Hive Prince
#: 17954-Dialogue.mod:9 17925-Dialogue.mod:10
msgid "I've come too far to let you kill me now"
msgstr "我太過分了，你現在不能殺了我"

#. Id: 17955-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin hiver defeats someone
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 17955-Dialogue.mod:0
msgid "Okay, who's next?"
msgstr "好的，誰是下一個？"

