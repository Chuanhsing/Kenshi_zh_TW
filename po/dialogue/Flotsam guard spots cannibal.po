#. Id: 50767-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:0
msgid "Bring it savages. I'll stick an arrow right through your skulls."
msgstr "收下我的箭吧, 野蠻人, 我會好好的把它插到你頭顱裏麵."

#. Id: 50767-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:1
msgid "Red alert!"
msgstr "紅色警報!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:2
msgid "Ready the turrets!"
msgstr "炮手準備!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:3
msgid "Man the turrets!"
msgstr "快去操作炮塔!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:4
msgid "Load 'em up! Fire!"
msgstr "裝填準備! 開火!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:5
msgid "Cannibals! Fire!"
msgstr "瞄準食人族! 射擊!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:6
msgid "Open fire!"
msgstr "開火!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:7
msgid "Shoot!"
msgstr "射擊!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:8
msgid "Fire 'em! It's time for turret practise!"
msgstr "幹掉他們! 是時候練習炮塔射擊技巧了!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:9
msgid "Cannibals on the horizon!"
msgstr "遠處發現食人族!"

#. Id: 50767-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Flotsam guard spots cannibal
#: 50767-Dialogue.mod:10
msgid "Cannibals ahead!"
msgstr "食人族來襲!"
