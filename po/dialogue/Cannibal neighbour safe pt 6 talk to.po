#. -- Index: 2.0
#. -- Parent line: 1 (13125-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Cannibal neighbour safe pt 6 talk to
#. -- Condition: Who: TARGET, Condition: WEAKER_THAN_ME=1, Tag: -1
#: 13129-Escapes.mod:0
msgid "You should stick around with me, it could still be dangerous..."
msgstr "你應該和我在一起，它可能仍然很危險......"

#. Id: 13129-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (13125-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Cannibal neighbour safe pt 6 talk to
#. -- Condition: Who: TARGET, Condition: WEAKER_THAN_ME=1, Tag: -1
#: 13129-Escapes.mod:1
msgid "You should stick with me, /BROTHER/, it's still dangerous here..."
msgstr ""

#. Id: 13133-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13123-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Cannibal neighbour safe pt 6 talk to
