#. Id: 5264-gamedata.base
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:0
msgid "Work! Work!"
msgstr "工作！工作！"

#. Id: 5264-gamedata.base
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. Id: 40890-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 5264-gamedata.base:1 40890-Dialogue.mod:4
msgid "Work!"
msgstr "工作！"

#. Id: 5264-gamedata.base
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:2
msgid "Work is everything!"
msgstr "工作就是一切！"

#. Id: 5264-gamedata.base
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. Id: 40890-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 5264-gamedata.base:3 40890-Dialogue.mod:0
msgid "Work harder!"
msgstr "努力工作！"

#. Id: 5264-gamedata.base
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:4
msgid "Work or die!"
msgstr "工作還是死！"

#. Id: 5264-gamedata.base
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:5
msgid "Work is life!"
msgstr "工作就是生活！"

#. Id: 5264-gamedata.base
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:6
msgid "Work is freedom!"
msgstr "工作就是自由！"

#. Id: 5264-gamedata.base
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:7
msgid "You're all fit to work!"
msgstr "你們都適合工作！"

#. Id: 5264-gamedata.base
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. Id: 5269-gamedata.base
#. -- Index: 1.6
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:8 5269-gamedata.base:6
msgid "Work is the best pain-killer!"
msgstr "工作是最好的止痛藥！"

#. Id: 5264-gamedata.base
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:9
msgid "If you can feel pain it's because you're not working hard enough!"
msgstr "如果你能感到痛苦，那是因為你工作不夠努力！"

#. Id: 5264-gamedata.base
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:10
msgid "Bloody work-shy! That's what you are!"
msgstr "血淋淋的工作 - 害羞！那就是你！"

#. Id: 5264-gamedata.base
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:11
msgid "Lazy /BASTARD/s!"
msgstr ""

#. Id: 5264-gamedata.base
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5264-gamedata.base:12
msgid "You are nothing without work!"
msgstr "沒有工作，你什麼都不是！"

#. Id: 5265-gamedata.base
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5265-gamedata.base:0
msgid "Move faster!"
msgstr "動作快點！"

#. Id: 5265-gamedata.base
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5265-gamedata.base:1
msgid "Work faster!"
msgstr "工作更快！"

#. Id: 5265-gamedata.base
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. Id: 12350-Dialogue (10).mod
#. -- Index: 1.2
#. -- Parent line: 0 (12351-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 5265-gamedata.base:2 12350-Dialogue?(10).mod:2
msgid "Move it!"
msgstr "移動它！"

#. Id: 5265-gamedata.base
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5265-gamedata.base:3
msgid "Hurry up!"
msgstr "趕快！"

#. Id: 5265-gamedata.base
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5265-gamedata.base:4
msgid "Faster!"
msgstr "快點！"

#. Id: 5265-gamedata.base
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. Id: 40890-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 5265-gamedata.base:5 40890-Dialogue.mod:3
msgid "Get to work!"
msgstr "開始工作！"

#. Id: 5265-gamedata.base
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5265-gamedata.base:6
msgid "Get to /FUCKING/ work!"
msgstr "去 /FUCKING/ 工作！"

#. Id: 5267-gamedata.base
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. -- Condition: Who: TARGET, Condition: IS_NEARLY_KO=1, Tag: -1
#: 5267-gamedata.base:0
msgid "Aww feeling a little woozy?"
msgstr "Aww感覺有點恍惚？"

#. Id: 5267-gamedata.base
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. -- Condition: Who: TARGET, Condition: IS_NEARLY_KO=1, Tag: -1
#: 5267-gamedata.base:1
msgid "Feeling unwell?"
msgstr "感覺不舒服？"

#. Id: 5267-gamedata.base
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. -- Condition: Who: TARGET, Condition: IS_NEARLY_KO=1, Tag: -1
#: 5267-gamedata.base:2
msgid "Injured?"
msgstr "受傷？"

#. Id: 5267-gamedata.base
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. -- Condition: Who: TARGET, Condition: IS_NEARLY_KO=1, Tag: -1
#: 5267-gamedata.base:3
msgid "Aww are you nearly dead?"
msgstr "哇，你快死了嗎？"

#. Id: 5267-gamedata.base
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#. -- Condition: Who: TARGET, Condition: IS_NEARLY_KO=1, Tag: -1
#: 5267-gamedata.base:4
msgid "Nearly dead, eh?"
msgstr "差不多死了，是嗎？"

#. Id: 5269-gamedata.base
#. -- Index: 1.0
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:0
msgid "You're fit to work!"
msgstr "你很適合工作！"

#. Id: 5269-gamedata.base
#. -- Index: 1.1
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:1
msgid "Work harder then!"
msgstr "那就更努力了！"

#. Id: 5269-gamedata.base
#. -- Index: 1.2
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:2
msgid "If you can't work you go in the furnace!"
msgstr "如果你不能工作，你就進入爐中！"

#. Id: 5269-gamedata.base
#. -- Index: 1.3
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:3
msgid "Non workers go in the furnace!"
msgstr "非工人進爐！"

#. Id: 5269-gamedata.base
#. -- Index: 1.4
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:4
msgid "If you can't keep up then it's the furnace for you!"
msgstr "如果你跟不上，那麼它就是你的爐子！"

#. Id: 5269-gamedata.base
#. -- Index: 1.5
#. -- Parent line: 0 (5267-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: shout at slave worker
#: 5269-gamedata.base:5
msgid "Better keep up!"
msgstr "更好地跟上！"

