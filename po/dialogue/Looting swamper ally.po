#. Id: 15268-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#: 15268-Dialogue.mod:0
msgid "Back up!"
msgstr "備份！"

#. Id: 15268-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#: 15268-Dialogue.mod:3
msgid "Just you try runnin', /INSULT/!"
msgstr ""

#. Id: 15268-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#: 15268-Dialogue.mod:4
msgid "You can't outrun me, thief!"
msgstr "小偷，你不能超過我！"

#. Id: 15269-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=0, Tag: -1
#: 15269-Dialogue.mod:0
msgid "I'll teach you not to loot from /MEUS/!"
msgstr ""

#. Id: 15269-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=0, Tag: -1
#: 15269-Dialogue.mod:1
msgid "Don't fuck with /MEUS/, /IDIOT/!"
msgstr ""

#. Id: 15269-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=0, Tag: -1
#: 15269-Dialogue.mod:3
msgid "Shouldn't 've tried that!"
msgstr "不應該試過！"

#. Id: 15269-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=0, Tag: -1
#: 15269-Dialogue.mod:4
msgid "Lootin' from /MEUS/?"
msgstr ""

#. Id: 15269-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=0, Tag: -1
#: 15269-Dialogue.mod:5
msgid "You lootin'?"
msgstr "你哄騙'？"

#. Id: 15270-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 15270-Dialogue.mod:0
msgid "I'll stab your eyes out for that!"
msgstr "我會為此捅你的眼睛！"

#. Id: 15270-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 15270-Dialogue.mod:1
msgid "I'll gut you like a fish for that!"
msgstr "我會把你當成一條魚！"

#. Id: 15270-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 15270-Dialogue.mod:2
msgid "I'll gut you like a /DOG/ for that!"
msgstr ""

#. Id: 15270-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 15270-Dialogue.mod:3
msgid "I'll make you eat my sword for that!"
msgstr "我會讓你吃掉我的劍！"

#. Id: 48768-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48768-Dialogue.mod:0
msgid "You steal from /MYFACTION/, you die!"
msgstr ""

#. Id: 48768-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48768-Dialogue.mod:2
msgid "You cheat /MYFACTION/, you don't walk away!"
msgstr ""

#. Id: 48768-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (15267-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48768-Dialogue.mod:3
msgid "Tryin' to cheat /MYFACTION/?!"
msgstr ""

#. Id: 48781-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48781-Dialogue.mod:0
msgid "Thief!.. Don't you know who we are?"
msgstr "小偷！你不知道我們是誰嗎？"

#. Id: 48781-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48781-Dialogue.mod:1
msgid "You don't steal from US, thief!"
msgstr "你不偷竊美國，小偷！"

#. Id: 48780-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48780-Dialogue.mod:0
msgid "We're /MYFACTION/, /INSULT/! And you're gonna die for messin' with us!"
msgstr ""

#. Id: 48780-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48780-Dialogue.mod:1
msgid "We're /MYFACTION/... Remember it! You'll pay with blood for crossin' us!"
msgstr ""

#. Id: 48780-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (48781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48780-Dialogue.mod:2
msgid "You're in for a /STABBIN/ for tryin' a cheat /MYFACTION/!"
msgstr ""

#. Id: 48780-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (48781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting swamper ally
#: 48780-Dialogue.mod:3
msgid "I'll teach you not to steal from a Stone Rat!"
msgstr "我會教你不要偷石鼠！"

#. Id: 48780-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (48781-Dialogue.mod)
#. -- Speaker: Me (0)
