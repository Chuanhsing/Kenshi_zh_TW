msgstr "做得好。我會從這裡照顧他們。"

#. Id: 40921-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40920-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Slave delivery holy nation
#: 40921-Dialogue.mod:2
msgid "You've served Okran well, /BROTHER/. Okay, hand them over."
msgstr ""

#. Id: 40921-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40920-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Slave delivery holy nation
#: 40921-Dialogue.mod:3
msgid "Blessings upon you, /BROTHER/. I'll take them from here."
msgstr ""

#. Id: 40922-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slave delivery holy nation
#: 40922-Dialogue.mod:2
msgid "Okran bless you, /BROTHER/"
msgstr ""

#. Id: 40922-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slave delivery holy nation
#: 40922-Dialogue.mod:3
msgid "Careful of this one, /BROTHER/. The darkness runs deep."
msgstr ""

#. Id: 40922-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (40921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slave delivery holy nation
