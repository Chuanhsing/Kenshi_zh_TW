#: 18568-Dialogue.mod:0
msgid "You'll live to regret opposing noble blood, /PEASANT/!"
msgstr ""

#. Id: 18568-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble defeats someone
#: 18568-Dialogue.mod:1
msgid "You've made yourself a powerful enemy, /WORM/!"
msgstr ""

#. Id: 18568-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble defeats someone
#: 18568-Dialogue.mod:2
msgid "You won't get away with crossing ME, /PEASANT/!"
msgstr ""

#. Id: 18568-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble defeats someone
#: 18568-Dialogue.mod:3
msgid "You should never have crossed me, /WORM/!"
msgstr ""

#. Id: 18568-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble defeats someone
#: 18568-Dialogue.mod:4
msgid "You have no chance against us, /PEASANT/"
