#. Id: 42601-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek or Scorchlander
#: 42601-Dialogue.mod:4
msgid "Exterminate! Exterminate all humans!"
msgstr "殄！消滅所有人類！"

#. Id: 42601-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek or Scorchlander
#: 42601-Dialogue.mod:5
msgid "Destroy! Destroy all humans!"
msgstr "破壞！摧毀所有人類！"

#. Id: 42601-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Hive Prince or Hive Soldier Drone or Hive Worker Drone or Shek or Scorchlander
#: 42601-Dialogue.mod:9
msgid "Now is the age of the Skeleton!"
msgstr "現在是骷髏的時代！"

#. Id: 42603-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42601-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 42603-Dialogue.mod:0
msgid "We will own you! You serve US now!"
msgstr "我們會擁有你！你現在在美國服務！"

#. Id: 42603-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42601-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 42603-Dialogue.mod:1
msgid "You'll fight for us now! Give in to your masters!"
msgstr "你現在就為我們而戰！屈服於你的主人！"

#. Id: 42603-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (42601-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 42603-Dialogue.mod:2
msgid "You belong to US! Submit, serve us!"
msgstr "你屬於美國！提交，服務我們！"

#. Id: 42602-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42601-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Greenlander
#: 42602-Dialogue.mod:0
msgid "We will own you, humans! You serve US now!"
msgstr "我們將擁有你，人類！你現在在美國服務！"

#. Id: 42602-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42601-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Greenlander
#: 42602-Dialogue.mod:1
msgid "You'll fight for us now, humans! Give in to your masters!"
msgstr "你們現在要為我們而戰，人類！屈服於你的主人！"

#. Id: 62793-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Skeleton
#: 62793-Dialogue.mod:0
msgid "Could this one be one of us?"
msgstr "這個人能成為我們中的一員嗎？"

#. Id: 62793-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Skeleton
#: 62793-Dialogue.mod:1
msgid "Is this a fellow skeleton?"
msgstr "這是一個同伴骨架嗎？"

#. Id: 62793-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Skeleton
#: 62793-Dialogue.mod:2
msgid "A lost skeleton?"
msgstr "丟失的骨架？"

#. Id: 62793-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#. -- Condition: Target Race(s): Skeleton
#: 62793-Dialogue.mod:3
msgid "You've returned to Stobe..."
msgstr "你已經回到了Stobe ......"

#. Id: 63206-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (62793-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 63206-Dialogue.mod:1
msgid "Stobe be with you, skele-ton."
msgstr "Stobe和你在一起，skele-ton。"

#. Id: 63206-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (62793-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 63206-Dialogue.mod:2
msgid "Continue the purge, skele-ton."
msgstr "繼續吹掃，加油。"

#. Id: 63206-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (62793-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tin can spot human attack --UNFINISHED-- (add masked branch)
#: 63206-Dialogue.mod:3
msgid "Fight for Stobe, skele-ton."
msgstr "爭取Stobe，skele-ton。"

