#. Id: 98071-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#. Id: 14091-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 98071-Dialogue.mod:2 14091-Dialogue?(12).mod:3
msgid "Help me..."
msgstr "幫我..."

#. Id: 98071-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98071-Dialogue.mod:3
msgid "Please, help me..."
msgstr "請幫我..."

#. Id: 98073-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98071-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PLAYERMONEY>500, Tag: -1
#: 98073-Dialogue.mod:0
msgid "Here, take this..."
msgstr "在這裡，拿這個......"

#. Id: 98074-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98071-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PLAYERMONEY>1500, Tag: -1
#: 98074-Dialogue.mod:0
msgid "Buy yourself some food..."
msgstr "給自己買點食物......"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:0
msgid "So the rumours are true..."
msgstr "所以謠言是真的......"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:1
msgid "Luck find you..."
msgstr "運氣找到你......"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:2
msgid "Fortune fall upon you..."
msgstr "財富落在你身上......"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:3
msgid "You'd really help me?"
msgstr "你真的幫幫我嗎？"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:4
msgid "Kindness is a rarity in the outlands..."
msgstr "善良在外域是罕見的......"

#. Id: 98083-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (98074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98083-Dialogue.mod:5
msgid "I'd forgotten that kindness existed in this land..."
msgstr "我忘記了這片土地上存在的善意......"

#. Id: 98077-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98071-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PLAYERMONEY>200, Tag: -1
#: 98077-Dialogue.mod:0
msgid "This is all I've got..."
msgstr "這就是我所有的......"

#. Id: 98086-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98084-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98086-Dialogue.mod:0
msgid "It's something..."
msgstr "這是......"

#. Id: 98085-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98071-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Starver interrupt 2
#: 98085-Dialogue.mod:0
msgid "I've nothing to give..."
msgstr "我什麼都沒有給..."

#. Id: 98139-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98139-Dialogue.mod:0
msgid "I guess I heard wrong about you..."
msgstr "我想我聽錯了你..."

#. Id: 98139-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (98095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98139-Dialogue.mod:1
msgid "Shoulda known better than to beg peacefully, eh?"
msgstr "應該知道比和平乞求更好，是嗎？"

#. Id: 98140-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 98140-Dialogue.mod:0
msgid "Let's go. There's nothing for us here."
msgstr "我們走吧。我們這裡沒什麼。"

#. Id: 98145-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 98145-Dialogue.mod:0
msgid "I've no energy to rob you. Guess that makes us both lucky..."
msgstr "我沒有能力搶劫你。猜猜這讓我們都很幸運......"

#. Id: 98094-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (98071-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Starver interrupt 2
#: 98094-Dialogue.mod:0
msgid "Shoo! Be off with you, vagrant!"
msgstr "噓！和你一起，流浪漢！"

#. Id: 98096-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (98094-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98096-Dialogue.mod:0
msgid "Shoulda known better than to beg peacefully, eh?... Well, desperate people do desperate things, outlander..."
msgstr "應該知道比和平乞求更好，呃？......好吧，絕望的人做絕望的事情，外面的人......"

#. Id: 98150-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (98094-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 98150-Dialogue.mod:0
msgid "I didn't want it to come to this..."
msgstr "我不想讓它來到這個......"

#. Id: 98153-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (98150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Starver interrupt 2
#: 98153-Dialogue.mod:0
msgid "But /IWE/ need food!"
msgstr ""
