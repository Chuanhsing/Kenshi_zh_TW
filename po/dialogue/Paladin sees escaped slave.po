#. Id: 41044-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41044-Dialogue.mod:0
msgid "Wait... hold it there"
msgstr "等等......把它抱在那裡"

#. Id: 41044-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41044-Dialogue.mod:1
msgid "Wiat... stop where you are"
msgstr "Wiat ......停在你身邊"

#. Id: 41045-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41044-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41045-Dialogue.mod:1
msgid "Come closer to me..."
msgstr "靠近我一點..."

#. Id: 41047-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41046-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41047-Dialogue.mod:0
msgid "You!... Escaped from Rebirth?!"
msgstr "你！......從重生逃脫？！"

#. Id: 41047-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (41046-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41047-Dialogue.mod:1
msgid "... Thought you could escape Rebirth, did you?!"
msgstr "......以為你可以逃脫重生，是嗎？！"

#. Id: 41053-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 41053-Dialogue.mod:0
msgid "Fugitive! Don't run from me!"
msgstr "逃犯！不要逃避我！"

#. Id: 41053-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (41045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 41053-Dialogue.mod:1
msgid "Fugitive! You can't run from me!"
msgstr "逃犯！你不能逃避我！"

#. Id: 41053-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (41045-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 41053-Dialogue.mod:2
msgid "Running from me? I knew it! You won't escape me, fugitive!"
msgstr "從我這裡跑？我就知道！你不會逃避我，逃亡！"

#. Id: 41055-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41053-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41055-Dialogue.mod:0
msgid "I'm sending you back to Rebirth!"
msgstr "我要把你送回重生！"

#. Id: 41055-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (41053-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41055-Dialogue.mod:1
msgid "Back to Rebirth for you!"
msgstr "回到重生為你服務！"

#. Id: 41055-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (41053-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Paladin sees escaped slave
#: 41055-Dialogue.mod:2
msgid "May Okran forgive you for your betrayal!"
msgstr "願Okran原諒你的背叛！"
