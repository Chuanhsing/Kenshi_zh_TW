#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97353-Dialogue.mod:3 97366-Dialogue.mod:3
msgid "To the capital..."
msgstr "到首都......"

#. Id: 97357-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (97353-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#. Id: 96066-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (96065-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97357-Dialogue.mod:0 96066-Dialogue.mod:2
msgid "Make haste..."
msgstr "趕快..."

#. Id: 97357-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (97353-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#. Id: 96066-Dialogue.mod
#. -- Index: 4.4
#. -- Parent line: 3 (96065-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97357-Dialogue.mod:1 96066-Dialogue.mod:4
msgid "Come now..."
msgstr "現在來..."

#. Id: 97362-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (97357-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=0, Tag: -1
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97362-Dialogue.mod:0
msgid "The skeletons must be purged!"
msgstr "必須清除骷髏！"

#. Id: 96062-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (96061-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#: 96062-Dialogue.mod:0
msgid "Are there no men in this outpost?"
msgstr "這個前哨站裡沒有男人嗎？"

#. Id: 96067-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (96062-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#: 96067-Dialogue.mod:0
msgid "This cannot be!"
msgstr "這不可能！"

#. Id: 96065-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (96067-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 96065-Dialogue.mod:0
msgid " It is against /SACRED/ law to take up residence without brotherly guidance..."
msgstr ""

#. Id: 96065-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (96067-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 96065-Dialogue.mod:1
msgid "Woman needs brotherly guidance and discipline that only a son of Okran can give..."
msgstr "女人需要兄弟般的指導和紀律，只有奧克蘭的兒子才能給..."

#. Id: 96068-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (96066-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_A_PLAYER_TOWN=0, Tag: -1
#: 96068-Dialogue.mod:0
msgid "Your blasphemy won't go unchecked, believe you me!"
msgstr "你的褻瀆不會不受控制，相信你我！"

#. Id: 97367-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (97366-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97367-Dialogue.mod:0
msgid "I must report back at once!"
msgstr "我必須馬上報到！"

#. Id: 97367-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (97366-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97367-Dialogue.mod:2
msgid "We must have them removed!"
msgstr "我們必須刪除它們！"

#. Id: 97367-Dialogue.mod
#. -- Index: 6.3
#. -- Parent line: 5 (97366-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97367-Dialogue.mod:3
msgid "I must report this!"
msgstr "我必須報告這個！"

#. Id: 96070-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 96070-Dialogue.mod:0
msgid "... Beasts?"
msgstr "......野獸？"

#. Id: 96070-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 96070-Dialogue.mod:1
msgid "... Darkened?"
msgstr "......變暗了？"

#. Id: 96070-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 96070-Dialogue.mod:2
msgid "... Darkened ones?"
msgstr "......變暗的？"

#. Id: 96070-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 96070-Dialogue.mod:3
msgid "... Dark beasts?"
msgstr "......黑暗的野獸？"

#. Id: 96071-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (96070-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#: 96071-Dialogue.mod:0
msgid "On this /SACRED/ /LAND/!?"
msgstr ""

#. Id: 96073-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (96071-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 96073-Dialogue.mod:0
msgid "What heresy is this?"
msgstr "這是什麼異端？"

#. Id: 96074-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (96073-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 96074-Dialogue.mod:0
msgid "The impure are not welcome here on Okran's soil..."
msgstr "在奧克蘭的土地上，這裡不受歡迎......"

#. Id: 96075-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (96074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#: 96075-Dialogue.mod:0
msgid "You will be removed."
msgstr "你將被刪除。"

#. Id: 97341-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (96070-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Target Race(s): Greenlander
#: 97341-Dialogue.mod:0
msgid "There are unchained beasts in this outpost?"
msgstr "這個前哨裡有沒有束縛的野獸？"

#. Id: 97342-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (97341-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97342-Dialogue.mod:0
msgid "Speak to me now, send me your leader!"
msgstr "現在跟我說說，把你的領導送給我吧！"

#. Id: 97343-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (97342-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97343-Dialogue.mod:0
msgid "I don't have time to wait around..."
msgstr "我沒有時間等待......"

#. Id: 97344-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (97343-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97344-Dialogue.mod:0
msgid "I cannot risk corruption from a tainted one."
msgstr "我不能冒被污染的腐敗風險。"

#. Id: 97345-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (97344-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97345-Dialogue.mod:0
msgid "Last chance. Send me your leader!"
msgstr "最後的機會。把你的領導送給我！"

#. Id: 97347-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (97346-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor searching player base
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 97347-Dialogue.mod:0
msgid "I'll be sure to report this back to Inquisitor Seta himself."
msgstr "我一定會把這個報告給Inquisitor Seta本人。"
