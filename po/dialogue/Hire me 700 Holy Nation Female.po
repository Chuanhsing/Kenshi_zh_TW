#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 19238-Dialogue.mod:4
msgid "I'm looking for work, you looking to /TEAMUP/, sister?"
msgstr ""

#. Id: 19240-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (19238-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19240-Dialogue.mod:0
msgid "I am. How much?"
msgstr "我是。多少？"

#. Id: 19240-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (19238-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19240-Dialogue.mod:1
