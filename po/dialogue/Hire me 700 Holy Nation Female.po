#. Id: 19234-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (19232-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19234-Dialogue.mod:1
msgid "... You a merc by any chance?"
msgstr ""

#. Id: 19234-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (19232-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19234-Dialogue.mod:2
msgid "Um... need a job by any chance?"
msgstr ""

#. Id: 19234-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (19232-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19234-Dialogue.mod:3
msgid "Er... looking for work? I need recruits."
msgstr ""

#. Id: 19235-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (19234-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19235-Dialogue.mod:0
msgid "... Yes. I charge 3000 Cats. Will we be leaving the city?"
msgstr ""

#. Id: 19235-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (19234-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19235-Dialogue.mod:1
msgid "... Actually, yes... My price is 3000 Cats. Will we be travelling out of town?"
msgstr ""

#. Id: 19235-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (19234-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19235-Dialogue.mod:2
msgid "... That's right. I charge 3000 Cats... You from out of town?"
msgstr ""

#. Id: 19235-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (19234-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me 700 Holy Nation Female
#: 19235-Dialogue.mod:3
msgid "... Yes. My fee is 3000 Cats... Are you heading out?"
msgstr ""

#. Id: 42242-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (19235-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me 700 Holy Nation Female
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>3000, Tag: -1
#: 42242-Dialogue.mod:0
msgid "That's right. Join /MEUS/."
