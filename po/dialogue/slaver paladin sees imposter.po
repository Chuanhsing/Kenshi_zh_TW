#. Id: 41026-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. -- Condition: Who: TARGET, Condition: TARGET_IS_SLAVE_OF_MY_FACTION=1, Tag: -1
#: 41026-Dialogue.mod:0
msgid "You're not one of us!"
msgstr "你不是我們中的一員！"

#. Id: 41026-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. -- Condition: Who: TARGET, Condition: TARGET_IS_SLAVE_OF_MY_FACTION=1, Tag: -1
#: 41026-Dialogue.mod:1
msgid "You... you're an imposter!"
msgstr "你......你是冒名頂替者！"

#. Id: 41026-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. -- Condition: Who: TARGET, Condition: TARGET_IS_SLAVE_OF_MY_FACTION=1, Tag: -1
#: 41026-Dialogue.mod:2
msgid "You... you're wearing our uniform!"
msgstr "你......你穿著我們的製服！"

#. Id: 41027-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41026-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#: 41027-Dialogue.mod:0
msgid "Heretic! /HEIS/ trying to escape!"
msgstr ""

#. Id: 41027-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41026-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#: 41027-Dialogue.mod:1
msgid "Don't let the heretic escape!"
msgstr "不要讓異教徒逃脫！"

#. Id: 41027-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41026-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#: 41027-Dialogue.mod:2
msgid "Trying to escape? Okran will punish you!"
msgstr "想逃跑？奧克蘭會懲罰你！"

#. Id: 41031-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. -- Condition: Who: TARGET, Condition: I_HATE_THIS_GUY=1, Tag: -1
#: 41031-Dialogue.mod:0
msgid "You! I know you!"
msgstr "您！我認識你！"

#. Id: 41031-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. -- Condition: Who: TARGET, Condition: I_HATE_THIS_GUY=1, Tag: -1
#: 41031-Dialogue.mod:1
msgid "... You! I know your face!"
msgstr "......你！我知道你的臉！"

#. Id: 41033-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41031-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#: 41033-Dialogue.mod:0
msgid "Wearing our uniform? You'll escape over my dead body!"
msgstr "穿我們的製服？你會逃脫我的屍體！"

#. Id: 41033-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41031-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#: 41033-Dialogue.mod:1
msgid "Imposter! Don't let /HIM/ get away!"
msgstr ""

#. Id: 41033-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41031-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. Id: 41034-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slaver paladin sees imposter
#. Id: 15696-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 41033-Dialogue.mod:2 41034-Dialogue.mod:0 15696-Dialogue.mod:0
msgid "Imposter! /SEIZE/ /HIM/!"
msgstr ""

