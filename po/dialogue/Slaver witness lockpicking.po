#. Id: 16680-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#. -- Condition: In town(s) of faction(s): Slave Traders
#: 16680-Dialogue.mod:0
msgid "Oi! What are you doing!?"
msgstr "餵！你在做什麼！？"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:0
msgid "Troublemakers get beaten!"
msgstr "麻煩製造者被毆打！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:1
msgid "Take your punishment, slave!"
msgstr "接受你的懲罰，奴隸！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:2
msgid "Disobedient slave!"
msgstr "不聽話的奴隸！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:3
msgid "Defiant slaves are disposed of!"
msgstr "違規的奴隸被處置掉！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:4
msgid "Unruly slave!"
msgstr "不守規矩的奴隸！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:5
msgid "You'll regret trying that!"
msgstr "你會後悔的！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:6
msgid "You'll be punished!"
msgstr "你會受到懲罰！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:7
msgid "I'll punish you!"
msgstr "我會懲罰你的！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#. Id: 16742-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness assault
#: 16681-Dialogue.mod:8 16742-Dialogue.mod:0
msgid "Bad slave!"
msgstr "壞奴隸！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:9
msgid "No! Bad slave!"
msgstr "沒有！壞奴隸！"

#. Id: 16681-Dialogue.mod
#. -- Index: 1.10
#. -- Parent line: 0 (16680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Slaver witness lockpicking
#: 16681-Dialogue.mod:10
msgid "No, slave! Bad slave!"
msgstr "不，奴隸！壞奴隸！"
