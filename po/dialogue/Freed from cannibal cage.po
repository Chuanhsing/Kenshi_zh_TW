#. Id: 6022-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: In town(s) of faction(s): Cannibals
#: 6022-Dialogue.mod:0
msgid "You, you saved me!"
msgstr "你，救了我！"

#. Id: 6023-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (6022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#: 6023-Dialogue.mod:0
msgid "Thank you! "
msgstr "謝謝！"

#. Id: 14107-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14107-Dialogue?(12).mod:1
msgid "I'm free"
msgstr "我自由了"

#. Id: 14107-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14107-Dialogue?(12).mod:3
msgid "Success!"
msgstr "成功！"

#. Id: 15836-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (14107-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#: 15836-Dialogue.mod:1
msgid "Let's get the hell out of here"
msgstr "讓我們離開這裡吧"

#. Id: 15836-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (14107-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#: 15836-Dialogue.mod:2
msgid "You lead, I'll follow you out!"
msgstr "你領導，我會跟著你！"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14122-Dialogue?(4).mod:0
msgid "I'm out! "
msgstr "我出去了！"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14122-Dialogue?(4).mod:2
msgid "... but, what if they're hiding, waiting for us, tricking us?!"
msgstr "......但是，如果他們在躲藏，等著我們，欺騙我們怎麼辦？！"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14122-Dialogue?(4).mod:4
msgid "It's open, you did it!"
msgstr "它打開了，你做到了！"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#. Id: 17983-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Unlocked hiver ronin
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: My Race(s): Hive Prince
#: 14122-Dialogue?(4).mod:5 17983-Dialogue.mod:0
msgid "You got it!"
msgstr "你說對了！"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14122-Dialogue?(4).mod:7
msgid "... oh god, what if they see us..."
msgstr "......天啊，如果他們看到我們怎麼辦......"

#. Id: 14122-Dialogue (4).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14122-Dialogue?(4).mod:8
msgid "I'm free! God, I have to get out of here!"
msgstr "我自由了！天啊，我得走出這裡！"

#. Id: 14123-Dialogue (4).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14122-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#: 14123-Dialogue?(4).mod:0
msgid "Please, take me with you"
msgstr "請帶我一起去"

#. Id: 14123-Dialogue (4).mod
#. -- Index: 1.1
#. -- Parent line: 0 (14122-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#: 14123-Dialogue?(4).mod:1
msgid "Please, don't leave me on my own"
msgstr "拜託，不要讓我自己離開"

#. Id: 14124-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14124-Dialogue?(4).mod:1
msgid "I'm out of here!"
msgstr "我離開了！"

#. Id: 14124-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14124-Dialogue?(4).mod:2
msgid "I'm running like hell!"
msgstr "我跑得像地獄！"

#. Id: 14124-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14124-Dialogue?(4).mod:3
msgid "I'm not hanging around, that's for sure"
msgstr "我沒有閒逛，這是肯定的"

#. Id: 14124-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14124-Dialogue?(4).mod:4
msgid "I can't let them catch me again! I have to get out of here!"
msgstr "我不能讓他們再次抓住我！我必須離開這裡！"

#. Id: 14124-Dialogue (4).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: In town(s) of faction(s): Cannibals
#: 14124-Dialogue?(4).mod:5
msgid "I gotta get out of here, before they come back!"
msgstr "在他們回來之前我得離開這裡！"
