#. -- Group title: Good Relations - bandit gang
#: 8783-Dialogue.mod:0
msgid "Hey, /INSULT/! Give me that nice /SWORD/! Go on, hand it over!"
msgstr ""

#. Id: 8785-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (8783-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Good Relations - bandit gang
#: 8785-Dialogue.mod:0
msgid "Don't you know who that is? /HEIS/ one of us!"
msgstr ""

#. Id: 8786-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (8785-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Good Relations - bandit gang
#: 8786-Dialogue.mod:0
msgid "Like I give a shit! I just want /HIS/ /SWORD/!"
msgstr ""

#. Id: 8787-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (8786-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Good Relations - bandit gang
#: 8787-Dialogue.mod:0
msgid "*Sigh*... /IDIOT/..."
msgstr ""

#. Id: 8788-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (8787-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Good Relations - bandit gang
#: 8788-Dialogue.mod:0
msgid "Now, you! Gimme the /FUCKING/ /SWORD/!"
msgstr ""

#. Id: 8789-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (8788-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Good Relations - bandit gang
#: 8789-Dialogue.mod:0
msgid "/THISGUY/ /IS/ off limits. Back off."
msgstr ""

#. Id: 8791-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (8789-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Good Relations - bandit gang
#: 8791-Dialogue.mod:0
msgid "Standin' in the way of my shiny new sword? You must be looking for a fight!"
msgstr "站在我閃亮的新劍的路上？你一定在尋找戰鬥！"

#. Id: 8792-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (8791-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Good Relations - bandit gang
#: 8792-Dialogue.mod:0
msgid "You don't wanna fight me, pal! Just try me!"
msgstr "伙計，你不想打我！試試吧！"

#. Id: 8793-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (8792-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Good Relations - bandit gang
#: 8793-Dialogue.mod:0
msgid ".... I'll stab you in your sleep."
msgstr "......我會在睡夢中刺傷你。"
