#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14035-Dialogue?(12).mod:2 42941-Dialogue.mod:1
msgid "Halt, /SLAVE/!"
msgstr ""

#. Id: 14036-Dialogue (12).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14035-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14036-Dialogue?(12).mod:0
msgid "Where is your master?!"
msgstr "你的主人在哪裡？！"

#. Id: 14038-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#. Id: 42941-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14038-Dialogue?(12).mod:0 42941-Dialogue.mod:0
msgid "/SLAVE/!"
msgstr ""

#. Id: 14039-Dialogue (12).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14039-Dialogue?(12).mod:0
msgid "You can't escape your masters!"
msgstr "你無法逃脫你的主人！"

#. Id: 14039-Dialogue (12).mod
#. -- Index: 1.1
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14039-Dialogue?(12).mod:1
msgid "You dare try and escape your masters?!"
msgstr "你敢試試逃脫你的主人嗎？！"

#. Id: 14045-Dialogue (12).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14045-Dialogue?(12).mod:0
msgid "You dare wander from your master?"
msgstr "你敢從主人那裡徘徊？"

#. Id: 14045-Dialogue (12).mod
#. -- Index: 1.1
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#. Id: 18909-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14045-Dialogue?(12).mod:1 18909-Dialogue.mod:0
msgid "Why are you without your master?"
msgstr "你為什麼沒有你的主人？"

#. Id: 14045-Dialogue (12).mod
#. -- Index: 1.2
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#. Id: 18909-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14045-Dialogue?(12).mod:2 18909-Dialogue.mod:2
msgid "Where is your master?"
msgstr "你的主人在哪裡？"

#. Id: 14045-Dialogue (12).mod
#. -- Index: 1.3
#. -- Parent line: 0 (14038-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14045-Dialogue?(12).mod:3
msgid "Why aren't you with your master?"
msgstr "為什麼不和你的主人在一起？"

#. Id: 14042-Dialogue (12).mod
#. -- Index: 2.0
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14042-Dialogue?(12).mod:0
msgid "You'll be severely punished!"
msgstr "你會受到嚴厲的懲罰！"

#. Id: 42942-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42942-Dialogue.mod:0
msgid "Heretic! A betrayal to Okran!"
msgstr "異教徒！背叛奧克蘭！"

#. Id: 42942-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42942-Dialogue.mod:1
msgid "You forsake the Lord Okran! And the Holy Lord Phoenix Himself, blessed be his name! /GREASY/ heretic!"
msgstr ""

#. Id: 42942-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42942-Dialogue.mod:2
msgid "You'd abandon your lord, Okran!? /TERRORIST/!"
msgstr ""

#. Id: 42942-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42942-Dialogue.mod:3
msgid "You'd run and betray the Lord of Light, Okran himself!?"
msgstr "你會跑掉並背叛光明之王，奧克蘭自己！？"

#. Id: 42942-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (14045-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42942-Dialogue.mod:4
msgid "/TERRORIST/! You betray your duties as a holy servant!"
msgstr ""

#. Id: 14043-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14043-Dialogue?(12).mod:0
msgid "Disobedient /SLAVE/!"
msgstr ""

#. Id: 14043-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14043-Dialogue?(12).mod:1
msgid "Insubordinate /SLAVE/!"
msgstr ""

#. Id: 14043-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14043-Dialogue?(12).mod:2
msgid "Stupid /SLAVE/!"
msgstr ""

#. Id: 14043-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 14043-Dialogue?(12).mod:3
msgid "Dumb /SLAVE/!"
msgstr ""

#. Id: 18909-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 18909-Dialogue.mod:1
msgid "You're without a master...?"
msgstr "你沒有主人......？"

#. Id: 42944-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18909-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42944-Dialogue.mod:0
msgid "You can't escape Rebirth!"
msgstr "你無法逃避重生！"

#. Id: 42944-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18909-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42944-Dialogue.mod:1
msgid "You can never escape Rebirth!"
msgstr "你永遠無法逃脫重生！"

#. Id: 42944-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18909-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42944-Dialogue.mod:2
msgid "You are a Holy Servant of Okran, how dare you escape your master?"
msgstr "你是奧克蘭的神聖僕人，你怎麼敢逃脫你的主人？"

#. Id: 42944-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (18909-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42944-Dialogue.mod:3
msgid "You serve the pure blooded, the children of Okran. You dare escape you /SACRED/ duties?!"
msgstr ""

#. Id: 42943-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42943-Dialogue.mod:0
msgid "To escape your duties as holy servant, it is one of the greatest sins!"
msgstr "為了逃避作為聖僕的職責，這是最大的罪惡之一！"

#. Id: 42943-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42943-Dialogue.mod:1
msgid "/GREASY/ /TERRORIST/! You'll burn in the holy flame for your treachery!"
msgstr ""

#. Id: 42943-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (42944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42943-Dialogue.mod:2
msgid "Treacherous /TERRORIST/! Your betrayal must be punished!"
msgstr ""

#. Id: 42943-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (42944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spots slave
#: 42943-Dialogue.mod:3
msgid "The Lord Okran will have his retribution, traitor!"
msgstr "奧克蘭勳爵將得到他的報應，叛徒！"
