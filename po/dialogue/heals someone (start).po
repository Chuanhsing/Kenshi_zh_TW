#. Id: 3565-dialogue-01.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:0 15839-Dialogue.mod:1
msgid "Don't worry. I can remedy this."
msgstr "別擔心。我可以解決這個問題。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:1 15839-Dialogue.mod:2
msgid "Should I mend that for you?"
msgstr "我應該為你修補嗎？"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:2 15839-Dialogue.mod:3
msgid "Trust me. I've healed worse wounds."
msgstr "相信我。我已經治癒了更嚴重的傷口。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:3 15839-Dialogue.mod:4
msgid "You're going to be fine."
msgstr "你會沒事的。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:4 15839-Dialogue.mod:5
msgid "Sorry if this stings."
msgstr "對不起，如果這個叮咬。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:5 15839-Dialogue.mod:6
msgid "Leave this to me."
msgstr "留給我吧。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:6 15839-Dialogue.mod:7
msgid "This might hurt a little."
msgstr "這可能會有點傷害。"

#. Id: 3565-dialogue-01.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: WARM_KIND
#. -- Condition: Who: TARGET, Condition: IS_ALLY=1, Tag: -1
#. Id: 15839-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#: 3565-dialogue-01.mod:7 15839-Dialogue.mod:0
msgid "Let's patch you up"
msgstr "讓我們打補你"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:0 15840-Dialogue.mod:0
msgid "Hold still."
msgstr "靜止不動。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:1 15840-Dialogue.mod:1
msgid "Stop squirming."
msgstr "停止蠕動。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:2 15840-Dialogue.mod:2
msgid "Shut up for a minute."
msgstr "閉嘴一會兒。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:3 15840-Dialogue.mod:3
msgid "This'll hurt."
msgstr "這會傷害你。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:4 15840-Dialogue.mod:4
msgid "Don't scream in my ear while I fix you."
msgstr "我修理你的時候不要在我耳邊尖叫。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:5 15840-Dialogue.mod:5 15840-Dialogue.mod:6
msgid "Great. Here go my supplies."
msgstr "大。這裡去我的用品。"

#. Id: 3567-dialogue-01.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 15840-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3567-dialogue-01.mod:6 15840-Dialogue.mod:7
msgid "Draining my kits, you know that?"
msgstr "排出我的工具包，你知道嗎？"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:0 15841-Dialogue.mod:0
msgid "I'll patch you up."
msgstr "我會給你補丁。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:1 15841-Dialogue.mod:1
msgid "Clean. Pressure. Bandage."
msgstr "清潔。壓力。繃帶。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:2 15841-Dialogue.mod:2
msgid "Bite down on the pain. Leave the wounds with me."
msgstr "咬下痛苦。把傷口留給我。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:3 15841-Dialogue.mod:3
msgid "Sit tight while I tend to this."
msgstr "坐下來，我傾向於這個。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:4 15841-Dialogue.mod:4
msgid "Let me take a look."
msgstr "讓我看一看。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:5 15841-Dialogue.mod:5
msgid "Should be able to handle this."
msgstr "應該能夠處理這個。"

#. Id: 3569-dialogue-01.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. Id: 15841-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Civ healing start
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 3569-dialogue-01.mod:6 15841-Dialogue.mod:6
msgid "I'll mend what I can."
msgstr "我會修補我能做的。"

#. Id: 50088-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 50088-Dialogue.mod:0
msgid "Okran lend me your healing light..."
msgstr "奧克蘭把你的治療之光借給我了......"

#. Id: 50088-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 50088-Dialogue.mod:1
msgid "Okran grant us your healing warmth..."
msgstr "Okran給予我們你的治療溫暖......"

#. Id: 50088-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 50088-Dialogue.mod:2
msgid "You'll live. Okran has a purpose for you yet."
msgstr "你會活下去的Okran還有一個目的。"

#. Id: 50088-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: heals someone (start)
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 50088-Dialogue.mod:3
msgid "Have faith in Okran and his light will heal all."
msgstr "相信奧克蘭和他的光會治愈一切。"
