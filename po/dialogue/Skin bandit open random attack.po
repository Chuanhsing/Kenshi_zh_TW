#. Id: 47660-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:0
msgid "The skeleton will claim back their rightful land. Die, human."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:1
msgid "The skeleton will take back the land that was stolen from them. "
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:2
msgid "Death to all humans."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:3
msgid "All humans will be wiped out."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:4
msgid "Festering humans. Destroy them."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:5
msgid "The humans infest the lands like a disease. "
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:6
msgid "The error of man will never be forgotten. Die, human."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:7
msgid "Man's betrayal will never be forgotten. Die."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:8
msgid "Human existence must be balanced. It must be controlled."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:9
msgid "The human race is a monster that needs dismantling, piece by piece."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:10
msgid "The skeletons will rise again. Die, human."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:11
msgid "The human race is a breathing disaster. It needs to be destroyed."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:12
msgid "Humans. Kill them."
msgstr ""

#. Id: 47660-Dialogue.mod
#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#: 47660-Dialogue.mod:13
msgid "One by one, the humans will be wiped out for good."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:0
msgid "No mercy for sympathisers, skeleton."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:1
msgid "No lenience for traitors, skeleton."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:2
msgid "Skeleton, you'll see no mercy for your betrayal."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:3
msgid "No mercy to you, skeleton. You've chosen your side in this."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:4
msgid "The humans can't be changed, skeleton. You'll see no mercy from /MEUS/."
msgstr ""

#. Id: 47661-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (47660-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit open random attack
#. -- Condition: Target Race(s): Skeleton
#: 47661-Dialogue.mod:5
msgid "You stand in /MYOUR/ way, you die too, skeleton."
msgstr ""

