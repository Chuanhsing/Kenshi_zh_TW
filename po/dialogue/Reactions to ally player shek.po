msgid "Don't let your strength wither, /YOURNAME/..."
msgstr ""

#. Id: 57260-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:1
msgid "Skin and bones, kin..."
msgstr "皮膚和骨骼，親屬......"

#. Id: 57260-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:2
msgid "Scrawny..."
msgstr "骨瘦如柴..."

#. Id: 57260-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:3
msgid "Puny..."
msgstr "弱小..."

#. Id: 57260-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:4
msgid "Skinny, kin..."
msgstr "瘦，親戚......"

#. Id: 57260-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:5
msgid "Too thin, /YOURNAME/..."
msgstr ""

#. Id: 57260-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:6
msgid "No good, /YOURNAME/..."
msgstr ""

#. Id: 57260-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#: 57260-Dialogue.mod:7
msgid "You shrivel as we speak, kin..."
msgstr "當我們說話時你萎縮，親戚......"

#. Id: 57262-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (57260-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Reactions to ally player shek
