#. Id: 99080-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99080-Dialogue.mod:0
msgid "A human village..."
msgstr "一個人類的村莊......"

#. Id: 99081-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (99080-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99081-Dialogue.mod:0
msgid "Perhaps we will find new skins among the inhabitants..."
msgstr "也許我們會在居民中找到新的皮膚......"

#. Id: 99081-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (99080-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99081-Dialogue.mod:1
msgid "Full of fresh skins.."
msgstr "充滿新鮮的皮膚.."

#. Id: 99082-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (99081-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99082-Dialogue.mod:0
msgid "And lots of human food for our hungry bellies..."
msgstr "為我們飢餓的肚子提供大量人類食物......"

#. Id: 99082-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (99081-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99082-Dialogue.mod:1
msgid "And lots of human food for our hungry digestive systems..."
msgstr "我們飢餓的消化系統有很多人類食物......"

#. Id: 99084-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (99083-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Skeleton bandit raid
#: 99084-Dialogue.mod:0
msgid "YUM"
msgstr "YUM"

#. Id: 99087-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#. Id: 65203-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (65201-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandits singing
#. Id: 63838-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit skin raid announce
#. -- Condition: Target Race(s): Greenlander
#: 99087-Dialogue.mod:0 65203-Dialogue.mod:0 63838-Dialogue.mod:0
msgid "Give us your skin!"
msgstr "給我們你的皮膚！"

#. Id: 99087-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#. Id: 65203-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (65201-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandits singing
#. Id: 63838-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit skin raid announce
#. -- Condition: Target Race(s): Greenlander
#: 99087-Dialogue.mod:1 65203-Dialogue.mod:2 63838-Dialogue.mod:2
msgid "Come to us, flesh brothers! We just want your skin..."
msgstr "來到我們這裡，肉體兄弟！我們只想要你的皮膚......"

#. Id: 99088-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (99087-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#. Id: 63839-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63838-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit skin raid announce
#: 99088-Dialogue.mod:0 63839-Dialogue.mod:1
msgid "Come share with your fellow human brothers!"
msgstr "與你的同胞兄弟分享吧！"

#. Id: 99088-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (99087-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#. Id: 63839-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63838-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit skin raid announce
#: 99088-Dialogue.mod:1 63839-Dialogue.mod:0
msgid "It's good to share!"
msgstr "分享真好！"

#. Id: 99089-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (99088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#. Id: 99091-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99089-Dialogue.mod:0 99091-Dialogue.mod:0
msgid "Come out, flesh brothers!"
msgstr "出來，肉體兄弟！"

#. Id: 99093-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (99091-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99093-Dialogue.mod:0
msgid "Come talk to us..."
msgstr "來跟我們說說......"

#. Id: 99093-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (99091-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99093-Dialogue.mod:1
msgid "Please come talk to your fellow humans!"
msgstr "請跟你的同胞談談！"

#. Id: 99092-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (99093-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99092-Dialogue.mod:0
msgid "We mean no harm..."
msgstr "我們的意思是無害......"

#. Id: 99092-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (99093-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99092-Dialogue.mod:2
msgid "We won't hurt you..."
msgstr "我們不會傷害你......"

#. Id: 99094-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (99092-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99094-Dialogue.mod:0
msgid "... We just want your skin."
msgstr "......我們只想要你的皮膚。"

#. Id: 99095-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (99094-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99095-Dialogue.mod:0
msgid "So fresh... so good."
msgstr "太新鮮了......真好。"

#. Id: 99095-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (99094-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99095-Dialogue.mod:1
msgid "That good skin..."
msgstr "那個好皮膚......"

#. Id: 99095-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (99094-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99095-Dialogue.mod:2
msgid "Your fresh.. clean... skin..."
msgstr "你的新鮮......乾淨......皮膚......"

#. Id: 99097-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (99095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton bandit raid
#: 99097-Dialogue.mod:0
msgid "Skin!"
msgstr "皮膚！"
