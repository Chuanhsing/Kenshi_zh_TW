#. Id: 6761-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (6758-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6761-Dialogue.mod:0
msgid "Wait! What do you want from me?"
msgstr ""

#. Id: 6761-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (6758-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6761-Dialogue.mod:1
msgid "Wait... I'll do whatever you want"
msgstr ""

#. Id: 6761-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (6758-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6761-Dialogue.mod:2
msgid "Don't hurt me, I'll do whatever you want"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:0
msgid "/IWE/ want your /MONEY/! Your food! Your gear! All of it!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:1
msgid "/IWE/ want your /MONEY/! Hand it over, all of it!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:2
msgid "You want to settle this peacefully? Then hand over your /CATS/, quickly!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:3
msgid "/IWE/ want your gear, your food, your money... everything! Give it to /MEUS/!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:4
msgid "/IWE/ want your /MONEY/! Your food! Your gear! Everything!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:5
msgid "/IWE/ want your /MONEY/! Go on, hand over your /CATS/... NOW!"
msgstr ""

#. Id: 6762-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (6761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6762-Dialogue.mod:6
msgid "Make this easy. Hand over your /CATS/. If you give /MEUS/ your money, we won't /SLITYOURTHROAT/. Do NOT make me ask you again!"
msgstr ""

#. Id: 6763-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6763-Dialogue.mod:0
msgid "I don't want to fight. Can't we work something out?"
msgstr ""

#. Id: 6763-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6763-Dialogue.mod:1
msgid "No violence. Maybe we can come to an agreement?"
msgstr ""

#. Id: 6763-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6763-Dialogue.mod:2
msgid "Okay, let's be civil about this... maybe we can come to an agreement?"
msgstr ""

#. Id: 6763-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6763-Dialogue.mod:3
msgid "Please, can't we settle this some other way?"
msgstr ""

#. Id: 14469-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (6763-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 14469-Dialogue?(4).mod:0
msgid "No! Give it up! I won't ask twice, quit screwin' around, /INSULT/..."
msgstr ""

#. Id: 14469-Dialogue (4).mod
#. -- Index: 4.1
#. -- Parent line: 3 (6763-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 14469-Dialogue?(4).mod:1
msgid "No! Hand the Cats over already, /INSULT/. I ain't gonna ask again!"
msgstr ""

#. Id: 7139-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7139-Dialogue.mod:0
msgid "Never! Just you try and take it from me, /GODDAMN/ starvers"
msgstr ""

#. Id: 7139-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7139-Dialogue.mod:1
msgid "Never! I won't surrender to you that easily, thieving /RAT/s"
msgstr ""

#. Id: 7140-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7140-Dialogue.mod:0
msgid "I've worked hard for my money, I'm not going to let you take it"
msgstr ""

#. Id: 7140-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7140-Dialogue.mod:2
msgid "No, I've worked hard for my money"
msgstr ""

#. Id: 7140-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7140-Dialogue.mod:3
msgid "... No. You're not taking my money"
msgstr ""

#. Id: 7140-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7140-Dialogue.mod:4
msgid "... I don't have any money, honestly"
msgstr ""

#. Id: 7149-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (7140-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 7149-Dialogue.mod:0
msgid "Damn /MISER/! I'll TAKE it from you one way or the other!"
msgstr ""

#. Id: 7149-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (7140-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 7149-Dialogue.mod:1
msgid "Damn /MISER/! Then you leave me no choice but to strip it from your skin!"
msgstr ""

#. Id: 7149-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (7140-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 7149-Dialogue.mod:2
msgid "Damn /MISER/! You brought this on yourself! "
msgstr ""

#. Id: 6765-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>300, Tag: -1
#: 6765-Dialogue.mod:0
msgid "That's fine, just let /MEUS/ go. Here (300 Cats)"
msgstr ""

#. Id: 6765-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>300, Tag: -1
#: 6765-Dialogue.mod:1
msgid "/FINE/. Here (300 Cats)"
msgstr ""

#. Id: 6765-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (6762-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>300, Tag: -1
#: 6765-Dialogue.mod:2
msgid "No trouble. Here (300 Cats)"
msgstr ""

#. Id: 6769-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#. Id: 17774-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (6765-Dialogue.mod)
