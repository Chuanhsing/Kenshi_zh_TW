#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: BROKEN_ARM=1, Tag: -1
#. Id: 17775-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: DAMAGED_HEAD=1, Tag: -1
#: 6769-Dialogue.mod:0 17774-Dialogue.mod:0 17775-Dialogue.mod:0
msgid "Stingy /INSULT/. You got no chance against us! Look at you, half dead, you look like a damn joke. MORE!"
msgstr ""

#. Id: 6769-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#. Id: 17774-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: BROKEN_ARM=1, Tag: -1
#. Id: 17775-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: DAMAGED_HEAD=1, Tag: -1
#: 6769-Dialogue.mod:1 17774-Dialogue.mod:1 17775-Dialogue.mod:1
msgid "/GODDAMN/ /MISER/! Lookit the state of you! You look half dead! This is your life on the line, gimme MORE!"
msgstr ""

#. Id: 6769-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 6769-Dialogue.mod:2
msgid "/GODDAMN/ /MISER/! Lookit that /GODDAMN/ gimpy leg o' yours! You got no chance against us in that state! Pay up, MORE!"
msgstr ""

#. Id: 6775-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 6775-Dialogue.mod:0
msgid "/FINE/! (500 Cats)"
msgstr ""

#. Id: 6775-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 6775-Dialogue.mod:1
msgid "No, wait! (500 Cats)"
msgstr ""

#. Id: 6775-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 6775-Dialogue.mod:2
msgid "Wait! (500 Cats)"
msgstr ""

#. Id: 6775-Dialogue.mod
#. -- Index: 5.3
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 6775-Dialogue.mod:3
msgid "Okay, okay! (500 Cats)"
msgstr ""

#. Id: 6779-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. Id: 16769-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (7137-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6779-Dialogue.mod:0 16769-Dialogue.mod:0
msgid "/IWE/ can't afford any more! Have pity"
msgstr ""

#. Id: 6779-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. Id: 16769-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (7137-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6779-Dialogue.mod:1 16769-Dialogue.mod:1
msgid "It's all /IWE/ can afford, have mercy"
msgstr ""

#. Id: 6779-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. Id: 16769-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (7137-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6779-Dialogue.mod:2 16769-Dialogue.mod:2
msgid "It's all /IWE/ can afford, honestly..."
msgstr ""

#. Id: 6784-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6784-Dialogue.mod:0
msgid "... Whatever. Don't need your blood attractin' the /DOG/s anyway.. Let's go!"
msgstr ""

#. Id: 6784-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6784-Dialogue.mod:1
msgid "... Fine. A bum like you ain't worth fightin' anyway.. Let's go!"
msgstr ""

#. Id: 6784-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6784-Dialogue.mod:2
msgid "... Fine. I ain't in the mood for fighting anyway.. Let's go!"
msgstr ""

#. Id: 6787-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6787-Dialogue.mod:0
msgid "/IWE/ gotta eat! 300 Cats ain't good enough! /IWE/'ll just have to take the clothes off your back!"
msgstr ""

#. Id: 6787-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6787-Dialogue.mod:1
msgid "300 Cats!? A /MEMAN/'s gotta eat! If you won't GIVE us anymore Cats, /IWE/'ll just have to STRIP 'em from you instead!"
msgstr ""

#. Id: 6787-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6787-Dialogue.mod:2
msgid "Then you leave me no choice, /MISER/! I'll just have to take the Cats from your rotting /CORPSE/!"
msgstr ""

#. Id: 6783-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#. Id: 7137-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 6783-Dialogue.mod:0 7137-Dialogue.mod:1
msgid "I don't believe you! Lookit all them backpacks, you can spare some more /CATS/. Your life is on the line here, /MISER/"
msgstr ""

#. Id: 6783-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#. Id: 7137-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 6783-Dialogue.mod:1 7137-Dialogue.mod:2
msgid "Don't LIE! I ain't blind, I see all them backpacks you're carryin'! You must have loads of /STUFF/ on you! Spare some more Cats or die, /MISER/!"
msgstr ""

#. Id: 6783-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#. Id: 7137-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 6783-Dialogue.mod:2 7137-Dialogue.mod:3
msgid "/GODDAMN/ /MISER/! Don't make me fight you, make this easy and give some more /CATS/. I know you gotta have more... Last chance."
msgstr ""

#. Id: 6790-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (6783-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6790-Dialogue.mod:0
msgid "No, I'm not paying you any more!"
msgstr ""

#. Id: 6790-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (6783-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 6790-Dialogue.mod:1
msgid "No, I won't give you any more!"
msgstr ""

#. Id: 15355-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (6783-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY<500, Tag: -1
#: 15355-Dialogue.mod:0
msgid "I can't..."
msgstr ""

#. Id: 15355-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (6783-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY<500, Tag: -1
#: 15355-Dialogue.mod:1
msgid "I really can't..."
msgstr ""

#. Id: 6792-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: ME, Condition: NUM_BACKPACKS=1, Tag: -1
#: 6792-Dialogue.mod:0
msgid "I don't believe you. I know a rich /BOY/ when I see one. Empty your pockets, let /MEUS/ see"
msgstr ""

#. Id: 6792-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (6779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#. -- Condition: Who: ME, Condition: NUM_BACKPACKS=1, Tag: -1
#: 6792-Dialogue.mod:1
msgid "Liar! You look like a rich /BOY/ to me... empty your /STUFF/, let me see!"
msgstr ""

#. Id: 7145-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (6792-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7145-Dialogue.mod:0
msgid "Okay okay, just take it (500 Cats)"
msgstr ""

#. Id: 7146-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (6792-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7146-Dialogue.mod:0
msgid "You ain't touching me with THOSE dirty hands!"
msgstr ""

#. Id: 7146-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (6792-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7146-Dialogue.mod:1
msgid "You ain't touching me with THOSE dirty fingers!"
msgstr ""

#. Id: 7146-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (6792-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 7146-Dialogue.mod:2
msgid "Stay the hell away from me! /GODDAMN/ /STARVERS/!"
msgstr ""

#. Id: 7147-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (7146-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 7147-Dialogue.mod:0
msgid "Fine. You've made your choice. We'll just have to strip it from your skin"
msgstr ""

#. Id: 16768-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (6792-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#: 16768-Dialogue.mod:0
msgid "No, I can't give you any more..."
msgstr ""

#. Id: 14472-Dialogue (4).mod
#. -- Index: 5.0
#. -- Parent line: 4 (6769-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Interrupt combat
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>400, Tag: -1
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY<700, Tag: -1
#: 14472-Dialogue?(4).mod:0
msgid "Ok... 400 Cats! Please."
msgstr ""

#. Id: 14476-Dialogue (4).mod
#. -- Index: 6.0
#. -- Parent line: 5 (14472-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 14476-Dialogue?(4).mod:0
msgid "... Fine. Let's go!"
msgstr ""

#. Id: 6772-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (6765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupt combat
#: 6772-Dialogue.mod:0
