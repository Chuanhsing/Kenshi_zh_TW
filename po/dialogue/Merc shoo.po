#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12633-Dialogue?(11)?(2).mod:0
msgid "/WHATTHEFUCK/ is this, a party?"
msgstr ""

#. Id: 12633-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>4, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12633-Dialogue?(11)?(2).mod:1
msgid "What are you guys doin' here? /GETLOST/."
msgstr ""

#. Id: 12635-Dialogue (11) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12635-Dialogue?(11)?(2).mod:1
msgid "/GETLOST/."
msgstr ""

#. Id: 12635-Dialogue (11) (2).mod
#. -- Index: 1.4
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12635-Dialogue?(11)?(2).mod:4
msgid "Get out of here."
msgstr "離開這裡。"

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.3
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:3
msgid "Hey, you! You hearin' me okay? /GETLOST/!"
msgstr ""

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.4
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:4
msgid "I told you to leave, so start movin'!"
msgstr "我叫你離開，所以開始行動'！"

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.5
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:5
msgid "Okay, last warning. Get out or I kick you out."
msgstr "好的，最後的警告。滾出去或我把你踢出去"

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.6
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:6
msgid "I'm not gonna warn you again..."
msgstr "我不會再警告你了......"

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.7
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:7
msgid "Don't make me have to kick you out..."
msgstr "不要讓我把你踢出去......"

#. Id: 12636-Dialogue (11) (2).mod
#. -- Index: 2.8
#. -- Parent line: 1 (12635-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12636-Dialogue?(11)?(2).mod:8
msgid "Am I gonna have to kick you out?"
msgstr "我要把你踢出去嗎？"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:0
msgid "That's it. If you won't leave, I'll MAKE you leave!"
msgstr "而已。如果你不離開，我會讓你離開！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:1
msgid "Looks like I'll have to remove you myself!"
msgstr "看起來我必須自己刪除你！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:2
msgid "What's wrong with you?! /GETLOST/!"
msgstr ""

#. Id: 42358-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:3
msgid "You leave me no choice, intruder!"
msgstr "你讓我別無選擇，入侵者！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:4
msgid "Got sand in your ears? Scram already!"
msgstr "你的耳朵裡有沙子嗎？ Scram已經！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:5
msgid "Why the hell are you still here?!"
msgstr "為什麼你還在這裡？！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:6
msgid "Why won't you leave!?"
msgstr "你為什麼不離開！？"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.7
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:7
msgid "Just leave already!"
msgstr "剛離開！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.8
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 43841-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (43844-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc found intruder
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:8 43841-Dialogue.mod:2
msgid "... Then you leave me no choice, intruder!"
msgstr "......然後你別無選擇，入侵者！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.9
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 43841-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (43844-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc found intruder
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:9 43841-Dialogue.mod:4
msgid "Then I guess we'll have to do this the hard way!"
msgstr "然後我想我們必須以艱難的方式做到這一點！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.10
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 43841-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (43844-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc found intruder
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:10 43841-Dialogue.mod:6
msgid "Fine, don't say I didn't warn you!"
msgstr "好吧，不要說我沒有警告你！"

#. Id: 42358-Dialogue.mod
#. -- Index: 3.11
#. -- Parent line: 2 (12636-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42358-Dialogue.mod:11
msgid "Fine, you asked for it! "
msgstr "好的，你問了！"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:0
msgid "That's it, door's that way..."
msgstr "就是這樣，門就是那樣......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:1
msgid "Door's that way..."
msgstr "門那樣......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:2
msgid "Exit's that way..."
msgstr "退出那樣......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:3
msgid "Go on, get out..."
msgstr "繼續，滾出去......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:4
msgid "That's it, leave..."
msgstr "就是這樣，離開......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:5
msgid "You can leave the way you came in..."
msgstr "你可以離開你進來的方式......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:6
msgid "Door's that way. Make it quick."
msgstr "門那樣。快一點。"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:7
msgid "At least learn to knock first next time..."
msgstr "至少下次學會先敲門......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:8
msgid "The nerve on that /GUY/..."
msgstr ""

#. Id: 45386-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:9
msgid "Can't just go waltzin' into people's houses..."
msgstr "不能只是去華爾茲'進入人們的房子......"

#. Id: 45386-Dialogue.mod
#. -- Index: 1.10
#. -- Parent line: 0 (12633-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45386-Dialogue.mod:10
msgid "Comin' in my house... This /GUY/'s real shifty..."
msgstr ""

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:0
msgid "Hey, you shouldn't be in here..."
msgstr "嘿，你不應該在這裡......"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#. Id: 45735-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton shoo
#. -- Condition: My Race(s): Skeleton
#: 12637-Dialogue?(11)?(2).mod:1 45735-Dialogue.mod:1
msgid "You shouldn't be here..."
msgstr "你不應該在這裡......"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:2
msgid "Who are you? You shouldn't be here..."
msgstr "你是誰？你不應該在這裡......"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:3
msgid "Hm? You shouldn't be here..."
msgstr "嗯？你不應該在這裡......"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:4
msgid "This is a private building..."
msgstr "這是一幢私人建築......"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:5
msgid "What are you doing in here? You need to leave."
msgstr "你在這做什麼？你需要離開。"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:6
msgid "Hey, you can't just come strolling on in here!"
msgstr "嘿，你不能只是在這裡漫步！"

#. Id: 12637-Dialogue (11) (2).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 12637-Dialogue?(11)?(2).mod:7
msgid "Hey, you can't come in here!"
msgstr "嘿，你不能進來！"

#. Id: 45376-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45376-Dialogue.mod:1
msgid "Do I know you or something?"
msgstr "我認識你還是什麼？"

#. Id: 45376-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45376-Dialogue.mod:2
msgid "And you are...?"
msgstr "你是......？"

#. Id: 45376-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45376-Dialogue.mod:3
msgid "Er, who are you?"
msgstr "呃，你是誰？"

#. Id: 45380-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45376-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45380-Dialogue.mod:0
msgid "This is a private residence, you need to leave."
msgstr "這是一個私人住宅，你需要離開。"

#. Id: 45380-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45376-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 45380-Dialogue.mod:2
msgid "You can't be here, you need to leave."
msgstr "你不能在這裡，你需要離開。"

#. Id: 45378-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#. Id: 12351-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 45378-Dialogue.mod:0 12351-Dialogue?(10).mod:1
msgid "/WHOTHEFUCK/ are you?"
msgstr ""

#. Id: 45378-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45378-Dialogue.mod:1
msgid "Eh? Who are you?"
msgstr "嗯？你是誰？"

#. Id: 45378-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45378-Dialogue.mod:2
msgid "Suuure, come in, make yourself at home, why don't you!"
msgstr "Suuure，進來，讓自己在家，你為什麼不！"

#. Id: 45378-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Merc shoo
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: My Sub-Race(s): Greenlander or Shek
#: 45378-Dialogue.mod:3
msgid "Yeah? Who are you?"
msgstr "是嗎？你是誰？"
