#. -- Condition: Who: ME, Condition: IS_INDOORS=1, Tag: -1
#. -- Condition: Target Race(s): Greenlander
#: 65175-Dialogue.mod:0
msgid "Oh hiiii, flesh /BROTHER/!"
msgstr ""

#. Id: 65175-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: ME, Condition: IN_A_NON_PLAYER_TOWN=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_INDOORS=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_INDOORS=1, Tag: -1
#. -- Condition: Target Race(s): Greenlander
#: 65175-Dialogue.mod:1
msgid "/HI/, flesh /BROTHER/!"
msgstr ""

#. Id: 65177-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (65175-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65177-Dialogue.mod:0
msgid "Welcome to your fellow human village!"
msgstr "歡迎來到你的人類村莊！"

#. Id: 65177-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (65175-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65177-Dialogue.mod:1
msgid "How nice of you to visit our human village!"
msgstr "你真高興參觀我們的人類村莊！"

#. Id: 65177-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (65175-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65177-Dialogue.mod:2
msgid "It's so good to have visitors"
msgstr "有訪客真是太好了"

#. Id: 65178-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (65177-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65178-Dialogue.mod:0
msgid "Come in, make yourself comfortable!"
msgstr "進來，讓自己舒服！"

#. Id: 65178-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (65177-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65178-Dialogue.mod:1
msgid "Come in, rest your tired sinews!"
msgstr "進來，休息你疲憊的肌肉！"

#. Id: 65178-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (65177-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65178-Dialogue.mod:2
msgid "You must be tired, come rest with us!"
msgstr "你一定很累，跟我們一起休息吧！"

#. Id: 65178-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (65177-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65178-Dialogue.mod:3
msgid "Come, eat some human food, and rest up!"
msgstr "來吧，吃些人的食物，休息吧！"

#. Id: 65181-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (65179-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65181-Dialogue.mod:0
msgid "That's some fresh looking skin you have, /BROTHER/."
msgstr ""

#. Id: 65181-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (65179-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65181-Dialogue.mod:1
msgid "Has anybody ever told you what fresh skin you have, flesh /BROTHER/?"
msgstr ""

#. Id: 65181-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (65179-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65181-Dialogue.mod:2
msgid "I like your skin..."
msgstr "我喜歡你的皮膚......"

#. Id: 65182-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (65181-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65182-Dialogue.mod:0
msgid "... Can I touch it?"
msgstr "... 我可以碰它嗎？"

#. Id: 65182-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (65181-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65182-Dialogue.mod:1
msgid "... Can I have it?"
msgstr "我能擁有它嗎？"

#. Id: 65184-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (65183-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65184-Dialogue.mod:0
msgid "I'll look after your skin, I promise..."
msgstr "我會照顧你的皮膚，我保證......"

#. Id: 65184-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (65183-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65184-Dialogue.mod:1
msgid "Your skin will look better on me..."
msgstr "你的皮膚看起來會更好......"

#. Id: 65184-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (65183-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65184-Dialogue.mod:2
msgid "I think it's best that I have it now..."
msgstr "我覺得我現在最好擁有它......"

#. Id: 65180-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (65178-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_INDOORS=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 65180-Dialogue.mod:0
msgid "No, not that way!"
msgstr "不，不是這樣！"

#. Id: 65186-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (65180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65186-Dialogue.mod:0
msgid "You tease, flesh /BROTHER/, showing off your skin like this..."
msgstr ""

#. Id: 65186-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (65180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65186-Dialogue.mod:1
msgid "You tease me with your lovely skin, flesh /BROTHER/..."
msgstr ""

#. Id: 65186-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (65180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65186-Dialogue.mod:2
msgid "You surely come here just to show off that fresh skin of yours..."
msgstr "你肯定來這裡只是為了炫耀你的新鮮皮膚......"

#. Id: 65187-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (65186-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65187-Dialogue.mod:0
msgid "... I must have it."
msgstr "......我必須擁有它。"

#. Id: 65187-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (65186-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65187-Dialogue.mod:1
msgid "... I want it."
msgstr "... 我要它。"

#. Id: 65188-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (65187-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees player
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#: 65188-Dialogue.mod:0
msgid "I must have your good skin!"
msgstr "我必須擁有你的好皮膚！"
