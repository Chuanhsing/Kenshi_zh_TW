#. Id: 15695-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:0
msgid "You! Where did you get that uniform?!"
msgstr "您！你從哪裡拿到那件製服？！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:1
msgid "You! You're not one of us!"
msgstr "您！你不是我們中的一員！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:2
msgid "You!... Who are you?!"
msgstr "你！...你是誰？！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:3
msgid "You! Why do you wear that armour!?"
msgstr "您！你為什麼穿那件盔甲！？"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:4
msgid "You there! You wear our armour... Why?!"
msgstr "你在那裡！你穿我的盔甲......為什麼？！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:5
msgid "You there! That is our armour that your wear!"
msgstr "你在那裡！那是我們穿的盔甲！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:6
msgid "Hold it! Who are you?! You're not one of us!"
msgstr "拿著它！你是誰？！你不是我們中的一員！"

#. Id: 15695-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15695-Dialogue.mod:7
msgid "Where did you get that uniform?!"
msgstr "你從哪裡拿到那件製服？！"

#. Id: 15696-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15696-Dialogue.mod:1
msgid "Fraudster! /SEIZE/ /HIM/!"
msgstr ""

#. Id: 15696-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15696-Dialogue.mod:3
msgid "Damn looters! /SEIZE/ /HIM/!"
msgstr ""

#. Id: 15697-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15697-Dialogue.mod:0
msgid "A darkened tries to infiltrate us! /SEIZE/ /HIM/! Destroy /HIM/!"
msgstr ""

#. Id: 15697-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15697-Dialogue.mod:1
msgid "A /TERRORIST/ tries to penetrate our holy walls! Purge /HIM/!"
msgstr ""

#. Id: 15697-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15697-Dialogue.mod:2
msgid "An unholy /TERRORIST/, sent from the darkness! Burn /HIM/, destroy /HIM/!"
msgstr ""

#. Id: 15698-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15698-Dialogue.mod:0
msgid "A /TERRORIST/ from the outside! /SEIZE/ /HIM/!"
msgstr ""

#. Id: 15698-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#: 15698-Dialogue.mod:1
msgid "A barbarian /TERRORIST/! /SEIZE/ /HIM/!"
msgstr ""

#. Id: 16035-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15695-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Military imposter
#. Id: 16075-Dialogue.mod
