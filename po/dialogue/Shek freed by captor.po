#. Id: 14909-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:0
msgid "... I'll be back for your head"
msgstr "......我會回來的"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:1
msgid "... A Shek never forgets their enemy"
msgstr "......一個石頭永遠不會忘記他們的敵人"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:2
msgid "... You'll live to regret caging up a Shek"
msgstr "......你會後悔養一個石頭"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:3
msgid "... I'll be back to bleed you dry"
msgstr "......我會回來讓你乾涸"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:4
msgid "... I'll make you bleed for this"
msgstr "......我會讓你流血的"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:5
msgid "... A Shek never forgives"
msgstr "......一個石頭永遠不會原諒"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:6
msgid "... Vengeance"
msgstr "......復仇"

#. Id: 14909-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14909-Dialogue.mod:7
msgid "... Wait for me. I'll be back for my revenge."
msgstr "... 等等我。我會回來報復。"

#. Id: 14925-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#: 14925-Dialogue.mod:0
msgid "You're a fool to set me free"
msgstr "你讓我自由是個傻瓜"

#. Id: 14925-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#: 14925-Dialogue.mod:1
msgid "To cage up a Shek is foolishness"
msgstr "把一個石頭籠罩起來是愚蠢的"

#. Id: 14925-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#: 14925-Dialogue.mod:2
msgid "To lock me in that /DIRTY/ hutch"
msgstr ""

#. Id: 14908-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (14925-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek freed by captor
#: 14908-Dialogue.mod:0
msgid "Taste the wrath of Kral"
msgstr "品味克拉爾的憤怒"
