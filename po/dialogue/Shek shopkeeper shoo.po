#. Id: 51563-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51563-Dialogue.mod:0
msgid "Need I tell you again, /FLATSKIN/? Leave."
msgstr ""

#. Id: 51563-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (51562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51563-Dialogue.mod:1
msgid "Do not waste my breath, /FLATSKIN/! Take exit."
msgstr ""

#. Id: 51563-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (51562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51563-Dialogue.mod:2
msgid "Must I waste my breath again, /FLATSKIN/? Remove yourself."
msgstr ""

#. Id: 51563-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (51562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51563-Dialogue.mod:3
msgid "I do not like being ignored, /FLATSKIN/. Move it."
msgstr ""

#. Id: 51563-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (51562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51563-Dialogue.mod:4
msgid "Annoyance. I will not be ignored, /FLATSKIN/. Go."
msgstr ""

#. Id: 51561-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (51560-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51561-Dialogue.mod:0
msgid "Ignorance! The whelp is surely looking for a fight!"
msgstr ""

#. Id: 51561-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (51560-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51561-Dialogue.mod:1
msgid "/IDIOCY/! The /FLATSKIN/ dog is asking for a thrashing!"
msgstr ""

#. Id: 51561-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (51560-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51561-Dialogue.mod:2
msgid "Anger! I'll rip out /HIS/ lungs for daring to ignore the mighty Shek!"
msgstr ""

#. Id: 51567-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (51561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51567-Dialogue.mod:0
msgid "Patience, kin. Remember the Stone Golem's orders. Go easy on the foreigners."
msgstr ""

#. Id: 51567-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (51561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51567-Dialogue.mod:1
msgid "Restraint, kin. The Stone Golem's word is final, have mercy on the /FLATSKIN/."
msgstr ""

#. Id: 51567-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (51561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51567-Dialogue.mod:2
msgid "Toleration. Remember the Stone Golem's words, kin, we cannot harm the outsiders."
msgstr ""

#. Id: 51568-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51567-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Target Race(s): Greenlander or Hive Worker Drone or Scorchlander
#: 51568-Dialogue.mod:0
msgid "Their feeble bodies cannot withstand the power of a Shek Warrior. And we need their supplies."
msgstr ""

#. Id: 51568-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (51567-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Target Race(s): Greenlander or Hive Worker Drone or Scorchlander
#: 51568-Dialogue.mod:1
msgid "Their fragile bones cannot take it. And if we want their supplies, we need them alive."
msgstr ""

#. Id: 51570-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (51568-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51570-Dialogue.mod:0
msgid "Hmph. The Stone Golem will have us be a laughing stock."
msgstr ""

#. Id: 51570-Dialogue.mod
#. -- Index: 8.1
#. -- Parent line: 7 (51568-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51570-Dialogue.mod:1
msgid "Hmph. The Stone Golem is soft, she'll make our nation a mockery of the lands."
msgstr ""

#. Id: 51571-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (51570-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#: 51571-Dialogue.mod:0
msgid "The ignorant /FLATSKIN/ needs /HIS/ neck wrung in a lesson of respect."
msgstr ""

#. Id: 51572-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (51571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51572-Dialogue.mod:0
msgid "Patience."
msgstr ""

#. Id: 51569-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51567-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51569-Dialogue.mod:0
msgid "Slaughtering the foreign traders will brings us no new supplies."
msgstr ""

#. Id: 51569-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (51567-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51569-Dialogue.mod:1
msgid "Crushing our foreign customers will surely be bad for business."
msgstr ""

#. Id: 51564-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (51561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Worker Drone
#: 51564-Dialogue.mod:0
msgid "The frail /FLATSKIN/ is obviously frozen in fear of the Shek's might. Take pity, kin."
msgstr ""

#. Id: 51564-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (51561-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Worker Drone
#: 51564-Dialogue.mod:1
msgid "Calm. Crushing our foreign customers will surely be bad for business."
msgstr ""

#. Id: 51574-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51564-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#: 51574-Dialogue.mod:0
msgid "Hmph. Foreign /CRAVEN/s."
msgstr ""

#. Id: 51565-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (51563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51565-Dialogue.mod:0
msgid "Do not be fooled by my horns, /FLATSKIN/."
msgstr ""

#. Id: 51565-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (51563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51565-Dialogue.mod:1
msgid "Hmph. Think you can ignore me because of my horns?"
msgstr ""

#. Id: 51566-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (51565-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51566-Dialogue.mod:0
msgid "I'll peel your flesh from your bones. Keep testing me."
msgstr ""

#. Id: 51566-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (51565-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51566-Dialogue.mod:1
msgid "Horns or no horns, I'll skin you alive, /FLATSKIN/. Keep trying my patience."
msgstr ""

#. Id: 51566-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (51565-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51566-Dialogue.mod:2
msgid "Horns or no horns, I'll rip your spine from neck. Try me."
msgstr ""

#. Id: 51577-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51576-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 51577-Dialogue.mod:0
msgid "Mourn your lost horns all you want, kin. It still will not make you a Shek Warrior again."
msgstr ""

#. Id: 51578-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (51577-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51578-Dialogue.mod:0
msgid "Insult. I need no horns to take on a foreign weakling."
msgstr ""

#. Id: 51578-Dialogue.mod
#. -- Index: 8.1
#. -- Parent line: 7 (51577-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51578-Dialogue.mod:1
msgid "Mockery. I need no horns to crush a foreign whelp."
msgstr ""

#. Id: 51578-Dialogue.mod
#. -- Index: 8.2
#. -- Parent line: 7 (51577-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shopkeeper shoo
#: 51578-Dialogue.mod:2
msgid "Slander. I need no horns to destroy a feeble outsider."
msgstr ""

#. Id: 51579-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (51578-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Shek shopkeeper shoo
#: 51579-Dialogue.mod:0
msgid "Do not fight your fate, kin. You are a servant, so serve. And do not waste energy proving your worth to a /FLATSKIN/."
msgstr ""

