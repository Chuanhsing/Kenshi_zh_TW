#. Id: 48746-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:0
msgid "Break /HIS/ legs!"
msgstr ""

#. Id: 48746-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:1
msgid "I'll cripple you for crossing the Blackshifter clan!"
msgstr "我會因為穿越Blackshifter戰隊而癱瘓你！"

#. Id: 48746-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:2
msgid "I'll have you crippled! Nobody crosses us and walks away!"
msgstr "我會讓你殘廢！沒有人穿過我們走開！"

#. Id: 48746-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:3
msgid "I'll make you pay... I'll make you pay with blood!"
msgstr "我會讓你付錢......我會讓你用血付錢！"

#. Id: 48746-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:4
msgid "You ain't walkin' away from this!"
msgstr "你不是在走這個！"

#. Id: 48746-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48746-Dialogue.mod:5
msgid "Nothin' but a penniless /IDIOT/... /MYFACTION/ will break you!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:0
msgid "Nobody crosses /MYFACTION/ and lives!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:1
msgid "You cross /MYFACTION/, you bleed!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:2
msgid "You'll bleed for crossing /MYFACTION/!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:3
msgid "Wrong clan to mess with, /INSULT/!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:4
msgid "No one messes with /MYFACTION/! You'll pay with blood!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:5
msgid "Oh Big Al ain't gonna like this!"
msgstr "哦，大阿爾不會喜歡這個！"

#. Id: 48747-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:6
msgid "Big Al ain't gonna like this one bit! You're playin' with the big boys now, /INSULT/!"
msgstr ""

#. Id: 48747-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:7
msgid "Big Al ain't gonna like you messin' with the Stone Rats!"
msgstr "Big Al不會像石鼠那樣混亂！"

#. Id: 48747-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=0, Tag: -1
#: 48747-Dialogue.mod:8
msgid "Stone Rats go!"
msgstr "石鼠去吧！"

#. Id: 48748-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48748-Dialogue.mod:0
msgid "You're messin' with the big bosses now, /BOY/..."
msgstr ""

#. Id: 48748-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 48748-Dialogue.mod:1
msgid "Pickin' a fight with /MYFACTION/ is a bad move, /BOY/..."
msgstr ""

#. Id: 48750-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (48748-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48750-Dialogue.mod:0
msgid "No one messes with the big bosses!"
msgstr "沒有人和大老闆混在一起！"

#. Id: 48750-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (48748-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48750-Dialogue.mod:1
msgid "You're in a mess now, /BOY/!"
msgstr ""

#. Id: 48752-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:1
msgid "Ahah.. haha..."
msgstr "啊哈哈哈哈......"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:2
msgid "Careful, I bite..."
msgstr "小心，我咬......"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:3
msgid "Lucky hit! I'll skin you alive for knocking me out like that!"
msgstr "幸運的打！我會讓你活著因為那樣把我打倒！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:4
msgid "I'll have your skin!"
msgstr "我會有你的皮膚！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:5
msgid "I'll gut you!"
msgstr "我會嚇到你的！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:6
msgid "I'll gut you good!"
msgstr "我會讓你好！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:7
msgid "I'm baaack!"
msgstr "我是baaack！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:8
msgid "Back from the dead!"
msgstr "死而復生！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:9
msgid "You made me bleed. And now I'm MAD!"
msgstr "你讓我流血了。現在我很瘋狂！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:10
msgid "I’ll enjoy guttin' you /BOY2/!"
msgstr ""

#. Id: 48752-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:11
msgid "After this you're gonna be a lot less you."
msgstr "在此之後，你將會少得多。"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:12
msgid "Oh the things I'm gonna do to you once this is over..."
msgstr "哦，一旦結束，我會對你做的事情......"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:13
msgid "Hunting time folks!"
msgstr "狩獵時間伙計們！"

#. Id: 48752-Dialogue.mod
#. -- Index: 0.14
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp gang member get up fight
#: 48752-Dialogue.mod:14
msgid "You ain’t leaving with your legs"
msgstr "你不是用腿離開的"
