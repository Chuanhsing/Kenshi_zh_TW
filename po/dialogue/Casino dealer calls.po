msgstr ""

#. Id: 48353-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:1
msgid "New game startin' up, Crawlin' for Cats. Take a seat and make some credits!"
msgstr "新遊戲開始，Crawlin為Cats。坐下來賺取一些積分！"

#. Id: 48353-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:2
msgid "Put your money where your mouth is and come play Crawlin' for Cats!"
msgstr "把你的錢放在嘴邊，然後來玩Crawlin'for Cats！"

#. Id: 48353-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:3
msgid "Hoy, /OUTLANDER/! Wanna make some money? Come play!"
msgstr ""

#. Id: 48353-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:4
msgid "You, /OUTLANDER/. Sit your ass down and come play some Cat Crawlers!"
msgstr ""

#. Id: 48353-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:5
msgid "Come try your luck at a game of Cat Crawlers, /OUTLANDER/! Sit on down!"
msgstr ""

#. Id: 48353-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:6
msgid "Here to gamble, /OUTLANDER/? Take a seat!"
msgstr ""

#. Id: 48353-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 48353-Dialogue.mod:7
msgid "Everyone's a winner with Cat Crawlers! Come play a game and make some Cats, /OUTLANDER/!"
msgstr ""

#. Id: 48354-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Casino dealer calls
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=1, Tag: -1
