#. Id: 19816-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19816-Dialogue.mod:0
msgid "I.. AM.. ME!"
msgstr ""

#. Id: 19817-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (19816-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19817-Dialogue.mod:0
msgid "They won't take that away from me!.. Not again!"
msgstr ""

#. Id: 19817-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (19816-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19817-Dialogue.mod:1
msgid "Those bastards will never take that away from me! Not again!"
msgstr ""

#. Id: 19818-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19818-Dialogue.mod:0
msgid "They won't break me... they'll never break me!"
msgstr ""

#. Id: 19819-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (19818-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19819-Dialogue.mod:0
msgid "Not again... never again..."
msgstr ""

#. Id: 19820-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19820-Dialogue.mod:0
msgid "I won't crack... those bastards will never crack me..."
msgstr ""

#. Id: 19821-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (19820-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19821-Dialogue.mod:0
msgid "I'm free now... my mind is free now..."
msgstr ""

#. Id: 19822-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19822-Dialogue.mod:0
msgid "I escaped 'that' place. I'll escape this one too..."
msgstr ""

#. Id: 19822-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19822-Dialogue.mod:1
msgid "I broke out of the Hive, I can break out of here too..."
msgstr ""

#. Id: 19822-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19822-Dialogue.mod:2
msgid "They'll never imprison my mind, never again..."
msgstr ""

#. Id: 19822-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (17976-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19822-Dialogue.mod:3
msgid "I broke free from one nightmare, I'll break free from this one too!"
msgstr ""

#. Id: 19823-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (19822-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Picklock attempt hiver ronin slave
#: 19823-Dialogue.mod:0
msgid "I'm a survivor..."
msgstr ""

