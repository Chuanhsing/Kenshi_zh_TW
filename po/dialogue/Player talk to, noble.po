#. Id: 16497-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 16497-Dialogue.mod:4
msgid "You! The /PEASANT/ over there!"
msgstr ""

#. Id: 43616-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 43616-Dialogue.mod:0
msgid "Bow to me!"
msgstr ""

#. Id: 43616-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 43616-Dialogue.mod:1
msgid "Show some respect, bow before me!"
msgstr ""

#. Id: 43616-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 43616-Dialogue.mod:2
msgid "Why aren't you kneeling? "
msgstr ""

#. Id: 43616-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (16497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 43616-Dialogue.mod:3
msgid "Kneel before your noble superiors!"
msgstr ""

#. Id: 51714-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (43616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 51714-Dialogue.mod:0
msgid "Hm?! Ignoring your noble lordship?!"
msgstr ""

#. Id: 51714-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (43616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 51714-Dialogue.mod:1
msgid "Are... are you ignoring me? Do you even know who I am?!"
msgstr ""

#. Id: 51714-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (43616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Player talk to, noble
#: 51714-Dialogue.mod:2
msgid "Why aren't you bowing?!"
msgstr ""

#. Id: 48160-Dialogue.mod
#. -- Index: 3.0
