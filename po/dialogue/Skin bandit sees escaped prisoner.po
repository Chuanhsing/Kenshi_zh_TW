#. Id: 1533278-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533278-Dialogue.mod:0
msgid "Get back in your cage!"
msgstr ""

#. Id: 1533278-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533278-Dialogue.mod:1
msgid "Come back with my skin!"
msgstr ""

#. Id: 1533279-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533279-Dialogue.mod:0
msgid "No! My skin!"
msgstr ""

#. Id: 1533279-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533279-Dialogue.mod:1
msgid "My new skin!"
msgstr ""

#. Id: 1533280-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1533279-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533280-Dialogue.mod:0
msgid "It's getting away!"
msgstr ""

#. Id: 1533283-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533283-Dialogue.mod:0
msgid "I'm not finished with you yet, /RACENAME/..."
msgstr ""

#. Id: 1533284-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533284-Dialogue.mod:0
msgid "Get back in your cage and behave."
msgstr ""

#. Id: 1533284-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533284-Dialogue.mod:1
msgid "Not until I have your skin."
msgstr ""

#. Id: 1533284-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533284-Dialogue.mod:2
msgid "You still need to go in the peeler."
msgstr ""

#. Id: 1533285-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533285-Dialogue.mod:0
msgid "You're not taking that pretty skin away from me..."
msgstr ""

#. Id: 1533286-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1533285-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533286-Dialogue.mod:0
msgid "That fresh, pretty skin..."
msgstr ""

#. Id: 1533288-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (1533286-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533288-Dialogue.mod:0
msgid "Soon it will be all mine."
msgstr ""

#. Id: 1533288-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (1533286-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533288-Dialogue.mod:1
msgid "We have to get it off you."
msgstr ""

#. Id: 1533288-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (1533286-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533288-Dialogue.mod:2
msgid "I want to make a beautiful suit out of it."
msgstr ""

#. Id: 1533288-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (1533286-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533288-Dialogue.mod:3
msgid "I want to wear it."
msgstr ""

#. Id: 1533289-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533289-Dialogue.mod:0
msgid "No... that skin has to come off first..."
msgstr ""

#. Id: 1533289-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533289-Dialogue.mod:1
msgid "No... we have to get that skin off you first..."
msgstr ""

#. Id: 1533289-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (1533283-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533289-Dialogue.mod:2
msgid "No... You have to go in the peeler first..."
msgstr ""

#. Id: 1533290-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (1533289-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skin bandit sees escaped prisoner
#: 1533290-Dialogue.mod:0
msgid "THEN you can go."
msgstr ""

