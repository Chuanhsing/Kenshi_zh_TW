#. Id: 62684-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62684-Dialogue.mod:0
msgid "We have wares for you, no-hives!"
msgstr "我們有適合你的商品，沒有蕁麻疹！"

#. Id: 62684-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62684-Dialogue.mod:1
msgid "We have special goods for you, no-hives!"
msgstr "我們為您準備了特殊商品，沒有蕁麻疹！"

#. Id: 62684-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62684-Dialogue.mod:2
msgid "We brought you wares, no-hives!"
msgstr "我們為您帶來了商品，沒有蕁麻疹！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:0
msgid "Come see our wares!"
msgstr "快來看看我們的商品！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:1
msgid "Come see!"
msgstr "來看看！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:2
msgid "Come look!"
msgstr "快來看！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:3
msgid "Don't be shy, come look!"
msgstr "別害羞，快來看！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:4
msgid "Look, look!"
msgstr "看看！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:5
msgid "We invite you to come browse!"
msgstr "我們邀請您來瀏覽！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:6
msgid "Come make trade with our wares!"
msgstr "來與我們的商品交易！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:7
msgid "Just for you!"
msgstr "只為你！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:8
msgid "For your eyes only!"
msgstr "只為你的眼睛！"

#. Id: 62685-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (62684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62685-Dialogue.mod:9
msgid "Special prices for you!"
msgstr "特價為您服務！"

#. Id: 62686-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62685-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62686-Dialogue.mod:0
msgid "Best in the world!"
msgstr "世界上最好的！"

#. Id: 62686-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (62685-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62686-Dialogue.mod:1
msgid "Best in the land!"
msgstr "最好的土地！"

#. Id: 62686-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (62685-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62686-Dialogue.mod:2
msgid "Best in all the outlands!"
msgstr "在所有的外域最好！"

#. Id: 62686-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (62685-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62686-Dialogue.mod:3
msgid "Finest in the land!"
msgstr "在這片土地上最好的！"

#. Id: 62687-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62686-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62687-Dialogue.mod:0
msgid "We promise you, you won't find better quality anywhere else..."
msgstr "我們向您保證，您在其他任何地方都找不到更好的品質......"

#. Id: 62687-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (62686-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62687-Dialogue.mod:1
msgid "We can't be beated on quality, you won't find better..."
msgstr "我們不能在質量上打敗，你會發現更好......"

#. Id: 62688-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62687-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62688-Dialogue.mod:0
msgid "Definitely not from the traders guild."
msgstr "絕對不是來自交易員公會。"

#. Id: 62689-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62688-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62689-Dialogue.mod:0
msgid "Junk!"
msgstr "垃圾！"

#. Id: 62689-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (62688-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62689-Dialogue.mod:1
msgid "Trash!"
msgstr "垃圾！"

#. Id: 62689-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (62688-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62689-Dialogue.mod:2
msgid "Rubbish!"
msgstr "垃圾！"

#. Id: 62690-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62689-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62690-Dialogue.mod:0
msgid "You deserve better, no-hives!"
msgstr "你值得更好，沒有蕁麻疹！"

#. Id: 62691-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62690-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62691-Dialogue.mod:0
msgid "And only the hive can provide your needs..."
msgstr "只有蜂巢可以滿足您的需求......"

#. Id: 62692-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62691-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62692-Dialogue.mod:0
msgid "Queen's seal of approval!"
msgstr "女王的批准印章！"

#. Id: 62693-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62692-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62693-Dialogue.mod:0
msgid "No better approval than the queen's..."
msgstr "沒有比女王更好的認可......"

#. Id: 62694-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (62693-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62694-Dialogue.mod:0
msgid "We love the queen!"
msgstr "我們愛女王！"

#. Id: 62696-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (62695-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hive merchant visits player base
#: 62696-Dialogue.mod:0
msgid "Love the queen!"
msgstr "愛女王！"

#. Id: 62697-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (62696-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62697-Dialogue.mod:0
msgid "Don't you love the queen, no-hives?"
msgstr "難道你不喜歡女王，沒有蕁麻疹？"

#. Id: 62698-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (62697-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62698-Dialogue.mod:0
msgid "Why won't you buy her wares?"
msgstr "你為什麼不買她的商品？"

#. Id: 62699-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (62698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62699-Dialogue.mod:0
msgid "Why are you ignoring us, no-hives?"
msgstr "你為什麼無視我們，沒有蕁麻疹？"

#. Id: 62700-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (62699-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62700-Dialogue.mod:0
msgid "How sad..."
msgstr "多麼悲傷..."

#. Id: 62701-Dialogue.mod
#. -- Index: 17.0
#. -- Parent line: 16 (62700-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62701-Dialogue.mod:0
msgid "We hope you don't starve to death out here in your stone houses..."
msgstr "我們希望你不要在你的石頭房子裡餓死！"

#. Id: 62702-Dialogue.mod
#. -- Index: 18.0
#. -- Parent line: 17 (62701-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62702-Dialogue.mod:0
msgid "You'll regret not buying our chewsticks..."
msgstr "你會後悔沒有買我們的嚼子......"

#. Id: 62703-Dialogue.mod
#. -- Index: 19.0
#. -- Parent line: 18 (62702-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62703-Dialogue.mod:0
msgid "Best in the world..."
msgstr "世界上最好的..."

#. Id: 62720-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62690-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62720-Dialogue.mod:0
msgid "Look at our snacks!"
msgstr "看看我們的小吃！"

#. Id: 62720-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (62690-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62720-Dialogue.mod:1
msgid "Look at our tasty rations!"
msgstr "看看我們的美味口糧吧！"

#. Id: 62720-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (62690-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62720-Dialogue.mod:2
msgid "Look at our fresh breads!"
msgstr "看看我們的新鮮麵包！"

#. Id: 62721-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62720-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62721-Dialogue.mod:0
msgid "Or how about some bandages?"
msgstr "或者一些繃帶怎麼樣？"

#. Id: 62721-Dialogue.mod
#. -- Index: 8.1
#. -- Parent line: 7 (62720-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62721-Dialogue.mod:1
msgid "Or how about some splints?"
msgstr "或者一些夾板怎麼樣？"

#. Id: 62721-Dialogue.mod
#. -- Index: 8.2
#. -- Parent line: 7 (62720-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62721-Dialogue.mod:2
msgid "Better prep for emergencies with our basic med kits!"
msgstr "使用我們的基本醫療包，為緊急情況做好準備！"

#. Id: 62721-Dialogue.mod
#. -- Index: 8.3
#. -- Parent line: 7 (62720-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62721-Dialogue.mod:3
msgid "Or how about the rare Lantern of Radiance?"
msgstr "或者罕見的光輝燈籠怎麼樣？"

#. Id: 62722-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62721-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62722-Dialogue.mod:0
msgid "New sandals for your leg ends?"
msgstr "腿部的新涼鞋？"

#. Id: 62722-Dialogue.mod
#. -- Index: 9.1
#. -- Parent line: 8 (62721-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62722-Dialogue.mod:1
msgid "New boots for your ankle hands?"
msgstr "你腳踝的新靴子？"

#. Id: 62723-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (62722-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62723-Dialogue.mod:0
msgid "They look so stylish!"
msgstr "它們看起來很時尚！"

#. Id: 62723-Dialogue.mod
#. -- Index: 10.1
#. -- Parent line: 9 (62722-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62723-Dialogue.mod:1
msgid "Have the trendiest leg ends in the outlands!"
msgstr "讓最時髦的腿在外域結束！"

#. Id: 62725-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (62724-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hive merchant visits player base
#: 62725-Dialogue.mod:0
msgid "Wow!"
msgstr "哇！"

#. Id: 62725-Dialogue.mod
#. -- Index: 12.1
#. -- Parent line: 11 (62724-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hive merchant visits player base
#: 62725-Dialogue.mod:1
msgid "Stylish!"
msgstr "時尚！"

#. Id: 62726-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (62725-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62726-Dialogue.mod:0
msgid "...No?"
msgstr "...沒有？"

#. Id: 62716-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (62726-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62716-Dialogue.mod:0
msgid "Okay, no-hives, we'll come again..."
msgstr "好的，沒有蕁麻疹，我們會再來......"

#. Id: 62717-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (62716-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62717-Dialogue.mod:0
msgid "Buy next time, okay?"
msgstr "下次購買，好嗎？"

#. Id: 62718-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (62717-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62718-Dialogue.mod:0
msgid "... But don't buy from the traders guild."
msgstr "......但是不要從交易員公會購買。"

#. Id: 62705-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62687-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62705-Dialogue.mod:0
msgid "How about rations?"
msgstr "口糧怎麼樣？"

#. Id: 62709-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62705-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62709-Dialogue.mod:0
msgid "We have.. er.. special ones, handmade..."
msgstr "我們有..呃..特別的，手工製作......"

#. Id: 62711-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62712-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hive merchant visits player base
#: 62711-Dialogue.mod:0
msgid "*retch*"
msgstr "*乾嘔*"

#. Id: 62710-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62711-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62710-Dialogue.mod:0
msgid "'Specially for no-hives like you!"
msgstr "'特別適合喜歡你的蕁麻疹！"

#. Id: 62713-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (62710-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#. Id: 17114-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (17112-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Holy nation gate - Dark beasts in party
#. Id: 17126-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (17118-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Holy nation gate - Dark beasts in party
#: 62713-Dialogue.mod:0 17114-Dialogue.mod:0 17126-Dialogue.mod:0
msgid "... No?"
msgstr "......不是嗎？"

#. Id: 62715-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (62713-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hive merchant visits player base
#: 62715-Dialogue.mod:0
msgid "You won't buy our snacks?"
msgstr "你不會買我們的小吃？"
