#. Id: 55662-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Greenlander or Scorchlander or Shek
#: 55662-Dialogue.mod:0
msgid "Acid rain! Take shelter!"
msgstr "酸雨! 找避難所!"

#. Id: 55662-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Greenlander or Scorchlander or Shek
#: 55662-Dialogue.mod:1
msgid "The rain, it's acid! Find cover!"
msgstr "這個雨, 是酸的! 快點找掩護!"

#. Id: 55662-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Greenlander or Scorchlander or Shek
#: 55662-Dialogue.mod:2
msgid "My skin, it's burning!... Acid rain!"
msgstr "我的皮膚, 它在燃燒!... 酸雨!"

#. Id: 55662-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Greenlander or Scorchlander or Shek
#: 55662-Dialogue.mod:3
msgid "Acid rain, find cover!"
msgstr "酸雨! 快點找掩護!"

#. Id: 55662-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Greenlander or Scorchlander or Shek
#: 55662-Dialogue.mod:4
msgid "Acid rain! Not good..."
msgstr "酸雨! 不妙..."

#. Id: 55759-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55759-Dialogue.mod:0
msgid "Ow! That burns!"
msgstr "Ow! 要燒起來了!"

#. Id: 55759-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55759-Dialogue.mod:1
msgid "Ouch, that stings!"
msgstr "Ouch, 要燒起來了!"

#. Id: 55759-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55759-Dialogue.mod:2
msgid "/AAH/, feels like daggers on my skin!"
msgstr "/AAH/, 感覺像是匕首在我身體上到處亂劃一樣!"

#. Id: 55759-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55759-Dialogue.mod:3
msgid "/OHSHIT/!... Acid rain!"
msgstr "/OHSHIT/!... 酸雨!"

#. Id: 55760-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55759-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55760-Dialogue.mod:0
msgid "/IWE/ need to take cover."
msgstr "/IWE/ 需要找掩體."

#. Id: 55760-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55759-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55760-Dialogue.mod:1
msgid "Take cover!"
msgstr "找掩體!"

#. Id: 55761-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:0
msgid "Burning rain..."
msgstr "燃燒之雨..."

#. Id: 55761-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:1
msgid "My skin... The rain is burning right through me..."
msgstr "我的皮膚.... 這個雨正在燒我..."

#. Id: 55761-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:2
msgid "The rain here burns like fire..."
msgstr "這裏的雨像火一樣燃燒..."

#. Id: 55761-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:3
msgid "Kch. Burning... This is no normal rain..."
msgstr "Kch. 好燙... 這不是普通的雨..."

#. Id: 55761-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:4
msgid "Rrgh. Burning rain, it's eating through my skin..."
msgstr "Rrgh. 好燙的雨, 它正在吃我的皮膚..."

#. Id: 55761-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55761-Dialogue.mod:5
msgid "This is no ordinary rain... It burns like fire."
msgstr "這不是普通的雨...它像火一樣燃燒."

#. Id: 55762-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55762-Dialogue.mod:0
msgid "/IWE/ must take shelter."
msgstr "/IWE/ 必須找避難所."

#. Id: 55762-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55761-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55762-Dialogue.mod:1
msgid "/IWE/ need to take shelter, quickly."
msgstr "/IWE/ 需要找避難所, 馬上."

#. Id: 55764-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55764-Dialogue.mod:0
msgid "My skin! It's /FUCKING/ fizzing! FIZZING!"
msgstr "我的皮膚! 它在 /FUCKING/ 滋滋響! 滋滋響!"

#. Id: 55764-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55764-Dialogue.mod:1
msgid "/WHATTHEFUCK/ /BULLSHIT/ is this?! My skin's burning holes!"
msgstr "/WHATTHEFUCK/ /BULLSHIT/ 這是什麼?! 我的皮膚在燃燒!"

#. Id: 55764-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55764-Dialogue.mod:2
msgid "/ARG/! Feels like daggers on my skin!"
msgstr "/ARG/! 感覺像是匕首在我身體上到處亂劃一樣!"

#. Id: 55764-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55764-Dialogue.mod:3
msgid "My skin, MY BEAUTIFUL SKIN!"
msgstr "我的皮膚, 我美麗的皮膚!"

#. Id: 55764-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: My Race(s): Hive Prince or Scorchlander or Shek
#: 55764-Dialogue.mod:4
msgid "/AAH/, burning!... Protect my face!"
msgstr "/AAH/, 好燙!... 得我的臉!"

#. Id: 55765-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55764-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid rain tough reaction
#: 55765-Dialogue.mod:0
msgid "Acid rain! Take cover!"
msgstr "酸雨! 找掩護!"

