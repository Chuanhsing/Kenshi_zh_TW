#. Id: 46137-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46137-Dialogue.mod:0
msgid "It's clear, we can part ways now..."
msgstr ""

#. Id: 46137-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46137-Dialogue.mod:1
msgid "The coast looks clear. Time to split..."
msgstr ""

#. Id: 46137-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46137-Dialogue.mod:2
msgid "I'll be parting ways here, we should be safe from here on out."
msgstr ""

#. Id: 46141-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (46137-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46141-Dialogue.mod:0
msgid "See you again."
msgstr ""

#. Id: 46141-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (46137-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46141-Dialogue.mod:2
msgid "Look after yourself."
msgstr ""

#. Id: 46141-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (46137-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46141-Dialogue.mod:4
msgid "Be safe."
msgstr ""

#. Id: 46138-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: IS_RECRUITABLE=1, Tag: -1
#: 46138-Dialogue.mod:0
msgid "I won't forget how you helped me. I owe you my life."
msgstr ""

#. Id: 46138-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: IS_RECRUITABLE=1, Tag: -1
#: 46138-Dialogue.mod:1
msgid "I owe you for helping me like you did."
msgstr ""

#. Id: 46138-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: IS_RECRUITABLE=1, Tag: -1
#: 46138-Dialogue.mod:2
msgid "I appreciate what you've done for me, outlander."
msgstr ""

#. Id: 46139-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (46138-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46139-Dialogue.mod:0
msgid "A life for a life. I'm joining you, to make sure I can repay the favour someday."
msgstr ""

#. Id: 46139-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (46138-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46139-Dialogue.mod:1
msgid "Let me make this even. I'll follow you, watch your back. We'll stick together from here."
msgstr ""

#. Id: 46139-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (46138-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: event skeleton escaped
#: 46139-Dialogue.mod:2
msgid "I'll repay the favour. I've got your back from here on out."
msgstr ""

