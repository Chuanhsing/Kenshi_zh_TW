#. Id: 12225-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12225-Dialogue?(10).mod:0
msgid "Just a flesh wound. Let me fix it."
msgstr "只是一個肉體傷口。我來解決它。"

#. Id: 12225-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12225-Dialogue?(10).mod:1
msgid "You'll live. Just needs some first aid."
msgstr "你會活下去的只需要一些急救。"

#. Id: 12225-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12225-Dialogue?(10).mod:2
msgid "You'll survive. Here..."
msgstr "你會活下來的。這裡..."

#. Id: 12225-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12225-Dialogue?(10).mod:3
msgid "Doesn't look so bad. Let me..."
msgstr "看起來不那麼糟糕。讓我來..."

#. Id: 12225-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12225-Dialogue?(10).mod:4
msgid "I've seen worse. Here..."
msgstr "我看到更糟了。這裡..."

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:0
msgid "... Keep still."
msgstr "... 保持靜止。"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:1
msgid "... Rest yourself."
msgstr "......休息一下"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:2
msgid "... Don't move for a minute."
msgstr "......不要動一會兒。"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:3
msgid "... Here."
msgstr "... 這裡。"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:4
msgid "... Be still."
msgstr "... 不要動。"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:5
msgid "... Relax."
msgstr "... 放鬆。"

#. Id: 12226-Dialogue (10).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12226-Dialogue?(10).mod:6
msgid "... Relax yourself."
msgstr "......放鬆自己。"

#. Id: 12230-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12230-Dialogue?(10).mod:0
msgid "Hrm, looks painful... Here, let me..."
msgstr "嗯，看起來很痛苦...在這裡，讓我......"

#. Id: 12230-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12230-Dialogue?(10).mod:1
msgid "Looks bad... Hold still..."
msgstr "看起來很糟糕......保持不動......"

#. Id: 12230-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12230-Dialogue?(10).mod:2
msgid "Looks serious... Be still for me..."
msgstr "看起來很嚴肅......對我來說還是......"

#. Id: 12230-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ healing other
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 12230-Dialogue?(10).mod:3
msgid "This will ease the pain"
msgstr "這將緩解疼痛"

