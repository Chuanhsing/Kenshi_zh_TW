#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:0
msgid "I've got a plan..."
msgstr "我有個計劃......"

#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:1
msgid "You want out of here?"
msgstr "你想離開這裡嗎？"

#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:2
msgid "Let's get out of here..."
msgstr "讓我們離開這裡......"

#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:3
msgid "You, let's get out of here..."
msgstr "你，讓我們離開這裡......"

#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:4
msgid "You want to get out of here?"
msgstr "你想離開這裡嗎？"

#. Id: 12974-Dialogue (11) (2).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12974-Dialogue?(11)?(2).mod:5
msgid "You want out of here, right?"
msgstr "你想離開這裡，對吧？"

#. Id: 12981-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12974-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12981-Dialogue?(11)?(2).mod:0
msgid "What's the plan?"
msgstr "計劃是什麼？"

#. Id: 12980-Dialogue (11) (2).mod
#. -- Index: 2.0
#. -- Parent line: 1 (12981-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12980-Dialogue?(11)?(2).mod:0
msgid "I hope you're good at lockpicking. Start working on the locks and let's get out of here"
msgstr "我希望你擅長開鎖。開始處理鎖定，讓我們離開這裡"

#. Id: 12980-Dialogue (11) (2).mod
#. -- Index: 2.1
#. -- Parent line: 1 (12981-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12980-Dialogue?(11)?(2).mod:1
msgid "You any good at lockpicking? We need to get free first, get started on the locks"
msgstr "你有什麼擅長開鎖的嗎？我們需要先獲得自由，開始使用鎖"

#. Id: 12980-Dialogue (11) (2).mod
#. -- Index: 2.2
#. -- Parent line: 1 (12981-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12980-Dialogue?(11)?(2).mod:2
msgid "Can you pick locks? Start working on the locks... then we'll sneak out"
msgstr "你能拿鎖嗎？開始研究鎖......然後我們就會偷偷溜出去"

#. Id: 12999-Dialogue (11) (2).mod
#. -- Index: 3.1
#. -- Parent line: 2 (12980-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12999-Dialogue?(11)?(2).mod:1
msgid "Got it"
msgstr "得到它了"

#. Id: 17278-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (12999-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 17278-Dialogue.mod:0
msgid "You can do this"
msgstr "你可以這樣做"

#. Id: 17278-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (12999-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. Id: 17276-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (17275-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 17278-Dialogue.mod:1 17276-Dialogue.mod:1
msgid "I'm counting on you"
msgstr "我指望你"

#. Id: 13001-Dialogue (11) (2).mod
#. -- Index: 3.0
#. -- Parent line: 2 (12980-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 13001-Dialogue?(11)?(2).mod:0
msgid "You ain't the boss of me!"
msgstr "你不是我的老闆！"

#. Id: 13002-Dialogue (11) (2).mod
#. -- Index: 4.0
#. -- Parent line: 3 (13001-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13002-Dialogue?(11)?(2).mod:0
msgid "Er.. okay"
msgstr "呃..好吧"

#. Id: 12983-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12974-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12983-Dialogue?(11)?(2).mod:0
msgid "I don't need help, thanks"
msgstr "我不需要幫助，謝謝"

#. Id: 12984-Dialogue (11) (2).mod
#. -- Index: 2.0
#. -- Parent line: 1 (12983-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12984-Dialogue?(11)?(2).mod:0
msgid "Miserable one, aren't ya?"
msgstr "悲慘的一個，不是嗎？"

#. Id: 12984-Dialogue (11) (2).mod
#. -- Index: 2.1
#. -- Parent line: 1 (12983-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12984-Dialogue?(11)?(2).mod:1
msgid "Then be like that"
msgstr "然後就是這樣"

#. Id: 12977-Dialogue (11) (2).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: My Race(s): Greenlander or Hive Prince or Shek
#: 12977-Dialogue?(11)?(2).mod:0
msgid "/FUCK/! You got a plan?"
msgstr ""

#. Id: 12977-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: My Race(s): Greenlander or Hive Prince or Shek
#: 12977-Dialogue?(11)?(2).mod:1
msgid "Well... isn't this fun... /FUCK/..."
msgstr ""

#. Id: 12995-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12977-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#. Id: 17292-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17291-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Caged neighbour talk to pt 2
#: 12995-Dialogue?(11)?(2).mod:0 17292-Dialogue.mod:0
msgid "I'll pick the locks. Follow my lead"
msgstr "我會選擇鎖。跟著我的指引"

#. Id: 13003-Dialogue (11) (2).mod
#. -- Index: 2.0
#. -- Parent line: 1 (12995-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13003-Dialogue?(11)?(2).mod:0
msgid "Y- yeah..."
msgstr "是的，是的......"

#. Id: 13003-Dialogue (11) (2).mod
#. -- Index: 2.1
#. -- Parent line: 1 (12995-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13003-Dialogue?(11)?(2).mod:1
msgid "R- right..."
msgstr "R-對..."

#. Id: 13006-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12977-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 13006-Dialogue?(11)?(2).mod:0
msgid "I don't know, I'm pretty attached to living here"
msgstr "我不知道，我非常依賴住在這裡"

#. Id: 13006-Dialogue (11) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12977-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 13006-Dialogue?(11)?(2).mod:1
msgid "I dunno, I think it's pretty cozy here"
msgstr "我不知道，我覺得這裡很舒服"

#. Id: 13007-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12977-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 13007-Dialogue?(11)?(2).mod:0
msgid "Don't talk to me"
msgstr "不要和我說話"

#. Id: 12978-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 12978-Dialogue?(11)?(2).mod:1
msgid "We need to escape"
msgstr "我們需要逃避"

#. Id: 12978-Dialogue (11) (2).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 12978-Dialogue?(11)?(2).mod:2
msgid "We can't sit around here doing nothing"
msgstr "我們不能坐在這裡什麼都不做"

#. Id: 12979-Dialogue (11) (2).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: NUM_DIALOG_EVENT_REPEATS<3, Tag: -1
#. -- Condition: My Race(s): Greenlander or Shek
#: 12979-Dialogue?(11)?(2).mod:0
msgid "Pretty cozy in here..."
msgstr "這裡很舒適......"

#. Id: 12979-Dialogue (11) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: NUM_DIALOG_EVENT_REPEATS<3, Tag: -1
#. -- Condition: My Race(s): Greenlander or Shek
#: 12979-Dialogue?(11)?(2).mod:1
msgid "Cozy in here, isn't it?"
msgstr "舒適在這裡，不是嗎？"

#. Id: 12979-Dialogue (11) (2).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: NUM_DIALOG_EVENT_REPEATS<3, Tag: -1
#. -- Condition: My Race(s): Greenlander or Shek
#: 12979-Dialogue?(11)?(2).mod:2
msgid "Eh, I've seen dirtier prisons..."
msgstr "呃，我看到了更臟的監獄......"

#. Id: 12979-Dialogue (11) (2).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: NUM_DIALOG_EVENT_REPEATS<3, Tag: -1
#. -- Condition: My Race(s): Greenlander or Shek
#: 12979-Dialogue?(11)?(2).mod:3
msgid "Scared?"
msgstr "害怕嗎？"

#. Id: 12985-Dialogue (11) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12979-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12985-Dialogue?(11)?(2).mod:1
msgid "We've got to get out of here..."
msgstr "我們得離開這裡......"

#. Id: 12988-Dialogue (11) (2).mod
#. -- Index: 2.0
#. -- Parent line: 1 (12985-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12988-Dialogue?(11)?(2).mod:0
msgid "This should be interesting. Better start picking the locks then, unless you've got any better ideas?"
msgstr "這應該很有趣。除非你有更好的想法，否則最好先開始選擇鎖？"

#. Id: 13009-Dialogue (11) (2).mod
#. -- Index: 3.0
#. -- Parent line: 2 (12988-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 13009-Dialogue?(11)?(2).mod:0
msgid "Follow my lead"
msgstr "跟著我的指引"

#. Id: 17268-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (13009-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. Id: 17274-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (17273-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 17268-Dialogue.mod:1 17274-Dialogue.mod:0
msgid "Heh"
msgstr "嘿"

#. Id: 12986-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12979-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12986-Dialogue?(11)?(2).mod:0
msgid "How are you so calm?!"
msgstr "你怎麼這麼冷靜？！"

#. Id: 12986-Dialogue (11) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12979-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12986-Dialogue?(11)?(2).mod:1
msgid "How are you so relaxed?!"
msgstr "你怎麼這麼放鬆？！"

#. Id: 12989-Dialogue (11) (2).mod
#. -- Index: 2.0
#. -- Parent line: 1 (12986-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12989-Dialogue?(11)?(2).mod:0
msgid "Eh, call it numbness I guess. You're planning on escaping I guess?"
msgstr "呃，我猜它叫麻木。我猜你想逃跑嗎？"

#. Id: 12989-Dialogue (11) (2).mod
#. -- Index: 2.1
#. -- Parent line: 1 (12986-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12989-Dialogue?(11)?(2).mod:1
msgid "Calm eh? I like to call it 'emotionally dead'. You're planning on escaping I guess?"
msgstr "平靜嗯？我喜歡稱它為“情緒化的死亡”。我猜你想逃跑嗎？"

#. Id: 12989-Dialogue (11) (2).mod
#. -- Index: 2.2
#. -- Parent line: 1 (12986-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12989-Dialogue?(11)?(2).mod:2
msgid "Calm you say? My ex likes to call it 'emotionally challenged'. You're planning on escaping I guess?"
msgstr "你說冷靜嗎？我的前任喜歡把它稱為“情緒挑戰”。我猜你想逃跑嗎？"

#. Id: 12990-Dialogue (11) (2).mod
#. -- Index: 3.0
#. -- Parent line: 2 (12989-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12990-Dialogue?(11)?(2).mod:0
msgid "Yeah, are you with me?"
msgstr "是的，你跟我在一起嗎？"

#. Id: 12990-Dialogue (11) (2).mod
#. -- Index: 3.1
#. -- Parent line: 2 (12989-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12990-Dialogue?(11)?(2).mod:1
msgid "Yeah, care to join me?"
msgstr "是的，關心加入我？"

#. Id: 13010-Dialogue (11) (2).mod
#. -- Index: 4.0
#. -- Parent line: 3 (12990-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13010-Dialogue?(11)?(2).mod:0
msgid "Sure, I'm no good at lockpicking though. You?"
msgstr "當然，我不擅長開鎖。您？"

#. Id: 13010-Dialogue (11) (2).mod
#. -- Index: 4.1
#. -- Parent line: 3 (12990-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13010-Dialogue?(11)?(2).mod:1
msgid "Sure. I can't lockpick to save my life though. Literally. How about you?"
msgstr "當然。我不能鎖定以挽救我的生命。從字面上看。你呢？"

#. Id: 13010-Dialogue (11) (2).mod
#. -- Index: 4.2
#. -- Parent line: 3 (12990-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 13010-Dialogue?(11)?(2).mod:2
msgid "I'm up for it. Can't picklock for shit though. You?"
msgstr "我很樂意。雖然不能為了糟糕而扒鎖。您？"

#. Id: 17269-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#. Id: 13388-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13337-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc asking for help
#. Id: 13363-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13342-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc asking for help
#: 17269-Dialogue.mod:0 13388-Escapes.mod:0 13363-Escapes.mod:1
msgid "Leave it to me"
msgstr "交給我"

#. Id: 17269-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17269-Dialogue.mod:1
msgid "I'll handle it"
msgstr "我會處理它"

#. Id: 17269-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17269-Dialogue.mod:2
msgid "Let me handle it"
msgstr "讓我來處理吧"

#. Id: 17271-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17271-Dialogue.mod:0
msgid "Me neither. I'll give it a go though"
msgstr "我也不。我會試一試"

#. Id: 17271-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17271-Dialogue.mod:1
msgid "Me neither. Doesn't hurt to try though"
msgstr "我也不。雖然嘗試沒有傷害"

#. Id: 17275-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (17271-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: In town(s) of faction(s): Cannibals
#: 17275-Dialogue.mod:0
msgid "Always a good time to learn when you're waiting for your turn to be cooked alive, eh"
msgstr "當你等待輪到你活著時，總是一個學習的好時機，呃"

#. Id: 17275-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (17271-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: In town(s) of faction(s): Cannibals
#: 17275-Dialogue.mod:1
msgid "Well, now's a better time to learn that ever, eh"
msgstr "嗯，現在是學習它的最佳時機，呃"

#. Id: 17275-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (17271-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. -- Condition: In town(s) of faction(s): Cannibals
#: 17275-Dialogue.mod:2
msgid "No better time to learn than now, eh?"
msgstr "沒有比現在更好的學習時間了，是嗎？"

#. Id: 17276-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (17275-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#. Id: 12207-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek civ get me out of this cage
#: 17276-Dialogue.mod:0 12207-Dialogue?(10).mod:0
msgid "Don't give up"
msgstr "不要放棄"

#. Id: 17272-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17272-Dialogue.mod:0
msgid "Nope"
msgstr "不"

#. Id: 17272-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (13010-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 17272-Dialogue.mod:1
msgid "I suck at it"
msgstr "我很糟糕"

#. Id: 17273-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (17272-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 17273-Dialogue.mod:0
msgid "/MERCLAUGH/. Guess we'll rot in these cozy old cages then"
msgstr ""

#. Id: 12991-Dialogue (11) (2).mod
#. -- Index: 3.0
#. -- Parent line: 2 (12989-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12991-Dialogue?(11)?(2).mod:0
msgid "Yeah, I don't need anyone slowing me down though"
msgstr "是的，我不需要任何人放慢我的速度"

#. Id: 12994-Dialogue (11) (2).mod
#. -- Index: 4.0
#. -- Parent line: 3 (12991-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Imprisoned merc
#: 12994-Dialogue?(11)?(2).mod:0
msgid "/MERCLAUGH/, suit yourself..."
msgstr ""

#. Id: 12992-Dialogue (11) (2).mod
#. -- Index: 3.0
#. -- Parent line: 2 (12989-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Imprisoned merc
#: 12992-Dialogue?(11)?(2).mod:0
msgid "No, I plan on waiting for my doom in this hutch..."
