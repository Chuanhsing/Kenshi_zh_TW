#. Id: 45287-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 45287-Dialogue.mod:1
msgid "I'm free to go?!"
msgstr "我有空嗎？！"

#. Id: 45872-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45872-Dialogue.mod:0
msgid "I'm outta here! Fuck the lot of you! /CRAZY/ bastards!"
msgstr ""

#. Id: 45872-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45872-Dialogue.mod:1
msgid "I'm gettin' the hell away from you buncha crazies!"
msgstr "我開始遠離你的地獄瘋狂！"

#. Id: 45872-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. Id: 45880-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 45872-Dialogue.mod:2 45880-Dialogue.mod:2
msgid "I gotta get outta here! "
msgstr "我得離開這裡！"

#. Id: 45872-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. Id: 45880-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 45872-Dialogue.mod:3 45880-Dialogue.mod:3
msgid "Leave me alone! Don't put me back in there!"
msgstr "請別打擾我！別把我放回去！"

#. Id: 45873-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45873-Dialogue.mod:0
msgid "Arghhh! Run, run, run!"
msgstr "Arghhh！跑，跑，跑！"

#. Id: 45873-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45873-Dialogue.mod:1
msgid "Screw this! I'm gettin' the hell outta here!"
msgstr "擰這個！我在這裡開始了！"

#. Id: 45873-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45873-Dialogue.mod:2
msgid "Don't try comin' after me! I'm warnin' you! Sadistic bastard!"
msgstr "不要試著跟我說話！我警告你了！虐待狂的混蛋！"

#. Id: 45873-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#: 45873-Dialogue.mod:3
msgid "I'm gettin' outta here! Leave me alone!"
msgstr "我開始離開了！請別打擾我！"

#. Id: 45880-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 45880-Dialogue.mod:0
msgid "I'm outta here! Fuck you! /CRAZY/ bastard!"
msgstr ""

#. Id: 45880-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45287-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 45880-Dialogue.mod:1
msgid "/CRAZY/ bastard! I'm gettin' the hell away from you!"
msgstr ""

#. Id: 45874-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 45874-Dialogue.mod:0
msgid "... You're setting me free now, are ya?"
msgstr "......你現在讓我自由，是嗎？"

#. Id: 45874-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 45874-Dialogue.mod:1
msgid "... Aw, you're letting me go now, are ya?"
msgstr "噢，你現在讓我走了，是嗎？"

#. Id: 45874-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 45874-Dialogue.mod:2
msgid "... So what, I'm free to go now, am I?"
msgstr "......那麼，我現在可以自由地去，是嗎？"

#. Id: 45875-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45874-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 45875-Dialogue.mod:0
msgid "Shoulda kept me locked up! I'll stab your face for putting me in there, bastard!"
msgstr "應該把我鎖起來！我會刺傷你的臉，讓我在那裡，混蛋！"

#. Id: 45875-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45874-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 45875-Dialogue.mod:1
msgid "Bad move! I'll /SLITYOURTHROAT/ for stickin' me in that dirty cage!"
msgstr ""

#. Id: 45876-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45874-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 45876-Dialogue.mod:0
msgid "BAD MOVE, ASSHOLE! I'LL KILL YOU!"
msgstr "BAD MOVE，ASSHOLE！我會殺了你！"

#. Id: 45876-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45874-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 45876-Dialogue.mod:1
msgid "Dumb move, scumbag! I'll kill you even if I die tryin'! "
msgstr "愚蠢的舉動，卑鄙！即使我死了，我也會殺了你！"

#. Id: 45876-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45874-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 45876-Dialogue.mod:2
msgid "Well ain't revenge just the sweetest thing, eh?! Ahahaha!"
msgstr "好吧，不是報復最甜蜜的事情，嗯？！ Ahahaha！"

#. Id: 45890-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 45890-Dialogue.mod:0
msgid "You ain't puttin' me back in those locks again!"
msgstr "你不會再讓我回到那些鎖中了！"

#. Id: 45890-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 45890-Dialogue.mod:1
msgid "I ain't lettin' you catch me again! I'll kill ya!"
msgstr "我不會讓你再次抓住我！我會殺了你！"

#. Id: 45940-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: IM_UNARMED=0, Tag: -1
#. Id: 16017-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Slave escapin spotted by civ
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45940-Dialogue.mod:0 16017-Dialogue.mod:3
msgid "...?!"
msgstr "...？！"

#. Id: 45944-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 45944-Dialogue.mod:0
msgid "If I had a sword I'd gut ya! "
msgstr "如果我有一把劍我會直覺！"

#. Id: 45944-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 45944-Dialogue.mod:1
msgid "I'll kill you! I'll find myself a sword, I'll come back, and I'll butcher you!"
msgstr "我會殺了你！我會發現自己是一把劍，我會回來的，我會屠殺你的！"

#. Id: 45944-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 45944-Dialogue.mod:2
msgid "I'm comin' back for you! You're just lucky I'm unarmed or I'd flay your skin off!"
msgstr "我正在為你回來！你很幸運我沒有武裝，或者我會脫掉你的皮膚！"

#. Id: 45944-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 45944-Dialogue.mod:3
msgid "Hear my words, I'm gonna come back for you and I'm gonna kill you dead!"
msgstr "聽到我的話，我會回來找你，我會殺了你死的！"

#. Id: 45944-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (45940-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw released by captor
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#: 45944-Dialogue.mod:4
msgid "I'll be back for your head!"
msgstr "我會回來的！"
