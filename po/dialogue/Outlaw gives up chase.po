#. Id: 53547-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 53547-Dialogue.mod:0
msgid "I see your face again and I'll gut you!"
msgstr ""

#. Id: 53547-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 53547-Dialogue.mod:1
msgid "Next time I'll get you! Just you wait!"
msgstr ""

#. Id: 53547-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 53547-Dialogue.mod:2
msgid "Next time you show your face I'll make you bleed!"
msgstr ""

#. Id: 53547-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 53547-Dialogue.mod:3
msgid "Show up here again and I'll cut you!"
msgstr ""

#. Id: 53547-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#: 53547-Dialogue.mod:4
msgid "Next time I'll skin you alive! Just you wait!"
msgstr ""

#. Id: 53548-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53547-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53548-Dialogue.mod:0
msgid "Stay away if you know what's good for you."
msgstr ""

#. Id: 53548-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (53547-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53548-Dialogue.mod:1
msgid "Run home to momma, eh..."
msgstr ""

#. Id: 53548-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (53547-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53548-Dialogue.mod:2
msgid "Keep runnin'. You don't wanna cross me again."
msgstr ""

#. Id: 53549-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 53549-Dialogue.mod:0
msgid "Forget it, I ain't cut out for all this runnin'!"
msgstr ""

#. Id: 53549-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 53549-Dialogue.mod:1
msgid "Screw it, let /HIM/ run. I'm done chasin' chickens."
msgstr ""

#. Id: 53549-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 53549-Dialogue.mod:2
msgid "I'm done runnin'. Let /HIM/ go."
msgstr ""

#. Id: 53550-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53550-Dialogue.mod:0
msgid "Next time you won't get away so easy though, /INSULT/!"
msgstr ""

#. Id: 53550-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (53549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53550-Dialogue.mod:1
msgid "Next time you're dead, /INSULT/!"
msgstr ""

#. Id: 53550-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (53549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53550-Dialogue.mod:2
msgid "Next time I'll skin you alive myself, /INSULT/!"
msgstr ""

#. Id: 53550-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (53549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53550-Dialogue.mod:3
msgid "But cross /MEUS/ again and you die!"
msgstr ""

#. Id: 53550-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (53549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53550-Dialogue.mod:4
msgid "Chasin' losers across the outlands, no sir."
msgstr ""

#. Id: 53551-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 53551-Dialogue.mod:0
msgid "Sonofabitch, /HE/ got away!"
msgstr ""

#. Id: 53551-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 53551-Dialogue.mod:1
msgid "/FUCK/, /HE/ outran us!"
msgstr ""

#. Id: 53551-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 53551-Dialogue.mod:2
msgid "/FUCKNOUN/, /HE/ escaped!"
msgstr ""

#. Id: 53553-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53551-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53553-Dialogue.mod:0
msgid "Forget it, /HE/ ain't worth it anyway."
msgstr ""

#. Id: 53553-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (53551-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53553-Dialogue.mod:1
msgid "/HE/'ll be back. /HE/'ll come crawlin' back and then BAM!"
msgstr ""

#. Id: 53553-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (53551-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53553-Dialogue.mod:2
msgid "Forget it. I'm done."
msgstr ""

#. Id: 53553-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (53551-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Outlaw gives up chase
#: 53553-Dialogue.mod:3
msgid "I'm done. Ain't worth it, head back."
msgstr ""

