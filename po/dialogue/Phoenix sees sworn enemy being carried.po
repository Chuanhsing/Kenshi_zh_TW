#. Id: 58762-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix sees sworn enemy being carried
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 58762-Dialogue.mod:0
msgid "Witch of Narko!... "
msgstr "Narko的女巫！..."

#. Id: 58763-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (58762-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix sees sworn enemy being carried
#: 58763-Dialogue.mod:0
msgid "Take her to the inquisitor, child..."
msgstr "把她帶到調查官，孩子......"

#. Id: 58764-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix sees sworn enemy being carried
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 58764-Dialogue.mod:0
msgid "Horned devil woman!..."
msgstr "有角的惡魔女人！..."

#. Id: 58765-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix sees sworn enemy being carried
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 58765-Dialogue.mod:0
msgid "That rotten tyrant!... You've caught him!"
msgstr "那個腐爛的暴君！......你抓住了他！"

#. Id: 58766-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (58765-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix sees sworn enemy being carried
#: 58766-Dialogue.mod:0
msgid "Take him to the inquisitor, child, straight away..."
msgstr "把他帶到審問者，孩子，馬上......"

