#. Id: 7768-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (7767-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7768-Dialogue.mod:0
msgid "Whoa whoa whoa! Buck the fuck up, /COWARD/!"
msgstr ""

#. Id: 7768-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (7767-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7768-Dialogue.mod:1
msgid "What?! Really? You're just gonna run away like a fucking chump /COWARD/?"
msgstr ""

#. Id: 7768-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (7767-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7768-Dialogue.mod:2
msgid "/WTF/!? Damn /COWARD/, are you serious?"
msgstr ""

#. Id: 7768-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (7767-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7768-Dialogue.mod:3
msgid "Dammit I ain't runnin' away like some scumbag /COWARD/!"
msgstr ""

#. Id: 7770-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (7768-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7770-Dialogue.mod:0
msgid "B-but, haven't you heard-"
msgstr "B-但是，你沒有聽說過 - "

#. Id: 7770-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (7768-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7770-Dialogue.mod:1
msgid "But wait, we're outnum-"
msgstr "但是，等等，我們的年齡 - "

#. Id: 7770-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (7768-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7770-Dialogue.mod:2
msgid "But look how many-"
msgstr "但看多少 - "

#. Id: 7770-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (7768-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7770-Dialogue.mod:3
msgid "We can't win agai-"
msgstr "我們不能贏得agai-"

#. Id: 7770-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (7768-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7770-Dialogue.mod:4
msgid "They'll kill us-"
msgstr "他們會殺了我們 - "

#. Id: 7771-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (7770-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7771-Dialogue.mod:0
msgid "Call yourself a bandit? I ain't running away from these /RUNT/s!"
msgstr ""

#. Id: 7771-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (7770-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7771-Dialogue.mod:1
msgid "A true warrior never runs from a fight!"
msgstr "一個真正的戰士永遠不會戰鬥！"

#. Id: 7771-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (7770-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7771-Dialogue.mod:2
msgid "Those /RUNT/s don't scare me! C'mon!"
msgstr ""

#. Id: 7771-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (7770-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7771-Dialogue.mod:3
msgid "No! We're NOT running away!"
msgstr "沒有！我們不會逃跑！"

#. Id: 7771-Dialogue.mod
#. -- Index: 4.4
#. -- Parent line: 3 (7770-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Thug get off my turf - I hate this guy (outnumbered)
#: 7771-Dialogue.mod:4
msgid "Run away and I'll /CUT/ your shins off!"
msgstr ""

