#. Id: 62491-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62491-Dialogue.mod:0
msgid "You're hive... but you're not hive... We.. I'm lost."
msgstr ""

#. Id: 62492-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62491-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62492-Dialogue.mod:0
msgid "How did you get lost?"
msgstr ""

#. Id: 62496-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62492-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62496-Dialogue.mod:0
msgid "I don't remember... I lost our queen, and I can't get back to her. Or the hive. Do you know where our queen is?"
msgstr ""

#. Id: 62497-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62496-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62497-Dialogue.mod:0
msgid "Probably dead."
msgstr ""

#. Id: 62506-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (62497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#. Id: 19743-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hiver witness generic assault
#: 62506-Dialogue.mod:1 19743-Dialogue.mod:2
msgid "Gahhh!"
msgstr ""

#. Id: 62507-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62506-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62507-Dialogue.mod:0
msgid "Knowing this place she died horribly as well..."
msgstr ""

#. Id: 62509-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62507-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62509-Dialogue.mod:0
msgid "[Tears stream down his cheeks]"
msgstr ""

#. Id: 62511-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62506-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62511-Dialogue.mod:0
msgid "Don't be sad. Everybody dies eventually."
msgstr ""

#. Id: 62512-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62506-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62512-Dialogue.mod:0
msgid "But you don't need her. I can take care of you."
msgstr ""

#. Id: 62513-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62512-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#. Id: 62516-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62515-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62513-Dialogue.mod:0 62516-Dialogue.mod:0
msgid "*Sniff*"
msgstr ""

#. Id: 62514-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62513-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62514-Dialogue.mod:0
msgid "Wanna hoe some soil?"
msgstr ""

#. Id: 62515-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62514-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#. Id: 62499-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (62498-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62515-Dialogue.mod:0 62499-Dialogue.mod:1
msgid "[He nods quietly]"
msgstr ""

#. Id: 62518-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62513-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62518-Dialogue.mod:0
msgid "Wanna mine some ore?"
msgstr ""

#. Id: 62501-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62496-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62501-Dialogue.mod:0
msgid "Far away. That's why you can't feel her."
msgstr ""

#. Id: 62519-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62501-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62519-Dialogue.mod:0
msgid "[His eyes widen and his mouth gapes open]"
msgstr ""

#. Id: 62502-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62496-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62502-Dialogue.mod:0
msgid "Why the hell would I know?"
msgstr ""

#. Id: 62522-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62502-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62522-Dialogue.mod:0
msgid "You don't know either? I'm not broken?... Broken hivers go in the furnace..."
msgstr ""

#. Id: 62525-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62522-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62525-Dialogue.mod:0
msgid "Fixed more like. Free of that slave-driver you call a queen."
msgstr ""

#. Id: 62526-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62525-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62526-Dialogue.mod:0
msgid "[He scratches his head]... I do feel less... cloudy?"
msgstr ""

#. Id: 62527-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62526-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62527-Dialogue.mod:0
msgid "Of course you do! Stick around here, you'll find your feet in no time."
msgstr ""

#. Id: 62529-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62527-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62529-Dialogue.mod:0
msgid "Oh, I didn't lose my feet, I still have those..."
msgstr ""

#. Id: 62530-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62529-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62530-Dialogue.mod:0
msgid "But thank you, sir. I'll live here now."
msgstr ""

#. Id: 62528-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62526-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62528-Dialogue.mod:0
msgid "That would be the pheromones wearing off. See? You'll be fine."
msgstr ""

#. Id: 62532-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62528-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#. Id: 62536-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62532-Dialogue.mod:0 62536-Dialogue.mod:0
msgid "[He looks even more confused than before]"
msgstr ""

#. Id: 62533-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62532-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62533-Dialogue.mod:0
msgid "O... okay. Thank you, sir."
msgstr ""

#. Id: 62531-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62522-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62531-Dialogue.mod:0
msgid "Keep talking at me and I'LL be throwing you in the furnace."
msgstr ""

#. Id: 62503-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62496-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62503-Dialogue.mod:0
msgid "I don't know. I'm sorry."
msgstr ""

#. Id: 62504-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62503-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62504-Dialogue.mod:0
msgid "[His body sags]"
msgstr ""

#. Id: 62504-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (62503-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62504-Dialogue.mod:1
msgid "[He goes teary eyed]"
msgstr ""

#. Id: 62504-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (62503-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62504-Dialogue.mod:2
msgid "[His lip quivers]"
msgstr ""

#. Id: 62505-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62504-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62505-Dialogue.mod:0
msgid "You don't need a queen though. You can live here."
msgstr ""

#. Id: 62520-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62504-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62520-Dialogue.mod:0
msgid "You'll make it on your own. You're free to live as you want now."
msgstr ""

#. Id: 62534-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62520-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62534-Dialogue.mod:0
msgid "[He turns to stare into the open distance, a wasteland of hiver-eating monsters]"
msgstr ""

#. Id: 62493-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62491-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62493-Dialogue.mod:0
msgid "You lost your queen?"
msgstr ""

#. Id: 62495-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62493-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62495-Dialogue.mod:0
msgid "I can't find my way back to her this time. Can you feel her? I can't feel her anymore."
msgstr ""

#. Id: 62535-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62535-Dialogue.mod:0
msgid "That would be the pheromones wearing off..."
msgstr ""

#. Id: 62498-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62498-Dialogue.mod:0
msgid "But I can help you. You don't need a queen."
msgstr ""

#. Id: 62499-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62498-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62499-Dialogue.mod:0
msgid "You can be my new hive?..."
msgstr ""

#. Id: 62499-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (62498-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62499-Dialogue.mod:2
msgid "You can? Thank you, sir..."
msgstr ""

#. Id: 62500-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62499-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62500-Dialogue.mod:0
msgid "What is my task, quee-.. Mr. /YOURNAME/?"
msgstr ""

#. Id: 62537-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62537-Dialogue.mod:0
msgid "She's probably dead."
msgstr ""

#. Id: 62538-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62538-Dialogue.mod:0
msgid "I can't feel her. I'm sorry."
msgstr ""

#. Id: 62494-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62491-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Ronin base talk to
#: 62494-Dialogue.mod:0
msgid "Well this is private property, Lost Guy. Scram!"
msgstr ""

#. Id: 62523-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62494-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Ronin base talk to
#: 62523-Dialogue.mod:0
msgid "S- sorry, sir..."
msgstr ""

