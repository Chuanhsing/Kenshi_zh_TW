#. Id: 50733-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:0
msgid "I... am... the spider killing queen! This is for Ned!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:1
msgid "You ain't suckin' MY guts! No fucking sir!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:2
msgid "I'm in hell."
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:3
msgid "Ngh.. I was having a dream... A dream I lived in a world free of GIANT GODDAMN SPIDERS!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:4
msgid "There is a hell. And I live in it. And it's full of goddamn giant BUGS!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:7
msgid "My mission.. is to rid this hell of goddamn insects!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:8
msgid "I'll squash you, bug! I'll make a soup out of you!"
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:9
msgid "I fucking hate bugs."
msgstr ""

#. Id: 50733-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#. -- Condition: Target Race(s): Spider
#: 50733-Dialogue.mod:10
msgid "I hate spiders."
msgstr ""

#. Id: 50734-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (50733-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50734-Dialogue.mod:0
msgid "Argh, lord, they're so gross..."
msgstr ""

#. Id: 50734-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (50733-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50734-Dialogue.mod:1
msgid "Agh, lord, it's hideous..."
msgstr ""

#. Id: 50734-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (50733-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50734-Dialogue.mod:3
msgid "I won't be killed by a stinkin' bug!"
msgstr ""

#. Id: 50738-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50738-Dialogue.mod:0
msgid "Ouch... sure did a number on me, didn't you? Then let's make this even!"
msgstr ""

#. Id: 50738-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50738-Dialogue.mod:2
msgid "Damn, that hurts..."
msgstr ""

#. Id: 50738-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50738-Dialogue.mod:3
msgid "Won't make that mistake again..."
msgstr ""

#. Id: 50738-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50738-Dialogue.mod:4
msgid "Boo, I'm back! Let's do this."
msgstr ""

#. Id: 50738-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Get up fight miu
#: 50738-Dialogue.mod:5
msgid "Now it's MY turn to shine!"
msgstr ""

