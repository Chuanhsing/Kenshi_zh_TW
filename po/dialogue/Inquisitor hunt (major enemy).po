#. Id: 40778-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:0
msgid "You've evaded Okran's retribution for long enough, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:1
msgid "You won't escape Okran's retribution, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:2
msgid "You won't escape the Holy Lord Phoenix' retribution, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:3
msgid "No-one escapes Okran's reckoning, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:4
msgid "No-one can outrun Okran's judgement, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:5
msgid "Okran's reckoning can never be escaped, /PLAYERNAME/..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:6
msgid "/PLAYERNAME/... I've come to deliver Okran's retribution..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:7
msgid "I will carry out Okran's will, /PLAYERNAME/. You've dodged your reckoning long enough..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:8
msgid "Okran's Will will be done, /PLAYERNAME/. You can't escape it any longer..."
msgstr ""

#. Id: 40778-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40778-Dialogue.mod:9
msgid "There's no escaping Okran's Will now, /PLAYERNAME/..."
msgstr ""

#. Id: 40779-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40779-Dialogue.mod:0
msgid "You will be purged! Once and for all!"
msgstr ""

#. Id: 40779-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40779-Dialogue.mod:1
msgid "Your darkness will be purged! I'll see to it personally!"
msgstr ""

#. Id: 40779-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40779-Dialogue.mod:2
msgid "There is a darkness within you that must be purged!"
msgstr ""

#. Id: 40779-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40779-Dialogue.mod:3
msgid "I'll see to it myself that your darkness is purged!"
msgstr ""

#. Id: 40779-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40779-Dialogue.mod:4
msgid "In the name of Okran, I will see to it that your darkness is purged from this world!"
msgstr ""

#. Id: 40780-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40780-Dialogue.mod:0
msgid "Purged in the divine flame of Okran!"
msgstr ""

#. Id: 40780-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40780-Dialogue.mod:1
msgid "Burned away in the divine flame of Okran!"
msgstr ""

#. Id: 40780-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40780-Dialogue.mod:2
msgid "Destroyed in Okran's flame!"
msgstr ""

#. Id: 40780-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40780-Dialogue.mod:3
msgid "Purged in the fire of Okran!"
msgstr ""

#. Id: 64459-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 64459-Dialogue.mod:0
msgid "Purged for the whore of Flotsam that you are!"
msgstr ""

#. Id: 64459-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 64459-Dialogue.mod:1
msgid "Burned away of all your sins as a Narko worshipping Flotsam whore!"
msgstr ""

#. Id: 64459-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 64459-Dialogue.mod:2
msgid "Destroyed in Okran's flame as Witch Moll's whore!"
msgstr ""

#. Id: 64459-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40779-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 64459-Dialogue.mod:3
msgid "Purged in the fire of Okran, where all whores of Flotsam belong!"
msgstr ""

#. Id: 40781-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 40781-Dialogue.mod:0
msgid "You can't fool me! I see through your disguise, dark one!"
msgstr ""

#. Id: 40781-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 40781-Dialogue.mod:1
msgid "You won't fool me, I see through your guise... agent of darkness!"
msgstr ""

#. Id: 40781-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 40781-Dialogue.mod:2
msgid "Your disguise won't fool me, ROBOT!"
msgstr ""

#. Id: 40781-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 40781-Dialogue.mod:3
msgid "Your disguise won't deceive me, AGENT OF DARKNESS!"
msgstr ""

#. Id: 40781-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (40778-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 40781-Dialogue.mod:4
msgid "Your disguise won't fool me, SKELETON!"
msgstr ""

#. Id: 40782-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (40781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40782-Dialogue.mod:0
msgid "The Skeletons must be burned, purged in the flame of Okran!"
msgstr ""

#. Id: 40782-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (40781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40782-Dialogue.mod:1
msgid "The robots must be purged, destroyed in the flame of Okran!"
msgstr ""

#. Id: 40782-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40782-Dialogue.mod:2
msgid "On my life, you'll be purged in the flame of Okran! "
msgstr ""

#. Id: 40782-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40781-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 40782-Dialogue.mod:3
msgid "In Okran's name, I will purge your evil from this world!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:0
msgid "You!... I won't be beaten again!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:1
msgid "You again!... You beat me once, but not this time..."
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:2
msgid "You again!... This time you won't be so lucky..."
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:3
msgid "You!... I won't let you defeat me again! By the will of Okran!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:4
msgid "You won't beat me so easily this time, /PLAYERNAME/!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:5
msgid "I won't go down so easily this time, /PLAYERNAME/!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:6
msgid "Let's try this again, /PLAYERNAME/! Your luck's run out!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:7
msgid "To harm a holy apostle of Okran, you truly are evil, /PLAYERNAME/!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:8
msgid "You!... I won't let you defeat me again! With god as my witness!"
msgstr ""

#. Id: 41125-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41125-Dialogue.mod:9
msgid "Light is on my side. I won't be beaten this time, /PLAYERNAME/."
msgstr ""

#. Id: 41127-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41127-Dialogue.mod:0
msgid "Looks like I didn't finish the job last time! This time you won't be so lucky."
msgstr ""

#. Id: 41127-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41127-Dialogue.mod:1
msgid "Still alive... This time I won't let you survive."
msgstr ""

#. Id: 41127-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41127-Dialogue.mod:2
msgid "So, you survived our last encounter... This time you won't live, /PLAYERNAME/."
msgstr ""

#. Id: 41127-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41127-Dialogue.mod:3
msgid "The darkness still lingers... This time I'll finish you for good, /PLAYERNAME/..."
msgstr ""

#. Id: 41127-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41127-Dialogue.mod:4
msgid "Persistent one, aren't you, /PLAYERNAME/?... This time I'll be sure to finish you."
msgstr ""

#. Id: 41381-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41381-Dialogue.mod:0
msgid "Darkness can only truly be destroyed with fire. It is stubborn. "
msgstr ""

#. Id: 41381-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41381-Dialogue.mod:1
msgid "The darkened can only truly be destroyed in flame. Evil is a persistant thing."
msgstr ""

#. Id: 41381-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#: 41381-Dialogue.mod:2
msgid "All darkened beings must be purged in flame. It is the only way to rid them of these /SACRED/ lands"
msgstr ""

#. Id: 41382-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41381-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41382-Dialogue.mod:0
msgid "But you won't survive our encounter this time, /PLAYERNAME/. I'll make sure of it."
msgstr ""

#. Id: 41382-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41381-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41382-Dialogue.mod:1
msgid "But you won't pull through this time, /PLAYERNAME/. I'll see to it with my own sword."
msgstr ""

#. Id: 41382-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41381-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41382-Dialogue.mod:2
msgid "But not this time, /PLAYERNAME/. This time you won't survive our meeting."
msgstr ""

#. Id: 41383-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41382-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41383-Dialogue.mod:0
msgid "Be purged, dark one!"
msgstr ""

#. Id: 41383-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (41382-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41383-Dialogue.mod:1
msgid "In the name of Okran, be purged, unholy one!"
msgstr ""

#. Id: 41383-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (41382-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41383-Dialogue.mod:2
msgid "In Okran's name, I will purge you from these lands!"
msgstr ""

#. Id: 41383-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (41382-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41383-Dialogue.mod:3
msgid "In Okran's name, I will destroy you!"
msgstr ""

#. Id: 41383-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (41382-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41383-Dialogue.mod:4
msgid "Okran give me strength to purge this darkness! Unholy one be purged!"
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41386-Dialogue.mod:0 59167-Dialogue.mod:0
msgid "Heinous butcherer! You've gone too far, /PLAYERNAME/... no more death."
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41386-Dialogue.mod:1 59167-Dialogue.mod:1
msgid "As Okran's sword, I will bring justice for the murder of his children."
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41386-Dialogue.mod:2 59167-Dialogue.mod:2
msgid "For the slaughter of my fellow Okranites, I sentence you to death!"
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41386-Dialogue.mod:3 59167-Dialogue.mod:3
msgid "Evil murderer!"
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#: 41386-Dialogue.mod:4 59167-Dialogue.mod:4
msgid "Bloodthirsty heretic! You will kill no more!"
msgstr ""

#. Id: 41386-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor hunt (major enemy)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#. -- Condition: Who: TARGET, Condition: TARGET_CHARACTER_EXISTS=1, Tag: -1
#. Id: 59167-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
