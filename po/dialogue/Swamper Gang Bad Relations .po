#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#: 7802-Dialogue.mod:1
msgid "Cut /HIM/ down! Make /HIM/ bleed! Make /HIM/ scream!!"
msgstr ""

#. Id: 7802-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#: 7802-Dialogue.mod:2
msgid "Get /HIM/, get /HIM/! Everyone round up!"
msgstr ""

#. Id: 7802-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>2, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#: 7802-Dialogue.mod:3
msgid "This just got serious. Everyone! Attack!"
msgstr "這很嚴重。大家！攻擊！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:0
msgid "No survivors!"
msgstr "沒有倖存者！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:1
msgid "I don't want ANY of 'em gettin' out of this one alive!"
msgstr "我不希望任何一個人活著離開這個人！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:2
msgid "Kill 'em all!"
msgstr "殺光他們！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:3
msgid "Wipe 'em all out like the rats they are!"
msgstr "像老鼠一樣擦乾它們！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:4
msgid "Stay on your toes! You slack and you'll die!"
msgstr "保持警惕！你懈怠，你會死！"

#. Id: 7806-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (7802-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper Gang Bad Relations
#: 7806-Dialogue.mod:5
msgid "No one lives!"
msgstr "沒人能活下來！"

#. Id: 7816-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (7814-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Swamper Gang Bad Relations
#: 7816-Dialogue.mod:0
msgid "No survivors! /ARG/!"
msgstr ""

#. Id: 7816-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (7814-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Swamper Gang Bad Relations
#: 7816-Dialogue.mod:2
msgid "You won't escape us!! Die!"
msgstr "你不會逃避我們！死！"

#. Id: 7816-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (7814-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Swamper Gang Bad Relations
#: 7816-Dialogue.mod:6
msgid "No survivors! /ATTACK/!"
msgstr ""

#. Id: 49539-Dialogue.mod
