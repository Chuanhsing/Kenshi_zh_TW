#. Id: 3307-dialogue-01.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:0
msgid "Let me out, asshole!"
msgstr "讓我出去，混蛋！"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:1
msgid "You'll pay. Oh, you'll fucking pay."
msgstr "你付錢。哦，你他媽的付錢。"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:2
msgid "The fuck did I ever do to you?"
msgstr "他媽的我曾經對你這麼做過嗎？"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:3
msgid "I am not an animal!"
msgstr "我不是動物！"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:4
msgid "Bastards, the lot of you."
msgstr "混蛋，很多你。"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:5
msgid "When they find me, you'll regret all of this."
msgstr "當他們找到我時，你會後悔所有這一切。"

#. Id: 3307-dialogue-01.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Greenlander
#: 3307-dialogue-01.mod:6
msgid "Free me, and maybe I won't kill you."
msgstr "釋放我，也許我不會殺了你。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:0
msgid "Let me go. I'm begging you."
msgstr "讓我走。我求你。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:1
msgid "I'll do anything. Just unlock the door."
msgstr "我會做任何事情。打開門。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:2
msgid "Let me find you someone else. Please!"
msgstr "讓我找到你的其他人。請！"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:3
msgid "Release me and I swear I won't say a word."
msgstr "釋放我，我發誓，我不會說一句話。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:4
msgid "This is it, isn't it."
msgstr "就是這樣，不是嗎。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:5
msgid "I'm at your mercy."
msgstr "我很憐憫。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#. Id: 57362-rebirth.mod
#. -- Index: 9.0
#. -- Parent line: 8 (57361-rebirth.mod)
#. -- Speaker: Me (0)
#. -- Group title: beep talk to recruit
#: 3308-dialogue-01.mod:6 57362-rebirth.mod:0
msgid "I have nothing."
msgstr "我一無所有。"

#. Id: 3308-dialogue-01.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: captured - meets imprisoner
#. -- Condition: My Race(s): Hive Prince
#: 3308-dialogue-01.mod:7
msgid "Will you at least make it quick?"
msgstr "你至少會快點嗎？"
