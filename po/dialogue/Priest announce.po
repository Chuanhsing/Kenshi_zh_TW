#: 99010-Dialogue.mod:0
msgid "Divine blessings to you, brothers!"
msgstr ""

#. Id: 99001-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (99010-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99001-Dialogue.mod:0
msgid "Today is a glorious day!"
msgstr ""

#. Id: 99004-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (99001-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99004-Dialogue.mod:0
msgid "Even better that it is Prayer Day..."
msgstr ""

#. Id: 99005-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (99004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99005-Dialogue.mod:0
msgid "I travel from the city with my faithful paladins..."
msgstr ""

#. Id: 99006-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (99005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99006-Dialogue.mod:0
msgid "So that we can pray together..."
msgstr ""

#. Id: 99007-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (99006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99007-Dialogue.mod:0
msgid "I bring the church to your very gates, my beloved brothers!"
msgstr ""

#. Id: 99008-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (99007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99008-Dialogue.mod:0
msgid "Come to me..."
msgstr ""

#. Id: 99009-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (99008-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99009-Dialogue.mod:0
msgid "Come and pray with me on this beautiful day..."
msgstr ""

#. Id: 99013-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (99012-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Priest announce
#: 99013-Dialogue.mod:0
msgid "The priest requests your audience, brothers. Come."
msgstr ""

#. Id: 99014-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (99013-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Priest announce
#: 99014-Dialogue.mod:0
msgid "Do you hear?"
msgstr ""

#. Id: 99015-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (99014-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Priest announce
#: 99015-Dialogue.mod:0
msgid "... A moment more, sir paladin, if you will..."
msgstr ""

#. Id: 99018-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (99017-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Priest announce
#: 99018-Dialogue.mod:0
msgid "To ignore the request of a holy priest is looked upon as sacrilege, my brothers..."
msgstr ""

#. Id: 99019-Dialogue.mod
#. -- Index: 16.0
#. -- Parent line: 15 (99018-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Priest announce
#: 99019-Dialogue.mod:0
msgid "I warn you once, and only once!"
msgstr ""

