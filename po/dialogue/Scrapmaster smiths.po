#. Id: 53031-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53043-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53031-Dialogue.mod:0
msgid "Oh? Another nice lady customer. Don't drop your hot tongs on your groin this time, eh?"
msgstr ""

#. Id: 53033-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (53032-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53033-Dialogue.mod:0
msgid "Ahahaha! Never fails to make me laugh!"
msgstr ""

#. Id: 53034-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (53033-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53034-Dialogue.mod:0
msgid "You... recovered yet?"
msgstr ""

#. Id: 53035-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (53034-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53035-Dialogue.mod:0
msgid "Best not to ask, eh? It's a 'sensitive' subject. Heh."
msgstr ""

#. Id: 53044-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (53035-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53044-Dialogue.mod:0
msgid "Stop flapping those lips and start hammering that steel, slackers!"
msgstr ""

#. Id: 53045-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (53044-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53045-Dialogue.mod:0
msgid "Uh- yes, boss. On it."
msgstr ""

#. Id: 53046-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (53045-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53046-Dialogue.mod:0
msgid "... Ya bossy old hag..."
msgstr ""

#. Id: 53047-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (53046-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53047-Dialogue.mod:0
msgid "Hm? Say something?"
msgstr ""

#. Id: 53048-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (53047-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53048-Dialogue.mod:0
msgid "I said YES BOSS. ON IT."
msgstr ""

#. Id: 53049-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (53048-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53049-Dialogue.mod:0
msgid "... Oh."
msgstr ""

#. Id: 53050-Dialogue.mod
#. -- Index: 15.0
#. -- Parent line: 14 (53049-Dialogue.mod)
#. -- Speaker: Extra 3 (4)
#. -- Group title: Scrapmaster smiths
#: 53050-Dialogue.mod:0
msgid "... Ya deaf dustbag..."
msgstr ""

#. Id: 53053-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53053-Dialogue.mod:0
msgid "Oh if I had a penny for every rusty piece of crap sword I see sheathed on the back of a clueless wanderer..."
msgstr ""

#. Id: 53054-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53055-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53054-Dialogue.mod:0
msgid "*Sigh* Here we go again..."
msgstr ""

#. Id: 53056-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (53054-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53056-Dialogue.mod:0
msgid "It takes at least four smiths and at least twenty nights to forge a fine blade worthy of a warrior..."
msgstr ""

#. Id: 53057-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (53058-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Scrapmaster smiths
#: 53057-Dialogue.mod:0
msgid "I'm sure the customer already knows that, boss..."
msgstr ""

#. Id: 53062-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (53057-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53062-Dialogue.mod:0
msgid "Don't even get me started on those Truth Two clowns. Overheat their blades like tough jerky on a bonfire..."
msgstr ""

#. Id: 53061-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (53062-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53061-Dialogue.mod:0
msgid "Alright, boss..."
msgstr ""

#. Id: 53059-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (53061-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53059-Dialogue.mod:0
msgid "One young warrior, thought he'd gotten a bargain. Blade snapped mid fight and took his nose clean off... "
msgstr ""

#. Id: 53064-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (53063-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53064-Dialogue.mod:0
msgid "Yep. Buy cheap, buy twice. And lose a nose."
msgstr ""

#. Id: 53065-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (53064-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Scrapmaster smiths
#: 53065-Dialogue.mod:0
msgid "Think yourself lucky, /OUTLANDER/. We gotta hear this lecture all day, everyday."
msgstr ""

#. Id: 53066-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (53065-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53066-Dialogue.mod:0
msgid "What was that?"
msgstr ""

#. Id: 53067-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (53066-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Scrapmaster smiths
#: 53067-Dialogue.mod:0
msgid "I said it's a PLEASURE TO WORK WITH YOU EVERYDAY."
msgstr ""

#. Id: 53068-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (53067-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Scrapmaster smiths
#: 53068-Dialogue.mod:0
msgid "... Ya toothless windbag..."
msgstr ""

#. Id: 53069-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53069-Dialogue.mod:0
msgid "Watch that heat, boy! Burn it and we start over!"
msgstr ""

#. Id: 53070-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53071-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53070-Dialogue.mod:0
msgid "Yes, boss."
msgstr ""

#. Id: 53072-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (53070-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53072-Dialogue.mod:0
msgid "Treat that hunk of steel as if it were your own child!"
msgstr ""

#. Id: 53073-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (53072-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Scrapmaster smiths
#: 53073-Dialogue.mod:0
msgid "A shiny metal child destined for greatness!"
msgstr ""

#. Id: 53074-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (53073-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Scrapmaster smiths
#: 53074-Dialogue.mod:0
msgid "Uhuh, yes, boss."
msgstr ""

