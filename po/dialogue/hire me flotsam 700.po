#: 50313-Dialogue.mod:1 18216-Recruits?Dialogue.mod:2 50547-Dialogue.mod:0
msgid "You're back?"
msgstr ""

#. Id: 50313-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire me flotsam 700
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<24, Tag: -1
#: 50313-Dialogue.mod:2
msgid "It's you again?"
msgstr ""

#. Id: 50313-Dialogue.mod
#. -- Index: 0.3
