#. Id: 41949-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41949-Dialogue.mod:0
msgid "Eyes down, /PEASANT/!"
msgstr ""

#. Id: 41949-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41949-Dialogue.mod:1
msgid "Why are you looking at me, /RAT/? Eyes down!"
msgstr ""

#. Id: 41949-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41949-Dialogue.mod:2
msgid "You looking at me, /PEASANT/? Eyes on the /DIRT/!"
msgstr ""

#. Id: 41949-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41949-Dialogue.mod:3
msgid "Hey, /WORM/, eyes down. You're on thin ice, remember?"
msgstr ""

#. Id: 41949-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41949-Dialogue.mod:4
msgid "Hey, /WORM/, eyes on the dirt. You're on thin ice round here..."
msgstr ""

#. Id: 41976-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41949-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41976-Dialogue.mod:0
msgid "...y- yes... sorry..."
msgstr ""

#. Id: 41976-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41949-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41976-Dialogue.mod:1
msgid "Of- of course..."
msgstr ""

#. Id: 41976-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41949-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41976-Dialogue.mod:2
msgid "... s- sorry!"
msgstr ""

#. Id: 41950-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41950-Dialogue.mod:0
msgid "You, /PEASANT/! I've had reports that you stole food from the nobility..."
msgstr ""

#. Id: 41951-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41950-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41951-Dialogue.mod:0
msgid "I- I needed to feed my family... Please!"
msgstr ""

#. Id: 41959-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41951-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41959-Dialogue.mod:0
msgid "Aw wahhhh... Do I look like I care? The nobles will have your head for this!"
msgstr ""

#. Id: 41959-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (41951-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41959-Dialogue.mod:1
msgid "Excuses! You DON'T screw with the nobles!"
msgstr ""

#. Id: 41960-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41959-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41960-Dialogue.mod:0
msgid "Noooo! Have mercy! I'll do anything!"
msgstr ""

#. Id: 42373-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (41960-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 42373-Dialogue.mod:0
msgid "Anything, eh? Heheh.. Okay, I'll hold you to that, /WORM/. You owe me one favour..."
msgstr ""

#. Id: 42374-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (42373-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 42374-Dialogue.mod:0
msgid "Ri -right... ah crap..."
msgstr ""

#. Id: 41955-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41950-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41955-Dialogue.mod:0
msgid "What? No... no! I haven't stolen anything!"
msgstr ""

#. Id: 41956-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41955-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41956-Dialogue.mod:0
msgid "Under Empire law, I'm placing you under arrest..."
msgstr ""

#. Id: 41957-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41956-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41957-Dialogue.mod:0
msgid "No! I've been set up! N- nooooo! "
msgstr ""

#. Id: 42370-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (41957-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 42370-Dialogue.mod:0
msgid "Then, I'll help you out. Just pay me the fine, /PEASANT/, and I'll let you off the hook..."
msgstr ""

#. Id: 42371-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (42370-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 42371-Dialogue.mod:0
msgid "O- okay... th- this is everything I have, here..."
msgstr ""

#. Id: 42372-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (42371-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 42372-Dialogue.mod:0
msgid "/TCH/, worthless /WORM/. You're lucky I'm in a good mood today..."
msgstr ""

#. Id: 41952-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41952-Dialogue.mod:0
msgid "Heard you've been pestering the nobility lately, /PEASANT/... This true?"
msgstr ""

#. Id: 41952-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41952-Dialogue.mod:3
msgid "Rumour has it that you've been fraternising with nobility, /PEASANT/..."
msgstr ""

#. Id: 41968-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41952-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#. Id: 9571-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (9569-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bag search (Shek)
#: 41968-Dialogue.mod:0 9571-Dialogue.mod:1
msgid "Wait, I can-"
msgstr ""

#. Id: 41969-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41968-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41969-Dialogue.mod:0
msgid "You know the rules, /WORM/. And you know what happens to troublemakers..."
msgstr ""

#. Id: 41970-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41969-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41970-Dialogue.mod:0
msgid "Wait! Here, um, perhaps a... donation can help clear this up... I promise you, everything is fine..."
msgstr ""

#. Id: 41971-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (41970-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41971-Dialogue.mod:0
msgid "Hum! Okay, move along and keep your nose clean, /WORM/..."
msgstr ""

#. Id: 41953-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41953-Dialogue.mod:0
msgid "Hey, /PEASANT/. I'm hungry."
msgstr ""

#. Id: 41953-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41953-Dialogue.mod:1
msgid "Hooo! I'm ravenous! Hey you, /WORM/!"
msgstr ""

#. Id: 41953-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41953-Dialogue.mod:2
msgid "I'm hungry. Hey you, /WORM/!"
msgstr ""

#. Id: 41953-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41953-Dialogue.mod:3
msgid "Hey, /WORM/, I'm feeling peckish"
msgstr ""

#. Id: 41953-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41953-Dialogue.mod:4
msgid "Hey, /WORM/, I'm hungry"
msgstr ""

#. Id: 41962-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41953-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41962-Dialogue.mod:0
msgid " O- okay, would you like some, er, b-bread?"
msgstr ""

#. Id: 41962-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41953-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41962-Dialogue.mod:1
msgid "... Uh... I- I think I have some bread you can have... /SIR/ "
msgstr ""

#. Id: 41962-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41953-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41962-Dialogue.mod:2
msgid "..Uh!... Here! I have some bread, /SIR/"
msgstr ""

#. Id: 41962-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (41953-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41962-Dialogue.mod:3
msgid "... I.. uh.. think I might have some food... /SIR/ "
msgstr ""

#. Id: 41963-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (41962-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41963-Dialogue.mod:0
msgid "... A lousy roll of bread? Is that all I get for protecting your butt?!"
msgstr ""

#. Id: 41964-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41963-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire guard bullying peasant
#: 41964-Dialogue.mod:0
msgid "I-I'm sorry! It's all I have to spare!"
msgstr ""

#. Id: 41965-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (41964-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire guard bullying peasant
#: 41965-Dialogue.mod:0
msgid "Worthless /GODDAMN/ /WORM/! Just you watch yourself!"
msgstr ""

