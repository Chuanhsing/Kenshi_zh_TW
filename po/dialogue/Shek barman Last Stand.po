#. Id: 51536-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (51534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51536-Dialogue.mod:0
msgid "Mmh, this place?... A fortress."
msgstr ""

#. Id: 51537-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51537-Dialogue.mod:0
msgid "... And...?"
msgstr ""

#. Id: 51544-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (51537-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51544-Dialogue.mod:0
msgid "Defence. It is here in this fortress that we hold the borders against the bugmaster."
msgstr ""

#. Id: 51538-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51538-Dialogue.mod:0
msgid "Is there a history to the fortress?"
msgstr ""

#. Id: 51545-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (51538-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51545-Dialogue.mod:0
msgid "Mmh, a glorious one. Here we mark where the great Kral died, in a battle against one hundred swordsmen. To walk upon this soil, it is an honor for all..."
msgstr ""

#. Id: 51548-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (51547-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51548-Dialogue.mod:0
msgid "Mmh. Kral."
msgstr ""

#. Id: 51552-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51548-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51552-Dialogue.mod:0
msgid "This is like pulling teeth..."
msgstr ""

#. Id: 51539-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51539-Dialogue.mod:0
msgid "Why is it here?"
msgstr ""

#. Id: 51546-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (51539-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51546-Dialogue.mod:0
msgid "Pest control. It is here in this fortress that we hold the borders against the bugmaster."
msgstr ""

#. Id: 51549-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (51546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51549-Dialogue.mod:0
msgid "Bugmaster?"
msgstr ""

#. Id: 51553-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (51549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51553-Dialogue.mod:0
msgid "Mmh. Bugmaster."
msgstr ""

#. Id: 51551-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (51553-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51551-Dialogue.mod:0
msgid "You're the worst barman ever..."
msgstr ""

#. Id: 51554-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (51546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51554-Dialogue.mod:0
msgid "Why are there so many old people here?"
msgstr ""

#. Id: 51540-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51540-Dialogue.mod:0
msgid "It looks more like a town to me..."
msgstr ""

#. Id: 51555-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (51540-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51555-Dialogue.mod:0
msgid "Mmh, a town. To accommodate the Shek Elders that visit."
msgstr ""

#. Id: 51556-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (51555-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51556-Dialogue.mod:0
msgid "What do the elders do here?"
msgstr ""

#. Id: 51557-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (51556-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek barman Last Stand
#: 51557-Dialogue.mod:0
msgid "... Passing. They come to die as Kral did. To find death in battle against the bugmaster."
msgstr ""

#. Id: 51541-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (51536-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek barman Last Stand
#: 51541-Dialogue.mod:0
msgid "Forget it, let's just trade instead"
msgstr ""

