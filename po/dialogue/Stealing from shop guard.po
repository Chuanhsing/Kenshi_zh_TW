#. Id: 14356-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#: 14356-Dialogue?(4).mod:1
msgid "Drop it!"
msgstr "算了吧！"

#. Id: 14356-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#: 14356-Dialogue?(4).mod:3
msgid "Get the hell out of here, thief!"
msgstr "小偷離開這兒吧！"

#. Id: 14356-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#: 14356-Dialogue?(4).mod:4
msgid "Get the hell away from there, shoplifter!"
msgstr "從那裡開始，扒手！"

#. Id: 14356-Dialogue (4).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#: 14356-Dialogue?(4).mod:5
msgid "Get away from there, thief!"
msgstr "小偷離開那裡！"

#. Id: 14356-Dialogue (4).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#: 14356-Dialogue?(4).mod:6
msgid "Don't even think about it, thief!"
msgstr "小伙子甚至不去想它！"

#. Id: 14359-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 14359-Dialogue?(4).mod:0
msgid "Shoplifters get broken fingers!"
msgstr "扒手手指斷了！"

#. Id: 14359-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 14359-Dialogue?(4).mod:1
msgid "Thieves get their fingers broken!"
msgstr "小偷把他們的手指弄壞了！"

#. Id: 14359-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 14359-Dialogue?(4).mod:2
msgid "I'll break your fingers, thief!"
msgstr "我會打破你的手指，小偷！"

#. Id: 14359-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 14359-Dialogue?(4).mod:3
msgid "Get your /GODDAMN/ hands off of that, thief!"
msgstr ""

#. Id: 14359-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 14359-Dialogue?(4).mod:4
msgid "Get your /GREASY/ /GODDAMN/ hands off of that!"
msgstr ""

#. Id: 14360-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14360-Dialogue?(4).mod:0
msgid "Get out, thief!"
msgstr "滾出去，小偷！"

#. Id: 14360-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14360-Dialogue?(4).mod:2
msgid "You're going down, thief!"
msgstr "你要下山了，小偷！"

#. Id: 14367-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 14367-Dialogue?(4).mod:0
msgid "Try and steal from there, I dare you!"
msgstr "試試並從那裡偷，我敢你！"

#. Id: 14367-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 14367-Dialogue?(4).mod:1
msgid "Try it!... go ahead and try it, thief!"
msgstr "試試吧！...繼續嘗試吧，小偷！"
