#. Id: 45397-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 45397-Dialogue.mod:0
msgid "Yeah that's it, /GETLOST/, I ain't takin' visitors..."
msgstr ""

#. Id: 45397-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 45397-Dialogue.mod:1
msgid "Good /BOY2/, make your way out..."
msgstr ""

#. Id: 45397-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 45397-Dialogue.mod:2
msgid "Yeah, door's that way, eh..."
msgstr "是的，門就是這樣，呃......"

#. Id: 45397-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 45397-Dialogue.mod:3
msgid "Good, that's it, get movin'..."
msgstr "很好，就是這樣，快走......"

#. Id: 45397-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 45397-Dialogue.mod:4
msgid "Yeah, go on, get goin'..."
msgstr "是的，繼續，開始吧......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:0
msgid "Hurry it up, I need to get some shut eye..."
msgstr "快點，我需要閉上眼睛......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:1
msgid "C'mon shoo, it's late, I gotta get my beauty sleep in..."
msgstr "來吧，已經很晚了，我得睡個好覺......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:2
msgid "C'mon, shoo, I got places to be..."
msgstr "來吧，噓，我有地方......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:3
msgid "C'mon already, I gotta close up shop..."
msgstr "來吧，我必須關閉店鋪......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:4
msgid "You need to leave already, c'mon..."
msgstr "你需要離開，來吧......"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:5
msgid "What part of 'we're closing' don't you understand, eh?"
msgstr "“我們關閉”的哪一部分你不理解，是嗎？"

#. Id: 45403-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:6
msgid "/OI/, I'm talkin' to you. Leave."
msgstr ""

#. Id: 45403-Dialogue.mod
#. -- Index: 2.7
#. -- Parent line: 1 (12479-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#: 45403-Dialogue.mod:7
msgid "/OI/, you got problems hearin' me? /GETLOST/..."
msgstr ""

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:0
msgid "/OI/, who are you?!"
msgstr ""

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:1
msgid "/OI/, what are you doing in here?!"
msgstr ""

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:3
msgid "What are you doin' here?"
msgstr "你在這做什麼？"

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:4
msgid "An' who might you be?"
msgstr "一個'你可能是誰？"

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:5
msgid "Who are you, why are you here?"
msgstr "你是誰，你為什麼來這裡？"

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#: 12481-Dialogue?(10)?(2).mod:6
msgid "What are you doin' in here?"
msgstr "你在這做什麼？"

#. Id: 12481-Dialogue (10) (2).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: IS_A_TRADER=0, Tag: -1
#. Id: 13809-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Thug witness assault
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12481-Dialogue?(10)?(2).mod:8 13809-Escapes.mod:3
msgid "Er?"
msgstr "呃？"

#. Id: 12482-Dialogue (10) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12482-Dialogue?(10)?(2).mod:0
msgid "Get outta my house!"
msgstr "離開我的房子！"

#. Id: 12482-Dialogue (10) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12482-Dialogue?(10)?(2).mod:1
msgid "Get outta it!"
msgstr "搞定！"

#. Id: 12482-Dialogue (10) (2).mod
#. -- Index: 1.2
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12482-Dialogue?(10)?(2).mod:2
msgid "Get outta here!"
msgstr "你出去！"

#. Id: 12482-Dialogue (10) (2).mod
#. -- Index: 1.3
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12482-Dialogue?(10)?(2).mod:3
msgid "This' my house! PSSHT!"
msgstr "這是'我的房子！ PSSHT！"

#. Id: 42302-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42302-Dialogue.mod:0
msgid "/OI/, yer ears need fixin'?"
msgstr ""

#. Id: 42302-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42302-Dialogue.mod:1
msgid "/OI/, /IDIOT/! You hearin' me? Yer brains need fixin'?"
msgstr ""

#. Id: 42302-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42302-Dialogue.mod:2
msgid "/OI/, /INSULT/! Somethin' wrong with your ears?"
msgstr ""

#. Id: 42302-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42302-Dialogue.mod:3
msgid "Well, well... Looks like yer EARS must be busted up..."
msgstr "嗯，好吧......看起來你的EARS必須被淘汰......"

#. Id: 42303-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42302-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42303-Dialogue.mod:0
msgid "I'll fix 'em for ya!"
msgstr "我會為你解決它們！"

#. Id: 42303-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (42302-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42303-Dialogue.mod:1
msgid "Let me fix 'em up for ya!"
msgstr "讓我來解決你的問題！"

#. Id: 42303-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (42302-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42303-Dialogue.mod:2
msgid "If it don't work, /PUNCH/ it 'til it does I always say!"
msgstr ""

#. Id: 42304-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42304-Dialogue.mod:0
msgid "Cocky /INSULT/, ain'tcha?"
msgstr ""

#. Id: 42304-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42304-Dialogue.mod:1
msgid "Still here? Wanna fight about it, eh?"
msgstr "還在？想要爭吵，是嗎？"

#. Id: 42304-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42304-Dialogue.mod:2
msgid "You must be lookin' for a /STABBIN/!"
msgstr ""

#. Id: 42304-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42304-Dialogue.mod:3
msgid "Why the hell is your ugly face still here?!"
msgstr "為什麼你的醜陋臉仍然在這裡？！"

#. Id: 42304-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (12482-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42304-Dialogue.mod:4
msgid "The hell! Do I gotta teach you some manners, /IDIOT/?!"
msgstr ""

#. Id: 42306-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (42304-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42306-Dialogue.mod:1
msgid "This' MY roof!"
msgstr "這個'我的屋頂！"

#. Id: 12484-Dialogue (10) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12484-Dialogue?(10)?(2).mod:0
msgid "I ain't a damn Flophouse, get out"
msgstr "我不是一個該死的Flophouse，滾出去"

#. Id: 12484-Dialogue (10) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12484-Dialogue?(10)?(2).mod:1
msgid "This ain't a damn Flophouse, /GETLOST/"
msgstr ""

#. Id: 12484-Dialogue (10) (2).mod
#. -- Index: 1.2
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12484-Dialogue?(10)?(2).mod:2
msgid "Bar's that way, get outta my house"
msgstr "酒吧那樣，走出我的房子"

#. Id: 12484-Dialogue (10) (2).mod
#. -- Index: 1.3
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12484-Dialogue?(10)?(2).mod:3
msgid "The hell do you think this is? Get outta my house"
msgstr "你覺得這到底是怎麼回事？離開我的房子"

#. Id: 12484-Dialogue (10) (2).mod
#. -- Index: 1.4
#. -- Parent line: 0 (12481-Dialogue (10) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12484-Dialogue?(10)?(2).mod:4
msgid "You're in my /FUCKING/ house"
msgstr ""

#. Id: 42307-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (14798-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42307-Dialogue.mod:0
msgid "Ohhh so it's like THAT is it! My knife's been itchin' for a fight!"
msgstr "哦，這就像是它！我的刀一直在爭吵！"

#. Id: 42307-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (14798-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42307-Dialogue.mod:1
msgid "Ohoho! I love a good knife fight anyway!"
msgstr "Ohoho！無論如何，我喜歡一個好的刀戰！"

#. Id: 42307-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (14798-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42307-Dialogue.mod:2
msgid "Come on then, knifey! We got a /INSULT/ needs /STABBIN/!"
msgstr ""

#. Id: 42307-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (14798-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper polite shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42307-Dialogue.mod:3
msgid "/WHATTHEFUCK/! Wise guy, eh? Come 'ere!"
msgstr ""
