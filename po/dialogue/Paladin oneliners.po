#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 57099-Dialogue.mod:2
msgid "A sister purified..."
msgstr "姐姐淨化了......"

#. Id: 57099-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Paladin oneliners
#. -- Condition: Who: TARGET, Condition: RELATIONS>75, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=0, Tag: -1
#: 57099-Dialogue.mod:3
msgid "Can it really be... a sister purified..."
msgstr "它真的可以......一個姐姐淨化了......"

#. Id: 57099-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
