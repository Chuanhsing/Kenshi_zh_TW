#. Id: 40789-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#. Id: 41488-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Priest sees robot/ beast
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 40789-Dialogue.mod:4 41488-Dialogue.mod:4
msgid "Darkness from the abyss!"
msgstr "深淵的黑暗！"

#. Id: 40790-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40789-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40790-Dialogue.mod:0
msgid "You won't escape Okran's purging!"
msgstr "你不會逃避奧克蘭的清洗！"

#. Id: 40790-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (40789-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40790-Dialogue.mod:6
msgid "You will not evade my sword, by Okran's will!"
msgstr "按照奧克蘭的意願，你不會逃避我的劍！"

#. Id: 40791-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40791-Dialogue.mod:0
msgid "The agents of darkness must be burned!"
msgstr "必須燒掉黑暗的代理人！"

#. Id: 40791-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40791-Dialogue.mod:2
msgid "The Skeletons will not prevail!"
msgstr "骷髏不會佔上風！"

#. Id: 40791-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40791-Dialogue.mod:3
msgid "Your reign of darkness must be stopped!"
msgstr "你的黑暗統治必須停止！"

#. Id: 40791-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40791-Dialogue.mod:4
msgid "Burn away all the darkness! Destroy /HIM/!"
msgstr ""

#. Id: 40791-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40791-Dialogue.mod:5
msgid "Burn away the darkness! Burn /HIM/ in flame!"
msgstr ""

#. Id: 40792-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (40790-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#: 40792-Dialogue.mod:1
msgid "/BURNHIM/, /BURNHIM/!"
msgstr ""

#. Id: 41132-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41132-Dialogue.mod:0
msgid "You!... This time I'll make sure you don't survive!"
msgstr "你！...這次我會確保你不活下去！"

#. Id: 41132-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41132-Dialogue.mod:1
msgid "Still alive?!.. I won't let you live this time!"
msgstr "還活著？！我這次不會讓你活下去！"

#. Id: 41132-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41132-Dialogue.mod:2
msgid "You survived?!... I failed Okran... This time you can't survive..."
msgstr "你活了下來？！......我失敗了Okran ......這次你無法生存......"

#. Id: 41134-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41134-Dialogue.mod:0
msgid "Defeated by a Skeleton.. You'll burn for this!"
msgstr "被骷髏擊敗..你會為此而燃燒！"

#. Id: 41134-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41134-Dialogue.mod:1
msgid "I won't be beaten by a Skeleton... I'll destroy you..."
msgstr "我不會被骷髏擊敗......我會摧毀你......"

#. Id: 41134-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor spots skeleton
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 41134-Dialogue.mod:2
msgid "This time I won't be defeated"
msgstr "這次我不會被打敗"
