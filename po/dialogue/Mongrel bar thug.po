#. Id: 47325-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47325-Dialogue.mod:0
msgid "Being stuck in this godforsaken place, it's enough to drive you stir crazy... take a seat, /OUTLANDER/. You're gonna be here a while."
msgstr ""

#. Id: 47326-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47325-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47326-Dialogue.mod:0
msgid "Stuck? Why can't you leave?"
msgstr ""

#. Id: 47328-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47326-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47328-Dialogue.mod:0
msgid "The Deathyards, /OUTLANDER/, you'd have to be out of your mind to set foot in those cursed valleys. I came here to escape prison... But turns out, this is just prison in another guise, more hellish than the last."
msgstr ""

#. Id: 47329-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (47328-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47329-Dialogue.mod:0
msgid "How about you escape here, with me?"
msgstr ""

#. Id: 47330-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (47329-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47330-Dialogue.mod:0
msgid "What are you crazy!? No one goes into the fog and comes out alive, only the lucky make it to Mongrel... Donâ€™t tempt fate, newbie."
msgstr ""

#. Id: 47331-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (47330-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47331-Dialogue.mod:0
msgid "Are you sure you don't want to join me?"
msgstr ""

#. Id: 47332-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (47331-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47332-Dialogue.mod:0
msgid "No means no..."
msgstr ""

#. Id: 47333-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (47332-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47333-Dialogue.mod:0
msgid "How about you go harrass somebody else?"
msgstr ""

#. Id: 47645-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (47330-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47645-Dialogue.mod:0
msgid "Or don't escape with me then..."
msgstr ""

#. Id: 47646-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (47645-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47646-Dialogue.mod:0
msgid "Don't do it, newbie... Don't go into the fog..."
msgstr ""

#. Id: 47640-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (47328-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47640-Dialogue.mod:0
msgid "What's so bad about the Deathyards?"
msgstr ""

#. Id: 47642-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (47640-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47642-Dialogue.mod:0
msgid "People who survive the Deathyard, they don't talk about the Deathyard. I ain't goin' back out there and I suggest you don't neither..."
msgstr ""

#. Id: 47643-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (47642-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47643-Dialogue.mod:0
msgid "Good luck sitting it out here then..."
msgstr ""

#. Id: 47647-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (47643-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Mongrel bar thug
#: 47647-Dialogue.mod:0
msgid "Nobody leaves Mongrel, /OUTLANDER/..."
msgstr ""

#. Id: 47644-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (47642-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47644-Dialogue.mod:0
msgid "Well, I came here and I'll leave here..."
msgstr ""

#. Id: 47641-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (47328-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Mongrel bar thug
#: 47641-Dialogue.mod:0
msgid "I ain't scared of no Deathyards..."
msgstr ""

