#. -- Condition: Who: ME, Condition: IN_MY_BUILDING=1, Tag: -1
#: 49687-Dialogue.mod:0
msgid "Do not speak to me, wretched woman"
msgstr ""

#. Id: 49687-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor heretic bounty
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_MY_BUILDING=1, Tag: -1
#: 49687-Dialogue.mod:1
msgid "You do not approach a holy inquisitor, /TEMPTRESS/."
msgstr ""

#. Id: 49687-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor heretic bounty
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IN_MY_BUILDING=1, Tag: -1
#: 49687-Dialogue.mod:2
msgid "It is improper to approach your holy brother in this way, woman."
msgstr ""

