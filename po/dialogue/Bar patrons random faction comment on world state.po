msgstr ""

#. Id: 96096-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (96095-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#: 96096-Dialogue.mod:0
msgid "What I'm sayin' is..."
msgstr ""

#. Id: 96097-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (96096-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#: 96097-Dialogue.mod:0
msgid "That stuff inside Twomoon, it looked real similar..."
msgstr ""

#. Id: 96098-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (96097-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Bar patrons random faction comment on world state
#: 96098-Dialogue.mod:0
msgid "... I don't under-... Wait... No..."
msgstr ""

#. Id: 96100-Dialogue.mod
#. -- Index: 13.0
#. -- Parent line: 12 (96098-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#: 96100-Dialogue.mod:0
msgid "Just sayin'."
msgstr ""

#. Id: 96099-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (96100-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Bar patrons random faction comment on world state
#: 96099-Dialogue.mod:0
msgid "Noooooo...!?"
msgstr ""

