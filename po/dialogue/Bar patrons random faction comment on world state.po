#. Id: 62288-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62287-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Bar patrons random faction comment on world state
#: 62288-Dialogue.mod:0
msgid "Damn shame."
msgstr ""

#. Id: 62289-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62288-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#: 62289-Dialogue.mod:0
msgid "Only a matter of time before the slavers start crossin' borders into the swamps..."
msgstr ""

#. Id: 62290-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62289-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Bar patrons random faction comment on world state
#: 62290-Dialogue.mod:0
msgid "Let 'em try!"
msgstr ""

#. Id: 62291-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62290-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Bar patrons random faction comment on world state
#: 62291-Dialogue.mod:0
msgid "The swampers ain't pushovers like those desert crawlers."
msgstr ""

#. Id: 62286-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#. -- Condition: In town(s) of faction(s): Fisher
