#. Id: 43624-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: The nobles are coming!
#: 43624-Dialogue.mod:0
msgid "Move, rats!"
msgstr "移動，老鼠！"

#. Id: 43624-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: The nobles are coming!
#: 43624-Dialogue.mod:1
msgid "Move it, dregs!"
msgstr "移動它，渣滓！"

#. Id: 43624-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: The nobles are coming!
#: 43624-Dialogue.mod:2
msgid "Move aside, peasants."
msgstr "離開農民。"

#. Id: 43624-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: The nobles are coming!
#: 43624-Dialogue.mod:3
msgid "Stand aside, dregs."
msgstr "站在一邊，渣滓。"

#. Id: 43624-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: The nobles are coming!
#: 43624-Dialogue.mod:4
msgid "Eurhhh, the stench of peasantry..."
msgstr "Eurhhh，農民的惡臭......"

#. Id: 16484-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 16484-Dialogue.mod:0
msgid "/DAMN/! Nobles..."
msgstr ""

#. Id: 16484-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 16484-Dialogue.mod:1
msgid "Nobles! I'm outta here..."
msgstr "貴族！我要出去了..."

#. Id: 16484-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 16484-Dialogue.mod:2
msgid "Nobles! I'm gettin' the hell outta here..."
msgstr "貴族！我在這裡開始了......"

#. Id: 16484-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 16484-Dialogue.mod:3
msgid "No... no! Nobles... I gotta get out of here"
msgstr "不，不！貴族......我得走出這裡"

#. Id: 16481-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 16481-Dialogue.mod:0
msgid "Watch out, the nobles are coming..."
msgstr "小心，貴族們即將到來......"

#. Id: 16481-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 16481-Dialogue.mod:1
msgid "Watch it. Nobility's on it's way over"
msgstr "看它。貴族就在它的路上"

#. Id: 16481-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 16481-Dialogue.mod:2
msgid "Watch yourself. Nobles heading this way."
msgstr "注意自己。貴族走這條路。"

#. Id: 16490-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (16481-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16490-Dialogue.mod:0
msgid "Trust me, you don't wanna get mixed up with the nobility"
msgstr "相信我，你不想與貴族混在一起"

#. Id: 16490-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (16481-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16490-Dialogue.mod:1
msgid "Stay well away from the nobility"
msgstr "遠離貴族"

#. Id: 16480-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:0
msgid "/GODDAMN/, the nobles are coming!"
msgstr ""

#. Id: 16480-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:1
msgid "/GODDAMN/, the nobles are heading this way!"
msgstr ""

#. Id: 16480-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:2
msgid "/GODDAMN/, nobility's heading this way!"
msgstr ""

#. Id: 16480-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:3
msgid "Oh god, the noble's are coming!"
msgstr "天啊，貴族的到來！"

#. Id: 16480-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:4
msgid "The noble's are coming!"
msgstr "貴族的到來！"

#. Id: 16480-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (43624-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: The nobles are coming!
#: 16480-Dialogue.mod:5
msgid "The nobles! Run!"
msgstr "貴族們！跑！"
