#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:0
msgid "Wha... Stop... St- stop doing that!"
msgstr "什麼......停止......停止這樣做！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:1
msgid "Wh- what are you doing?!"
msgstr "什麼，你在做什麼？！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:2
msgid "N- n- no! Don't do that!"
msgstr "N  - 沒有！不要那樣做！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:3
msgid "W- wait! Leave me alone!"
msgstr "等等！請別打擾我！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:4
msgid "...No! No, stop!"
msgstr "...沒有！不停！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:5
msgid "Stop! No!"
msgstr "停止！沒有！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:8
msgid "Stop! What are you doing?"
msgstr "停止！你在做什麼？"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:9
msgid "No! Don't be a fool!"
msgstr "沒有！別傻了！"

#. Id: 14075-Dialogue (12).mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14075-Dialogue?(12).mod:10
msgid "Stop, you fool!"
msgstr "停下來，你這個笨蛋！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:0
msgid "The masters! Th- the masters will punish us!"
msgstr "大師們！主人會懲罰我們！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.1
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:1
msgid "The masters will catch you!"
msgstr "大師會抓住你！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.2
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:2
msgid "The masters will see!"
msgstr "大師們會看到！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.3
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:3
msgid "Th- they'll punish us!"
msgstr "他們會懲罰我們！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.4
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:4
msgid "They'll kill us both if they catch you!"
msgstr "如果他們抓住你，他們會殺了我們兩個！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.5
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:5
msgid "They'll punish us both!"
msgstr "他們會懲罰我們兩個！"

#. Id: 14076-Dialogue (12).mod
#. -- Index: 1.6
#. -- Parent line: 0 (14075-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14076-Dialogue?(12).mod:6
msgid "They'll torture us BOTH for this!"
msgstr "他們會為此折磨我們！"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:0
msgid "No.. stop, they'll see..."
msgstr "不......停下來，他們會看到......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:1
msgid "No... you'll get us both in trouble..."
msgstr "不......你會讓我們兩個都陷入困境......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:3
msgid "Wait... you can't do th- that..."
msgstr "等等......你做不到......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:5
msgid "Wait... the masters..."
msgstr "等等......主人......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:6
msgid "No... they'll punish us..."
msgstr "不......他們會懲罰我們......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:7
msgid "Wh- what about the masters?"
msgstr "什麼關於大師？"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:8
msgid "Th- the masters... they'll catch you..."
msgstr "主人......他們會抓住你......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:9
msgid "But... the masters..."
msgstr "但是......主人......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:10
msgid "Y- you..."
msgstr "你 - 你......"

#. Id: 14077-Dialogue (12).mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 14077-Dialogue?(12).mod:11
msgid "Wh- what if they see you?"
msgstr "如果他們看到你怎麼辦？"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:0
msgid "Don't let them catch you, they'll torture us both..."
msgstr "不要讓他們抓住你，他們會折磨我們兩個......"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:2
msgid "Please... let me out..."
msgstr "請...讓我出去......"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:3
msgid "I... want to be free again..."
msgstr "我......想再次獲得自由......"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:4
msgid "What... what if they see?"
msgstr "什麼......如果他們看到了什麼？"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:5
msgid "... Wait... what if the masters see you..?"
msgstr "......等等......如果主人見到你怎麼辦？"

#. Id: 14078-Dialogue (12).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 14078-Dialogue?(12).mod:6
msgid "Careful, they'll punish us both if they see..."
msgstr "小心，如果他們看到他們，他們會懲罰我們兩個......"

#. Id: 14079-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14079-Dialogue?(12).mod:1
msgid "Help me"
msgstr "幫我"

#. Id: 14079-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14079-Dialogue?(12).mod:2
msgid "Get me out of here..."
msgstr "帶我離開這裡..."

#. Id: 14079-Dialogue (12).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14079-Dialogue?(12).mod:3
msgid "Take me with you"
msgstr "帶我一起走"

#. Id: 14080-Dialogue (12).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 14080-Dialogue?(12).mod:1
msgid "Yes..."
msgstr "是..."

#. Id: 14080-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 14080-Dialogue?(12).mod:2
msgid "Help..."
msgstr "救命..."

#. Id: 14080-Dialogue (12).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 14080-Dialogue?(12).mod:5
msgid "You're doing it..."
msgstr "你這樣做......"

#. Id: 14173-Dialogue (4).mod
#. -- Index: 1.0
#. -- Parent line: 0 (14082-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14173-Dialogue?(4).mod:0
msgid "I can't take it anymore..."
msgstr "我不能再忍受了......"

#. Id: 14173-Dialogue (4).mod
#. -- Index: 1.1
#. -- Parent line: 0 (14082-Dialogue (12).mod)
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#: 14173-Dialogue?(4).mod:1
msgid "I can't take this anymore..."
msgstr "我不能再拿這個......"

#. Id: 14091-Dialogue (12).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14091-Dialogue?(12).mod:0
msgid "Get me out..."
msgstr "讓我出去......"

#. Id: 14091-Dialogue (12).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14091-Dialogue?(12).mod:2
msgid "Quickly, get it open!"
msgstr "快點，把它打開！"

#. Id: 14091-Dialogue (12).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14091-Dialogue?(12).mod:4
msgid "Hurry, get it open!"
msgstr "快點，把它打開！"
