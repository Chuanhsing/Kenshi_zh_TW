#. Id: 16494-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16494-Dialogue.mod:0
msgid "Out of my way, scum!"
msgstr "在我的方式，渣滓！"

#. Id: 16494-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16494-Dialogue.mod:1
msgid "Out of my way, /PEASANT/!"
msgstr ""

#. Id: 16494-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16494-Dialogue.mod:2
msgid "You're in my way! Move it, /PEASANT/!"
msgstr ""

#. Id: 16500-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (16499-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble outta my way
#: 16500-Dialogue.mod:0
msgid "Show some respect for your lordship, /PEASANT/!"
msgstr ""

#. Id: 16500-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (16499-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble outta my way
#: 16500-Dialogue.mod:1
msgid "Bow down before your lordship, scum!"
msgstr "在你的主權之前鞠躬，渣滓！"

#. Id: 16500-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (16499-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble outta my way
#: 16500-Dialogue.mod:2
msgid "Fall to your lordship's knees, /PEASANT/!"
msgstr ""

#. Id: 41939-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (16500-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41939-Dialogue.mod:0
msgid "Y- yes, your lordship... Sorry, your lordship..."
msgstr "是的，你的主權......抱歉，你的主權......"

#. Id: 41939-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (16500-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41939-Dialogue.mod:1
msgid "Forgive me, your lordship! "
msgstr "請原諒我，你的主權！"

#. Id: 41939-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (16500-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41939-Dialogue.mod:2
msgid "I sh- shouldn't have gotten in y- your way... forgive me, your lordship!"
msgstr "我不應該以你的方式進入......原諒我，你的主權！"

#. Id: 16501-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:0
msgid "Your miserable face depresses me, /PEASANT/!"
msgstr ""

#. Id: 16501-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:1
msgid "Your /DIRTY/ rags offend my nose, commoner /BOY2/!"
msgstr ""

#. Id: 16501-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:2
msgid "Your ugly face outrages me, /PEASANT/!"
msgstr ""

#. Id: 16501-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:3
msgid "Your stench of poverty sickens me, /PEASANT/!"
msgstr ""

#. Id: 16501-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:4
msgid "Your beastly odour nauseates me, /PEASANT/!"
msgstr ""

#. Id: 16501-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16501-Dialogue.mod:5
msgid "Your /DIRTY/ odour outrages me, commoner /BOY2/!"
msgstr ""

#. Id: 16495-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16501-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16495-Dialogue.mod:0
msgid "/GROVEL/ for my forgiveness!"
msgstr ""

#. Id: 16495-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16501-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. Id: 16495-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16501-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#: 16495-Dialogue.mod:1 16495-Dialogue.mod:2
msgid "/GROVEL/ before me!"
msgstr ""

#. Id: 41941-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (16495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41941-Dialogue.mod:0
msgid "Forgive me, your lordship!"
msgstr "請原諒我，你的主權！"

#. Id: 41941-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (16495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41941-Dialogue.mod:1
msgid "Of course, have mercy your lordship!"
msgstr "當然，請憐憫你的主權！"

#. Id: 41941-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (16495-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41941-Dialogue.mod:2
msgid "R-right, forgive me, your lordship!"
msgstr "R-right，原諒我，你的主權！"

#. Id: 41943-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (41941-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41943-Dialogue.mod:0
msgid "I'll take my offensiveness elsewhere!"
msgstr "我會把我的冒犯性帶到別的地方！"

#. Id: 41943-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (41941-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41943-Dialogue.mod:1
msgid "I'm sorry for my offensive presence!"
msgstr "對不起我的進攻存在感到抱歉！"

#. Id: 41943-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (41941-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Noble outta my way
#: 41943-Dialogue.mod:2
msgid "I'm sorry for my ugly existence!"
msgstr "對不起我的醜陋生活感到抱歉！"

#. Id: 53808-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 53808-Dialogue.mod:0
msgid "Had a bath lately, dreg?"
msgstr "最近洗了個澡，渣滓？"

#. Id: 53808-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 53808-Dialogue.mod:1
msgid "Try combing your hair some time, dreg..."
msgstr "嘗試梳理你的頭髮一段時間，dreg ..."

#. Id: 53808-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 53808-Dialogue.mod:2
msgid "Oh dear. Just awful. Awful, awful, awful pants you're wearing..."
msgstr "噢親愛的。太可怕了。你穿的可怕，可怕，可怕的褲子......"

#. Id: 53808-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 53808-Dialogue.mod:3
msgid "Another dreg for the outer city... Who keeps letting these vagrants through the gates?"
msgstr "外城的另一個渣滓......誰讓這些流浪者通過大門？"

#. Id: 53808-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 53808-Dialogue.mod:4
msgid "Ugh. This dreg stinks as bad as /HE/ looks..."
msgstr ""

#. Id: 53809-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Target Race(s): Hive Prince
#: 53809-Dialogue.mod:0
msgid "Ugh, more stick men. Turning the city into a barbarian hive..."
msgstr "呃，更多堅持男人。將這座城市變成了一個野蠻的蜂巢......"

#. Id: 53809-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Target Race(s): Hive Prince
#: 53809-Dialogue.mod:1
msgid "Ugh... More stick men to lower the tone of our great city..."
msgstr "呃...更多堅持男人降低我們偉大城市的基調......"

#. Id: 53809-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble outta my way
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Target Race(s): Hive Prince
#: 53809-Dialogue.mod:2
msgid "Filthy, filthy stick men..."
msgstr "骯髒，骯髒的棍子男人......"
