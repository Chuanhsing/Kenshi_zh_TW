#. Id: 96224-__Southern hive.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: I_HATE_THIS_GUY=0, Tag: -1
#: 96224-__Southern?hive.mod:0
msgid "Welcome, /RACENAMEHIVE/!"
msgstr ""

#. Id: 96225-__Southern hive.mod
#. -- Index: 1.0
#. -- Parent line: 0 (96224-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96225-__Southern?hive.mod:0
msgid "Take a look at our robotics..."
msgstr "看看我們的機器人......"

#. Id: 96225-__Southern hive.mod
#. -- Index: 1.1
#. -- Parent line: 0 (96224-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96225-__Southern?hive.mod:1
msgid "Come see our robotics..."
msgstr "快來看看我們的機器人......"

#. Id: 96225-__Southern hive.mod
#. -- Index: 1.2
#. -- Parent line: 0 (96224-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96225-__Southern?hive.mod:2
msgid "You will love our robotics, come look..."
msgstr "你會喜歡我們的機器人，來看看..."

#. Id: 96225-__Southern hive.mod
#. -- Index: 1.3
#. -- Parent line: 0 (96224-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96225-__Southern?hive.mod:3
msgid "You can buy many robotics here..."
msgstr "你可以在這裡購買很多機器人......"

#. Id: 96226-__Southern hive.mod
#. -- Index: 2.0
#. -- Parent line: 1 (96225-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96226-__Southern?hive.mod:0
msgid "Best in the land..."
msgstr "最好的土地......"

#. Id: 96226-__Southern hive.mod
#. -- Index: 2.1
#. -- Parent line: 1 (96225-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96226-__Southern?hive.mod:1
msgid "We make the best!"
msgstr "我們做到最好！"

#. Id: 96226-__Southern hive.mod
#. -- Index: 2.2
#. -- Parent line: 1 (96225-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96226-__Southern?hive.mod:2
msgid "Listen, listen, /RACENAMEHIVE/, these are the best goods in the land."
msgstr ""

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.0
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:0
msgid "Hand made by hive."
msgstr "蜂巢手工製作。"

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.1
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:1
msgid "Special made by hive hands."
msgstr "由蜂巢手製作的特殊。"

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.2
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:2
msgid "Such quality, we give hive guarantee... *cough* Conditions apply."
msgstr "這樣的質量，我們給蜂巢保證... *咳嗽*條件適用。"

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.3
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:3
msgid "Built using the smallest hive hands... well made, well made..."
msgstr "使用最小的蜂巢手建造......製作精良，製作精良......"

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.4
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:4
msgid "We make piece by piece with hive hands..."
msgstr "我們用蜂巢手一塊一塊地製作......"

#. Id: 96227-__Southern hive.mod
#. -- Index: 3.5
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96227-__Southern?hive.mod:5
msgid "Won't break..."
msgstr "不會破..."

#. Id: 96228-__Southern hive.mod
#. -- Index: 4.0
#. -- Parent line: 3 (96227-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96228-__Southern?hive.mod:0
msgid "Not like the junks those Tech Hunters find in outdated ruins!"
msgstr "不像那些技術獵人在過時的廢墟中發現的那些帆船！"

#. Id: 96228-__Southern hive.mod
#. -- Index: 4.1
#. -- Parent line: 3 (96227-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96228-__Southern?hive.mod:1
msgid "Not like the scrap those Tech Hunters scavenge in junky ruins!"
msgstr "不像那些技術獵人在垃圾廢墟中清除的廢料！"

#. Id: 96229-__Southern hive.mod
#. -- Index: 5.0
#. -- Parent line: 4 (96228-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96229-__Southern?hive.mod:0
msgid "Scammers!"
msgstr "騙子！"

#. Id: 96229-__Southern hive.mod
#. -- Index: 5.1
#. -- Parent line: 4 (96228-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96229-__Southern?hive.mod:1
msgid "Lazy!"
msgstr "懶！"

#. Id: 96229-__Southern hive.mod
#. -- Index: 5.2
#. -- Parent line: 4 (96228-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96229-__Southern?hive.mod:2
msgid "No good!"
msgstr "不好！"

#. Id: 96229-__Southern hive.mod
#. -- Index: 5.3
#. -- Parent line: 4 (96228-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96229-__Southern?hive.mod:3
msgid "Bad!"
msgstr "壞！"

#. Id: 96229-__Southern hive.mod
#. -- Index: 5.4
#. -- Parent line: 4 (96228-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96229-__Southern?hive.mod:4
msgid "Very bad!"
msgstr "很壞！"

#. Id: 96231-__Southern hive.mod
#. -- Index: 7.0
#. -- Parent line: 6 (96230-__Southern hive.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Robotics hive i see neutral
#: 96231-__Southern?hive.mod:0
msgid "Junk hunters more like!"
msgstr "垃圾獵人更喜歡！"

#. Id: 96231-__Southern hive.mod
#. -- Index: 7.1
#. -- Parent line: 6 (96230-__Southern hive.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Robotics hive i see neutral
#: 96231-__Southern?hive.mod:1
msgid "Why buy second hand when you can buy new?"
msgstr "為什麼在買新手時買二手？"

#. Id: 96231-__Southern hive.mod
#. -- Index: 7.2
#. -- Parent line: 6 (96230-__Southern hive.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Robotics hive i see neutral
#: 96231-__Southern?hive.mod:2
msgid "Second hand junk!"
msgstr "二手垃圾！"

#. Id: 96232-__Southern hive.mod
#. -- Index: 8.0
#. -- Parent line: 7 (96231-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96232-__Southern?hive.mod:0
msgid "We make our robotics from scratch with best hive technology..."
msgstr "我們用最好的蜂巢技術從零開始製作機器人......"

#. Id: 96232-__Southern hive.mod
#. -- Index: 8.1
#. -- Parent line: 7 (96231-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96232-__Southern?hive.mod:1
msgid "We use best hive technology to make our own robotics..."
msgstr "我們使用最好的蜂巢技術製作我們自己的機器人......"

#. Id: 96232-__Southern hive.mod
#. -- Index: 8.2
#. -- Parent line: 7 (96231-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96232-__Southern?hive.mod:2
msgid "Don't worry, /RACENAMEHIVE/, we have very good technology here, queen approved..."
msgstr ""

#. Id: 96233-__Southern hive.mod
#. -- Index: 9.0
#. -- Parent line: 8 (96232-__Southern hive.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Robotics hive i see neutral
#: 96233-__Southern?hive.mod:0
msgid "Good price too."
msgstr "價格也不錯。"

#. Id: 96234-__Southern hive.mod
#. -- Index: 10.0
#. -- Parent line: 9 (96233-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96234-__Southern?hive.mod:0
msgid "Even better price for you, /RACENAMEHIVE/."
msgstr ""

#. Id: 96235-__Southern hive.mod
#. -- Index: 11.0
#. -- Parent line: 10 (96234-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96235-__Southern?hive.mod:0
msgid "Because we like you."
msgstr "因為我們喜歡你"

#. Id: 96236-__Southern hive.mod
#. -- Index: 12.0
#. -- Parent line: 11 (96235-__Southern hive.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Robotics hive i see neutral
#: 96236-__Southern?hive.mod:0
msgid "You have good musks."
msgstr "你有好的麝香。"

#. Id: 96237-__Southern hive.mod
#. -- Index: 13.1
#. -- Parent line: 12 (96236-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96237-__Southern?hive.mod:1
msgid "Yummy."
msgstr "好吃。"

#. Id: 96238-__Southern hive.mod
#. -- Index: 3.0
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=0, Tag: -1
#: 96238-__Southern?hive.mod:0
msgid "Where are you going, /RACENAMEHIVE/?"
msgstr ""

#. Id: 96238-__Southern hive.mod
#. -- Index: 3.1
#. -- Parent line: 2 (96226-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=0, Tag: -1
#: 96238-__Southern?hive.mod:1
msgid "Wait, /RACENAMEHIVE/..."
msgstr ""

#. Id: 96239-__Southern hive.mod
#. -- Index: 4.0
#. -- Parent line: 3 (96238-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96239-__Southern?hive.mod:0
msgid "Shop's this way!"
msgstr "以這種方式購物！"

#. Id: 96240-__Southern hive.mod
#. -- Index: 5.0
#. -- Parent line: 4 (96239-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96240-__Southern?hive.mod:0
msgid "You're going the wrong way, /RACENAMEHIVE/!"
msgstr ""

#. Id: 96241-__Southern hive.mod
#. -- Index: 6.0
#. -- Parent line: 5 (96240-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96241-__Southern?hive.mod:0
msgid "You're missing out on our fly tech!"
msgstr "你錯過了我們的飛行技術！"

#. Id: 96243-__Southern hive.mod
#. -- Index: 8.0
#. -- Parent line: 7 (96242-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96243-__Southern?hive.mod:0
msgid "You'll rue the day you didn't buy our goods, /RACENAMEHIVE/!"
msgstr ""

#. Id: 96244-__Southern hive.mod
#. -- Index: 9.0
#. -- Parent line: 8 (96243-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96244-__Southern?hive.mod:0
msgid "Rue the day!"
msgstr "Rue the day！"

#. Id: 96245-__Southern hive.mod
#. -- Index: 5.0
#. -- Parent line: 4 (96239-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 96245-__Southern?hive.mod:0
msgid "Good choice, this is very smart, /RACENAMEHIVE/..."
msgstr ""

#. Id: 96246-__Southern hive.mod
#. -- Index: 6.0
#. -- Parent line: 5 (96245-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96246-__Southern?hive.mod:0
msgid "First step is to enter our shop."
msgstr "第一步是進入我們的商店。"

#. Id: 96247-__Southern hive.mod
#. -- Index: 7.0
#. -- Parent line: 6 (96246-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96247-__Southern?hive.mod:0
msgid "The biggest step!"
msgstr "最大的一步！"

#. Id: 96248-__Southern hive.mod
#. -- Index: 8.0
#. -- Parent line: 7 (96247-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96248-__Southern?hive.mod:0
msgid "Now we just need to go into the pocket..."
msgstr "現在我們只需要進入口袋......"

#. Id: 96249-__Southern hive.mod
#. -- Index: 9.0
#. -- Parent line: 8 (96248-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96249-__Southern?hive.mod:0
msgid "And grab the Cat sack..."
msgstr "抓住貓袋......"

#. Id: 96250-__Southern hive.mod
#. -- Index: 10.0
#. -- Parent line: 9 (96249-__Southern hive.mod)
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#: 96250-__Southern?hive.mod:0
msgid "And receive great bargains!"
msgstr "並獲得很好的便宜貨！"

#. Id: 96257-__Southern hive.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robotics hive i see neutral
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 96257-__Southern?hive.mod:0
msgid "....You are Hive...?"
msgstr "....你是蜂巢......？"
