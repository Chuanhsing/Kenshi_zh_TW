#. -- Index: 1.9
#. -- Parent line: 0 (44615-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Vigilante witness looting ally
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44546-Dialogue.mod:7 44616-Dialogue.mod:9
msgid "Coward! Surrender!"
msgstr ""

#. Id: 44546-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (16201-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. Id: 44616-Dialogue.mod
#. -- Index: 1.10
#. -- Parent line: 0 (44615-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Vigilante witness looting ally
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44546-Dialogue.mod:8 44616-Dialogue.mod:10
msgid "You can run but you can't hide, criminal!"
msgstr ""

#. Id: 44546-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (16201-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
