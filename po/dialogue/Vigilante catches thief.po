#: 16202-Dialogue.mod:3 45364-Dialogue.mod:1 12589-gamedata.base:0
#: 44508-Dialogue.mod:2
msgid "Burglar!"
msgstr "小偷！"

#. Id: 16202-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16202-Dialogue.mod:4
msgid "We don't tolerate thieves around here!"
msgstr "我們不能容忍周圍有小偷在轉悠!"

#. Id: 16202-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16202-Dialogue.mod:5
msgid "Thieves get beaten!"
msgstr "偷竊理應被懲罰!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:1
msgid "/GODDAMN/ /SKULK/er!"
msgstr "/GODDAMN/ /SKULK/er!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:2
msgid "Don't take too kindly to thieves 'round here!"
msgstr "對小偷不要客氣!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:3
msgid "Thieves aren't welcome around here!"
msgstr "小偷在這不受歡迎!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:4
msgid "Thieving /IDIOT/!"
msgstr "有人偷東西 /IDIOT/!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:5
msgid "Stealing? Not on my watch!"
msgstr "偷東西? 在我眼皮底下想都別想!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#. Id: 14367-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Stealing from shop guard
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 16203-Dialogue.mod:6 14367-Dialogue?(4).mod:2
msgid "Not on my watch, thief!"
msgstr "在我眼皮底下想都別想, 小偷!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:7
msgid "No stealing on MY watch!"
msgstr "別想在我眼皮底下偷東西!"

#. Id: 16203-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IS_IMPRISONED=0, Tag: -1
#: 16203-Dialogue.mod:9
msgid "No thieves allowed!"
msgstr "這裏不允許有小偷!"
