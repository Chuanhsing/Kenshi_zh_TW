#. Id: 16201-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:0
msgid "/OI/! Cut that out!"
msgstr "/OI/! 不要碰那個!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:2
msgid "Put your hands where I can see 'em!"
msgstr "把你手放在我看得到的地方!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:3
msgid "Hands off!"
msgstr "放手!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:4
msgid "/OI/! Get away from there!"
msgstr "/OI/! 從那裏滾開!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:6
msgid "Back away from there!"
msgstr "離開那裏!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:7
msgid "/OI/! Back up!"
msgstr "/OI/! 給我回來!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:8
msgid "You! What are you up to?!"
msgstr "你! 你想幹什麼?!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:9
msgid "No funny business!"
msgstr "不要耍任何把戲!"

#. Id: 16201-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante catches thief
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 16201-Dialogue.mod:10
msgid "Get out of it!"
msgstr "滾出來！"

