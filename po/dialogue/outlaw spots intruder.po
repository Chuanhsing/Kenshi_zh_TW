#. Id: 45418-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#: 45418-Dialogue.mod:2
msgid "Intruder's get cut!"
msgstr "入侵者被削減了！"

#. Id: 45418-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#: 45418-Dialogue.mod:3
msgid "Breakin' in, eh?!"
msgstr "打破，嗯？！"

#. Id: 45418-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#: 45418-Dialogue.mod:4
msgid "You picked the wrong house to break into!"
msgstr "你選錯了房子闖入！"

#. Id: 45418-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#: 45418-Dialogue.mod:6
msgid "/OI/, you're not meant to be here!"
msgstr ""

#. Id: 45418-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#: 45418-Dialogue.mod:7
msgid "You must think I'm some kinda /IDIOT/!"
msgstr ""

#. Id: 45420-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (45418-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45420-Dialogue.mod:0
msgid "You won't escape me! You're dead!"
msgstr "你不會逃避我！你死定了！"

#. Id: 45420-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (45418-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45420-Dialogue.mod:1
msgid "You can't outrun me!"
msgstr "你不能超過我！"

#. Id: 45420-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (45418-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45420-Dialogue.mod:2
msgid "Come back 'ere!"
msgstr "回來吧！"

#. Id: 45420-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (45418-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45420-Dialogue.mod:3
msgid "Get back 'ere, /INSULT/!"
msgstr ""

#. Id: 45420-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (45418-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: outlaw spots intruder
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45420-Dialogue.mod:4
msgid "Just try an' run! I'll get you!"
msgstr "試試吧！我會去接你！"
