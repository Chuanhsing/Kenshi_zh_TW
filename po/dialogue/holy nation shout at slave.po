#. Id: 40890-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40890-Dialogue.mod:1
msgid "Work, work!"
msgstr ""

#. Id: 40890-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40890-Dialogue.mod:2
msgid "Work! Work to the bone!"
msgstr ""

#. Id: 40890-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40890-Dialogue.mod:5
msgid "Work! Work until you bleed!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:0
msgid "Repent for your sins!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:1
msgid "Atone for your sins!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:2
msgid "Work cleanses the darkness from the core!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:3
msgid "You must repent for your sins!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:4
msgid "Repent for your heresy!"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:5
msgid "Okran will forgive hard workers for their sins"
msgstr ""

#. Id: 40891-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (40890-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40891-Dialogue.mod:6
msgid "Okran shows mercy to repentant workers"
msgstr ""

#. Id: 40892-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40892-Dialogue.mod:0
msgid "Undisciplined! You're too slow!"
msgstr ""

#. Id: 40892-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40892-Dialogue.mod:1
msgid "Lazy! You're too slow!"
msgstr ""

#. Id: 40892-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40892-Dialogue.mod:2
msgid "To slack work is to scorn the Lord Okran!"
msgstr ""

#. Id: 40892-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40892-Dialogue.mod:3
msgid "Slow!"
msgstr ""

#. Id: 40892-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40892-Dialogue.mod:4
msgid "You're too slow!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:0
msgid "Work harder for the cause of Okran!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:1
msgid "Devote yourself to Okran's cause! Repent!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:2
msgid "Only a heretic slacks!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:3
msgid "Only the unfaithful shy from work!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:4
msgid "Work for Okran's forgiveness!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:5
msgid "You must earn Okran's forgiveness through work!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:6
msgid "Work for Okran's crusade!"
msgstr ""

#. Id: 40888-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (40892-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40888-Dialogue.mod:7
msgid "Work hard for the cause of Light!"
msgstr ""

#. Id: 40895-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 40895-Dialogue.mod:1
msgid "Bad leg? Work through the pain!"
msgstr ""

#. Id: 40895-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: BROKEN_LEG=1, Tag: -1
#: 40895-Dialogue.mod:2
msgid "Got a limp? Work through it!"
msgstr ""

#. Id: 40896-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40895-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40896-Dialogue.mod:0
msgid "It is Okran's punishment! Repent!"
msgstr ""

#. Id: 40896-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40895-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40896-Dialogue.mod:1
msgid "It is your punishment from Okran! Repent for your sins!"
msgstr ""

#. Id: 40896-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40895-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40896-Dialogue.mod:2
msgid "Work for Okran's mercy!"
msgstr ""

#. Id: 40896-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40895-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 40896-Dialogue.mod:3
msgid "It is Okran's Will! Work for his forgiveness!"
msgstr ""

#. Id: 40898-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: BROKEN_ARM=1, Tag: -1
#: 40898-Dialogue.mod:1
msgid "Bad arm? Work through the pain!"
msgstr ""

#. Id: 40898-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: BROKEN_ARM=1, Tag: -1
#. Id: 40901-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: DAMAGED_HEAD=1, Tag: -1
#: 40898-Dialogue.mod:2 40901-Dialogue.mod:2
msgid "Injured? It doesn't matter!"
msgstr ""

#. Id: 40901-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: DAMAGED_HEAD=1, Tag: -1
#: 40901-Dialogue.mod:0
msgid "Headache?"
msgstr ""

#. Id: 40901-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: DAMAGED_HEAD=1, Tag: -1
#: 40901-Dialogue.mod:1
msgid "Head hurts? Work through the pain!"
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:0
msgid "A wench of Narko must live a life of humility and discipline..."
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:1
msgid "The first Holy Phoenix taught us that woman must be restrained and tamed..."
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:2
msgid "A daughter of Narko must live a life of restraint and discipline..."
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:3
msgid "You are in Rebirth now, blackened woman! You cannot seduce the manfolk with your womanly charms here!"
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:4
msgid "You are in Okran's care now, give in to his light.This is your path to a life of purity and righteousness."
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:5
msgid "Surrender to Okran's warming light! Relinquish your temptress ways!"
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:6
msgid "Let Okran guide you child. Under the watch of your holy brothers, Narko will /TAINT/ you no more!"
msgstr ""

#. Id: 49545-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49545-Dialogue.mod:7
msgid "One day, woman will no longer walk unleashed. No longer will you /TAINT/ and seduce your holy brethren!"
msgstr ""

#. Id: 49546-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49546-Dialogue.mod:0
msgid "Work hard to serve Okran's cause! Only through hard work can you redeem yourself, dark woman."
msgstr ""

#. Id: 49546-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49546-Dialogue.mod:1
msgid "Repent! You must sweat Narko's darkness out from the depths of your being!"
msgstr ""

#. Id: 49546-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (49545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49546-Dialogue.mod:2
msgid "Only through hard work and discipline can you shed the darkness that /TAINT/s your soul!"
msgstr ""

#. Id: 49546-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (49545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49546-Dialogue.mod:3
msgid "You can still shed the darkness that /TAINT/s your core. But a tainted woman must work hard to overcome her wicked nature."
msgstr ""

#. Id: 49546-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (49545-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49546-Dialogue.mod:4
msgid "Laziness is in your nature, woman. You must work hard to overcome it, to cleanse yourself of Narko's taint!"
msgstr ""

#. Id: 49547-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49547-Dialogue.mod:0
msgid "Work 'til your fingers bleed. Work 'til you bleed tears of devotion for Okran!"
msgstr ""

#. Id: 49547-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49547-Dialogue.mod:1
msgid "Carry out Okran's Will with blood and sweat, this is your righteous path. This is your /SACRED/ destiny, woman."
msgstr ""

#. Id: 49547-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49547-Dialogue.mod:2
msgid "Your holy brothers have guided you to your righteous path. Bleed, sweat... work for Okran's mercy!"
msgstr ""

#. Id: 49564-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Target Race(s): Greenlander
#: 49564-Dialogue.mod:0
msgid "Yes, holy brother. For Okran's cause, holy brother."
msgstr ""

#. Id: 49564-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Target Race(s): Greenlander
#: 49564-Dialogue.mod:1
msgid "Yes, holy one. For the Lord Of Light, holy brother."
msgstr ""

#. Id: 49564-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Target Race(s): Greenlander
#: 49564-Dialogue.mod:2
msgid "Okran's Will is my purpose, holy brother."
msgstr ""

#. Id: 49564-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (49546-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Target Race(s): Greenlander
#: 49564-Dialogue.mod:3
msgid "With my faith in Okran, I will rid myself of all darkness."
msgstr ""

#. Id: 49549-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49549-Dialogue.mod:0
msgid "Man was created in the image of Okran, our sacred father, the Lord of Light."
msgstr ""

#. Id: 49550-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49549-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49550-Dialogue.mod:0
msgid "Woman was created in the image of Narko, mankind's seductress, Demoness of Darkness."
msgstr ""

#. Id: 49551-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49550-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49551-Dialogue.mod:0
msgid "Woman must be disciplined, she must work hard to overcome the darkness in her nature!"
msgstr ""

#. Id: 49551-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49550-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49551-Dialogue.mod:1
msgid "Woman must be tamed, she must be put to work so that she has the strength to defeat the darkness in her heart!"
msgstr ""

#. Id: 49552-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (49551-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49552-Dialogue.mod:0
msgid "Lest she seduce and tempt the purity of man into the darkness with her!"
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:0
msgid "Lazy! Woman lacks discipline! "
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:1
msgid "Woman is lazy by nature! Work harder!... "
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:2
msgid "The nature of woman is pure laziness!"
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:3
msgid "A poor display of devotion! Woman is lazy at heart!"
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:4
msgid "Lazy like Narko! Get yourself together, woman!"
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:5
msgid "Wretched woman! Work harder, you're slacking!"
msgstr ""

#. Id: 49553-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49553-Dialogue.mod:6
msgid "Blackened woman! There is no place for slacking here! Work harder!"
msgstr ""

#. Id: 49554-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49553-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49554-Dialogue.mod:0
msgid "An idle woman leaves her heart open to be /TAINT/ed by the /AGENTSOFDARKNESS/!"
msgstr ""

#. Id: 49554-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49553-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49554-Dialogue.mod:1
msgid "It is this exact lack of discipline that breeds such darkness in a woman's heart!"
msgstr ""

#. Id: 49555-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49554-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49555-Dialogue.mod:0
msgid "Rebirth is your second chance. Work hard for the Lord Okran's crusade!"
msgstr ""

#. Id: 49556-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (49555-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49556-Dialogue.mod:0
msgid "Only through drudgery and hardship can you learn true discipline, the path to a /HOLY/ existence."
msgstr ""

#. Id: 49556-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (49555-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49556-Dialogue.mod:1
msgid "Only through hard labour and discipline can you learn the way to a higher, /HOLY/ existence."
msgstr ""

#. Id: 49560-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49560-Dialogue.mod:0
msgid "Pray to Okran! Beg his forgiveness for your /TEMPTRESS/ ways!"
msgstr ""

#. Id: 49560-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49560-Dialogue.mod:1
msgid "Work harder, wretched woman! Pray to Okran for his mercy!"
msgstr ""

#. Id: 49560-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49560-Dialogue.mod:2
msgid "You, woman! Pray to the Lord of Light, show your devotion to Okran!"
msgstr ""

#. Id: 49560-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49560-Dialogue.mod:3
msgid "It is time to give prayer to Okran. Work without prayer is work wasted under the eyes of the Lord of Light."
msgstr ""

#. Id: 49560-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 49560-Dialogue.mod:4
msgid "Say your prayers to Okran, woman. Beg for his mercy for your wretched sins!"
msgstr ""

#. Id: 49562-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49560-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#: 49562-Dialogue.mod:0
msgid "Okran, Lord Of Light, forgive me of my sins... Have pity on this lowly woman..."
msgstr ""

#. Id: 49562-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49560-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#: 49562-Dialogue.mod:1
msgid "Oh, merciful one, Lord Of Light, forgive me of my sins... Cleanse me of my dark ways..."
msgstr ""

#. Id: 49562-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (49560-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: holy nation shout at slave
#: 49562-Dialogue.mod:2
msgid "Love and devotion to the Lord Of Light... Forgive this lowly woman of her sinful ways..."
msgstr ""

#. Id: 49563-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49563-Dialogue.mod:0
msgid "Your body may tire from the labour, but your faith must always remain strong!"
msgstr ""

#. Id: 49563-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49563-Dialogue.mod:1
msgid "Now, work! Your body may give out, but your devotion to the Lord Okran must always withstand!"
msgstr ""

#. Id: 49563-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (49562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49563-Dialogue.mod:2
msgid "Your body may burn out from your task, but your faith in Okran must burn eternally bright!"
msgstr ""

#. Id: 49563-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (49562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49563-Dialogue.mod:3
msgid "Your body may ache, but your devotion to Okran must endure! Okran's Will is your duty!"
msgstr ""

#. Id: 49563-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (49562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49563-Dialogue.mod:4
msgid "Pain is but a distraction to your /HOLY/ duty! The Lord Of Light must always be in your heart!"
msgstr ""

#. Id: 49565-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (49563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49565-Dialogue.mod:0
msgid "Through continuous prayer and reflection, we can strengthen our faith in Okran."
msgstr ""

#. Id: 49565-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (49563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49565-Dialogue.mod:1
msgid "Through continuous worship and hardship, we can fortify our love for Okran."
msgstr ""

#. Id: 49565-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (49563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49565-Dialogue.mod:2
msgid "With daily worship and prayer, we can strengthen our devotion to Okran."
msgstr ""

#. Id: 49565-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (49563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49565-Dialogue.mod:3
msgid "Develop your piousness with daily prayer. Reflect on Okran's shining light."
msgstr ""

#. Id: 49565-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (49563-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49565-Dialogue.mod:4
msgid "Strengthen your devoutness to Okran with constant reflection on the Scriptures of Radiance. "
msgstr ""

#. Id: 49566-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49566-Dialogue.mod:0
msgid "An impure race is an unholy one!"
msgstr ""

#. Id: 49566-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49566-Dialogue.mod:1
msgid "The darkened beast is rotten to the core!"
msgstr ""

#. Id: 49566-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49566-Dialogue.mod:2
msgid "The foreign beast is no brother of the Okranite!"
msgstr ""

#. Id: 49566-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49566-Dialogue.mod:3
msgid "There is no light in the heart of an impure beast!"
msgstr ""

#. Id: 49566-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49566-Dialogue.mod:4
msgid "The impure beast is devoid of all light and goodness!"
msgstr ""

#. Id: 49568-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49566-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49568-Dialogue.mod:0
msgid "You must work for Okran's forgiveness. Work to repent for your sins"
msgstr ""

#. Id: 49568-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49566-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49568-Dialogue.mod:1
msgid "Work hard to find Okran's light. Repel the darkness from your bones"
msgstr ""

#. Id: 49568-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (49566-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49568-Dialogue.mod:2
msgid "Toil hard for Okran's mercy, serve Okran's cause, repent for your wicked ways"
msgstr ""

#. Id: 49568-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (49566-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49568-Dialogue.mod:3
msgid "Work for the Lord of Light's mercy. Serve his cause and repent for your sinful ways"
msgstr ""

#. Id: 49568-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (49566-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49568-Dialogue.mod:4
msgid "Work hard for Okran's cause. Focus on his task and do not give in to distraction"
msgstr ""

#. Id: 49569-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49568-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49569-Dialogue.mod:0
msgid "Bleed, sweat; do not yearn for the pleasures of the flesh"
msgstr ""

#. Id: 49569-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49568-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49569-Dialogue.mod:1
msgid "Only through suffering and drudgery will you learn true discipline."
msgstr ""

#. Id: 49569-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (49568-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49569-Dialogue.mod:2
msgid "Only through blood and sweat will you find the strength to resist temptation."
msgstr ""

#. Id: 49569-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (49568-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49569-Dialogue.mod:3
msgid "Pleasures of the flesh are false temptations, they are Narko's trickery."
msgstr ""

#. Id: 49570-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (49569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49570-Dialogue.mod:0
msgid "Darkness preys on the undisplined mind. Shed your worldly desires and serve the cause of Okran."
msgstr ""

#. Id: 49570-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (49569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49570-Dialogue.mod:1
msgid "Serve Okran devotedly, and one day you will shed your monstrous flesh like a jacket. And you will be reborn a child of Okran"
msgstr ""

#. Id: 49570-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (49569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49570-Dialogue.mod:2
msgid "Serve Okran's cause, and one day you will cast away that shell you call a body. And you will be a true child of Okran."
msgstr ""

#. Id: 49570-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (49569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49570-Dialogue.mod:3
msgid "Only through hardship can we live a wholesome existence. We must earn Okran's acceptance."
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:0
msgid "Work harder, horned devil!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:1
msgid "Work through the pain, beast!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:2
msgid "Work! To endure is to thrive, dark one!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:3
msgid "To suffer is to flourish, beast! Work harder!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:4
msgid "Work harder, beast! Work through the pain and you will blossom into a pure child of Okran!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:5
msgid "Work harder, beast! Work through the pain and you will flourish!"
msgstr ""

#. Id: 49571-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49571-Dialogue.mod:6
msgid "Do not slack, horned devil! Work hard for Okran's mercy!"
msgstr ""

#. Id: 49573-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (49571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49573-Dialogue.mod:0
msgid "You can still repel the darkness from within!"
msgstr ""

#. Id: 49573-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49573-Dialogue.mod:1
msgid "Expel your inner darkness! Only through discipline can you overcome your dark nature!"
msgstr ""

#. Id: 49573-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (49571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49573-Dialogue.mod:2
msgid "Okran has given you a second chance!"
msgstr ""

#. Id: 49573-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (49571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49573-Dialogue.mod:3
msgid "Shed the darkness that rots your core! Okran has given you a second chance!"
msgstr ""

#. Id: 49573-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (49571-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49573-Dialogue.mod:4
msgid "The beast is impure by nature, but you can still overcome Narko's darkening!"
msgstr ""

#. Id: 49574-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (49573-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49574-Dialogue.mod:0
msgid "With hard work, blood and sweat, you can still redeem yourself!"
msgstr ""

#. Id: 49574-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (49573-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49574-Dialogue.mod:1
msgid "Repent for your wicked ways!"
msgstr ""

#. Id: 49574-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (49573-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#: 49574-Dialogue.mod:2
msgid "With grind and sweat, you can still repent for your sins!"
msgstr ""

#. Id: 49575-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: holy nation shout at slave
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 49575-Dialogue.mod:0
