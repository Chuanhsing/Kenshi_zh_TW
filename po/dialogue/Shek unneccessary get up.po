#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:1
msgid "... rarrgh..gh!"
msgstr "...... rarrgh..gh！"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:2
msgid "Grrrh! ... gh!"
msgstr "Grrrh！ ......呃！"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:3
msgid "That... all.. you got?!"
msgstr "那......所有......你有嗎？！"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:4
msgid "I'm just... getting started..."
msgstr "我只是......開始......"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:5
msgid "I'm just... getting... warmed up"
msgstr "我只是......得到......熱身"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:6
msgid "Just... needed... a nap..."
msgstr "只是......需要......小睡......"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:7
msgid "Ha...haha..."
msgstr "哈......哈哈......"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 12326-Dialogue?(10).mod:8
msgid "Aha... hahaha!... ha..."
msgstr "啊啊......哈哈哈！......哈......"

#. Id: 12328-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 12328-Dialogue?(10).mod:0
msgid "I won't... stay down... that easy"
msgstr "我不會......堅持下去......那很容易"

#. Id: 12328-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 12328-Dialogue?(10).mod:1
