#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:0
msgid "I'm... not... dead yet"
msgstr "我......不......死了"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:1
msgid "You forgot... to ... finish me off"
msgstr "你忘了......讓我完蛋"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:2
msgid "...next... round..!"
msgstr "......接下來......圓......"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:3
msgid "...again..."
msgstr "...再次..."

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:4
msgid "You never... finished... the job"
msgstr "你從未......完成......這份工作"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:5
msgid "Finish... the... job... next time"
msgstr "完成......工作......下一次"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:6
msgid "I'm not dead... just yet"
msgstr "我還沒死......還沒死"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:7
msgid "Still got... some breath... in me"
msgstr "還有......有些氣息......在我身上"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:8
msgid "Still got some... life left.. in me"
msgstr "還有一些...生活離開......在我身上"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:9
msgid "I'm.. still.. breathing"
msgstr "我......還在......呼吸"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:10
msgid "As long as I'm... still... breathing... I'm still fighting"
msgstr "只要我......還在......呼吸......我還在戰鬥"

#. Id: 12324-Dialogue (10).mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek unneccessary get up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 12324-Dialogue?(10).mod:11
msgid "... another...r-round!"
msgstr "......另一個...... r-round！"

#. Id: 12326-Dialogue (10).mod
#. -- Index: 0.0
#. -- Parent line: -
