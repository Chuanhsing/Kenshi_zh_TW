#. Id: 41156-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:0
msgid "You escaped your fate once. But you won't escape it twice."
msgstr ""

#. Id: 41156-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:1
msgid "This time you won't survive my sword."
msgstr ""

#. Id: 41156-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:2
msgid "This time you won't survive. It is Okran's will that you be purged."
msgstr ""

#. Id: 41156-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:3
msgid "I won't let you survive this time, heretic..."
msgstr ""

#. Id: 41156-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:4
msgid "Still alive? The darkness is persistent, but this time I'll end you for good!"
msgstr ""

#. Id: 41156-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: SQUAD_LOST_TO_ME_ONCE
#: 41156-Dialogue.mod:5
msgid "Your sins won't escape the judgement of my sword this time, heretic!"
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:0
msgid "You again! I won't be beaten this time!"
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:1
msgid "Luck was on your side last time. But I won't fail again!"
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:2
msgid "Last time you got lucky. But you won't defeat me again."
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:3
msgid "With Okran as my witness, I won't let you beat me twice!"
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:4
msgid "Defeating a holy inquisitor? Your heresy will be punished!"
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:5
msgid "One who harms a holy inquisitor will not escape Okran's retribution."
msgstr ""

#. Id: 41158-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: DEFEATED_MY_SQUAD_ONCE
#: 41158-Dialogue.mod:6
msgid "To harm a holy apostle of Okran.. it is one of the greatest sins. But you won't beat me this time!"
msgstr ""

#. Id: 41160-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (41158-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#: 41160-Dialogue.mod:3
msgid "Heretics be purged!"
msgstr ""

