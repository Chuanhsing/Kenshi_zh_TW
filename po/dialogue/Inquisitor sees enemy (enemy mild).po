#. Id: 41371-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:0
msgid "For the murder of an Okranite apostle!"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:1
msgid "To slaughter an apostle of Okran, it is the ultimate sin."
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:2
msgid "One of Okran's apostles, slaughtered in cold blood! The ultimate heresy!"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:3
msgid "For the slaughter of a holy Okranite, you will be judged and purged!"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:4
msgid "Murderer! You will be judged and purged of your sins, heretic!"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:5
msgid "With god as my witness, I will not let you kill another Okranite, butcher!"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:6
msgid "For the murder of his children, Okran will have his retribution"
msgstr ""

#. Id: 41371-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Inquisitor sees enemy (enemy mild)
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: KILLED_MY_FRIEND
#: 41371-Dialogue.mod:7
msgid "For the slaugher of Okran's children, god's retribution will be served"
msgstr ""

