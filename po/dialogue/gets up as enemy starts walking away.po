#. Id: 3687-dialogue-01.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3687-dialogue-01.mod:1
msgid "Aagh!"
msgstr "Aagh！"

#. Id: 3687-dialogue-01.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3687-dialogue-01.mod:2
msgid "Mmmph!"
msgstr "Mmmph！"

#. Id: 3687-dialogue-01.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3687-dialogue-01.mod:3
msgid "Urrgh!"
msgstr "Urrgh！"

#. Id: 3687-dialogue-01.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3687-dialogue-01.mod:4
msgid "Unnfff!"
msgstr "Unnfff！"

#. Id: 3687-dialogue-01.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3687-dialogue-01.mod:5
msgid "Eurrk!"
msgstr "Eurrk！"

#. Id: 3688-dialogue-01.mod
#. -- Index: 1.0
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3688-dialogue-01.mod:0
msgid "This isn't over."
msgstr "這還沒有結束。"

#. Id: 3688-dialogue-01.mod
#. -- Index: 1.1
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3688-dialogue-01.mod:1
msgid "You haven't ended me yet!"
msgstr "你還沒有結束我！"

#. Id: 3688-dialogue-01.mod
#. -- Index: 1.2
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3688-dialogue-01.mod:2
msgid "Don't turn away from me!"
msgstr "別轉身離開我！"

#. Id: 3688-dialogue-01.mod
#. -- Index: 1.4
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3688-dialogue-01.mod:4
msgid "Draw and face me."
msgstr "畫畫並面對我。"

#. Id: 3689-dialogue-01.mod
#. -- Index: 1.0
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 3689-dialogue-01.mod:0
msgid "I will avenge my honor."
msgstr "我會報復我的榮譽。"

#. Id: 3689-dialogue-01.mod
#. -- Index: 1.1
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 3689-dialogue-01.mod:1
msgid "I am not defeated."
msgstr "我沒有被打敗。"

#. Id: 3689-dialogue-01.mod
#. -- Index: 1.2
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 3689-dialogue-01.mod:2
msgid "While I still live, I will fight you."
msgstr "雖然我還活著，但我會打你。"

#. Id: 3689-dialogue-01.mod
#. -- Index: 1.3
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 3689-dialogue-01.mod:3
msgid "Honor over life."
msgstr "尊重生命。"

#. Id: 3689-dialogue-01.mod
#. -- Index: 1.4
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 3689-dialogue-01.mod:4
msgid "This can only end with death."
msgstr "這只能以死亡告終。"

#. Id: 3690-dialogue-01.mod
#. -- Index: 1.0
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3690-dialogue-01.mod:0
msgid "I'll cut your fucking throat!"
msgstr "我會削減他媽的喉嚨！"

#. Id: 3690-dialogue-01.mod
#. -- Index: 1.1
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3690-dialogue-01.mod:1
msgid "Don't you dare ignore me!"
msgstr "你不敢不理我！"

#. Id: 3690-dialogue-01.mod
#. -- Index: 1.3
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3690-dialogue-01.mod:3
msgid "I'll gut you groin to mouth!"
msgstr "我會讓你腹股溝！"

#. Id: 3690-dialogue-01.mod
#. -- Index: 1.4
#. -- Parent line: 0 (3687-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 3690-dialogue-01.mod:4
msgid "Don't have the balls to finish this, do you?!"
msgstr "沒有球完成這個，對嗎？！"

#. Id: 3695-dialogue-01.mod
#. -- Index: 2.0
#. -- Parent line: 1 (3693-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3695-dialogue-01.mod:0
msgid "Haha."
msgstr "哈哈。"

#. Id: 3696-dialogue-01.mod
#. -- Index: 3.1
#. -- Parent line: 2 (3695-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3696-dialogue-01.mod:1
msgid "Kill! Kill!"
msgstr "殺！殺！"

#. Id: 3698-dialogue-01.mod
#. -- Index: 4.0
#. -- Parent line: 3 (3696-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3698-dialogue-01.mod:0
msgid "HAHAHAHAHA!"
msgstr "哈哈哈哈哈！"

#. Id: 3698-dialogue-01.mod
#. -- Index: 4.1
#. -- Parent line: 3 (3696-dialogue-01.mod)
#. -- Speaker: Me (0)
#. -- Group title: gets up as enemy starts walking away
#: 3698-dialogue-01.mod:1
msgid "KILL! KILL! KILL! KILL!"
msgstr "殺！殺！殺！殺！"
