#. Id: 17616-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 17616-Dialogue.mod:0
msgid "Suspicious..."
msgstr "可疑..."

#. Id: 17616-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 17616-Dialogue.mod:1
msgid "Up to something?"
msgstr "什麼東西？"

#. Id: 17616-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 17616-Dialogue.mod:3
msgid "Dubious..."
msgstr "可疑..."

#. Id: 17616-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 17616-Dialogue.mod:4
msgid "Suspect..."
msgstr "疑似..."

#. Id: 44684-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44684-Dialogue.mod:1
msgid "Face your punishment, thief!"
msgstr "面對你的懲罰，小偷！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:0
msgid "I'll show you the door, thief!"
msgstr "我會告訴你門，小偷！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:1
msgid "Raise your sword, thief!"
msgstr "提高你的劍，小偷！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:2
msgid "Unsheathe your weapon, thief!"
msgstr "拔掉你的武器，小偷！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:3
msgid "You try stealing from me? Then raise your sword!"
msgstr "你試著偷我？然後舉起你的劍！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:4
msgid "Devious /FLATSKIN/! Raise your sword!"
msgstr ""

#. Id: 44687-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:5
msgid "I'll teach you not to steal from a Shek Warrior!"
msgstr "我會教你不要從石戰士那裡偷東西！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:6
msgid "Thief! You're mine!"
msgstr "賊！你是我的！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:7
msgid "Thief! I will end you!"
msgstr "賊！我會結束你的！"

#. Id: 44687-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (17616-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_KO=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 44687-Dialogue.mod:8
msgid "I'll bring your doom, thief!"
msgstr "我會把你的厄運，小偷！"

#. Id: 17617-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17617-Dialogue.mod:0
msgid "Trickery!"
msgstr "詭計！"

#. Id: 17617-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17617-Dialogue.mod:1
msgid "Crookedness!"
msgstr "彎彎曲曲！"

#. Id: 17617-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17617-Dialogue.mod:3
msgid "Cunning!"
msgstr "狡猾！"

#. Id: 17618-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 17618-Dialogue.mod:0
msgid "Stop that."
msgstr "不要那麼做。"

#. Id: 17618-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 17618-Dialogue.mod:1
msgid "You, /FLATSKIN/, stop that."
msgstr ""

#. Id: 17618-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek sees player lockpicking
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 17618-Dialogue.mod:2
msgid "Stop doing that."
msgstr "別那樣做。"
