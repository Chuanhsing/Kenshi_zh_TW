#. Id: 64676-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64676-Dialogue.mod:0
msgid "What is it you want?... Money?"
msgstr "你想要什麼？...錢？"

#. Id: 64677-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (64676-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64677-Dialogue.mod:0
msgid "I'll give you money! I'll give you a damn CITY!"
msgstr "我給你錢！我會給你一個該死的城市！"

#. Id: 64678-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (64677-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64678-Dialogue.mod:0
msgid "Let me out! Please let me go!"
msgstr "讓我出去！請讓我離開！"

#. Id: 64681-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (64680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64681-Dialogue.mod:0
msgid "Think you can take on an army?"
msgstr "認為你可以接受軍隊？"

#. Id: 64682-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (64681-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#. Id: 55458-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55457-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Vigilante generic assault
#: 64682-Dialogue.mod:0 55458-Dialogue.mod:1
msgid "Fool!"
msgstr "傻子！"

#. Id: 64683-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (64680-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64683-Dialogue.mod:0
msgid "Eyegore will hunt you to the edge of the land!"
msgstr "Eyegore會將你追捕到這片土地的邊緣！"

#. Id: 64684-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64684-Dialogue.mod:0
msgid "You'll regret this..."
msgstr "你會後悔的......"

#. Id: 64685-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (64684-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64685-Dialogue.mod:0
msgid "Mark my words YOU'LL REGRET THIS!"
msgstr "記下我的話，你要記住這個！"

#. Id: 64686-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble prisoner
#: 64686-Dialogue.mod:0
msgid "Please... *sob*"
msgstr "請...... *嗚咽*"
