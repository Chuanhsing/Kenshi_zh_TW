#. Id: 41888-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:0
msgid "Can you get it open?"
msgstr "你能把它打開嗎？"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:1
msgid "Keep trying... it will open..."
msgstr "繼續嘗試......它會打開......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:2
msgid "Don't give up... it will open..."
msgstr "不要放棄......它會打開......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:3
msgid "All doors can be opened... don't give up..."
msgstr "所有門都可以打開......不要放棄......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:4
msgid "Open... open up..."
msgstr "打開......打開......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:5
msgid "Don't give up on me..."
msgstr "不要放棄我......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:6
msgid "Keep picking... you can do this..."
msgstr "繼續採摘......你可以這樣做......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:7
msgid "Give it time... you can open it..."
msgstr "給它時間......你可以打開它......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:8
msgid "Any minute now... "
msgstr "現在任何一分鐘......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:9
msgid "Any second now, wait for it..."
msgstr "現在任何時候，等待它......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:10
msgid "Wait for it... wait for it..."
msgstr "等等......等待......"

#. Id: 41888-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton unlock my cage attempt
#. -- Condition: My Race(s): Skeleton
#: 41888-Dialogue.mod:11
msgid "Almost... almost..."
msgstr "幾乎......幾乎......"
