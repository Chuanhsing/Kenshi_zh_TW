#. Id: 42483-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42483-Dialogue.mod:0
msgid "You! I challenge you to a battle! Do you have the spine to battle a Shek Warrior?"
msgstr ""

#. Id: 42484-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42483-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42484-Dialogue.mod:0
msgid "You're no Shek Warrior! Where are your horns? Ha!"
msgstr ""

#. Id: 42487-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42484-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42487-Dialogue.mod:0
msgid "THEY WERE RIPPED AWAY FROM ME ALONG WITH MY LOST GLORY AND WARRIOR RENOWN!"
msgstr ""

#. Id: 42488-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42487-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42488-Dialogue.mod:0
msgid "But that won't stop me from crushing your spine!"
msgstr ""

#. Id: 42489-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (42488-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42489-Dialogue.mod:0
msgid "A true Shek warrior needs no horns!"
msgstr ""

#. Id: 42485-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42483-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42485-Dialogue.mod:0
msgid "Okay! Bring it, bone head!"
msgstr ""

#. Id: 42490-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42485-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42490-Dialogue.mod:0
msgid "Raise your sword, /FLATSKIN/!"
msgstr ""

#. Id: 42491-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42490-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42491-Dialogue.mod:0
msgid "Today will be a good day to die!"
msgstr ""

#. Id: 42486-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42483-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42486-Dialogue.mod:0
msgid "Er.. no... thank you..."
msgstr ""

#. Id: 42492-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42486-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42492-Dialogue.mod:0
msgid "Tch... /CRAVEN/..."
msgstr ""

#. Id: 42583-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42492-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42583-Dialogue.mod:0
msgid "You don't deserve the priviledge of carrying a sword..."
msgstr ""

#. Id: 42585-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42585-Dialogue.mod:0
msgid "You! You stare at my horns! You have the gall to gawk at a Shek Warrior's horns, you must have the fight to back it up!"
msgstr ""

#. Id: 42586-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42585-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42586-Dialogue.mod:0
msgid "Why don't you have horns like the other Shek?"
msgstr ""

#. Id: 42587-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42586-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42587-Dialogue.mod:0
msgid "Because they were taken from me. The biggest of shames when a warrior is defeated in battle! But we don't need horns, we still have fight and honour in us. See for yourself, why don't you, hrm?!"
msgstr ""

#. Id: 42588-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42587-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42588-Dialogue.mod:0
msgid "I'd rather not... er.. sorry for staring?"
msgstr ""

#. Id: 42597-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42587-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42597-Dialogue.mod:0
msgid "If it makes you happy... (Draw your sword)"
msgstr ""

#. Id: 56591-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (42597-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 56591-Dialogue.mod:0
msgid "Hm! Good. Do not disappoint me, /FLATSKIN/..."
msgstr ""

#. Id: 42598-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42587-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42598-Dialogue.mod:0
msgid "I really don't want to fight... I'm no warrior..."
msgstr ""

#. Id: 42599-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42587-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42599-Dialogue.mod:0
msgid "Then show me what you got, stubby!"
msgstr ""

#. Id: 42589-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42585-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42589-Dialogue.mod:0
msgid "I didn't mean to stare..."
msgstr ""

#. Id: 42594-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42589-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42594-Dialogue.mod:0
msgid "Hrm! Then keep your eyes to the /DIRT/, /FLATSKIN/..."
msgstr ""

#. Id: 42595-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42594-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42595-Dialogue.mod:0
msgid "Stay out of my way..."
msgstr ""

#. Id: 42590-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42585-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42590-Dialogue.mod:0
msgid "Of course! You wanna rumble, then let's go!"
msgstr ""

#. Id: 42596-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42590-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42596-Dialogue.mod:0
msgid "For Kral's honor!"
msgstr ""

#. Id: 42591-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (42585-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Shek attack (non shek biome)
#: 42591-Dialogue.mod:0
msgid "Got a lot of balls for a peasant, don't you?"
msgstr ""

#. Id: 42592-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42591-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42592-Dialogue.mod:0
msgid "Hmph! I'm no peasant, I'm a Shek Warrior! My fate will not be decided by horn size!"
msgstr ""

#. Id: 42593-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42592-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack (non shek biome)
#: 42593-Dialogue.mod:0
msgid "And I'll prove it through battle! For Kral!"
msgstr ""

