#. Id: 54744-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54744-Dialogue.mod:0
msgid "So, you're my new 'master'...?"
msgstr "所以，你是我的新“主人”......？"

#. Id: 54744-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54744-Dialogue.mod:1
msgid "Nice meetin' you... boss"
msgstr "很高興見到你......老闆"

#. Id: 54744-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54744-Dialogue.mod:3
msgid "... Surely can't be as bad as the masters..."
msgstr "......當然不能像大師一樣糟糕......"

#. Id: 54747-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54747-Dialogue.mod:0
msgid "You freein' me? Or you enslavin' me?"
msgstr "你釋放了我嗎？或者你奴役我？"

#. Id: 54747-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54747-Dialogue.mod:1
msgid "Better not be plannin' on keeping me a slave, stranger..."
msgstr "最好不要計劃讓我成為奴隸，陌生人......"

#. Id: 54747-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54747-Dialogue.mod:2
msgid "You buying my freedom? Or you buying ME?"
msgstr "你買我的自由？或者你買我？"

#. Id: 54748-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54747-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#: 54748-Dialogue.mod:0
msgid "'Cause I'll let you in on a secret: I ain't no victim"
msgstr "因為我會告訴你一個秘密：我不是沒有受害者"

#. Id: 54748-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (54747-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#: 54748-Dialogue.mod:1
msgid "'Cause I don't like slavers and I don't like playin' nice"
msgstr "因為我不喜歡奴隸，我不喜歡玩耍"

#. Id: 54748-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (54747-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#: 54748-Dialogue.mod:2
msgid "'Cause you try keeping me in shackles and I'll strangle you in your sleep"
msgstr "因為你試著讓我戴上鐐銬，我會在睡夢中扼殺你"

#. Id: 54749-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. Id: 54831-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54749-Dialogue.mod:0 54831-Dialogue.mod:0
msgid "Freedom at last..."
msgstr "自由終於......"

#. Id: 54749-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. Id: 54831-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54749-Dialogue.mod:1 54831-Dialogue.mod:1
msgid "Finally free from this cage..."
msgstr "終於擺脫了這個籠子......"

#. Id: 54749-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. Id: 54831-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54749-Dialogue.mod:2 54831-Dialogue.mod:2
msgid "You freed me..."
msgstr "你釋放了我......"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#. Id: 3298-gamedata.base
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: DIALOGUE3297
#: 54803-Dialogue.mod:0 3298-gamedata.base:5
msgid "Guess I owe you."
msgstr "猜猜我欠你的。"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:1
msgid "Feeling charitable, are we? I'll believe that when I see it."
msgstr "感覺慈善，是嗎？當我看到它時，我會相信。"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:2
msgid "Everything comes at a price, even freedom. So what's your price in this?"
msgstr "一切都是有代價的，甚至是自由。那你的價格是多少？"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:3
msgid "Been waiting too long for freedom."
msgstr "等待太久的自由。"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:4
msgid "Looks like I'm following you now."
msgstr "看起來我現在跟著你。"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:5
msgid "Looks like I got myself a new master."
msgstr "看起來我得到了一個新的主人。"

#. Id: 54803-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 54803-Dialogue.mod:6
msgid "You don't look like a slaver. You look different."
msgstr "你看起來不像一個奴隸。你看起來不一樣。"

#. Id: 54805-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54805-Dialogue.mod:0
msgid "I'm free?"
msgstr "我自由了？"

#. Id: 54805-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54805-Dialogue.mod:1
msgid "Is this it? I'm finally free?"
msgstr "是這個嗎？我終於自由了嗎？"

#. Id: 54805-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54805-Dialogue.mod:2
msgid "You're freeing me?"
msgstr "你釋放了我嗎？"

#. Id: 54805-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: MY_CAPTOR
#: 54805-Dialogue.mod:3
msgid "Am I really free again?"
msgstr "我真的再次自由了嗎？"

#. Id: 54750-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54831-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. Id: 54814-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (54813-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bought from slavery run
#. -- Condition: Who: ME, Condition: IS_RECRUITABLE=1, Tag: -1
#: 54750-Dialogue.mod:0 54814-Dialogue.mod:0
msgid "I'll make this up to you, /BROTHER/, you can count on me"
msgstr ""

#. Id: 55391-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#. Id: 55422-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#: 55391-Dialogue.mod:0 55422-Dialogue.mod:0
msgid "You must think me /DUMB/! You're the one who put me here!"
msgstr ""

#. Id: 55391-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#: 55391-Dialogue.mod:1
msgid "First you sell me off like a piece of meat..."
msgstr "首先，你像一塊肉一樣把我賣掉......"

#. Id: 55391-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#. -- Condition: Who: ME, Condition: IS_SLAVE=1, Tag: -1
#. Id: 55422-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#: 55391-Dialogue.mod:2 55422-Dialogue.mod:2
msgid "You must be /DUMB/ if you think I'd forget what you did to me!"
msgstr ""

#. Id: 55399-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55391-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#: 55399-Dialogue.mod:0
msgid "And now you want me to follow you?... Rot in the dirt!"
msgstr "現在你想讓我跟著你？......在泥土中腐爛！"

#. Id: 55422-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Freed from slavery tough
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: MY_CAPTOR
#: 55422-Dialogue.mod:1
msgid "First you knock me out and put me in this cage..."
msgstr "首先，你把我打倒，把我放在籠子裡......"
