#. -- Parent line: 9 (3854-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: bandit fight picking new target in wilds
#. Id: 4690-dialogue-02.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: bandit fight picking new target in wilds
#: 4782-dialogue-02.mod:0 3855-gamedata.base:0 4690-dialogue-02.mod:0
msgid "/OI/."
msgstr ""

#. Id: 4783-dialogue-02.mod
#. -- Index: 2.0
#. -- Parent line: 1 (4782-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4783-dialogue-02.mod:0
msgid "Where's my money?"
msgstr "我的錢在哪裡？"

#. Id: 4783-dialogue-02.mod
#. -- Index: 2.1
#. -- Parent line: 1 (4782-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4783-dialogue-02.mod:1
msgid "Where is it?"
msgstr "它在哪裡？"

#. Id: 4784-dialogue-02.mod
#. -- Index: 3.0
#. -- Parent line: 2 (4783-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4784-dialogue-02.mod:0
msgid "I said, where's my /FUCKING/ money?"
msgstr ""

#. Id: 4786-dialogue-02.mod
#. -- Index: 4.0
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4786-dialogue-02.mod:0
msgid "/OI/! You owe me!"
msgstr ""

#. Id: 4786-dialogue-02.mod
#. -- Index: 4.1
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4786-dialogue-02.mod:1
msgid "You gonna square away your debt or what?"
msgstr "你要平衡你的債務還是什麼？"

#. Id: 4787-dialogue-02.mod
#. -- Index: 5.0
#. -- Parent line: 4 (4786-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4787-dialogue-02.mod:0
msgid "You think medkits are free? Huh?!"
msgstr "你認為medkits是免費的嗎？咦？"

#. Id: 4787-dialogue-02.mod
#. -- Index: 5.1
#. -- Parent line: 4 (4786-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4787-dialogue-02.mod:1
msgid "Those medkits cost something!"
msgstr "那些medkits花費了一些東西！"

#. Id: 4787-dialogue-02.mod
#. -- Index: 5.2
#. -- Parent line: 4 (4786-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4787-dialogue-02.mod:2
msgid "I needed those medkits, too!"
msgstr "我也需要那些medkits！"

#. Id: 4788-dialogue-02.mod
#. -- Index: 6.0
#. -- Parent line: 5 (4787-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4788-dialogue-02.mod:0
msgid "You think I'm some idiot just giving the damn things away?!"
msgstr "你覺得我是個白痴，只是把那些該死的東西拿走了？！"

#. Id: 4788-dialogue-02.mod
#. -- Index: 6.1
#. -- Parent line: 5 (4787-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4788-dialogue-02.mod:1
msgid "It's called 'give and take', you /INSULT/!"
msgstr ""

#. Id: 4797-dialogue-02.mod
#. -- Index: 7.0
#. -- Parent line: 6 (4788-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4797-dialogue-02.mod:0
msgid "Fuck that! You're dead!"
msgstr "他媽的！你死定了！"

#. Id: 4789-dialogue-02.mod
#. -- Index: 6.0
#. -- Parent line: 5 (4787-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4789-dialogue-02.mod:0
msgid "Unbelievable."
msgstr "難以置信的。"

#. Id: 4789-dialogue-02.mod
#. -- Index: 6.1
#. -- Parent line: 5 (4787-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4789-dialogue-02.mod:1
msgid "I can't believe this shit."
msgstr "我簡直不敢相信。"

#. Id: 4790-dialogue-02.mod
#. -- Index: 7.0
#. -- Parent line: 6 (4789-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4790-dialogue-02.mod:0
msgid "People these days..."
msgstr "這些天人們......"

#. Id: 4790-dialogue-02.mod
#. -- Index: 7.1
#. -- Parent line: 6 (4789-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4790-dialogue-02.mod:1
msgid "Can't trust anyone anymore."
msgstr "再也不能相信任何人了。"

#. Id: 4790-dialogue-02.mod
#. -- Index: 7.2
#. -- Parent line: 6 (4789-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4790-dialogue-02.mod:2
msgid "Not a shred of decency."
msgstr "不是一絲正派。"

#. Id: 4808-dialogue-02.mod
#. -- Index: 8.0
#. -- Parent line: 7 (4807-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#: 4808-dialogue-02.mod:0
msgid "Can you believe this shit?"
msgstr "你能相信這個狗屎嗎？"

#. Id: 4809-dialogue-02.mod
#. -- Index: 9.0
#. -- Parent line: 8 (4808-dialogue-02.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: civ wants payment for used up medkits
#: 4809-dialogue-02.mod:0
msgid "You should've known better."
msgstr "你應該知道的更好。"

#. Id: 4809-dialogue-02.mod
#. -- Index: 9.1
#. -- Parent line: 8 (4808-dialogue-02.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: civ wants payment for used up medkits
#: 4809-dialogue-02.mod:1
msgid "What'd you expect?"
msgstr "你期待什麼？"

#. Id: 4792-dialogue-02.mod
#. -- Index: 4.0
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 4792-dialogue-02.mod:0
msgid "You've got to be kidding me."
msgstr "你在開玩笑吧。"

#. Id: 4792-dialogue-02.mod
#. -- Index: 4.1
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 4792-dialogue-02.mod:1
msgid "Are you shitting me."
msgstr "你在跟我說話嗎？"

#. Id: 4792-dialogue-02.mod
#. -- Index: 4.2
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 4792-dialogue-02.mod:2
msgid "You're joking."
msgstr "你在開玩笑麼。"

#. Id: 4794-dialogue-02.mod
#. -- Index: 4.0
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 4794-dialogue-02.mod:0
msgid "Are you seriously running away?!"
msgstr "你是否真的逃跑了？！"

#. Id: 4794-dialogue-02.mod
#. -- Index: 4.1
#. -- Parent line: 3 (4784-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 4794-dialogue-02.mod:1
msgid "You really trying to get away?!"
msgstr "你真的試圖逃脫？！"

#. Id: 4796-dialogue-02.mod
#. -- Index: 5.0
#. -- Parent line: 4 (4794-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: civ wants payment for used up medkits
#. Id: 44620-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (12802-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc thief caught stealing from me
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. Id: 5252-gamedata.base
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: slave escape
#: 4796-dialogue-02.mod:0 44620-Dialogue.mod:4 5252-gamedata.base:2
msgid "Get back here!"
msgstr "回到這裡！"

#. Id: 4796-dialogue-02.mod
#. -- Index: 5.2
