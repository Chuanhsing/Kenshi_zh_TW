#. Id: 43015-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:1
msgid "Shop's closing now. I need to lock up."
msgstr "商店現在關門了。我需要鎖定。"

#. Id: 43015-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:2
msgid "Closing time, I'm locking up now..."
msgstr "關閉時間，我現在鎖定了......"

#. Id: 43015-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:3
msgid "It's closing time. Time to go home."
msgstr "這是關閉時間。該回家了。"

#. Id: 43015-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:4
msgid "Time to go, we're closing up now."
msgstr "時間到了，我們現在關門了。"

#. Id: 43015-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:5
msgid "It's getting late, time to close up."
msgstr "現在已經很晚了，是時候關門了。"

#. Id: 43015-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:6
msgid "Shutting up shop now. Please finish up and make your way out."
msgstr "現在關店。請完成並走出去。"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:0
msgid "I said we're closing..."
msgstr "我說我們要結束了......"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 56379-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Longen shoo
#: 43016-Dialogue.mod:1 56379-Dialogue.mod:0
msgid "You need to leave..."
msgstr "你需要離開......"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:2
msgid "I said we're shutting now..."
msgstr "我說我們現在關門了......"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:3
msgid "You. Shop's shut..."
msgstr "您。商店關門......"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:4
msgid "I'm talking to you. Shop's closed now..."
msgstr "我在跟你說話。商店現在關門了......"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:5
msgid "Hear what I said? We're closed."
msgstr "聽到我說的話？我們關門了。"

#. Id: 43016-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (46944-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43016-Dialogue.mod:6
msgid "Right, doors that way..."
msgstr "對，門那樣......"

#. Id: 46922-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (43016-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46922-Dialogue.mod:0
msgid "Come back in the morning."
msgstr "早上回來吧。"

#. Id: 46922-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (43016-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46922-Dialogue.mod:1
msgid "Open as usual tomorrow."
msgstr "明天打開。"

#. Id: 46922-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (43016-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46922-Dialogue.mod:2
msgid "Unless you want to get locked in here?"
msgstr "除非你想被鎖在這裡？"

#. Id: 46922-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (43016-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46922-Dialogue.mod:3
msgid "Move it."
msgstr "移動它。"

#. Id: 46922-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (43016-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46922-Dialogue.mod:4
msgid "Make your way out."
msgstr "走出去。"

#. Id: 43017-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (46922-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43017-Dialogue.mod:0
msgid "Having trouble hearing me, woman?"
msgstr "女人，聽到我的聲音有困難嗎？"

#. Id: 43017-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (46922-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43017-Dialogue.mod:1
msgid "Problems with your hearing, woman? We're closed."
msgstr "你的聽力問題，女人？我們關門了。"

#. Id: 43017-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (46922-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43017-Dialogue.mod:2
msgid "Is there a problem, woman? Why aren't you leaving?"
msgstr "女人，有問題嗎？你為什麼不離開？"

#. Id: 43017-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (46922-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43017-Dialogue.mod:3
msgid "What's wrong with you? Got a problem, woman?"
msgstr "你這人怎麼回事？有問題，女人？"

#. Id: 43018-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (43017-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43018-Dialogue.mod:0
msgid "Perhaps the dark demoness has clouded your senses?!"
msgstr "也許黑暗的惡魔已經讓你的感官蒙上了陰影？！"

#. Id: 43018-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (43017-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43018-Dialogue.mod:1
msgid "Only a /TAINT/ed woman would harass her holy brethren like this! Leave!"
msgstr ""

#. Id: 43018-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (43017-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43018-Dialogue.mod:2
msgid "This looks like the work of the dark demoness! Leave, woman, or I'll call the /GUARD/s!"
msgstr ""

#. Id: 46913-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46912-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46913-Dialogue.mod:0
msgid "You think she's tainted? Keep away from her!"
msgstr "你認為她被污染了嗎？遠離她！"

#. Id: 46914-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (46913-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46914-Dialogue.mod:0
msgid "Stay away woman! Get out! Go!"
msgstr "遠離女人！出去！走！"

#. Id: 46915-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (46914-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. Id: 15536-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation guards witness crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#. Id: 15548-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation guards witness crime
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 46346-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Robot hunt Okranite
#. -- Condition: Who: TARGET, Condition: HAS_ROBOT_LIMBS=1, Tag: -1
#: 46915-Dialogue.mod:0 15536-Dialogue.mod:0 15548-Dialogue.mod:0 46346-Dialogue.mod:4
msgid "Evil!"
msgstr "邪惡！"

#. Id: 43023-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (46915-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43023-Dialogue.mod:0
msgid "Wretched woman! /GUARD/s! I have an intruder!"
msgstr ""

#. Id: 43023-Dialogue.mod
#. -- Index: 10.1
#. -- Parent line: 9 (46915-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43023-Dialogue.mod:1
msgid "/GREASY/ woman! /GUARD/s! I have an intruder!"
msgstr ""

#. Id: 46955-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (43023-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46955-Dialogue.mod:0
msgid "Move!"
msgstr "移動！"

#. Id: 46919-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (46918-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46919-Dialogue.mod:0
msgid "Why in Okran's name aren't you leaving!?"
msgstr "你為什麼不以奧克蘭的名義離開！？"

#. Id: 46916-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46919-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46916-Dialogue.mod:0
msgid "You need to learn some respect!"
msgstr "你需要學習一些尊重！"

#. Id: 46917-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (46916-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46917-Dialogue.mod:0
msgid "Only a /TAINT/ed woman would so rudely ignore her holy brother like this!"
msgstr ""

#. Id: 43024-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (46917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43024-Dialogue.mod:0
msgid "Maybe the /GUARD/s can help show you out! /GUARD/s!"
msgstr ""

#. Id: 43024-Dialogue.mod
#. -- Index: 9.1
#. -- Parent line: 8 (46917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43024-Dialogue.mod:1
msgid "Wretched woman! Guards!"
msgstr "猥瑣的女人！逆天！"

#. Id: 43024-Dialogue.mod
#. -- Index: 9.2
#. -- Parent line: 8 (46917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43024-Dialogue.mod:2
msgid "Guards! This woman is harassing me!"
msgstr "逆天！這個女人在騷擾我！"

#. Id: 46920-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (43017-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46920-Dialogue.mod:0
msgid "You're something else, aren't you? You need to learn to LISTEN."
msgstr "你不是別的，不是嗎？你需要學習聽。"

#. Id: 46921-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (46920-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46921-Dialogue.mod:0
msgid "I've no patience for troublesome women!"
msgstr "對麻煩的女人我沒有耐心！"

#. Id: 43022-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:0
msgid "Why aren't you leaving?! Are you trying to /TAINT/ me, /TEMPTRESS/?!"
msgstr ""

#. Id: 43022-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:1
msgid "Why are you still here? You try to seduce me in my own home?!"
msgstr "你為什麼還在這兒？你試著在我家裡引誘我？！"

#. Id: 43022-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:2
msgid "Why are you still here, woman?! We're closed!"
msgstr "為什麼你還在這裡，女人？！我們關門了！"

#. Id: 43022-Dialogue.mod
#. -- Index: 7.3
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:3
msgid "Why are you harassing me, woman!? Leave!"
msgstr "你為什麼要騷擾我，女人！？離開！"

#. Id: 43022-Dialogue.mod
#. -- Index: 7.4
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:4
msgid "You're still here!? What are you, trying to /TAINT/ me, wretched woman?!"
msgstr ""

#. Id: 43022-Dialogue.mod
#. -- Index: 7.5
#. -- Parent line: 6 (46921-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43022-Dialogue.mod:5
msgid "Leave me alone, woman! I won't be /TAINT/ed by you!"
msgstr ""

#. Id: 43021-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (43022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43021-Dialogue.mod:0
msgid "/GUARD/s! This /TEMPTRESS/ tries to /TAINT/ me with her /GREASY/ ways!"
msgstr ""

#. Id: 43021-Dialogue.mod
#. -- Index: 8.1
#. -- Parent line: 7 (43022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43021-Dialogue.mod:1
msgid "/GUARD/s! I'm being harassed!"
msgstr ""

#. Id: 43021-Dialogue.mod
#. -- Index: 8.2
#. -- Parent line: 7 (43022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43021-Dialogue.mod:2
msgid "/GUARD/s! The /TAINT/ed woman is harassing me!"
msgstr ""

#. Id: 43021-Dialogue.mod
#. -- Index: 8.3
#. -- Parent line: 7 (43022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43021-Dialogue.mod:3
msgid "/GUARD/s! Make her go away! Wretched woman!"
msgstr ""

#. Id: 43021-Dialogue.mod
#. -- Index: 8.4
#. -- Parent line: 7 (43022-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43021-Dialogue.mod:4
msgid "/GUARD/s! Help me! This /TEMPTRESS/ tries to /TAINT/ me!"
msgstr ""

#. Id: 43025-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43025-Dialogue.mod:0
msgid "We're closing. Leave already."
msgstr "我們要結束了。離開了。"

#. Id: 43025-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43025-Dialogue.mod:1
msgid "I'm closing up now. Leave."
msgstr "我現在正在關門。離開。"

#. Id: 43025-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43025-Dialogue.mod:2
msgid "Shop's closing up now. Leave."
msgstr "商店現在關門了。離開。"

#. Id: 43026-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (43025-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43026-Dialogue.mod:4
msgid "Leave my shop!"
msgstr "離開我的店鋪！"

#. Id: 43026-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (43025-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43026-Dialogue.mod:5
msgid "I don't want you here!"
msgstr "我不要你在這裡！"

#. Id: 43026-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (43025-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43026-Dialogue.mod:6
msgid "You're not wanted here!"
msgstr "你不想在這裡！"

#. Id: 43027-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (43026-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43027-Dialogue.mod:0
msgid "Get out of my shop, beast! Paladins!"
msgstr "走出我的店鋪，野獸！聖騎士！"

#. Id: 43027-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (43026-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43027-Dialogue.mod:1
msgid "Paladins! The dark one, won't leave!"
msgstr "聖騎士！黑暗的一個，不會離開！"

#. Id: 43027-Dialogue.mod
#. -- Index: 2.3
