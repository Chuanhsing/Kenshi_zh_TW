#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43004-Dialogue.mod:0
msgid "Okran bless you, brother. Come back again."
msgstr ""

#. Id: 43004-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43004-Dialogue.mod:1
msgid "Bless you, brother. Come again."
msgstr ""

#. Id: 43004-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43004-Dialogue.mod:2
msgid "Come back again soon, brother. Love and devotion to you."
msgstr ""

#. Id: 43004-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43004-Dialogue.mod:3
msgid "It's getting dark out, tread carefully, brother. "
msgstr ""

#. Id: 43004-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43004-Dialogue.mod:4
msgid "Thanks for visiting! May the light protect you, brother. "
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:0
msgid "Shop's closing now..."
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:1
msgid "I'm closing up now so..."
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:2
msgid "Shop's just about to close up now, customer..."
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:3
msgid "It's getting late, time to close up shop..."
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:4
msgid "It's getting dark out. Time to close up."
msgstr ""

#. Id: 43005-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (43004-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43005-Dialogue.mod:5
msgid "It's closing time now. Thank you for your custom..."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:0
msgid "I need to lock up, brother."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:1
msgid "I need to lock up for the night, brother."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:2
msgid "I need to lock up. You can come back tomorrow, brother."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:3
msgid "We'll be open for business in the morning, brother."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:4
msgid "Come back tomorrow morning if you need anything, brother."
msgstr ""

#. Id: 43006-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (43005-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43006-Dialogue.mod:5
msgid "Open as usual tomorrow morning, brother."
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:0
msgid "... Why aren't you leaving? We're closed..."
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:1
msgid "... You're not leaving...?"
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:2
msgid "... Did you hear me? I said we're closed."
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:3
msgid "... You need to leave already."
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:4
msgid "... I need you to leave now."
msgstr ""

#. Id: 43007-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (43006-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43007-Dialogue.mod:5
msgid "... I said we're closed... why are you still here?"
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:1
msgid "No, don't worry about me, I have no life outside of work or anything..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:2
msgid "I don't want any ugliness now. Come on..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:3
msgid "I'd rather not have to get the /GUARD/s involved..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.4
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:4
msgid "... Or just take your time, sure, I LOVE working overtime..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.5
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:5
msgid "... Tch..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.6
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:6
msgid "... Tch, foreigners..."
msgstr ""

#. Id: 43008-Dialogue.mod
#. -- Index: 4.7
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 43008-Dialogue.mod:7
msgid "... Hmph!"
msgstr ""

#. Id: 46884-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46884-Dialogue.mod:0
msgid "Is there a problem here, brother?"
msgstr ""

#. Id: 46885-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (46884-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46885-Dialogue.mod:0
msgid "I'm not sure..."
msgstr ""

#. Id: 46886-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46885-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46886-Dialogue.mod:0
msgid "You there. You need to leave. "
msgstr ""

#. Id: 46888-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (46886-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46888-Dialogue.mod:0
msgid "This is odd... Hey... Do you... do you think they're a... a r-o-b-o-t?"
msgstr ""

#. Id: 46888-Dialogue.mod
#. -- Index: 8.1
#. -- Parent line: 7 (46886-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46888-Dialogue.mod:1
msgid "Do you... do you think they could be a... a s-k-e-l-e-t-o-n?"
msgstr ""

#. Id: 46888-Dialogue.mod
#. -- Index: 8.2
#. -- Parent line: 7 (46886-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46888-Dialogue.mod:2
msgid "How odd... maybe... maybe they're a robot, a robot in disguise!"
msgstr ""

#. Id: 46889-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (46888-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46889-Dialogue.mod:0
msgid ".... Or being controlled by one..."
msgstr ""

#. Id: 46889-Dialogue.mod
#. -- Index: 9.1
#. -- Parent line: 8 (46888-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46889-Dialogue.mod:1
msgid "... Or... possessed by one..."
msgstr ""

#. Id: 46889-Dialogue.mod
#. -- Index: 9.2
#. -- Parent line: 8 (46888-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46889-Dialogue.mod:2
msgid "Don't. Just don't. You're giving me the heebie jeebies."
msgstr ""

#. Id: 46890-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (46889-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46890-Dialogue.mod:0
msgid "Just... just keep an eye on /HIM/. But don't provoke /HIM/..."
msgstr ""

#. Id: 46891-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46891-Dialogue.mod:0
msgid "You. Do as he says, you need to leave."
msgstr ""

#. Id: 46891-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46891-Dialogue.mod:1
msgid "Be quick. We got places to be."
msgstr ""

#. Id: 46891-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46891-Dialogue.mod:2
msgid "Make it quick, customer. I ain't paid overtime."
msgstr ""

#. Id: 46893-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46892-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46893-Dialogue.mod:0
msgid "Don't make this unpleasant for yourself..."
msgstr ""

#. Id: 46894-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (46893-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46894-Dialogue.mod:0
msgid "... He's still here..."
msgstr ""

#. Id: 46896-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (46894-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46896-Dialogue.mod:0
msgid "Only a darkened one would ignore their holy brothers like this!"
msgstr ""

#. Id: 46897-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (46896-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46897-Dialogue.mod:0
msgid "It's okay, it's okay... I'm sure it's fine..."
msgstr ""

#. Id: 46898-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (46897-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46898-Dialogue.mod:0
msgid "I won't let him out of my sight, you can count on me."
msgstr ""

#. Id: 46899-Dialogue.mod
#. -- Index: 12.0
#. -- Parent line: 11 (46898-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46899-Dialogue.mod:0
msgid "I'm sure he's just shopping... it's fine. No need to get the paladins in here smashing the place up."
msgstr ""

#. Id: 46902-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SNEAKING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46902-Dialogue.mod:0
msgid "/THISGUY/ is acting really wierd..."
msgstr ""

#. Id: 46903-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (46902-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46903-Dialogue.mod:0
msgid "I know, I mean, are they sneaking? Like we can't see /HIM/ or something?"
msgstr ""

#. Id: 46904-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (46903-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46904-Dialogue.mod:0
msgid "Makes me think this one's been darkened or something..."
msgstr ""

#. Id: 46904-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (46903-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#: 46904-Dialogue.mod:1
msgid "... A darkened one maybe?"
msgstr ""

#. Id: 46905-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (46904-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46905-Dialogue.mod:0
msgid "Don't even go there. Last thing I need is the paladins pulling my shop apart on a robot hunt."
msgstr ""

#. Id: 46950-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (46911-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46950-Dialogue.mod:0
msgid "Maybe if we just ignore /HIM/ /HE/'ll go away..."
msgstr ""

#. Id: 46906-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SNEAKING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46906-Dialogue.mod:0
msgid "/THISGUY/ acting really wierd..."
msgstr ""

#. Id: 46906-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (46883-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SNEAKING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 46906-Dialogue.mod:1
msgid "What /IS/ /HE/ actually doing? /HEIS/ not even shopping..."
msgstr ""

#. Id: 46907-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (46906-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#: 46907-Dialogue.mod:0
msgid "I know. Odd. /HE/ seemed fine when /HE/ first came in..."
msgstr ""

#. Id: 46900-Dialogue.mod
#. -- Index: 4.6
#. -- Parent line: 3 (43007-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=0, Tag: -1
#: 46900-Dialogue.mod:6
msgid "Quickly, shut /HIM/ out..."
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:0
msgid "Shop's closing now. Please, make your way out"
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:1
msgid "Shop's closing now. I need to lock up."
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:2
msgid "Closing time, I'm locking up now..."
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:3
msgid "It's closing time. Time to go home."
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:4
msgid "Time to go, we're closing up now."
msgstr ""

#. Id: 43015-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep holy shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: BUILDING_IS_CLOSED_AND_SECURED=0, Tag: -1
#: 43015-Dialogue.mod:5
msgid "It's getting late, time to close up."
