#. Id: 46037-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: My Race(s): Skeleton
#. Id: 46044-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (46040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#: 46037-Dialogue.mod:0 46044-Dialogue.mod:2
msgid "Intruder."
msgstr "入侵者。"

#. Id: 46037-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: My Race(s): Skeleton
#: 46037-Dialogue.mod:1
msgid "Trespasser."
msgstr "侵入者。"

#. Id: 46037-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: My Race(s): Skeleton
#: 46037-Dialogue.mod:2
msgid "Intruding."
msgstr "侵入。"

#. Id: 46038-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#: 46038-Dialogue.mod:1
msgid "You'll see no mercy from me."
msgstr "你不會看到我的憐憫。"

#. Id: 46049-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: Who: ME, Condition: IS_RUNNING=1, Tag: -1
#: 46049-Dialogue.mod:0
msgid "Don't try and run. You won't escape me, intruder."
msgstr "不要試著跑。入侵者，你不會逃避我。"

#. Id: 46049-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: Who: ME, Condition: IS_RUNNING=1, Tag: -1
#: 46049-Dialogue.mod:1
msgid "It's pointless to run, intruder. I'll deal with you eventually."
msgstr "入侵者跑步毫無意義。我最終會和你打交道。"

#. Id: 46049-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: Who: ME, Condition: IS_RUNNING=1, Tag: -1
#: 46049-Dialogue.mod:2
msgid "I'll make sure of it that you can't intrude on /MEUS/ again."
msgstr ""

#. Id: 46049-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: Who: ME, Condition: IS_RUNNING=1, Tag: -1
#: 46049-Dialogue.mod:3
msgid "I'll catch you, and I'll deal with you myself, intruder."
msgstr "我會抓住你的，我會自己處理你，入侵者。"

#. Id: 46049-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (46037-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: Who: ME, Condition: IS_RUNNING=1, Tag: -1
#: 46049-Dialogue.mod:4
msgid "You won't escape me, intruder. No mercy."
msgstr "入侵者，你不會逃避我。沒有憐憫。"

#. Id: 46040-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: My Race(s): Skeleton
#: 46040-Dialogue.mod:0
msgid "You're not supposed to be here."
msgstr "你不應該在這裡。"

#. Id: 46040-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#. -- Condition: My Race(s): Skeleton
#: 46040-Dialogue.mod:1
msgid "You shouldn't be here."
msgstr "你不應該在這裡。"

#. Id: 46044-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (46040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#: 46044-Dialogue.mod:1
msgid "Intruders don't deserve mercy."
msgstr "入侵者不值得憐憫。"

#. Id: 46044-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (46040-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Skeleton spots intruder
#: 46044-Dialogue.mod:3
msgid "Trespassing."
msgstr "非法侵入。"
