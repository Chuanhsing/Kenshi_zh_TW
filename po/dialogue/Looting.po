#. Id: 7152-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: ME, Condition: PLAYERMONEY<500, Tag: -1
#: 7152-Dialogue.mod:1
msgid "Pff, good for nothing drifters"
msgstr "Pff，對任何流浪者都沒有好處"

#. Id: 7152-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: ME, Condition: PLAYERMONEY<500, Tag: -1
#: 7152-Dialogue.mod:2
msgid "Damn /MISER/"
msgstr ""

#. Id: 7152-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: ME, Condition: PLAYERMONEY<500, Tag: -1
#: 7152-Dialogue.mod:3
msgid "Where's all the damn /CATS/?"
msgstr ""

#. Id: 7154-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:0
msgid "All these backpacks, I reckon /IWE/ hit gold here..."
msgstr ""

#. Id: 7154-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:1
msgid "Lookit all this stuff!"
msgstr "看看這些東西吧！"

#. Id: 7154-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:2
msgid "Lookit all this!"
msgstr "看看這一切！"

#. Id: 7154-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:3
msgid "So much stuff!"
msgstr "這麼多東西！"

#. Id: 7154-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:4
msgid "Hit the jackpot here"
msgstr "在這裡贏得大獎"

#. Id: 7154-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 7154-Dialogue.mod:5
msgid "Damn, all this loot!"
msgstr "該死的，所有這些戰利品！"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:0
msgid "Oooh, rum! "
msgstr "哦，朗姆酒！"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:1
msgid "Rum! Just what I needed..."
msgstr "朗姆酒！正是我需要的......"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:2
msgid "Rum! Score!"
msgstr "朗姆酒！得分了！"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:3
msgid "This should quench my thirst"
msgstr "這應該解渴"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:4
msgid "I'm getting drunk tonight"
msgstr "我今晚喝醉了"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:5
msgid "Now I can drown my sorrows with rum"
msgstr "現在我可以用朗姆酒淹沒我的悲傷"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:6
msgid "Rum, This should hit the spot"
msgstr "朗姆酒，這應該到位"

#. Id: 7156-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7156-Dialogue.mod:7
msgid "Rum, my favourite"
msgstr "朗姆酒，我最喜歡的"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:0
msgid "Crack it open then!"
msgstr "然後打開吧！"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:1
msgid "C'mon then, open it! "
msgstr "來吧，打開它吧！"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:2
msgid "Pass it over here then!"
msgstr "把它傳遞到這裡然後！"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:3
msgid "You gonna share it or what?"
msgstr "你會分享它還是什麼？"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:4
msgid "Don't be a hoarder, share it around"
msgstr "不要成為囤積者，分享它"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:5
msgid "You found rum? C'mon share it round"
msgstr "朗姆酒發現了？來吧分享一下"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:6
msgid "Rum? Open it up already then"
msgstr "朗姆酒？然後打開它"

#. Id: 7163-Dialogue.mod
#. -- Index: 2.7
#. -- Parent line: 1 (7162-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7163-Dialogue.mod:7
msgid "No way, you got rum? Chuck it over here!"
msgstr "沒辦法，你有朗姆酒嗎？把它扔到這裡！"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:0
msgid "No! I'm saving it!"
msgstr "沒有！我在救它！"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:1
msgid "Back off! It's mine!"
msgstr "退後！這是我的！"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:2
msgid "/GETLOST/, I found it first!"
msgstr ""

#. Id: 7164-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:3
msgid "Finders keepers. Get your own"
msgstr "發現者守護者。得到你自己的"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:4
msgid "Yeah, yeah whatever"
msgstr "是的，是的，無論如何"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:5
msgid "Calm down, I'll open it later"
msgstr "冷靜下來，我稍後再打開它"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:6
msgid "Okay, let's celebrate"
msgstr "好的，我們慶祝吧"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.7
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:7
msgid "/FINE/ but no backwash"
msgstr ""

#. Id: 7164-Dialogue.mod
#. -- Index: 3.8
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:8
msgid "Fine but if I find backwash in it I'm gonna /SLITYOURTHROAT/"
msgstr ""

#. Id: 7164-Dialogue.mod
#. -- Index: 3.9
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:9
msgid "Me first"
msgstr "我先"

#. Id: 7164-Dialogue.mod
#. -- Index: 3.10
#. -- Parent line: 2 (7163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7164-Dialogue.mod:10
msgid "I get first swigs"
msgstr "我得到第一次打擊"

#. Id: 7157-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#. Id: 47251-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47250-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 7157-Dialogue.mod:0 47251-Dialogue.mod:2
msgid "Food!"
msgstr "餐飲！"

#. Id: 7157-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7157-Dialogue.mod:1
msgid "/IMWERE/ gonna eat well tonight"
msgstr ""

#. Id: 7157-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7157-Dialogue.mod:2
msgid "Finally, some decent grub"
msgstr "最後，一些體面的gr .."

#. Id: 7157-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7157-Dialogue.mod:3
msgid "/IMWERE/ gonna get so fat!"
msgstr ""

#. Id: 7157-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7157-Dialogue.mod:4
msgid "Grub!"
msgstr "蠐螬！"

#. Id: 7161-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (7160-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7161-Dialogue.mod:0
msgid "You got food? Gimme some!"
msgstr "你有食物嗎？給我一些！"

#. Id: 7161-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (7160-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7161-Dialogue.mod:1
msgid "Food? I'm hungry, let's eat now!"
msgstr "餐飲？我很餓，現在就吃吧！"

#. Id: 7161-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (7160-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Looting
#: 7161-Dialogue.mod:2
msgid "You found food?! Pass some my way!"
msgstr "你找到了食物？！通過我的方式！"

#. Id: 7158-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:0
msgid "This should get us some extra cash for sure, /QUICKLAUGH/"
msgstr ""

#. Id: 7158-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:1
msgid "Well well well, what do we have here?"
msgstr "那麼，我們在這裡有什麼？"

#. Id: 7158-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:2
msgid "What's this little gem? This'll make /MEUS/ some good money"
msgstr ""

#. Id: 7158-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:3
msgid "/QUICKLAUGH/! Looks like we heisted us some smugglers!"
msgstr ""

#. Id: 7158-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:4
msgid "...Tut! Naughty /MAN/! I'll take those drugs off your hands..."
msgstr ""

#. Id: 7158-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:5
msgid "... Drugs? Someone's been a bad /MAN/, /QUICKLAUGH/..."
msgstr ""

#. Id: 7158-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7158-Dialogue.mod:6
msgid "... Caught us some smugglers, eh? Jackpot..."
msgstr "......抓住了一些走私者，嗯？大獎..."

#. Id: 7159-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7159-Dialogue.mod:0
msgid "Ooh, lookit all this"
msgstr "哦，看看這一切"

#. Id: 7159-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7159-Dialogue.mod:2
msgid "Oh? What's this?"
msgstr "哦？這是什麼？"

#. Id: 7159-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7159-Dialogue.mod:3
msgid "Jackpot"
msgstr "大獎"

#. Id: 7159-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Looting
#: 7159-Dialogue.mod:4
msgid "This outta fetch us some good monies"
msgstr "這個outta給我們帶來了一些好的錢"
