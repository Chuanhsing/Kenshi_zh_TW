msgstr ""

#. Id: 15247-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (15236-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15247-Dialogue.mod:0
msgid "No deal! (Attack)"
msgstr ""

#. Id: 18963-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (18962-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 18963-Dialogue.mod:0
msgid "/TCH/, /GODDAMN/ /SCROUNGER/s... not even got anything of any value... just get out of here."
msgstr ""

#. Id: 18964-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (18963-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 18964-Dialogue.mod:0
msgid "Go! Before I change my mind"
msgstr ""

#. Id: 18988-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (15221-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18988-Dialogue.mod:2
msgid "Do I have to?"
msgstr ""

#. Id: 18988-Dialogue.mod
#. -- Index: 5.3
#. -- Parent line: 4 (15221-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18988-Dialogue.mod:3
msgid "What? No, I don't trust you..."
msgstr ""

#. Id: 18990-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15221-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18990-Dialogue.mod:0
msgid "Er... okay I guess"
msgstr ""

#. Id: 18990-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15221-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18990-Dialogue.mod:1
msgid "Um... sure, I guess I have no choice"
msgstr ""

#. Id: 18961-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15174-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 18961-Dialogue.mod:0
msgid "Good, /OBEDIENT/ I see. Open up your pockets... Let me check your body, see what you've got."
msgstr ""

#. Id: 18961-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15174-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 18961-Dialogue.mod:1
msgid "Good to see, a /OBEDIENT/ one. Let me see your belongings.. I need to check you over."
msgstr ""

#. Id: 15192-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (15168-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15192-Dialogue.mod:0
msgid "Forget it (Run Away)"
msgstr ""

#. Id: 16516-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (15163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16516-Dialogue.mod:0
msgid "The Holy Nation has no use for a /SNAKE/'s /GREASY/ bribe money!"
msgstr ""

#. Id: 16516-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (15163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16516-Dialogue.mod:1
msgid "Bribe money?! An insult! /GREASY/ /SNAKE/... you disgust me."
msgstr ""

#. Id: 16516-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (15163-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16516-Dialogue.mod:2
msgid "The /GREASY/ action of a /TERRORIST/. You disgust me, /SNAKE/..."
msgstr ""

#. Id: 16517-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (16516-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16517-Dialogue.mod:0
msgid "Bring /HIM/ back! This one will need rebirthing."
msgstr ""

#. Id: 16517-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (16516-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16517-Dialogue.mod:1
msgid "Take /HIM/ for rebirthing. This one needs cleansing."
msgstr ""

#. Id: 16517-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (16516-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16517-Dialogue.mod:2
msgid "Perhaps /HE/ can still be cleansed. Take /HIM/ to Rebirth."
msgstr ""

#. Id: 53254-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15157-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53254-Dialogue.mod:0
msgid "Easy now... What do you want from /MEUS/?"
msgstr ""

#. Id: 53254-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15157-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53254-Dialogue.mod:1
msgid "I want no trouble. What do you want from /MEUS/?"
msgstr ""

#. Id: 53264-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53254-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 53264-Dialogue.mod:0
msgid "Good start. I'm glad to see you're law abiding so far..."
msgstr ""

#. Id: 53302-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15157-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53302-Dialogue.mod:0
msgid "You messed with the wrong /MEMAN/ today, /TINCAN/... (Attack)"
msgstr ""

#. Id: 53302-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15157-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53302-Dialogue.mod:1
msgid "Chasing me, shouting wild accusations... You looking to die, /TINCAN/?"
msgstr ""

#. Id: 53302-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (15157-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53302-Dialogue.mod:2
msgid "I've dealt with worse than you before, /TINCAN/. Try me. (Attack)"
msgstr ""

#. Id: 16508-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 16508-Dialogue.mod:0
msgid "You! You're no Okranite! What are you /SKULK/ing around Holy Nation /LAND/ for?! You must be a /TERRORIST/! Did the /AGENTSOFDARKNESS/ send you?!"
msgstr ""

#. Id: 16508-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 16508-Dialogue.mod:1
msgid "You! Where do you come from, /OUTLANDER/? This is Holy Nation /LAND/, /TERRORIST/s aren't welcome on our sacred soil. Where do your loyalties lie? Are you a /TERRORIST/, controlled by the /AGENTSOFDARKNESS/?"
msgstr ""

#. Id: 16508-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 16508-Dialogue.mod:2
msgid "You! You're no Okranite! What are you /SKULK/ing around Holy Nation /LAND/ for?! Did the /AGENTSOFDARKNESS/ send you, /TERRORIST/?!"
msgstr ""

#. Id: 16508-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 16508-Dialogue.mod:3
msgid "You! You're no Okranite! What reason would an unholy one have to /SKULK/ around Holy Nation /LAND/?! Did the /AGENTSOFDARKNESS/ send you, /TERRORIST/?!"
msgstr ""

#. Id: 18940-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18940-Dialogue.mod:0
msgid "I was just trying to spread the word of darkness and witchcraft..."
msgstr ""

#. Id: 18940-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18940-Dialogue.mod:1
msgid "I was just trying to preach the doctrine of darkness and sorcery..."
msgstr ""

#. Id: 18940-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18940-Dialogue.mod:2
msgid "I just wanted to have a chat about darkness and the black arts..."
msgstr ""

#. Id: 18940-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18940-Dialogue.mod:3
msgid "I don't have time for this, I'm busy spreading the word of darkness..."
msgstr ""

#. Id: 18940-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 18940-Dialogue.mod:4
msgid "I've come to carry out the grand will of the dark demoness..."
msgstr ""

#. Id: 15164-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15164-Dialogue.mod:0
msgid "Just try and catch me, /TINCAN/!"
msgstr ""

#. Id: 15164-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15164-Dialogue.mod:1
msgid "Catch me if you can, /TINCAN/!"
msgstr ""

#. Id: 15164-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15164-Dialogue.mod:2
msgid "Up yours, /TINCAN/!"
msgstr ""

#. Id: 15175-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (15164-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15175-Dialogue.mod:3
msgid "/TERRORIST/! Get /HIM/!"
msgstr ""

#. Id: 15175-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (15164-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15175-Dialogue.mod:4
msgid "Catch /HIM/! Don't let the /TERRORIST/ escape!"
msgstr ""

#. Id: 16519-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (15175-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16519-Dialogue.mod:1
msgid "Burn away /HIS/ sins!"
msgstr ""

#. Id: 53369-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53369-Dialogue.mod:0
msgid "You're mistaken, brother. (Show the Holy Flame)"
msgstr ""

#. Id: 53369-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53369-Dialogue.mod:1
msgid "You mistake me, brother. (Show the Holy Flame)"
msgstr ""

#. Id: 53369-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (16508-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 53369-Dialogue.mod:2
msgid "I'm no darkened, brother. (Show the Holy Flame)"
msgstr ""

#. Id: 53370-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53369-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 53370-Dialogue.mod:0
msgid "I see now... Accept my apologies, brother. Okran's crusade takes it's toll on us, and our judgement too."
msgstr ""

#. Id: 53370-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (53369-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 53370-Dialogue.mod:1
msgid "... Accept my apologies, brother. Okran's crusade takes it's toll on us, and our judgement too. Take refuge in the Holy Flame."
msgstr ""

#. Id: 53370-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (53369-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 53370-Dialogue.mod:2
msgid "... Apologies for my rudeness, brother. Our /SACRED/ duty takes it's toll on us. Take refuge in the Holy Flame."
msgstr ""

#. Id: 53372-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (53369-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 53372-Dialogue.mod:0
msgid "[He looks you up and down]... Today I will trust in you. Let the Holy Flame guide you to goodness."
msgstr ""

#. Id: 53372-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (53369-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 53372-Dialogue.mod:1
msgid "[He squints at you]... May the Holy Flame guide you to goodness. Return swiftly to the city, lest the darkness taint your impure blood."
msgstr ""

