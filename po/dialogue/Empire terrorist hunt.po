#. -- Index: 4.0
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#: 15193-Dialogue.mod:0
msgid "You think you can buy me with chump change!? /SEIZE/ the /SCROUNGER/! Send /HIM/ to the slave mines!"
msgstr ""

#. Id: 15193-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#: 15193-Dialogue.mod:1
msgid "You think you can buy me with pocket change!? /SEIZE/ the /WORM/! Send /HIM/ to the slave mines!"
msgstr ""

#. Id: 15194-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15194-Dialogue.mod:0
msgid "1000 Cats? Are you trying to insult me, /RAT/?!"
msgstr ""

#. Id: 15194-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15194-Dialogue.mod:1
msgid "1000 Cats? Are you mocking me, /RAT/?!"
msgstr ""

#. Id: 15219-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15194-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15219-Dialogue.mod:0
msgid "No sir!"
msgstr ""

#. Id: 15219-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15194-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15219-Dialogue.mod:1
msgid "I meant no offense..."
msgstr ""

#. Id: 15313-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15219-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15313-Dialogue.mod:0
msgid "... Well if you've nothing better to offer me then you're only useful for target practise. I'm tired of dealing with /SCROUNGER/ /RAT/s..."
msgstr ""

#. Id: 15220-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15194-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15220-Dialogue.mod:0
msgid "It's all I've got! Please!"
msgstr ""

#. Id: 15309-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15220-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15309-Dialogue.mod:0
msgid "Well I don't have time for /CRAPPY/ /SCROUNGER/s!"
msgstr ""

#. Id: 16590-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (15309-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 16590-Dialogue.mod:0
msgid "Teach /HIM/ to fear us"
msgstr ""

#. Id: 15310-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15220-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15310-Dialogue.mod:0
msgid "Fine! Just get the hell out of my face, /DIRTY/ /RAT/"
msgstr ""

#. Id: 15310-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (15220-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15310-Dialogue.mod:1
msgid "Fine! Just get the hell out of my face, /GODDAMN/ /RAT/"
msgstr ""

#. Id: 15312-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (15310-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15312-Dialogue.mod:0
msgid "Be quick, before I change my mind"
msgstr ""

#. Id: 15312-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (15310-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15312-Dialogue.mod:1
msgid "You're lucky I'm in a good mood"
msgstr ""

#. Id: 15195-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 15195-Dialogue.mod:0
msgid "1000 Cats?! /GODDAMN/ /SCROUNGER/ .../FINE/... but /GROVEL/ to me first, /RAT/. Beg me."
msgstr ""

#. Id: 15195-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15173-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#: 15195-Dialogue.mod:1
msgid "1000 Cats?! Nothing but a /SCROUNGER/ .../FINE/... but I want you to bend down and /LICKMYBOOTS/ first, /RAT/. "
msgstr ""

#. Id: 15210-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15195-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15210-Dialogue.mod:0
msgid "Yes sir... (/GROVEL/)"
msgstr ""

#. Id: 15211-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15210-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15211-Dialogue.mod:0
msgid "/IDIOT/. Get out of here."
msgstr ""

#. Id: 15174-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (15168-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15174-Dialogue.mod:0
msgid "I'll do anything you want."
msgstr ""

#. Id: 15221-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15174-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>0, Tag: -1
#: 15221-Dialogue.mod:0
msgid "Good, /OBEDIENT/ I see. Open up your bags. Let me check inside and see what you've got..."
msgstr ""

#. Id: 15221-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15174-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>0, Tag: -1
#: 15221-Dialogue.mod:1
msgid "Good to see, a /OBEDIENT/ one. Hand me your backpack.. I need to check it over, see what you've got on you."
msgstr ""

#. Id: 15221-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (15174-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>0, Tag: -1
#: 15221-Dialogue.mod:2
msgid "That's a start. Then let me check your backpack, give it here."
msgstr ""

#. Id: 15236-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (15225-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15236-Dialogue.mod:0
msgid "Alright. I'll take some of these Cats as a donation to /MYFACTION/. Then you're free to go."
msgstr ""

#. Id: 15236-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (15225-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15236-Dialogue.mod:1
msgid "I'll take some of these Cats as a gift to /MYFACTION/. Then you can go."
msgstr ""

#. Id: 15236-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (15225-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#: 15236-Dialogue.mod:2
msgid "I'll take all of these Cats and gift them to /MYFACTION/ on your behalf. Then you'll be off the hook."
msgstr ""

#. Id: 15237-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (15236-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15237-Dialogue.mod:0
msgid "What?! No!"
msgstr ""

#. Id: 15238-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (15236-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Empire terrorist hunt
#: 15238-Dialogue.mod:0
msgid "Uh... /OFCOURSE/"
msgstr ""

#. Id: 15229-Dialogue.mod
#. -- Index: 9.1
#. -- Parent line: 8 (15238-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Empire terrorist hunt
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>1000, Tag: -1
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY<2000, Tag: -1
