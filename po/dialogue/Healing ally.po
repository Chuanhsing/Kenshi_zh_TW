#. Id: 8442-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:0
msgid "Uh, I'll just shove that in there..."
msgstr "呃，我會把那裡的東西推到......"

#. Id: 8442-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:1
msgid "Hmm, guess I'll just poke that bit back in there..."
msgstr "嗯，我想我只會在那裡戳一下......"

#. Id: 8442-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:2
msgid "Uhh, that don't look right... Here */SNAP/*"
msgstr ""

#. Id: 8442-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:3
msgid "Urk! What's that?! I'll just try push it back in there..."
msgstr "烏爾克！那是什麼？！我會試著把它推回去......"

#. Id: 8442-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:4
msgid "Umm... I'll just try an- */SNAP/*"
msgstr ""

#. Id: 8442-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:5
msgid "/AAH/! There's so much blood! Make it stop, make it stop"
msgstr ""

#. Id: 8442-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:6
msgid "Maybe I should pull this out... /OHSHIT/ */SNAP/*"
msgstr ""

#. Id: 8442-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:7
msgid "Maybe if I just twist that bit back..."
msgstr "也許如果我只是扭轉那一點......"

#. Id: 8442-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:8
msgid "/YUCK/! ... I'll just whack a bandage over the top"
msgstr ""

#. Id: 8442-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:9
msgid "How is there so much blood?!"
msgstr "怎麼這麼多血？！"

#. Id: 8442-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 8442-Dialogue.mod:10
msgid "Hmm... that don't look right. Maybe if I try and..."
msgstr "嗯......看起來不對勁。也許如果我嘗試......"

#. Id: 8444-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 8444-Dialogue.mod:0
msgid "Suck it up"
msgstr "吸吮它"

#. Id: 8444-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 8444-Dialogue.mod:1
msgid "Quit squirming"
msgstr "放棄蠕動"

#. Id: 8444-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 8444-Dialogue.mod:3
msgid "Don't move a muscle"
msgstr "不要移動肌肉"

#. Id: 8444-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 8444-Dialogue.mod:4
msgid "Keep still a sec"
msgstr "保持靜止一秒鐘"

#. Id: 8449-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8449-Dialogue.mod:1
msgid "Let me have a go"
msgstr "我來吧"

#. Id: 8449-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8449-Dialogue.mod:2
msgid "I'll see what I can do"
msgstr "我會看看我能做什麼"

#. Id: 8449-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8449-Dialogue.mod:5
msgid "Let me help"
msgstr "讓我來幫忙"

#. Id: 8464-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (8449-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: TARGET, Condition: IS_KO=1, Tag: -1
#: 8464-Dialogue.mod:0
msgid "Yeesh. What a mess..."
msgstr "Yeesh。真是一團糟..."

#. Id: 8464-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (8449-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: TARGET, Condition: IS_KO=1, Tag: -1
#: 8464-Dialogue.mod:1
msgid "/YUCK/, that don't look good"
msgstr ""

#. Id: 8451-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#: 8451-Dialogue.mod:0
msgid "Oh god, why won't it stop bleeding?"
msgstr "天啊，為什麼不止血呢？"

#. Id: 8451-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. Id: 42675-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Sundemon healing someone
#. -- Condition: My Sub-Race(s): Greenlander
#: 8451-Dialogue.mod:2 42675-Dialogue.mod:2
msgid "That's gotta hurt..."
msgstr "那會受傷......"

#. Id: 8453-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8453-Dialogue.mod:0
msgid "Annnnd, nearly done"
msgstr "Annnnd，差不多完成了"

#. Id: 8453-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8453-Dialogue.mod:1
msgid "Easy. I can take care of this..."
msgstr "簡單。我可以照顧這個......"

#. Id: 8453-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8453-Dialogue.mod:2
msgid "This should heal up okay"
msgstr "這應該癒合好"

#. Id: 8453-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8453-Dialogue.mod:3
msgid "Ah that ain't so bad..."
msgstr "啊那不是那麼糟糕......"

#. Id: 8455-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8455-Dialogue.mod:0
msgid "I've seen worse. You'll live"
msgstr "我看到更糟了。你會活下去的"

#. Id: 8455-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8455-Dialogue.mod:1
msgid "Hold still a sec..."
msgstr "等一下......"

#. Id: 8455-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8455-Dialogue.mod:2
msgid "You'll survive"
msgstr "你會活下來的"

#. Id: 8455-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8455-Dialogue.mod:3
msgid "That ain't so bad..."
msgstr "那不是那麼糟糕......"

#. Id: 8455-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 8455-Dialogue.mod:4
msgid "I'll patch that up"
msgstr "我會修補它"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:0
msgid "I can handle this"
msgstr "我應付得來"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:1
msgid "I can take care of this"
msgstr "我可以照顧這個"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:2
msgid "Let me fix that up"
msgstr "讓我解決這個問題"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:3
msgid "Let me bandage that up"
msgstr "讓我把它包紮起來"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:4
msgid "Just needs a few bandages"
msgstr "只需要一些繃帶"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:5
msgid "Let me patch that up"
msgstr "讓我補丁一下"

#. Id: 8458-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: SMART
#: 8458-Dialogue.mod:6
msgid "I'll bandage that up for you"
msgstr "我會為你打包繃帶"

#. Id: 8462-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8462-Dialogue.mod:0
msgid "Here, I'll take a look"
msgstr "在這裡，我來看看"

#. Id: 8462-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8462-Dialogue.mod:1
msgid "I ain't leavin' you like this"
msgstr "我不是喜歡你喜歡這個"

#. Id: 8462-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8462-Dialogue.mod:2
msgid "Let me see"
msgstr "讓我看看"

#. Id: 8462-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 8462-Dialogue.mod:3
msgid "Let's stop that bleeding"
msgstr "讓我們停止流血"

#. Id: 9307-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 9307-Dialogue.mod:0
msgid "I'll save you!"
msgstr "我會救你的！"

#. Id: 9307-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 9307-Dialogue.mod:1
msgid "I'll fix you!"
msgstr "我會解決你的！"

#. Id: 9307-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Healing ally
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 9307-Dialogue.mod:2
msgid "Man down!"
msgstr "人員受傷倒地！"
