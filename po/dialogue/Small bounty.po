#: 44348-Dialogue.mod:4 96209-Dialogue.mod:0 54975-Dialogue.mod:2 13819-Escapes.mod:1
msgid "Fugitive!"
msgstr "逃犯！"

#. Id: 44348-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44348-Dialogue.mod:5
msgid "I know your face!"
msgstr "我知道你的臉！"

#. Id: 44348-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. Id: 96209-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44348-Dialogue.mod:7 96209-Dialogue.mod:1
msgid "Felon!"
msgstr "重罪犯！"

#. Id: 44348-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44348-Dialogue.mod:8
msgid "I know you!"
msgstr "我認識你！"

#. Id: 44348-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44348-Dialogue.mod:9
msgid "Did you really think you could sneak by /MEUS/!?"
msgstr ""

#. Id: 44349-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. Id: 16193-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Vigilante generic assault
#. Id: 13645-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13644-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Law spotting a generic crime
#. Id: 18439-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (18437-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 44349-Dialogue.mod:1 16193-Dialogue.mod:1 13645-Escapes.mod:1 18439-Dialogue.mod:4
msgid "Give yourself up!"
msgstr "放棄自己吧！"

#. Id: 44349-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44349-Dialogue.mod:2
msgid "I can't let you walk away."
msgstr "我不能讓你走開。"

#. Id: 44349-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44349-Dialogue.mod:3
msgid "There's no escape now, surrender yourself or be taken by force!"
msgstr "現在沒有逃脫，投降自己或被武力奪走！"

#. Id: 44350-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (44349-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44350-Dialogue.mod:0
msgid "Settle your charges, your crimes won't go unpaid for..."
msgstr "解決你的指控，你的罪行將不會被取消..."

#. Id: 44350-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (44349-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44350-Dialogue.mod:1
msgid "Pay for your crimes, clear your name..."
msgstr "支付你的罪行，清楚你的名字......"

#. Id: 44350-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (44349-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#: 44350-Dialogue.mod:2
msgid "You need to pay for your crimes..."
msgstr "你需要支付你的罪行......"

#. Id: 44148-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. Id: 15018-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek law spots bounty
#: 44148-Dialogue.mod:0 15018-Dialogue.mod:0
msgid "You, stop!"
msgstr "你，停！"

#. Id: 44148-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. Id: 15155-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: You Stop!
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: STARVING=0, Tag: -1
#. Id: 54002-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: You Stop!
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: ME, Condition: STARVING=0, Tag: -1
#: 44148-Dialogue.mod:1 15155-Dialogue.mod:5 54002-Dialogue.mod:5
msgid "Stop right where you are!"
msgstr "停在你身邊！"

#. Id: 44148-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44148-Dialogue.mod:2
msgid "Come back! You can't run from the law!"
msgstr "回來！你不能逃避法律！"

#. Id: 44148-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (44348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44148-Dialogue.mod:3
msgid "You can't run from us! Come back here!"
msgstr "你不能逃避我們！回到這來！"

#. Id: 44150-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (44148-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44150-Dialogue.mod:0
msgid "Keep running and I'll have you arrested!"
msgstr "繼續跑，我會讓你被捕！"

#. Id: 44150-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (44148-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44150-Dialogue.mod:1
msgid "Running will only land you in more trouble!"
msgstr "跑步只會給你帶來更多麻煩！"

#. Id: 44150-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (44148-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44150-Dialogue.mod:2
msgid "I'm warning you, don't run from me!"
msgstr "我警告你，不要逃避我！"

#. Id: 44150-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (44148-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44150-Dialogue.mod:3
msgid "Last warning, don't move!"
msgstr "最後警告，不要動！"

#. Id: 44151-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (44150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44151-Dialogue.mod:0
msgid "/SEIZE/ /HIM/! There's a bounty on /HIS/ head!"
msgstr ""

#. Id: 44151-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (44150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44151-Dialogue.mod:1
msgid "/SEIZE/ /HIM/! /HEIS/ making a run for it!"
msgstr ""

#. Id: 44151-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (44150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44151-Dialogue.mod:2
msgid "/SEIZE/ /HIM/! Don't let /HIM/ get away!"
msgstr ""

#. Id: 44151-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (44150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44151-Dialogue.mod:3
msgid "You'll never outrun the law, fugitive! /SEIZE/ /HIM/!"
msgstr ""

#. Id: 44151-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (44150-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Small bounty
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 44151-Dialogue.mod:4
msgid "You can run, but you can't hide, fugitive! /SEIZE/ /HIM/!"
msgstr ""
