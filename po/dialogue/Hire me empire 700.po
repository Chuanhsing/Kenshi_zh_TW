msgid "Whatever"
msgstr ""

#. Id: 17479-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (12721-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 17479-Dialogue.mod:0
msgid "I'm telling!"
msgstr ""

#. Id: 17481-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (17479-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me empire 700
#: 17481-Dialogue.mod:0
msgid "No! You traitor!"
msgstr ""

#. Id: 12720-Dialogue (11) (2).mod
#. -- Index: 4.0
#. -- Parent line: 3 (12734-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12720-Dialogue?(11)?(2).mod:0
msgid "No, I'm not getting involved in this"
msgstr ""

#. Id: 12715-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12712-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12715-Dialogue?(11)?(2).mod:0
msgid "Yeah, why?"
msgstr ""

#. Id: 12715-Dialogue (11) (2).mod
#. -- Index: 1.1
#. -- Parent line: 0 (12712-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12715-Dialogue?(11)?(2).mod:1
msgid "I am, why do you ask?"
msgstr ""

#. Id: 12715-Dialogue (11) (2).mod
#. -- Index: 1.2
#. -- Parent line: 0 (12712-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12715-Dialogue?(11)?(2).mod:2
msgid "I am, why?"
msgstr ""

#. Id: 12715-Dialogue (11) (2).mod
#. -- Index: 1.3
#. -- Parent line: 0 (12712-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12715-Dialogue?(11)?(2).mod:3
msgid "Why do you ask?"
msgstr ""

#. Id: 12715-Dialogue (11) (2).mod
#. -- Index: 1.4
#. -- Parent line: 0 (12712-Dialogue (11) (2).mod)
#. -- Speaker: Target (1)
#. -- Group title: Hire me empire 700
#: 12715-Dialogue?(11)?(2).mod:4
msgid "Yes, why do you ask?"
msgstr ""

#. Id: 12716-Dialogue (11) (2).mod
#. -- Index: 1.0
