#. Id: 64534-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64534-Dialogue.mod:0
msgid "WAR!"
msgstr "戰爭！"

#. Id: 64535-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:0
msgid "Come to my blade, Okranite dog/S/!"
msgstr ""

#. Id: 64535-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:1
msgid "Far away from home, aren't you, Okranite?"
msgstr "Okranite，遠離家鄉，不是嗎？"

#. Id: 64535-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:2
msgid "Taste my blade, Okranite/S/!"
msgstr ""

#. Id: 64535-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:3
msgid "You stray too far south, Okranite/S/!"
msgstr ""

#. Id: 64535-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:4
msgid "Pray to your god, Okranite!"
msgstr "向你的上帝祈禱，Okranite！"

#. Id: 64535-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:5
msgid "Where's your god now, Okranite/S/?"
msgstr ""

#. Id: 64535-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:6
msgid "It's a beautiful day to die, Okranite/S/!"
msgstr ""

#. Id: 64535-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (64534-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64535-Dialogue.mod:7
msgid "Okranite/S/, delivered straight to my blade!"
msgstr ""

#. Id: 64536-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64536-Dialogue.mod:0
msgid "Hm hm hm HM!"
msgstr "嗯嗯HM！"

#. Id: 64536-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64536-Dialogue.mod:1
msgid "Action at last!"
msgstr "行動終於來了！"

#. Id: 64536-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#. Id: 12856-Dialogue (11) (2).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12853-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: merc interrupted by bandit
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 64536-Dialogue.mod:2 12856-Dialogue?(11)?(2).mod:0
msgid "This should be fun!"
msgstr "這應該是有趣的！"

#. Id: 64536-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#: 64536-Dialogue.mod:3
msgid "Entertaining."
msgstr "娛樂性。"

#. Id: 64538-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 64538-Dialogue.mod:0
msgid "Outnumbered too? I live for the challenge!"
msgstr "數量也超過了？我為挑戰而活！"

#. Id: 64538-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 64538-Dialogue.mod:1
msgid "And the numbers are against us... Fun!"
msgstr "數字反對我們......好玩！"

#. Id: 64538-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (64535-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek hundred guardian see HN ally
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 64538-Dialogue.mod:2
msgid "So many of you too... More bones for me to crush!"
msgstr "你們這麼多人......更多的骨頭讓我粉碎！"
