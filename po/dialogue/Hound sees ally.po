#: 61677-Dialogue.mod:0
msgid "Got any smokes, /YOURNAME/?"
msgstr ""

#. Id: 61677-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61677-Dialogue.mod:1
msgid "Got any hash, /YOURNAME/?"
msgstr ""

#. Id: 61680-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61677-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#. Id: 61679-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (61678-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61680-Dialogue.mod:0 61679-Dialogue.mod:3
msgid "Go onnnn, I know you got some spare..."
msgstr "去吧，我知道你有一些空餘......"

#. Id: 61680-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (61677-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61680-Dialogue.mod:1
msgid "Come on, us hounds gotta back eachother, eh..."
msgstr "來吧，我們獵犬要互相回复，呃......"

#. Id: 61678-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61678-Dialogue.mod:0
msgid "Got any fish, /YOURNAME/?"
msgstr ""

#. Id: 61678-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61678-Dialogue.mod:1
msgid "Got any grub, /YOURNAME/?"
msgstr ""

#. Id: 61679-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61678-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61679-Dialogue.mod:0
msgid "I'm ravenous!"
msgstr "我很貪心！"

#. Id: 61679-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (61678-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61679-Dialogue.mod:1
msgid "Could eat a gorrillo right now..."
msgstr "現在可以吃一個gorrillo ..."

#. Id: 61679-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (61678-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#. Id: 49408-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Pickles begging
#: 61679-Dialogue.mod:2 49408-Dialogue.mod:0
msgid "*Rumble*"
msgstr "*隆隆*"

#. Id: 61681-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61681-Dialogue.mod:0
msgid "New haircut?"
msgstr "新髮型？"

#. Id: 61681-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61681-Dialogue.mod:1
msgid "New hairdo, /YOURNAME/?"
msgstr ""

#. Id: 61682-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61681-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61682-Dialogue.mod:0
msgid "I don't like it."
msgstr "我不喜歡它。"

#. Id: 61682-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (61681-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61682-Dialogue.mod:1
msgid "Looks like shit."
msgstr "看起來很糟糕。"

#. Id: 61683-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61683-Dialogue.mod:0
msgid "Good to see yer ugly mug again, /YOURNAME/."
msgstr ""

#. Id: 61685-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61683-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61685-Dialogue.mod:0
msgid "... I think."
msgstr "... 我認為。"

#. Id: 61686-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61686-Dialogue.mod:0
msgid "Whew, /YOURNAME/, you look like shit!"
msgstr ""

#. Id: 61698-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61698-Dialogue.mod:0
msgid "I tell ya hounds, Big Gray, he's gettin' outta hand..."
msgstr "我告訴你一隻獵犬，Big Grey，他已經開始了......"

#. Id: 61699-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61698-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61699-Dialogue.mod:0
msgid "Someone needs to put him in his place if he don't start behavin'..."
msgstr "如果他不開始行為，有人需要把他放在他的位置......"

#. Id: 61701-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (61700-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hound sees ally
#: 61701-Dialogue.mod:0
msgid "Keep steppin' on our toes n' the only place for him is in a coffin..."
msgstr "繼續我們的腳趾'對他來說唯一的地方是棺材......"

#. Id: 61702-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (61701-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Hound sees ally
#. Id: 61709-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61708-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61702-Dialogue.mod:0 61709-Dialogue.mod:0
msgid "Ain't that right, /YOURNAME/?"
msgstr ""

#. Id: 61703-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (61702-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#: 61703-Dialogue.mod:0
msgid "/YOURNAME/ knows."
msgstr ""

#. Id: 61705-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (61704-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Hound sees ally
#: 61705-Dialogue.mod:0
msgid "Hoy, don't encourage the new /GUY/..."
msgstr ""

#. Id: 61706-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (61705-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Hound sees ally
#: 61706-Dialogue.mod:0
msgid "Big Grim'll have our necks if somethin' happens to Gray."
msgstr "如果有些事情發生在Gray身上，Big Grim會有我們的脖子。"

#. Id: 61707-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (61706-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hound sees ally
#. Id: 1533252-__world reactions NE.mod
#. -- Index: 8.0
#. -- Parent line: 7 (1533251-__world reactions NE.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bar patrons random faction comment on world state
#. Id: 49966-Dialogue.mod
