#. Id: 4871-dialogue-02.mod
#. -- Index: 1.0
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4871-dialogue-02.mod:0
msgid "/IMWERE/ taking your food."
msgstr ""

#. Id: 4871-dialogue-02.mod
#. -- Index: 1.1
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4871-dialogue-02.mod:1
msgid "Your food's /MINEOURS/ now."
msgstr ""

#. Id: 4871-dialogue-02.mod
#. -- Index: 1.2
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4871-dialogue-02.mod:2
msgid "Hand over your food."
msgstr "交出你的食物。"

#. Id: 4871-dialogue-02.mod
#. -- Index: 1.3
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4871-dialogue-02.mod:3
msgid "Food. Now!"
msgstr "餐飲。現在！"

#. Id: 4871-dialogue-02.mod
#. -- Index: 1.4
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4871-dialogue-02.mod:4
msgid "Give /MEUS/ your food."
msgstr ""

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.0
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:0
msgid "Back off, /INSULT/."
msgstr ""

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.1
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:1
msgid "You're not getting a damn thing."
msgstr "你沒有得到一個該死的東西。"

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.2
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:2
msgid "You won't take shit."
msgstr "你不會屎。"

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.3
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:3
msgid "This isn't yours, /INSULT/."
msgstr ""

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.4
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:4
msgid "/FUCKVERB/ off."
msgstr ""

#. Id: 4872-dialogue-02.mod
#. -- Index: 2.5
#. -- Parent line: 1 (4871-dialogue-02.mod)
#. -- Speaker: Target (1)
#. -- Group title: starver marks a target
#. -- Condition: Who: TARGET, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4872-dialogue-02.mod:5
msgid "/FUCKING/ /STARVERS/."
msgstr ""

#. Id: 4877-dialogue-02.mod
#. -- Index: 3.0
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 4877-dialogue-02.mod:0
msgid "Wrong answer."
msgstr "錯誤的答案。"

#. Id: 4877-dialogue-02.mod
#. -- Index: 3.1
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 4877-dialogue-02.mod:1
msgid "Oh? Let's try that again!"
msgstr "哦？讓我們再試一次！"

#. Id: 4877-dialogue-02.mod
#. -- Index: 3.2
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 4877-dialogue-02.mod:2
msgid "I said, /IMWERE/ taking it!"
msgstr ""

#. Id: 4877-dialogue-02.mod
#. -- Index: 3.3
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 4877-dialogue-02.mod:3
msgid "You'll regret that."
msgstr "你會後悔的。"

#. Id: 4893-dialogue-02.mod
#. -- Index: 3.0
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4893-dialogue-02.mod:0
msgid "Hard way, it is/PRD/"
msgstr ""

#. Id: 4893-dialogue-02.mod
#. -- Index: 3.1
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4893-dialogue-02.mod:1
msgid "Well, /IWE/ tried asking/PRD/"
msgstr ""

#. Id: 4893-dialogue-02.mod
#. -- Index: 3.2
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 4893-dialogue-02.mod:2
msgid "Have it your way/PRD/"
msgstr ""

#. Id: 4894-dialogue-02.mod
#. -- Index: 3.1
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 4894-dialogue-02.mod:1
msgid "Why's it always come to this."
msgstr "為什麼總是這樣。"

#. Id: 4894-dialogue-02.mod
#. -- Index: 3.2
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 4894-dialogue-02.mod:2
msgid "No way around this, then."
msgstr "那麼就沒辦法了。"

#. Id: 4894-dialogue-02.mod
#. -- Index: 3.3
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 4894-dialogue-02.mod:3
msgid "No other way."
msgstr "沒有其它的方法。"

#. Id: 4894-dialogue-02.mod
#. -- Index: 3.4
#. -- Parent line: 2 (4872-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 4894-dialogue-02.mod:4
msgid "Wish it hadn't come to this, but..."
msgstr "希望它沒有來到這個，但......"

#. Id: 4879-dialogue-02.mod
#. -- Index: 1.0
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4879-dialogue-02.mod:0
msgid "Hear /MEUS/ out."
msgstr ""

#. Id: 4879-dialogue-02.mod
#. -- Index: 1.1
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4879-dialogue-02.mod:1
msgid "Let's make a deal."
msgstr "我們來做個交易吧。"

#. Id: 4879-dialogue-02.mod
#. -- Index: 1.2
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4879-dialogue-02.mod:2
msgid "How about this:"
msgstr "這個怎麼樣："

#. Id: 4880-dialogue-02.mod
#. -- Index: 2.0
#. -- Parent line: 1 (4879-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4880-dialogue-02.mod:0
msgid "You give us food..."
msgstr "你給我們食物......"

#. Id: 4880-dialogue-02.mod
#. -- Index: 2.1
#. -- Parent line: 1 (4879-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4880-dialogue-02.mod:1
msgid "Hand over your food..."
msgstr "交出你的食物......"

#. Id: 4880-dialogue-02.mod
#. -- Index: 2.2
#. -- Parent line: 1 (4879-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4880-dialogue-02.mod:2
msgid "We take everything we can eat..."
msgstr "我們採取一切可以吃的東西......"

#. Id: 4880-dialogue-02.mod
#. -- Index: 2.3
#. -- Parent line: 1 (4879-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4880-dialogue-02.mod:3
msgid "Leave your food here..."
msgstr "把食物留在這裡......"

#. Id: 4881-dialogue-02.mod
#. -- Index: 3.0
#. -- Parent line: 2 (4880-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4881-dialogue-02.mod:0
msgid "...and we won't slaughter you."
msgstr "......我們不會屠殺你。"

#. Id: 4881-dialogue-02.mod
#. -- Index: 3.1
#. -- Parent line: 2 (4880-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4881-dialogue-02.mod:1
msgid "...and you might live."
msgstr "......你可能活著。"

#. Id: 4881-dialogue-02.mod
#. -- Index: 3.2
#. -- Parent line: 2 (4880-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4881-dialogue-02.mod:2
msgid "...and you won't die."
msgstr "......你不會死"

#. Id: 4881-dialogue-02.mod
#. -- Index: 3.3
#. -- Parent line: 2 (4880-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4881-dialogue-02.mod:3
msgid "...and we won't have to /THREAT_ACTION/ your /BODYPART/."
msgstr ""

#. Id: 98117-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (98039-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 98117-Dialogue.mod:0
msgid "... Not gonna share anything?"
msgstr "......不會分享任何東西？"

#. Id: 98118-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (98117-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#: 98118-Dialogue.mod:0
msgid "Fine. Have it your way..."
msgstr "精細。按你的方式行事......"

#. Id: 4883-dialogue-02.mod
#. -- Index: 1.0
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4883-dialogue-02.mod:0
msgid "/IWE/ don't want violence, friend."
msgstr ""

#. Id: 4883-dialogue-02.mod
#. -- Index: 1.1
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4883-dialogue-02.mod:1
msgid "/IMWERE/ not out for blood, friend."
msgstr ""

#. Id: 4883-dialogue-02.mod
#. -- Index: 1.2
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4883-dialogue-02.mod:2
msgid "Not looking for trouble, friend."
msgstr "朋友，不是在尋找麻煩。"

#. Id: 4883-dialogue-02.mod
#. -- Index: 1.3
#. -- Parent line: 0 (4860-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4883-dialogue-02.mod:3
msgid "/IMWERE/ not here for violence, friend."
msgstr ""

#. Id: 4888-dialogue-02.mod
#. -- Index: 2.0
#. -- Parent line: 1 (4883-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4888-dialogue-02.mod:0
msgid "Just food."
msgstr "只是食物。"

#. Id: 4888-dialogue-02.mod
#. -- Index: 2.1
#. -- Parent line: 1 (4883-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4888-dialogue-02.mod:1
msgid "Only food."
msgstr "只有食物。"

#. Id: 4889-dialogue-02.mod
#. -- Index: 3.0
#. -- Parent line: 2 (4888-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4889-dialogue-02.mod:0
msgid "/IWE/'ll only take what /IWE/ need."
msgstr ""

#. Id: 4889-dialogue-02.mod
#. -- Index: 3.1
#. -- Parent line: 2 (4888-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4889-dialogue-02.mod:1
msgid "/IWE/ won't take more than /IWE/ need."
msgstr ""

#. Id: 4889-dialogue-02.mod
#. -- Index: 3.2
#. -- Parent line: 2 (4888-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4889-dialogue-02.mod:2
msgid "Enough for a day or two."
msgstr "足夠一兩天。"

#. Id: 4890-dialogue-02.mod
#. -- Index: 4.0
#. -- Parent line: 3 (4889-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4890-dialogue-02.mod:0
msgid "And everyone gets to walk away."
msgstr "每個人都走開了。"

#. Id: 4890-dialogue-02.mod
#. -- Index: 4.1
#. -- Parent line: 3 (4889-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4890-dialogue-02.mod:1
msgid "Then everyone's happy."
msgstr "然後大家都很開心。"

#. Id: 4890-dialogue-02.mod
#. -- Index: 4.2
#. -- Parent line: 3 (4889-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4890-dialogue-02.mod:2
msgid "All swords stay undrawn."
msgstr "所有劍都未被拉伸。"

#. Id: 4890-dialogue-02.mod
#. -- Index: 4.3
#. -- Parent line: 3 (4889-dialogue-02.mod)
#. -- Speaker: Me (0)
#. -- Group title: starver marks a target
#. -- Condition: Who: ME, Condition: IN_COMBAT=0, Tag: -1
#: 4890-dialogue-02.mod:3
msgid "No one has to be a killer today."
msgstr "今天沒有人必須成為殺手。"
