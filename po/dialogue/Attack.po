#. Id: 8421-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 8421-Dialogue.mod:2
msgid "Bleed /HIM/ dry!"
msgstr "把 /HIM/的血放幹!"

#. Id: 8421-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: IS_LEADER=1, Tag: -1
#: 8421-Dialogue.mod:3
msgid "Make 'em scream!"
msgstr "讓他們鬼哭狼嚎!"

#. Id: 8434-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8434-Dialogue.mod:0
msgid "Stab 'em in the eyes!"
msgstr "戳瞎他們的眼睛!"

#. Id: 8434-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8434-Dialogue.mod:2
msgid "Carve their throats out!"
msgstr "割開他們的喉嚨!"

#. Id: 8434-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8434-Dialogue.mod:3
msgid "Slice their guts open!"
msgstr "切開他們的內髒!"

#. Id: 8434-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Attack
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 8434-Dialogue.mod:4
