#. Id: 47246-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47246-Dialogue.mod:0
msgid "/THESEGUYS/ should be easy enough... Get /HIM/!"
msgstr ""

#. Id: 47246-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47246-Dialogue.mod:1
msgid "/THESEGUYS/ should be easy pickings... Get /HIM/!"
msgstr ""

#. Id: 47246-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47246-Dialogue.mod:2
msgid "/THESEGUYS/ MUST have food on /HIM/... Attack!"
msgstr ""

#. Id: 47246-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47246-Dialogue.mod:3
msgid "Food... Now's our chance! Run 'em through! "
msgstr "食物......現在是我們的機會！跑完他們！"

#. Id: 47246-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47246-Dialogue.mod:4
msgid "Today we eat! Gut /HIM/, take every last scrap of food from /HIM/!"
msgstr ""

#. Id: 47251-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47250-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47251-Dialogue.mod:1
msgid "Give us your food!"
msgstr "給我們你的食物！"

#. Id: 47247-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47247-Dialogue.mod:0
msgid "/THESEGUYS/ must have some food on /HIM/! Strip /HIM/ bare!"
msgstr ""

#. Id: 47247-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47247-Dialogue.mod:1
msgid "Food!?... Food! Take /HIM/ down and strip /HIM/ bare!"
msgstr ""

#. Id: 47247-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47247-Dialogue.mod:2
msgid "... Food?... I need food! Gimme your food!"
msgstr "......食物？......我需要食物！給我你的食物！"

#. Id: 47247-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47247-Dialogue.mod:3
msgid "Gimme your food!"
msgstr "給我你的食物！"

#. Id: 47247-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47247-Dialogue.mod:4
msgid "Foooood! I'm taking your food! "
msgstr "Foooood！我正在吃你的食物！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:0
msgid "So... hungry... This one's just gotta have food!"
msgstr "所以...飢餓......這個人只需要食物！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:1
msgid "I just... want... food... I need your food!"
msgstr "我只是......想要......食物......我需要你的食物！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:2
msgid "I need your food! I need to eat!"
msgstr "我需要你的食物！我需要吃東西！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:3
msgid "Even if you have to die, I'm taking your food!"
msgstr "即使你必須死，我也會帶你的食物！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:4
msgid "I'm taking your food! "
msgstr "我正在吃你的食物！"

#. Id: 47248-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Starving mugging one line
#. -- Condition: Target Race(s): Greenlander or Hive Prince or Shek
#: 47248-Dialogue.mod:5
msgid "I don't care who you are or what you are, I'm taking your food!"
msgstr "我不在乎你是誰或你是誰，我正在吃你的食物！"

#. Id: 47253-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47252-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47253-Dialogue.mod:1
msgid "I'm not starving today! Take /HIM/ down!"
msgstr ""

#. Id: 47253-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47252-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47253-Dialogue.mod:2
msgid "Did someone say food?... Quit hoggin' all that food!"
msgstr "有人說食物嗎？...退出'所有的食物！"

#. Id: 47254-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47252-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47254-Dialogue.mod:0
msgid "Take /HIM/ down! /HIS/ food is MINE!"
msgstr ""

#. Id: 47254-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47252-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47254-Dialogue.mod:1
msgid "Don't let /HIM/ get away! /HIS/ food is mine!"
msgstr ""

#. Id: 47254-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47252-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Starving mugging one line
#: 47254-Dialogue.mod:2
msgid "Mine... Your food is mine!"
msgstr "我...你的食物是我的！"

#. Id: 47256-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (47255-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Starving mugging one line
#: 47256-Dialogue.mod:0
msgid "Mine!"
msgstr "礦！"

#. Id: 47256-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (47255-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Starving mugging one line
#: 47256-Dialogue.mod:1
msgid "No, mine! MINE!"
msgstr "不是我的！礦！"

#. Id: 47256-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (47255-Dialogue.mod)
#. -- Speaker: Extra 2 (3)
#. -- Group title: Starving mugging one line
#: 47256-Dialogue.mod:2
msgid "My food! MINE!"
msgstr "我的食物！礦！"
