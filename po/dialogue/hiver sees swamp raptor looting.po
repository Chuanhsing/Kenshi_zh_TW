#. Id: 44071-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:0
msgid "Nooooo! They eat everything!"
msgstr "真是沒有！他們吃了一切！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:1
msgid "Nooo! They're eating everything!"
msgstr "拿去！他們正在吃一切！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:2
msgid "Noooo! Gobblers!"
msgstr "不要！ Gobblers的！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:3
msgid "They're eating everything!"
msgstr "他們正在吃一切！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:4
msgid "Nooo! The gobblers are coming!"
msgstr "拿去！狼吞虎咽的來了！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:5
msgid "Gobblers! They're gobbling the crops!"
msgstr "Gobblers的！他們正在吃莊稼！"

#. Id: 44071-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#: 44071-Dialogue.mod:6
msgid "The gobblers be gobbling!"
msgstr "狼吞虎咽的狼吞虎咽！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:0
msgid "Not fair! Those belong to the queen! Stop eating them!"
msgstr "不公平！那些屬於女王！別吃了！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:1
msgid "Noooo! No, stop that! That's greedy!"
msgstr "不要！不，停止！那太貪了！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:2
msgid "Noooo! They're stealing from us! Greedy!"
msgstr "不要！他們偷了我們！貪婪！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:3
msgid "They're eating our fortunes!"
msgstr "他們正在吃我們的財富！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:4
msgid "They're eating from us! No, stop!"
msgstr "他們正在吃我們的東西！不停！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:5
msgid "That's not fair! Stop eating everything!"
msgstr "這不公平！停止吃一切！"

#. Id: 44072-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 44072-Dialogue.mod:6
msgid "No! Greedy! Greedy gobbler, stop that!"
msgstr "沒有！貪婪！貪吃的狼吞虎咽，停下來！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:0
msgid "Erase! Erase them!"
msgstr "刪除！擦掉它們！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:1
msgid "Pests! Kill!"
msgstr "害蟲！殺！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:2
msgid "Pests! Erase them!"
msgstr "害蟲！擦掉它們！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:3
msgid "Pests! Erase!"
msgstr "害蟲！刪除！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:4
msgid "Protect the hive, kill the pests!"
msgstr "保護蜂巢，殺死害蟲！"

#. Id: 44073-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hiver sees swamp raptor looting
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 44073-Dialogue.mod:5
msgid "Kill them! Kill the pests!"
msgstr "殺了他們！殺死害蟲！"
