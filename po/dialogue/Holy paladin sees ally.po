#. Id: 57169-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:0
msgid "You look like death, holy brother!"
msgstr "你看起來像死神，聖潔的兄弟！"

#. Id: 57169-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:1
msgid "Haven't you any food, holy one?"
msgstr "你有沒有食物，聖潔的？"

#. Id: 57169-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:3
msgid "Okran have mercy... Are you okay, holy one?"
msgstr "奧克蘭有憐憫......你還好嗎，聖潔嗎？"

#. Id: 57169-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:4
msgid "You look unwell, holy brother..."
msgstr "你看起來不舒服，聖潔的兄弟......"

#. Id: 57169-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:6
msgid "Holy one!"
msgstr "聖者！"

#. Id: 57169-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. -- Condition: Who: TARGET, Condition: STARVING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: RELATIONS>50, Tag: -1
#: 57169-Dialogue.mod:7
msgid "Lord, Okran!"
msgstr "主啊，奧克蘭！"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:0
msgid "Eat, be well..."
msgstr "吃，好吧......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. Id: 57179-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (57170-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:1 57179-Dialogue.mod:3
msgid "We suffer together, as children, as brothers..."
msgstr "作為孩子，我們像兄弟一樣受苦......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. Id: 57179-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (57170-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:2 57179-Dialogue.mod:2
msgid "Your suffering is my suffering. Be well..."
msgstr "你的痛苦是我的痛苦。好吧......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:3
msgid "Eat well, regain your strength..."
msgstr "吃得好，恢復體力......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. Id: 57179-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (57170-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:4 57179-Dialogue.mod:4
msgid "You will be strong again..."
msgstr "你會再強大......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#. Id: 57179-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (57170-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:5 57179-Dialogue.mod:5
msgid "Have faith in the light..."
msgstr "對光有信心......"

#. Id: 57178-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (57169-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy paladin sees ally
#: 57178-Dialogue.mod:6
msgid "Eat well. The Holy Nation needs you..."
msgstr "吃好。神聖的國度需要你......"

