#. -- Index: 1.5
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 41437-Dialogue.mod:5 41440-Dialogue.mod:5
msgid "The second extinction is coming!"
msgstr "第二次滅絕即將到來！"

#. Id: 41437-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 41437-Dialogue.mod:6
msgid "The second extinction is upon us!"
msgstr "第二次滅絕在我們身上！"

#. Id: 41437-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#: 41437-Dialogue.mod:7
msgid "Save us Holy Lord Phoenix!"
msgstr "拯救我們聖靈鳳凰！"

#. Id: 41438-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:0
msgid "The darkness is here!"
msgstr "黑暗就在這裡！"

#. Id: 41438-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:1
msgid "The /AGENTSOFDARKNESS/ attack!"
msgstr ""

#. Id: 41438-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:2
msgid "An attack from the /AGENTSOFDARKNESS/!"
msgstr ""

#. Id: 41438-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:3
msgid "We're under attack! Dark ones are here!"
msgstr "我們受到了攻擊！黑暗的人在這裡！"

#. Id: 41438-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:4
msgid "They've come for us!"
msgstr "他們來找我們！"

#. Id: 41438-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:5
msgid "The dark ones are upon us!"
msgstr "黑暗的人在我們身上！"

#. Id: 41438-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 41438-Dialogue.mod:6
msgid "The darkened have come for us!"
msgstr "黑暗的人來了！"

#. Id: 41439-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 41439-Dialogue.mod:0
msgid "We're doomed!"
msgstr "我們完蛋了！"

#. Id: 41439-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 41439-Dialogue.mod:1
msgid "We're all gonna die!"
msgstr "我們都會死！"

#. Id: 41439-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (41434-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy nation civ sees skeleton
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. Id: 49898-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (49897-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo civilian
