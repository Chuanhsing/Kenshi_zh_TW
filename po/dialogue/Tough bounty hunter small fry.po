#. Id: 18437-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:0
msgid "I'm taking your bounty, have fun in prison..."
msgstr "我正在拿你的賞金，在監獄裡玩得開心......"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:1
msgid "I'm taking you in to jail..."
msgstr "我帶你進監獄......"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:2
msgid "You're coming to the police station with me..."
msgstr "你和我一起來警察局......"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:3
msgid "I'm taking you to jail..."
msgstr "我帶你去監獄......"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:4
msgid "I'm taking that bounty of yours..."
msgstr "我拿你的那個賞金......"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:5
msgid "We're going on a trip to the police station"
msgstr "我們要去警察局旅行"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:6
msgid "You're coming with me to the police station"
msgstr "你和我一起去警察局"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:7
msgid "Your bounty is mine"
msgstr "你的恩惠是我的"

#. Id: 18437-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (18436-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18437-Dialogue.mod:8
msgid "I'm taking that bounty of yours"
msgstr "我正在接受你的賞金"

#. Id: 18439-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (18437-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18439-Dialogue.mod:0
msgid "Be a good /BOY2/ and make this easy for me, okay?"
msgstr ""

#. Id: 18439-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (18437-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18439-Dialogue.mod:1
msgid "Don't try running from /MEUS/, you can't escape..."
msgstr ""

#. Id: 18439-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (18437-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18439-Dialogue.mod:2
msgid "Make this easy and give yourself up!"
msgstr "讓這很容易，讓自己放棄！"

#. Id: 18439-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (18437-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18439-Dialogue.mod:3
msgid "Resist me and this is going to get painful"
msgstr "抵制我，這將變得痛苦"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:0
msgid "I'm taking you in, outlaw!"
msgstr "我帶你進去，歹徒！"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:1
msgid "I'm taking you down, outlaw!"
msgstr "我把你帶走了，歹徒！"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:2
msgid "You're going down, outlaw!"
msgstr "你要走了，歹徒！"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:3
msgid "Say goodbye to your freedom, outlaw!"
msgstr "告別你的自由，告別！"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:4
msgid "You're going to jail, outlaw!"
msgstr "你要坐牢，歹徒！"

#. Id: 18441-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#. -- Condition: Who: TARGET, Condition: BOUNTY_AMOUNT_ACTUAL>4000, Tag: -1
#: 18441-Dialogue.mod:5
msgid "Your bounty's mine, outlaw!"
msgstr "你的賞金是我的，歹徒！"

#. Id: 18442-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18441-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18442-Dialogue.mod:0
msgid "Resist /MEUS/ and this is gonna get real painful"
msgstr ""

#. Id: 18442-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18441-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18442-Dialogue.mod:1
msgid "Resistance will only make this more painful for you, criminal!"
msgstr "抵抗只會讓你更加痛苦，犯罪！"

#. Id: 18442-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18441-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18442-Dialogue.mod:2
msgid "We can do this the easy way or the hard way..."
msgstr "我們可以通過簡單的方式或艱難的方式來做到這一點......"

#. Id: 18442-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (18441-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Tough bounty hunter small fry
#: 18442-Dialogue.mod:3
msgid "A cage is where you belong, thug scum!"
msgstr "籠子是你所屬的地方，暴徒渣滓！"
