#. Id: 63290-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63290-Dialogue.mod:0
msgid "There!"
msgstr "那裡！"

#. Id: 63290-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63290-Dialogue.mod:1
msgid "More /PEASANT/s to run down?"
msgstr ""

#. Id: 63290-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63290-Dialogue.mod:2
msgid "Quiet!... Over there."
msgstr "安靜！......在那邊。"

#. Id: 63290-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63290-Dialogue.mod:3
msgid "Shh! Over there!"
msgstr "噓！在那邊！"

#. Id: 63291-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. Id: 63310-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (63304-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63291-Dialogue.mod:0 63310-Dialogue.mod:1
msgid "Begin the hunt!"
msgstr "開始狩獵！"

#. Id: 63291-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. Id: 63310-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63304-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63291-Dialogue.mod:1 63310-Dialogue.mod:0
msgid "Start the hunt!"
msgstr "開始狩獵！"

#. Id: 63291-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63291-Dialogue.mod:2
msgid "Watch me shoot /THISONE/ down!"
msgstr ""

#. Id: 63291-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. Id: 63310-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (63304-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63291-Dialogue.mod:3 63310-Dialogue.mod:2
msgid "Release the dregs!"
msgstr "釋放渣滓！"

#. Id: 63291-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63291-Dialogue.mod:4
msgid "I do love /PEASANT/ hunting..."
msgstr ""

#. Id: 63308-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 63308-Dialogue.mod:0
msgid "/NOBLELAUGH/! Run little /PEASANT//S/! Run, run, run!"
msgstr ""

#. Id: 63308-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 63308-Dialogue.mod:1
msgid "Run little /PEASANT//S/! I love the challenge!"
msgstr ""

#. Id: 63308-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 63308-Dialogue.mod:2
msgid "/NOBLELAUGH/, you can't outrun my blade, little /PEASANT/!"
msgstr ""

#. Id: 63309-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63309-Dialogue.mod:0
msgid "/NOBLELAUGH/! Be a dear and die quietly for me."
msgstr ""

#. Id: 63309-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63309-Dialogue.mod:1
msgid "/NOBLELAUGH/, this will be a messy one."
msgstr ""

#. Id: 63309-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63309-Dialogue.mod:2
msgid "Down, /PEASANT/, /NOBLELAUGH/!"
msgstr ""

#. Id: 63309-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63309-Dialogue.mod:3
msgid "Fall down!"
msgstr "摔倒！"

#. Id: 63309-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (63291-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63309-Dialogue.mod:5
msgid "Don't worry, you'll live on eternal on my mantle."
msgstr "別擔心，你將永生在我的地幔上。"

#. Id: 63296-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63296-Dialogue.mod:0
msgid "Time to try out my new bow!"
msgstr "是時候嘗試我的新弓了！"

#. Id: 63296-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63296-Dialogue.mod:1
msgid "Time to try out my new armour!"
msgstr "是時候嘗試我的新裝甲了！"

#. Id: 63296-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63296-Dialogue.mod:2
msgid "Now's my chance to try my new bow!"
msgstr "現在是我嘗試新弓的機會！"

#. Id: 63297-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63296-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63297-Dialogue.mod:0
msgid "It's a Skeleton Smith, you know."
msgstr "你知道，這是一個骷髏史密斯。"

#. Id: 63297-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (63296-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63297-Dialogue.mod:1
msgid "It's an Edgewalker, you know."
msgstr "你知道，這是一個Edgewalker。"

#. Id: 63297-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (63296-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63297-Dialogue.mod:2
msgid "This bad boy cost more than you could ever earn in a lifetime, you know."
msgstr "你知道，這個壞孩子的成本比一輩子的成本高。"

#. Id: 63299-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (63298-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble hunt attack
#: 63299-Dialogue.mod:0
msgid "... That's /GREAT/, my lord."
msgstr ""

#. Id: 63299-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (63298-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble hunt attack
#: 63299-Dialogue.mod:1
msgid "... Yes, my lord. You tell me everyday..."
msgstr "... 是的我的主。你每天告訴我......"

#. Id: 63301-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:0
msgid "Fetch my /PEASANT/ sword!"
msgstr ""

#. Id: 63301-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:1
msgid "Fetch my running boots, servant!"
msgstr "拿起我的跑步靴，僕人！"

#. Id: 63301-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:2
msgid "Fetch me my /PEASANT/ sword, servant!"
msgstr ""

#. Id: 63301-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:3
msgid "I'll need my five sword. Pass me my five, servant!"
msgstr "我需要五劍。把我的五個僕人遞給我！"

#. Id: 63301-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:4
msgid "Sword!"
msgstr "劍！"

#. Id: 63301-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:5
msgid "Dab my forehead, servant!"
msgstr "輕拍我的額頭，僕人！"

#. Id: 63301-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63301-Dialogue.mod:6
msgid "Servant! Forehead dabbing duties!"
msgstr "僕人！額頭輕拍的職責！"

#. Id: 63304-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (63302-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Noble hunt attack
#. Id: 63334-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63333-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: noble manhunter defeated squad
#: 63304-Dialogue.mod:0 63334-Dialogue.mod:0
msgid "Yes, my lord."
msgstr "是的我的主。"

#. Id: 63306-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63290-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 63306-Dialogue.mod:0
msgid "This one's a pretty thing..."
msgstr "這個很漂亮......"

#. Id: 63307-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (63306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63307-Dialogue.mod:0
msgid "Her head will look lovely mounted on my wall."
msgstr "她的頭看起來很可愛，掛在我的牆上。"

#. Id: 63314-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Target Race(s): Skeleton
#: 63314-Dialogue.mod:0
msgid "A Skeleton! This one's a rare one!"
msgstr "骷髏！這是一個罕見的！"

#. Id: 63314-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Target Race(s): Skeleton
#: 63314-Dialogue.mod:1
msgid "There! A Skeleton in all it's rarity..."
msgstr "那裡！所有這一切都很罕見的骷髏......"

#. Id: 63314-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Target Race(s): Skeleton
#: 63314-Dialogue.mod:2
msgid "An endangered Skeleton! Today must be my lucky day!"
msgstr "瀕臨滅絕的骷髏！今天一定是我的幸運日！"

#. Id: 63314-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#. -- Condition: Target Race(s): Skeleton
#: 63314-Dialogue.mod:3
msgid "Such a magnificent sight... An endangered Skeleton!"
msgstr "如此壯觀的景象......瀕臨滅絕的骷髏！"

#. Id: 63313-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (63314-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63313-Dialogue.mod:0
msgid "Can't let you escape my bolt now, can I, old chap?"
msgstr "現在不能讓你逃脫我的閂，我可以，老兄？"

#. Id: 63313-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (63314-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63313-Dialogue.mod:1
msgid "I want his head for my collection!"
msgstr "我想把他的腦袋收藏起來！"

#. Id: 63313-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (63314-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63313-Dialogue.mod:2
msgid "Another treasure for my trophy collection!"
msgstr "我的獎杯收藏的另一個寶藏！"

#. Id: 63313-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (63314-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble hunt attack
#: 63313-Dialogue.mod:3
msgid "His head will look excellent on my mantel!"
msgstr "他的頭在我的壁爐架上看起來很棒！"
