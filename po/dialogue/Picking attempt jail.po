#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:0
msgid "Don't give up now"
msgstr "現在不要放棄"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:1
msgid "Try again"
msgstr "再試一次"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:2
msgid "They'll spot us if you don't hurry."
msgstr "如果你不趕快，他們會發現我們。"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:3
msgid "Quickly."
msgstr "很快。"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:4
msgid "Faster."
msgstr "快點。"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:5
msgid "Keep trying."
msgstr "繼續嘗試。"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:6
msgid "Try harder."
msgstr "更加努力。"

#. Id: 14177-Dialogue (4).mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#: 14177-Dialogue?(4).mod:7
msgid "Any time now..."
msgstr "現在任何時候......"

#. Id: 14178-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14178-Dialogue?(4).mod:0
msgid "Shh. Not so loud."
msgstr "噓。別那麼大聲。"

#. Id: 14178-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14178-Dialogue?(4).mod:1
msgid "Is it that hard?"
msgstr "這難嗎？"

#. Id: 14178-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14178-Dialogue?(4).mod:2
msgid "Come on, quicker."
msgstr "來吧，更快。"

#. Id: 14178-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14178-Dialogue?(4).mod:3
msgid "Hurry it up."
msgstr "快點吧。"

#. Id: 14178-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14178-Dialogue?(4).mod:4
msgid "Keep at it."
msgstr "堅持下去。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:0
msgid "Come on. You can do this."
msgstr "來吧。你可以這樣做。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:1
msgid "Good try."
msgstr "不錯的嘗試。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:2
msgid "One more time."
msgstr "再一次。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:3
msgid "We'll make it."
msgstr "我們會做到的。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:4
msgid "Just keep at it."
msgstr "堅持下去吧。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:5
msgid "We'll be out of here soon. I know it."
msgstr "我們很快就會離開這裡。我知道。"

#. Id: 14179-Dialogue (4).mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Picking attempt jail
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#: 14179-Dialogue?(4).mod:6
msgid "Don't give up."
msgstr "不要放棄。"
