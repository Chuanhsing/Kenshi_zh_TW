#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:0
msgid "Why!? "
msgstr "為什麼！？"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:1
msgid "Why me!?"
msgstr "為什麼是我！？"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:2
msgid "Please, no!"
msgstr "請不！"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. Id: 6004-Dialogue.mod
#. -- Index: 0.15
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: In cannibal cage
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: In town(s) of faction(s): Cannibals
#: 7083-Dialogue.mod:3 6004-Dialogue.mod:15
msgid "Please don't hurt me!"
msgstr "請不要傷害我！"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:4
msgid "Why are you doing this to me?!"
msgstr "你為什麼這樣對我？！"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:5
msgid "I don't deserve this!"
msgstr "我不值得這個！"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:6
msgid "Wh- why!?"
msgstr "為什麼！？"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:7
msgid "Wh- why do you do this?!"
msgstr "你為什麼這樣做？！"

#. Id: 7083-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: STRONGER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7083-Dialogue.mod:8
msgid "Why?! Whyyy?!"
msgstr "為什麼？！ Whyyy？"

#. Id: 7087-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 7087-Dialogue.mod:0
msgid "Huh?! You won't get away with this!"
msgstr "咦？你不會僥倖逃脫！"

#. Id: 7087-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 7087-Dialogue.mod:1
msgid "What?! You'll never get away with this!"
msgstr "什麼？！你永遠不會僥倖逃脫！"

#. Id: 7087-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 7087-Dialogue.mod:2
msgid "You /CRAZY/ /BASTARD/! "
msgstr ""

#. Id: 7093-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7093-Dialogue.mod:0
msgid "Please! No!"
msgstr "請！沒有！"

#. Id: 7093-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7093-Dialogue.mod:1
msgid "Please! I have no chance against all of you!"
msgstr "請！我沒有機會反對你們所有人！"

#. Id: 7093-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. Id: 7097-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7093-Dialogue.mod:2 7097-Dialogue.mod:0
msgid "Have mercy!"
msgstr "憐憫！"

#. Id: 7093-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IS_OUTNUMBERED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#: 7093-Dialogue.mod:3
msgid "Please! I can't beat all of you!"
msgstr "請！我不能打敗你們所有人！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:1
msgid "I'm unarmed! Have mercy!"
msgstr "我沒有武裝！憐憫！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:2
msgid "I have no weapon! Please stop!"
msgstr "我沒有武器！請停止！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:3
msgid "No! Please!"
msgstr "沒有！請！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:4
msgid "Wh- why!? Why!??"
msgstr "為什麼！？為什麼！？？"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:7
msgid "Please, I'm unarmed!"
msgstr "拜託，我沒有武裝！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:8
msgid "Please, I yield! I yield!"
msgstr "拜託，我屈服！我屈服了！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:9
msgid "Why?! Please, no!"
msgstr "為什麼？！請不！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.10
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:10
msgid "Why?! I'm unarmed!"
msgstr "為什麼？！我沒有武裝！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.11
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:11
msgid "Why?! Have mercy!"
msgstr "為什麼？！憐憫！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.12
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:12
msgid "Please, have mercy!"
msgstr "請憐憫！"

#. Id: 7097-Dialogue.mod
#. -- Index: 1.15
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#: 7097-Dialogue.mod:15
msgid "Mercy!"
msgstr "憐憫！"

#. Id: 7100-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: WEAKER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 7100-Dialogue.mod:0
msgid "Think you can take me?! /IDIOT/..."
msgstr ""

#. Id: 7100-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: WEAKER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 7100-Dialogue.mod:1
msgid "You really think you have a chance against ME? /QUICKLAUGH/"
msgstr ""

#. Id: 7100-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: WEAKER_THAN_ME=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 7100-Dialogue.mod:2
msgid "Big mistake! I'll squash you like an insect!"
msgstr "大錯！我會像昆蟲一樣壓扁你！"

#. Id: 13688-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 13688-Escapes.mod:0
msgid "I can't even defend myself! You /CUR/!"
msgstr ""

#. Id: 13688-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 13688-Escapes.mod:1
msgid "Have you no honour? I'm unarmed!"
msgstr "你沒有榮譽嗎？我沒有武裝！"

#. Id: 13688-Escapes.mod
#. -- Index: 1.2
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 13688-Escapes.mod:2
msgid "You /CUR/! Do you enjoy attacking unarmed /MEN/?"
msgstr ""

#. Id: 13688-Escapes.mod
#. -- Index: 1.3
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: ME, Condition: IM_UNARMED=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 13688-Escapes.mod:3
msgid "I'm unarmed, you /CUR/!"
msgstr ""

#. Id: 15965-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#: 15965-Dialogue.mod:0
msgid "Huh?! Why are you doing this?!"
msgstr "咦？你為什麼做這個？！"

#. Id: 15965-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#: 15965-Dialogue.mod:1
msgid "Why?!!!"
msgstr "為什麼？！！！"

#. Id: 43991-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (13682-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Interrupted by target
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=1, Tag: -1
