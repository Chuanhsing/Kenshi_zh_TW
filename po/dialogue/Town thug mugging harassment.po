#. Id: 18298-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:0
msgid "Shouldn't have pissed me off..."
msgstr "不應該生氣我..."

#. Id: 18298-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:1
msgid "Should never have pissed me off"
msgstr "永遠不應該生氣"

#. Id: 18298-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:2
msgid "Shoulda just done what I said"
msgstr "應該做的就是我說的"

#. Id: 18298-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:3
msgid "Shouldn't have disobeyed me, /BOY/"
msgstr ""

#. Id: 18298-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:4
msgid "Yer in my BAD books now"
msgstr "你現在在我的BAD書中"

#. Id: 18298-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:5
msgid "Got me in a BAD mood now"
msgstr "現在讓我心情不好"

#. Id: 18298-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:6
msgid "Got me feelin' crabby now"
msgstr "讓我覺得'蟹'現在"

#. Id: 18298-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18298-Dialogue.mod:7
msgid "Got you on my naughty list now"
msgstr "現在讓你在我的頑皮名單上"

#. Id: 18304-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:0
msgid "Now we'll have to do this the hard way"
msgstr "現在我們必須以艱難的方式做到這一點"

#. Id: 18304-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:1
msgid "I guess we'll just have to do this the painful way"
msgstr "我想我們只需要以痛苦的方式做到這一點"

#. Id: 18304-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:2
msgid "Now I'm gonna have to take the Cats from your /CORPSE/"
msgstr ""

#. Id: 18304-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:3
msgid "Now I'm just gonna have to beat your Cats out of you"
msgstr "現在我要打敗你的貓了"

#. Id: 18304-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:4
msgid "The guards ain't gonna do shit to help you now!"
msgstr "守衛現在不會幫你了！"

#. Id: 18304-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (18298-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18304-Dialogue.mod:5
msgid "The guards can't always protect you, ya know!"
msgstr "你知道，警衛不能總是保護你！"

#. Id: 18299-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18299-Dialogue.mod:0
msgid "You won't like me when I'm angry"
msgstr "我生氣的時候你不會喜歡我的"

#. Id: 18299-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18299-Dialogue.mod:1
msgid "You ain't gonna like me when I'm mad"
msgstr "我瘋了的時候你不會喜歡我的"

#. Id: 18299-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18299-Dialogue.mod:2
msgid "You don't wanna see me when I'm REALLY angry"
msgstr "當我真的生氣時，你不想見我"

#. Id: 18300-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18299-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18300-Dialogue.mod:0
msgid "I get real stabby!"
msgstr "我變得真正的邋！！"

#. Id: 18300-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18299-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18300-Dialogue.mod:1
msgid "I like to stab stuff when I'm angry!"
msgstr "我生氣的時候喜歡刺東西！"

#. Id: 18300-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18299-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18300-Dialogue.mod:2
msgid "I like to smash stuff when I'm angry. You know, faces and that"
msgstr "我生氣的時候喜歡粉碎東西。你知道，面孔和那個"

#. Id: 18300-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (18299-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18300-Dialogue.mod:3
msgid "I get real murdery!"
msgstr "我得到了真正的謀殺！"

#. Id: 18303-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:0
msgid "GIVE ME YOUR CATS!"
msgstr "給我你的貓！"

#. Id: 18303-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:1
msgid "Gimme your money NOW!"
msgstr "現在給我錢！"

#. Id: 18303-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:2
msgid "I'm confiscating your /CATS/ from you!"
msgstr ""

#. Id: 18303-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:3
msgid "/LAUGH/! The guards can't save you now!"
msgstr ""

#. Id: 18303-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:4
msgid "If you won't give me your money, I'll take it!"
msgstr "如果你不給我錢，我會接受的！"

#. Id: 18303-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:7
msgid "Just have to take the Cats from your /CORPSE/!"
msgstr ""

#. Id: 18303-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:8
msgid "No guards about now, /BOY/! DIE!"
msgstr ""

#. Id: 18303-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 18303-Dialogue.mod:9
msgid "Ripe for the pickin's... come 'ere!"
msgstr "為了挑選而成熟......來吧！"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:0
msgid "I love a good fight!"
msgstr "我喜歡好戰！"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:2
msgid "Gonna get you!"
msgstr "要你的！"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:3
msgid "You're a piece of work, ain't ya?"
msgstr "你是一件工作，不是嗎？"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:4
msgid "Peekaboo!"
msgstr "躲貓貓！"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:5
msgid "Gonna get me some /CATS/!"
msgstr ""

#. Id: 18306-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:6
msgid "I bite, mind!"
msgstr "我咬了，記住！"

#. Id: 18306-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:7
msgid "BOO! /LAUGH/!"
msgstr ""

#. Id: 18306-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: BRAVE
#: 18306-Dialogue.mod:10
msgid "Come see my new knife!"
msgstr "快來看看我的新刀！"

#. Id: 18307-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18307-Dialogue.mod:0
msgid "Gimme your money!"
msgstr "給我你的錢！"

#. Id: 18307-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18307-Dialogue.mod:1
msgid "Stabby time!"
msgstr "Stabby時間！"

#. Id: 18307-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (18306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18307-Dialogue.mod:4
msgid "You won't escape me"
msgstr "你不會逃避我"

#. Id: 18307-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (18306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18307-Dialogue.mod:5
msgid "You can't hide from me"
msgstr "你無法向我隱瞞"

#. Id: 18307-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (18306-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18307-Dialogue.mod:6
msgid "STAB!"
msgstr "刺！"

#. Id: 18309-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18308-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: COPS_AROUND=1, Tag: -1
#: 18309-Dialogue.mod:0
msgid "/FUCKING/ guards! You got off lucky this time..."
msgstr ""

#. Id: 18309-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18308-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: COPS_AROUND=1, Tag: -1
#: 18309-Dialogue.mod:1
msgid "Yer lucky the guards are out..."
msgstr "你很幸運衛兵出局了......"

#. Id: 18310-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (18309-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18310-Dialogue.mod:0
msgid "But I'll get you next time"
msgstr "但是下次我會幫你的"

#. Id: 18310-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (18309-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#: 18310-Dialogue.mod:1
msgid "But next time I'ma stab you up"
msgstr "但是下次我會刺傷你"
