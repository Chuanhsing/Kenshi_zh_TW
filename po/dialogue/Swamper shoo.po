#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 12351-Dialogue?(10).mod:0
msgid "Well, /WHOTHEFUCK/ are you?"
msgstr ""

#. Id: 12351-Dialogue (10).mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 12351-Dialogue?(10).mod:2
msgid "And /WHOTHEFUCK/ are you?"
msgstr ""

#. Id: 12351-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 12351-Dialogue?(10).mod:3
msgid "How the hell d' YOU get in?"
msgstr "你到底怎麼了？"

#. Id: 12351-Dialogue (10).mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 12351-Dialogue?(10).mod:4
msgid "Who let the /INSULT/ in?"
msgstr ""

#. Id: 12350-Dialogue (10).mod
#. -- Index: 1.0
#. -- Parent line: 0 (12351-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 12350-Dialogue?(10).mod:0
msgid "Go on, /GETLOST/"
msgstr ""

#. Id: 42323-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42323-Dialogue.mod:0
msgid "I said /GETLOST/!!"
msgstr ""

#. Id: 42323-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42323-Dialogue.mod:1
msgid "You tryin' to steal from me?! Eh, /INSULT/!?"
msgstr ""

#. Id: 42323-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42323-Dialogue.mod:2
msgid "I ain't tellin' you again, /INSULT/!"
msgstr ""

#. Id: 42323-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42323-Dialogue.mod:3
msgid "I ain't a patient /MEMAN/, /INSULT/! /GETLOST/!"
msgstr ""

#. Id: 47442-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:1
msgid "Intruders get cut!"
msgstr "入侵者被切斷了！"

#. Id: 47442-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:2
msgid "Then let me show you the door, /QUICKLAUGH/..."
msgstr ""

#. Id: 47442-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:3
msgid "That's it! I'll take you out myself!"
msgstr "而已！我會帶你出去的！"

#. Id: 47442-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:4
msgid "You ain't welcome here!"
msgstr "你在這裡不受歡迎！"

#. Id: 47442-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:5
msgid "Annoyin' son of a bitch!"
msgstr "Annoyin'婊子的兒子！"

#. Id: 47442-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (42323-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#: 47442-Dialogue.mod:6
msgid "No more waiting! Get out and stay out!"
msgstr "沒等了！滾出去吧！"

#. Id: 45415-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45415-Dialogue.mod:0
msgid "Good riddance..."
msgstr "甩掉包袱..."

#. Id: 45415-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45415-Dialogue.mod:1
msgid "Stay out..."
msgstr "待在外面......"

#. Id: 45415-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45415-Dialogue.mod:2
msgid "Keep out and stay out..."
msgstr "保持外出..."

#. Id: 45415-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (12350-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamper shoo
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#: 45415-Dialogue.mod:3
msgid "That's it, get movin'..."
msgstr "就是這樣，得到移動......"
