#. Id: 17539-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_ALLY=0, Tag: -1
#: 17539-Dialogue.mod:1
msgid "I need no help from you"
msgstr "我不需要你的幫助"

#. Id: 17539-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_ALLY=0, Tag: -1
#: 17539-Dialogue.mod:2
msgid "Leave me, I need no help"
msgstr "離開我，我不需要幫助"

#. Id: 17539-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_ALLY=0, Tag: -1
#: 17539-Dialogue.mod:3
msgid "I can handle it myself"
msgstr "我可以自己處理"

#. Id: 17540-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#: 17540-Dialogue.mod:1
msgid "Nothing but a scratch"
msgstr "只有划痕"

#. Id: 17540-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#: 17540-Dialogue.mod:2
msgid "I've suffered worse"
msgstr "我的情況更糟"

#. Id: 17540-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: HONORABLE
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=0, Tag: -1
#: 17540-Dialogue.mod:4
msgid "I've had worse injuries"
msgstr "我受傷更嚴重了"

#. Id: 17546-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#: 17546-Dialogue.mod:0
msgid "This little scratch? They got lucky, that is all"
msgstr "這個小划痕？他們很幸運，就是這樣"

#. Id: 17546-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#: 17546-Dialogue.mod:1
msgid "It's just a scratch. I got careless, that's all"
msgstr "這只是一個划痕。我粗心大意，就是這樣"

#. Id: 17546-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#: 17546-Dialogue.mod:2
msgid "I won't be broken, I'll only grow back stronger"
msgstr "我不會被打破，我只會變得更強壯"

#. Id: 17548-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17548-Dialogue.mod:0
msgid "Ah..."
msgstr "啊..."

#. Id: 17548-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17548-Dialogue.mod:1
msgid "Much better..."
msgstr "好多了..."

#. Id: 17548-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 17548-Dialogue.mod:2
msgid "That's good..."
msgstr "非常好..."

#. Id: 17550-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: BROKEN_LEG=1, Tag: -1
#: 17550-Dialogue.mod:0
msgid "It will take more than a broken leg to keep ME down"
msgstr "讓我失望，需要的不僅僅是斷腿"

#. Id: 17550-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: BROKEN_LEG=1, Tag: -1
#: 17550-Dialogue.mod:1
msgid "I'll be walking again in no time"
msgstr "我馬上就會再次走路"

#. Id: 17550-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: BROKEN_LEG=1, Tag: -1
#. Id: 17552-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: DAMAGED_HEAD=1, Tag: -1
#: 17550-Dialogue.mod:2 17552-Dialogue.mod:2
msgid "They can break my bones all they want, but they'll never break my spirit"
msgstr "他們可以隨心所欲地打破我的骨頭，但他們永遠不會打破我的精神"

#. Id: 17550-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: BROKEN_LEG=1, Tag: -1
#: 17550-Dialogue.mod:3
msgid "Just a couple of broken bones, that's all"
msgstr "只是幾塊破碎的骨頭，就是這樣"

#. Id: 17550-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: BROKEN_LEG=1, Tag: -1
#: 17550-Dialogue.mod:4
msgid "Break my bones and they'll only grow back stronger"
msgstr "打破我的骨頭，他們只會變得更強壯"

#. Id: 17552-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: DAMAGED_HEAD=1, Tag: -1
#: 17552-Dialogue.mod:0
msgid "It will take more than a cracked skull to keep ME down"
msgstr "讓我失望的不僅僅是破裂的頭骨"

#. Id: 17552-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: DAMAGED_HEAD=1, Tag: -1
#: 17552-Dialogue.mod:1
msgid "My skull is sturdy. I'll be fit again in no time"
msgstr "我的頭骨很堅固。我很快就會適應"

#. Id: 17552-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek being healed
#. -- Condition: Who: TARGET, Condition: I_LOVE_THIS_GUY=1, Tag: -1
#. -- Condition: Who: ME, Condition: DAMAGED_HEAD=1, Tag: -1
#: 17552-Dialogue.mod:3
msgid "Just a cracked skull, that's all"
msgstr "只是一個破裂的頭骨，就是這樣"
