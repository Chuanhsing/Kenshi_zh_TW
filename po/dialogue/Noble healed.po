#. Id: 18489-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=0, Tag: -1
#: 18489-Dialogue.mod:0
msgid "What do you want, a cookie?!"
msgstr "你想要什麼，一個cookie？！"

#. Id: 18489-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=0, Tag: -1
#: 18489-Dialogue.mod:1
msgid "What do you want, a medal?!"
msgstr "你想要什麼，一枚獎牌？！"

#. Id: 18491-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18489-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18491-Dialogue.mod:0
msgid "Get back to work!"
msgstr "回去工作！"

#. Id: 18492-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18492-Dialogue.mod:0
msgid "Good... Now get away from me"
msgstr "好......現在離開我吧"

#. Id: 18492-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18492-Dialogue.mod:1
msgid "Good... Now begone"
msgstr "好......現在開始了"

#. Id: 18494-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18492-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18494-Dialogue.mod:0
msgid "Your breath stinks!"
msgstr "你的嘴真臭！"

#. Id: 18494-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18492-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18494-Dialogue.mod:1
msgid "Your breath reeks!"
msgstr "你的呼吸充滿了氣息！"

#. Id: 18494-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (18492-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18494-Dialogue.mod:2
msgid "You STINK of poor!"
msgstr "你好窮！"

#. Id: 18495-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18495-Dialogue.mod:0
msgid "You shall be my new favourite /PEASANT/ for the day"
msgstr ""

#. Id: 18495-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18495-Dialogue.mod:1
msgid "Good /BOY2/. You have served me well."
msgstr ""

#. Id: 18495-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18495-Dialogue.mod:2
msgid "Today you have served me well, minion"
msgstr "今天你服務我很好，僕從"

#. Id: 18495-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18495-Dialogue.mod:3
msgid "Good. Now get out of my way."
msgstr "好。現在離開我的路。"

#. Id: 18495-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 18495-Dialogue.mod:4
msgid "Good /BOY2/. Now give me my space."
msgstr ""

#. Id: 18497-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18497-Dialogue.mod:0
msgid "I feel like I've been hit by a leviathyn! A LEVIATHYN!"
msgstr "我覺得我被leviathyn擊中了！一個LEVIATHYN！"

#. Id: 18497-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#: 18497-Dialogue.mod:1
msgid "I feel like I've been in a fight with a cheese grater."
msgstr "我覺得我和奶酪刨絲器打架了。"

#. Id: 18498-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (18497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18498-Dialogue.mod:0
msgid "*sigh*... now I am in a most foul mood"
msgstr "*嘆氣......現在我的情緒最糟糕"

#. Id: 18498-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (18497-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Noble healed
#: 18498-Dialogue.mod:1
msgid "Now I am feeling QUITE unhappy"
msgstr "現在我感到很不開心"
