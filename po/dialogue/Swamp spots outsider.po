#. Id: 53725-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:0
msgid "Say goodbye to all morals and say hello to the swamps!"
msgstr ""

#. Id: 53725-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:1
msgid "Fancy lending a Cat or two, /OUTLANDER/?"
msgstr ""

#. Id: 53725-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:2
msgid "One day I'll rule this swamp. Screw the Hounds."
msgstr ""

#. Id: 53725-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:3
msgid "Hasn't anybody in this god forsaken city ever heard of a bath? "
msgstr ""

#. Id: 53725-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:4
msgid "Hoy, I need to lay off the hashish..."
msgstr ""

#. Id: 53725-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53725-Dialogue.mod:5
msgid "Nothin' worse than facing the day sober"
msgstr ""

#. Id: 53736-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53736-Dialogue.mod:0
msgid "One day I'll make my riches..."
msgstr ""

#. Id: 53737-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53736-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53737-Dialogue.mod:0
msgid "Then I'll move to the United Cities and fill my belly 'til I sprout a second chin."
msgstr ""

#. Id: 53738-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#: 53738-Dialogue.mod:0
msgid "One of these days I'll make it rich..."
msgstr ""

#. Id: 53775-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (53738-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53775-Dialogue.mod:0
msgid "And I'll buy m'self a hundred slave wenches in the United Cities!"
msgstr ""

#. Id: 53739-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 53739-Dialogue.mod:0
msgid "Euch!... The looks of a goddess but the breath of a dead dog..."
msgstr ""

#. Id: 61671-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61671-Dialogue.mod:0
msgid "Well lah-dee-dah, a fat Cat merchant straight outta the desert, eh?"
msgstr ""

#. Id: 61671-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61671-Dialogue.mod:1
msgid "Lost your way outta the desert, fat Cats?"
msgstr ""

#. Id: 61674-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61671-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61674-Dialogue.mod:0
msgid "/DAMN/ Guilders... want all the drug monies for 'emselves but they can't touch us! "
msgstr ""

#. Id: 61674-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (61671-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61674-Dialogue.mod:1
msgid "/DAMN/ Guilders... 'n all yer rules. Well you ain't rulin' my swamps!"
msgstr ""

#. Id: 61675-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (61674-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61675-Dialogue.mod:0
msgid "/LAUGH/, you can't touch us, lord licker!"
msgstr ""

#. Id: 61672-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61672-Dialogue.mod:0
msgid "Got us some high flyin' chump straight outta the desert, eh?"
msgstr ""

#. Id: 61672-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61672-Dialogue.mod:1
msgid "Kch, don't much like Empire fat Cats lordin' 'round my turf..."
msgstr ""

#. Id: 61672-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61672-Dialogue.mod:2
msgid "Seem to have lost your way outta the desert, fat Cats..."
msgstr ""

#. Id: 61673-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (61672-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Swamp spots outsider
#: 61673-Dialogue.mod:0
msgid "Better watch where ya step around 'ere, lord licker."
msgstr ""

