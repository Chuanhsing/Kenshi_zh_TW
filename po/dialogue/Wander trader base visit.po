#. Id: 62063-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62063-Dialogue.mod:0
msgid "Well, /HELLO/!"
msgstr "那個, /HELLO/!"

#. Id: 62063-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62063-Dialogue.mod:1
msgid "Nice place you got out here..."
msgstr "你們這地方不錯啊..."

#. Id: 62063-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62063-Dialogue.mod:2
msgid "*pant* *pant*"
msgstr "*喘氣* *喘氣*"

#. Id: 62063-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62063-Dialogue.mod:3
msgid "Hoy, /HELLO/!"
msgstr "喂, /HELLO/!"

#. Id: 62064-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62063-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62064-Dialogue.mod:0
msgid "Open for trade?"
msgstr "來交易嗎?"

#. Id: 62064-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (62063-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62064-Dialogue.mod:1
msgid "Looking to stock up on supplies?"
msgstr "在找地方補給?"

#. Id: 62067-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62064-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62067-Dialogue.mod:0
msgid "I bring only the highest quality goods..."
msgstr "我隻賣最上乘的商品..."

#. Id: 62067-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (62064-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62067-Dialogue.mod:1
msgid "I bring all the necessities, right to your doorstop..."
msgstr "各種必需品我應有盡有, 全送你家門口"

#. Id: 62067-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (62064-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62067-Dialogue.mod:2
msgid "I have a good selection for you..."
msgstr "我幫你挑了件好的..."

#. Id: 62077-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62067-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62077-Dialogue.mod:0
msgid "Come see me when you're ready."
msgstr "準備好了就來我這看看."

#. Id: 62077-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (62067-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62077-Dialogue.mod:1
msgid "Take a look at my stock."
msgstr "來看看我的庫存."

#. Id: 62079-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62078-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62079-Dialogue.mod:0
msgid "Leaving now..."
msgstr "走了啊..."

#. Id: 62730-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62080-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. Id: 62074-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62073-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. Id: 62097-Dialogue.mod
#. -- Index: 14.0
#. -- Parent line: 13 (62096-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62730-Dialogue.mod:0 62074-Dialogue.mod:0 62097-Dialogue.mod:0
msgid "Trade caravan going!"
msgstr "交易商隊走了!"

#. Id: 62081-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62730-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62081-Dialogue.mod:0
msgid "Have a good day!"
msgstr "過得愉快!"

#. Id: 62068-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. Id: 62070-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (62069-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. Id: 62082-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>1, Tag: -1
#: 62068-Dialogue.mod:0 62070-Dialogue.mod:0 62082-Dialogue.mod:0
msgid "Trade caravan!"
msgstr "交易商隊!"

#. Id: 62069-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (62068-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#. Id: 62071-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (62070-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62069-Dialogue.mod:0 62071-Dialogue.mod:0
msgid "Trade caravan waiting!"
msgstr "交易商隊暫駐!"

#. Id: 62072-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62071-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62072-Dialogue.mod:0
msgid "When you're ready!"
msgstr "做好準備!"

#. Id: 62075-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62074-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62075-Dialogue.mod:0
msgid "Going!"
msgstr "走了!"

#. Id: 62076-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62075-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62076-Dialogue.mod:0
msgid "Gone!"
msgstr "出發!"

#. Id: 62087-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (62086-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Wander trader base visit
#: 62087-Dialogue.mod:0
msgid "Boss, I'm thirsty..."
msgstr "頭, 我渴了..."

#. Id: 62088-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (62087-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62088-Dialogue.mod:0
msgid "Then go drink some water."
msgstr "去喝點水吧."

#. Id: 62089-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (62088-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62089-Dialogue.mod:0
msgid "... Trade caravan!"
msgstr "... 交易商隊!"

#. Id: 62090-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (62089-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Wander trader base visit
#: 62090-Dialogue.mod:0
msgid "I ran out."
msgstr "我喝光了."

#. Id: 62091-Dialogue.mod
#. -- Index: 8.0
#. -- Parent line: 7 (62090-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62091-Dialogue.mod:0
msgid "Then go find a cactus to suck on."
msgstr "那就去找顆仙人掌吸一吸."

#. Id: 62092-Dialogue.mod
#. -- Index: 9.0
#. -- Parent line: 8 (62091-Dialogue.mod)
#. -- Speaker: Extra 1 (2)
#. -- Group title: Wander trader base visit
#: 62092-Dialogue.mod:0
msgid "Just gimme some water."
msgstr "直接給我點水吧."

#. Id: 62093-Dialogue.mod
#. -- Index: 10.0
#. -- Parent line: 9 (62092-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62093-Dialogue.mod:0
msgid "I already gave you your allowance!"
msgstr "我已經給過你限額了!"

#. Id: 62094-Dialogue.mod
#. -- Index: 11.0
#. -- Parent line: 10 (62093-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Wander trader base visit
#: 62094-Dialogue.mod:0
msgid "I'm not MADE of water."
msgstr "我不是水做的."

#~ msgid "'Til next time..."
#~ msgstr "再見..."

#~ msgid "*Cough*"
#~ msgstr "*咳嗽*"

#~ msgid "Kch. "
#~ msgstr "咳."
