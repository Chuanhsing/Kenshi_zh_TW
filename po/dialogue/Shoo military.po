#. Id: 15636-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15636-Dialogue.mod:0
msgid "No foreigners!"
msgstr "沒有外國人！"

#. Id: 15636-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15636-Dialogue.mod:1
msgid "No outlanders allowed"
msgstr "沒有外國人允許"

#. Id: 15636-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15636-Dialogue.mod:2
msgid "No foreigners allowed"
msgstr "沒有外國人允許"

#. Id: 15636-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. Id: 10727-Dialogue (10).mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bad Relations Empire <-45
#: 15636-Dialogue.mod:3 10727-Dialogue?(10).mod:3
msgid "You're not welcome here"
msgstr "你在這裡不受歡迎"

#. Id: 15636-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15636-Dialogue.mod:4
msgid "You're not allowed here, barbarian"
msgstr "你不允許這裡，野蠻人"

#. Id: 15636-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15636-Dialogue.mod:5
msgid "You're not allowed here, savage"
msgstr "野蠻人，你不允許在這裡"

#. Id: 15638-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (15636-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 15638-Dialogue.mod:0
msgid "Leave. I won't ask again."
msgstr "離開。我不會再問了。"

#. Id: 15638-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15636-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 15638-Dialogue.mod:1
msgid "Get out. Don't make me ask you again."
msgstr "出去。別再讓我問你了。"

#. Id: 15638-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (15636-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 15638-Dialogue.mod:2
msgid "Clear out!"
msgstr "清除！"

#. Id: 15638-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (15636-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 15638-Dialogue.mod:4
msgid "Take leave!"
msgstr "拜別！"

#. Id: 42193-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (15638-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42193-Dialogue.mod:1
msgid "I warned you! Arrest /HIM/!"
msgstr ""

#. Id: 42193-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (15638-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42193-Dialogue.mod:2
msgid "Then don't leave! Get /HIM/!"
msgstr ""

#. Id: 42193-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (15638-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42193-Dialogue.mod:3
msgid "No? Then rot in jail!"
msgstr "沒有？然後在監獄腐爛！"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. Id: 42840-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:0 42840-Dialogue.mod:0
msgid "Off limits. Leave here."
msgstr "禁區。離開這裡。"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. Id: 42840-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:1 42840-Dialogue.mod:4
msgid "You're not allowed here, leave at once"
msgstr "你不能在這裡，馬上離開"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:2
msgid "You're not allowed here, clear out"
msgstr "你不能在這裡，明確表示"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:3
msgid "You're not allowed here, leave"
msgstr "你不能在這裡離開"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. Id: 42840-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:4 42840-Dialogue.mod:5
msgid "You're not allowed here, take exit"
msgstr "你不能在這裡，退出"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:5
msgid "Out of bounds, get out"
msgstr "出界，出去"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:6
msgid "Out of bounds, clear off"
msgstr "出界，清除"

#. Id: 15635-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 15635-Dialogue.mod:7
msgid "Out of bounds, leave"
msgstr "出界，離開"

#. Id: 47317-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (15635-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 47317-Dialogue.mod:1
msgid "Leave now."
msgstr "現在離開。"

#. Id: 42179-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42179-Dialogue.mod:0
msgid "Are you deaf? I said leave, /WORM/!"
msgstr ""

#. Id: 42179-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42179-Dialogue.mod:1
msgid "I'm talking to you, /PEASANT/! LEAVE!"
msgstr ""

#. Id: 42179-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42179-Dialogue.mod:2
msgid "Got sand in your ears, /WORM/?! GO!"
msgstr ""

#. Id: 42179-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (47317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42179-Dialogue.mod:3
msgid "Why are you still here, /PEASANT/?! I said leave!"
msgstr ""

#. Id: 42179-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (47317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42179-Dialogue.mod:5
msgid "I won't ask you twice, /WORM/!"
msgstr ""

#. Id: 42840-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 42840-Dialogue.mod:1
msgid "You're not allowed in here, what are you doing? Leave."
msgstr "你不被允許進入這裡，你在做什麼？離開。"

#. Id: 42840-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 42840-Dialogue.mod:2
msgid "What are you doing? Leave, you're not allowed here."
msgstr "你在做什麼？離開，你不能在這裡。"

#. Id: 42840-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 42840-Dialogue.mod:3
msgid "Out of bounds. Get out."
msgstr "出界。出去。"

#. Id: 42840-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#: 42840-Dialogue.mod:6
msgid "Out of bounds. Leave at once."
msgstr "出界。馬上離開。"

#. Id: 47318-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42840-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 14706-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (12103-Dialogue (10).mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 47318-Dialogue.mod:1 14706-Dialogue.mod:1
msgid "Leave. Now."
msgstr "離開。現在。"

#. Id: 42182-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:0
msgid "/GREASY/ woman! Why aren't you leaving?! Do as you're told!"
msgstr ""

#. Id: 42182-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:1
msgid "Damn you, /TEMPTRESS/! Obey me, leave!"
msgstr ""

#. Id: 42182-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:2
msgid "Damn /TEMPTRESS/! I said leave!"
msgstr ""

#. Id: 42182-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:3
msgid "/GREASY/ woman! Why aren't you leaving?! Go!"
msgstr ""

#. Id: 42182-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:4
msgid "I said get out, woman! Leave!"
msgstr "我說出去了，女人！離開！"

#. Id: 42182-Dialogue.mod
#. -- Index: 2.5
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 40944-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:5 40944-Dialogue.mod:1
msgid "Leave, woman!"
msgstr "離開，女人！"

#. Id: 42182-Dialogue.mod
#. -- Index: 2.6
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:6
msgid "Get out, woman! Or I'll have you thrown in jail!"
msgstr "滾出去，女人！或者我會把你扔進監獄！"

#. Id: 42182-Dialogue.mod
#. -- Index: 2.7
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:7
msgid "Leave! Wretched woman!"
msgstr "離開！猥瑣的女人！"

#. Id: 42182-Dialogue.mod
#. -- Index: 2.8
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42182-Dialogue.mod:8
msgid "I said leave, wretched woman! Get out!"
msgstr "我說離開，可憐的女人！出去！"

#. Id: 42180-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42180-Dialogue.mod:0
msgid "Why aren't you leaving?! You're trespassing, get out!"
msgstr "你為什麼不離開？！你擅自闖入，滾出去！"

#. Id: 42180-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42180-Dialogue.mod:1
msgid "I said leave! No trespassing!"
msgstr "我說離開！沒有擅自進入！"

#. Id: 42180-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42180-Dialogue.mod:2
msgid "You there! You're not welcome here, don't make me ask you twice!"
msgstr "你在那裡！你在這裡不受歡迎，不要讓我問你兩次！"

#. Id: 42180-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42180-Dialogue.mod:3
msgid "I'm talking to you, /OUTLANDER/! Leave!"
msgstr ""

#. Id: 42180-Dialogue.mod
#. -- Index: 2.4
#. -- Parent line: 1 (47318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42180-Dialogue.mod:4
msgid "Why are you still here, /OUTLANDER/? I won't ask you twice!"
msgstr ""

#. Id: 53513-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (42180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 53513-Dialogue.mod:0
msgid "No more warnings!"
msgstr "沒有更多的警告！"

#. Id: 53513-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (42180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 53513-Dialogue.mod:2
msgid "This is your last warning!"
msgstr "這是你的最後警告！"

#. Id: 53513-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (42180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 53513-Dialogue.mod:3
msgid "I won't ask you again, intruder!"
msgstr "我不會再問你了，入侵者！"

#. Id: 53513-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (42180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 53513-Dialogue.mod:4
msgid "Don't make me tell you again, trespasser!"
msgstr "不要讓我再告訴你，侵入者！"

#. Id: 53513-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (42180-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 53513-Dialogue.mod:5
msgid "Ignore me and you'll pay, intruder!"
msgstr "忽略我，你會支付，入侵者！"

#. Id: 42184-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (53513-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42184-Dialogue.mod:0
msgid "I warned you...!"
msgstr "我警告過你...！"

#. Id: 42184-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (53513-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42184-Dialogue.mod:1
msgid "Trespassing won't be tolerated!"
msgstr "不會容忍擅自進入！"

#. Id: 42184-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (53513-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42184-Dialogue.mod:2
msgid "Trespasser! Arrest /HIM/!"
msgstr ""

#. Id: 42184-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (53513-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42184-Dialogue.mod:3
msgid "Don't say I didn't warn you! Punish /HIM/!"
msgstr ""

#. Id: 42184-Dialogue.mod
#. -- Index: 4.4
#. -- Parent line: 3 (53513-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shoo military
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 42184-Dialogue.mod:4
msgid "Still here? Then you leave me no choice!"
msgstr "還在？然後你別無選擇！"
