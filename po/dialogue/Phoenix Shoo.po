#. Id: 40942-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40942-Dialogue.mod:0
msgid "You're interrupting my meditation, child!"
msgstr "孩子，你在打擾我的冥想！"

#. Id: 40942-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40942-Dialogue.mod:1
msgid "Leave me, I'm praying, I need silence"
msgstr "離開我，我在祈禱，我需要沉默"

#. Id: 40942-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40942-Dialogue.mod:2
msgid "Silence! You interrupt my prayers!"
msgstr "安靜！你打斷了我的祈禱！"

#. Id: 40943-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40942-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40943-Dialogue.mod:0
msgid "Leave me!"
msgstr "離開我吧！"

#. Id: 40943-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (40942-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40943-Dialogue.mod:1
msgid "Leave now!"
msgstr "現在離開！"

#. Id: 40943-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (40942-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 40944-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 40946-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40943-Dialogue.mod:3 40944-Dialogue.mod:6 40946-Dialogue.mod:4
msgid "Go, now!"
msgstr "現在出發！"

#. Id: 40945-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40945-Dialogue.mod:0
msgid "You're not supposed to be here, why do you disturb my meditation?"
msgstr "你不應該在這裡，為什麼你打擾我的冥想？"

#. Id: 40945-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40945-Dialogue.mod:1
msgid "What are you doing here?! You disturb the Holy Phoenix!"
msgstr "你在這裡做什麼？！你打擾了聖鳳凰！"

#. Id: 40945-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40945-Dialogue.mod:2
msgid "You're not welcome here. Why do you disturb the Holy Lord Phoenix?!"
msgstr "你在這裡不受歡迎。你為什麼打擾聖靈鳳凰？！"

#. Id: 40945-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 40945-Dialogue.mod:3
msgid "Who are you? You're interrupting my meditation, you shouldn't be here"
msgstr "你是誰？你打擾了我的冥想，你不應該在這裡"

#. Id: 40944-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40944-Dialogue.mod:0
msgid "Paladins, get rid of this wretched woman"
msgstr "聖騎士，擺脫這個可憐的女人"

#. Id: 40944-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40944-Dialogue.mod:7
msgid "Wretched woman, get out!"
msgstr "猥瑣的女人，滾出去！"

#. Id: 40944-Dialogue.mod
#. -- Index: 1.8
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40944-Dialogue.mod:8
msgid "Paladins, see to this woman"
msgstr "聖騎士，看到這個女人"

#. Id: 40944-Dialogue.mod
#. -- Index: 1.9
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#. Id: 40946-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40944-Dialogue.mod:9 40946-Dialogue.mod:5
msgid "Paladins, I will not be disturbed!"
msgstr "聖騎士，我不會被打擾！"

#. Id: 40946-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40946-Dialogue.mod:0
msgid "Paladins, get rid of him"
msgstr "聖騎士，擺脫他"

#. Id: 40946-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40946-Dialogue.mod:2
msgid "Get out, now!"
msgstr "現在滾出去吧！"

#. Id: 40946-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (40945-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IN_MY_BUILDING=1, Tag: -1
#: 40946-Dialogue.mod:6
msgid "Paladins, see him out"
msgstr "聖騎士，見他了"

#. Id: 50569-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 50569-Dialogue.mod:0
msgid "A beast! In my holy quarters!"
msgstr "一個怪獸！在我的聖地！"

#. Id: 50569-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 50569-Dialogue.mod:1
msgid "A dark one! In my holy quarters!"
msgstr "一個黑暗的！在我的聖地！"

#. Id: 50569-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#: 50569-Dialogue.mod:2
msgid "An abomination! In my sacred quarters!"
msgstr "憎惡！在我神聖的宿舍！"

#. Id: 50571-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (50569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 50571-Dialogue.mod:0
msgid "The darkness comes for us! /BURNHIM/, lest we all become /TAINT/ed!"
msgstr ""

#. Id: 50571-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (50569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 50571-Dialogue.mod:1
msgid "The darkness will not /TAINT/ me! Paladins, assist! /BURNHIM/!"
msgstr ""

#. Id: 50571-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (50569-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Phoenix Shoo
#: 50571-Dialogue.mod:2
msgid "/BURNHIM/! Our city must be cleansed!"
msgstr ""
